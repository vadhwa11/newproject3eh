	<queryString><![CDATA[SELECT 'Payment Dispensing Advice ' as payment_dispensing_advice,
(select nomenclature from mas_store_item msi where msi.item_id = d.item_id) as  drugdescription,
(select batch_no from store_item_batch_stock sib  where sib.item_id = d.item_id and sib.stock_id = d.batch_id) as  batchno,
 b.*,d.* FROM bl_pymnt_advice_disp_header b,
bl_pymnt_advice_disp_details d
,patient p
where p.hin_no=$P{hinNo} and b.pymnt_advice_disp_header_id=d.pymnt_advice_disp_header_id
and b.hin_id=p.hin_id
and inpatient_id is null
 and b.hospital_id=$P{hospitalId}
;]]></queryString>

