<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="RouteOfAdministration" table="route_of_administration">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="route_id">
			<generator class="sequence">
				<param name="sequence">route_of_administration_route_id_seq</param>
			</generator>
		</id>

		<property name="RouteCode" column="route_code" type="string"
			not-null="false" length="8" />
		<property name="RouteName" column="route_name" type="string"
			not-null="false" length="32" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" length="13" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<property name="OrderNo" column="order_no" type="integer"
			not-null="false" length="8" />
		<many-to-one name="LastChgBy" column="last_chg_by" class="Users"
		
			not-null="false">
		</many-to-one>


		<set name="PatientPrescriptionDetails" inverse="true">
			<key column="route_id" />
			<one-to-many class="PatientPrescriptionDetails" />
		</set>


	</class>
</hibernate-mapping>