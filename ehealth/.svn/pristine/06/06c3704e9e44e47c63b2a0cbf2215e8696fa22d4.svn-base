<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="HesEquipmentCallLogEntry"
		table="hes_equipment_call_log_entry"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="java.lang.Long"
			column="equipment_call_id"
		>
			<generator class="sequence">
				<param name="sequence">hes_equipment_call_log_entry_equipment_call_id_seq</param>
			</generator>
		</id>

		<property
			name="EntryNo"
			column="entry_no"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Date"
			column="date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="CallDate"
			column="call_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="CallTime"
			column="call_time"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Employee"
			column="employee_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Equipment"
			column="equipment_id"
			class="HesEquipmentMaster"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>