	<parameter name="fromEmpId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="toEmpId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
     mas_rank.`rank_name` as mas_rank_rank_name,
     concat(mas_employee.`first_name`," ",mas_employee.`middle_name`," ",mas_employee.`last_name`)as ename,
     mas_employee.`join_date` as mas_employee_join_date,
     mas_employee.`employee_code` as mas_employee_employee_code,
     mas_hospital.`hospital_name` as mas_hospital_hospital_name,
     hr_employee_experience.`exp_yrs` as tot_year,
     concat(hr_employee_experience.`exp_yrs`," years ",hr_employee_experience.`exp_months`," months")as exper,
     hr_employee_experience.`designation` as hr_employee_experience_designation
from
     `mas_employee` mas_employee left outer join `mas_hospital` mas_hospital on mas_employee.`hospital_id` = mas_hospital.`hospital_id`
     left outer join `mas_rank` mas_rank on mas_employee.`rank_id` = mas_rank.`rank_id`
     left outer join `hr_employee_experience` hr_employee_experience on mas_employee.`experience_id` = hr_employee_experience.`experience_id`
where

    mas_employee.`employee_id`  between $P{fromEmpId} and $P{toEmpId}
]]></queryString>

