<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrAdvanceDetail" table="hr_advance_detail">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="advance_detail_id">
			<generator class="sequence">
				<param name="sequence">hr_advance_detail_advance_detail_id_seq</param>
			</generator>
		</id>

		<property name="RecoveredAmount" column="recovered_amount"
			type="big_decimal" not-null="false" length="11" />
		<property name="RecoveryDate" column="recovery_date" type="date"
			not-null="false" />
		<property name="Remark" column="remark" type="string"
			not-null="false" length="40" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="10" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="Hospital" column="hospital_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="Advance" column="advance_id" class="HrAdvance"
			not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>