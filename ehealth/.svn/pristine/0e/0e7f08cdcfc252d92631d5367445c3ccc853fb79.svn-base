	<queryString><![CDATA[SELECT
     (patient.p_first_name::text || ' ' || coalesce(patient.p_middle_name,'') || ' ' || coalesce(patient.p_last_name,''))as patientname,
     mas_administrative_sex.administrative_sex_name AS mas_administrative_sex_administrative_sex_name,
     mas_diagnosis_conclusion.diagnosis_conclusion_name AS mas_diagnosis_conclusion_diagnosis_conclusion_name,
     visit.hospital_staff AS visit_hospital_staff,
     visit.age AS visit_age,
     (mas_hospital.hospital_name::text || ' ' || coalesce(mas_hospital.address,''))as hospitalName,
     patient.hin_no AS patient_hin_no,
     mas_department.department_name AS mas_department_department_name
FROM
     patient patient LEFT OUTER JOIN visit visit ON patient.hin_id = visit.hin_id
     LEFT OUTER JOIN mas_administrative_sex mas_administrative_sex ON patient.sex_id = mas_administrative_sex.administrative_sex_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON mas_hospital.hospital_id =$P{HOSPITAL_ID}
     LEFT OUTER JOIN mas_diagnosis_conclusion mas_diagnosis_conclusion ON visit.diagnosis_id = mas_diagnosis_conclusion.diagnosis_conclusion_id
     LEFT OUTER JOIN mas_department mas_department ON visit.department_id = mas_department.department_id
WHERE
     visit.visit_date = $P{date} and visit.hospital_id = $P{HOSPITAL_ID}]]></queryString>

