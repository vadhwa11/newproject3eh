<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="OpdNursingPatientDetails"
		table="opd_nursing_patient_details"
	>
		<meta attribute="sync-DAO">false</meta>


		<id
			name="Id"
			type="integer"
			column="opd_nursing_patient_details_id"
		>
			<generator class="sequence">
				<param name="sequence">opd_nursing_patient_details_opd_nursing_patient_details_id_seq</param>
			</generator>
		</id>

		<property
			name="PresentComplaintHistory"
			column="present_complaint_history"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="PastIllnessHistory"
			column="past_illness_history"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="PresentHistory"
			column="present_history"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="FamilyHistory"
			column="family_history"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="MadicationHistory"
			column="madication_history"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Pulse"
			column="pulse"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Temperature"
			column="temperature"
			type="java.lang.Float"
			not-null="false"
			length="4"
		/>
		<property
			name="Bp"
			column="bp"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Height"
			column="height"
			type="java.lang.Double"
			not-null="false"
			length="10"
		/>
		<property
			name="Weight"
			column="weight"
			type="java.lang.Double"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Bmi"
			column="bmi"
			type="java.lang.Float"
			not-null="false"
			length="11"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Visit"
			column="visit_id"
			class="Visit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>

	</class>	
</hibernate-mapping>