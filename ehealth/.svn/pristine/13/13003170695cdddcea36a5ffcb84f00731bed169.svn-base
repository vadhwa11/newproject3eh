	<queryString><![CDATA[SELECT
     mas_store_item.pvms_no AS mas_store_item_pvms_no,
     mas_store_item.nomenclature AS mas_store_item_nomenclature,
     mas_store_section.section_name AS mas_store_section_section_name,
mas_item_type.item_type_name,
mas_store_group.group_name, CASE
    WHEN   mas_store_item.status = 'y' or mas_store_item.status='Y' THEN 'Active'
    ELSE 'Inactive'
 	 END as status
FROM
     mas_store_item  mas_store_item 
LEFT OUTER JOIN mas_store_section mas_store_section  ON  mas_store_section.section_id = mas_store_item.section_id
LEFT OUTER JOIN mas_store_group mas_store_group  ON  mas_store_group.group_id = mas_store_item.group_id
LEFT OUTER JOIN mas_item_type mas_item_type  ON  mas_item_type.item_type_id = mas_store_item.item_type_id
$P!{qry}
ORDER BY mas_store_item.nomenclature]]></queryString>

