<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="DeliveryDetails"
		table="delivery_details"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="delivery_id">
			<generator class="sequence">
				<param name="sequence">delivery_details_delivery_id_seq</param>
			</generator>
		</id>

		<property
			name="AdNo"
			column="ad_no"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="BloodLoss"
			column="blood_loss"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Placenta"
			column="placenta"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="RuptureUterus"
			column="rupture_uterus"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="DateOfLabor"
			column="date_of_labor"
			type="timestamp"
			not-null="false"
			length="29"
		/>
		<property
			name="Treatment"
			column="treatment"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="TimeOfLabor"
			column="time_of_labor"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Puperium"
			column="puperium"
			type="string"
			not-null="false"
		/>
		<property
			name="MothersCondition"
			column="mothers_condition"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Pulse"
			column="pulse"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Perinum"
			column="perinum"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Bp"
			column="bp"
			type="integer"
			not-null="false"
			length="15"
		/>
		<property
			name="AdditionalNotes"
			column="additional_notes"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
	 		name="Complications"
		 	column="complications"
		  	type="string"
			not-null="false" 
			length="45" 
			/>
			
		<property
			name="StageFiveComplications"
			column="stage_five_complications"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="Gestation"
			column="gestation"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="DeliveryDuration"
			column="delivery_duration"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="StageThree"
			column="stage_three"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Episiotomy"
			column="episiotomy"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Anaesthesia"
			column="anaesthesia"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="PerinealTears"
			column="perineal_tears"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="Bleeding"
			column="bleeding"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="BloodTransfusion"
			column="blood_transfusion"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="SutureMaterial"
			column="suture_material"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="StateOfUterus"
			column="state_of_uterus"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Lactation"
			column="lactation"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="OtherRemarks"
			column="other_remarks"
			type="string"
			not-null="false"
			length="300"
		/>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="ConductedBy"
			column="conducted_by"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="AssistedBy"
			column="assisted_by"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>