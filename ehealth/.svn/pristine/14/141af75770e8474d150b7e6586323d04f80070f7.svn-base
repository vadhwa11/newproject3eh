	</parameter>
	<parameter name="inTimeFrom" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     hr_attendance_loader.date AS hr_attendance_loader_date,
     hr_attendance_loader.time_in AS hr_attendance_loader_time_in,
     hr_attendance_loader.time_out AS hr_attendance_loader_time_out,
     mas_employee.employee_code AS mas_employee_employee_code,
     hr_mas_shift_codes.shift_name AS hr_mas_shift_codes_shift_name,
     (mas_employee.first_name+ISNULL(mas_employee.middle_name,'')+ISNULL(mas_employee.last_name,''))AS mas_employee_first_name,
     hr_attendance_loader.out_date AS hr_attendance_loader_out_date,
    mas_hospital.hospital_name AS mas_hospital_hospital_name,
    cast(datediff(minute,(hr_attendance_loader.out_date+''+hr_attendance_loader.time_out),
  (hr_attendance_loader.date+''+hr_attendance_loader.time_in))as char) As difference
FROM

      hr_attendance_loader hr_attendance_loader left join hr_mas_shift_codes  hr_mas_shift_codes ON hr_mas_shift_codes.shift_codes_id = hr_attendance_loader.shift_codes_id
     INNER JOIN mas_employee mas_employee ON hr_attendance_loader.employee_id = mas_employee.employee_id
LEFT JOIN mas_hospital mas_hospital ON mas_employee.hospital_id = mas_hospital.hospital_id
WHERE

      hr_attendance_loader.date between $P{fromDate}
     and $P{toDate} and hr_attendance_loader.time_in> $P{inTimeFrom}]]></queryString>

