input.button, input.buttonHighlight, input[type="button"], input[type="submit"], input[type="reset"] {
	border: none;	
    border-radius: 4px; 
	color: #FFFFFF;
	font-size: 12px;
	font-family: arial, tahoma, sans-serif;
	font-weight:bold;
	text-align: center;
	height: 25px;
	float: left;
	margin: 5px 5px 5px 0px;	
	padding: 0px;
	text-transform:uppercase;
