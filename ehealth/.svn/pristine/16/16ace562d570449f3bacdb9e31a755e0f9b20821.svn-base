div#menu UL UL { 
	width:260px;
	POSITION: absolute;
	z-index: 1;
	margin: -14px 0px 0px -70px;	
	background: -webkit-linear-gradient(#1f1e1e, #b7b7b7, #1f1e1e);
	background: -o-linear-gradient(#1f1e1e, #b7b7b7, #1f1e1e);
	background: -moz-linear-gradient(#1f1e1e, #b7b7b7, #1f1e1e);
	background: linear-gradient(#1f1e1e, #b7b7b7, #1f1e1e);
}

div#menu UL UL LI {
	/* border-style: outset; */
    border-color: #0000 ;
	PADDING: 0px;
	MARGIN: 0px;
	WIDTH: 100%;
	TEXT-ALIGN: left;
	COLOR: #ffffff;
	text-shadow: none;
	background: none;
	text-indent: 10px;
	line-height: 30px;
	height: 30px;
	vertical-align: middle;
}

div#menu UL UL LI A {
	BORDER: none;
	font-size:11px;
	COLOR: #ffffff;
	text-shadow:none;
	FONT-WEIGHT: lighter;
	border-radius:0px;
	height: 30px;
	text-align: left;
	padding: 0 0 0 5px;	
}

div#menu UL UL LI A:hover {
	 color: #000000;
	 padding: 0 0 0 5px;
	 background: -webkit-linear-gradient(#ffffff, #ADDFFF, #ffffff);
	 background: -o-linear-gradient(#ffffff, #ADDFFF, #ffffff);
	 background: -moz-linear-gradient(#ffffff, #ADDFFF, #ffffff);
	 background: linear-gradient(#ffffff, #ADDFFF, #ffffff);	
}

div#menu UL.level2 {
	LEFT: -1px
}

