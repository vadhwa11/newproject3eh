/*--- Button DropDown Css Start ---*/
.dropDwnbuttn {
    background:#ffd966;
    color: white;
    padding:5px 10px;
    font-size:12px;
    font-weight:bold;
    color:#000;
    border: none;
    border-top:solid 1px #ccc;
}
.dropDownMainDiv {
    position: relative;
    display: inline-block;
}
.dropDownContent {
    display: none;
    position: absolute;
    background: #f1f1f1;
    min-width:150px;
    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
    z-index: 1;
}
.dropDownContent a {
    color: black;
    padding:6px 10px;
    text-decoration: none;
    display: block;
}
.dropDownContent a:hover {background-color: #ddd}
.dropDownMainDiv:hover .dropDownContent {display: block;}
.dropDownMainDiv:hover .dropDwnbuttn {background-color:#ddd;}
/*--- Button DropDown Css end ---*/

