	<queryString><![CDATA[SELECT aa.*, (SELECT hospital_name FROM  mas_hospital m WHERE m.hospital_id = aa.hospital_id) as hname
FROM (SELECT * FROM (SELECT hin_no, hin_id as hinid, 
(p_first_name + ' ' + p_middle_name + ' ' + p_last_name) as name,
hospital_id FROM  patient WHERE	patient_type_id  in (SELECT patient_type_id 
FROM  mas_patient_type WHERE patient_type_code  = 'stf')
and hin_no = $P{hinno}
) AS  firstsql
LEFT OUTER JOIN
(SELECT * FROM (SELECT b.hin_id, ip.ad_no as adno,chg_slp_amt as bamt,
chg_slp_date as bdate, 0 as visit_no
FROM  bl_charge_slip_main b LEFT JOIN inpatient ip ON b.inpatient_id  = ip.inpatient_id  
UNION ALL
SELECT b.hin_id,ip.ad_no as adno,bill_amt as bamt,bill_date as bdate,
0 as visit_no FROM  bl_dispensing_header b 
LEFT JOIN inpatient ip ON b.inpatient_id  = ip.inpatient_id  
WHERE b.inpatient_id  is not null 
UNION ALL
SELECT b.hin_id,0 as adno,bill_amt,bill_date,
CASE WHEN b.visit_id is null THEN 0 ELSE v.visit_no END as visit_id
FROM  bl_op_bill_header b LEFT JOIN visit v ON b.visit_id  = v.visit_id  
UNION ALL
SELECT hin_id, 0 as adno, bill_amt as bamt, bill_date as bdate, 0 as visit_id
FROM  bl_dispensing_header b  WHERE	 inpatient_id  is null
) AS  mainsql 
where bdate between $P{fromdate} and $P{todate}
) AS  secondsql ON firstsql.hinid  = secondsql.hin_id  
) AS  aa]]></queryString>

