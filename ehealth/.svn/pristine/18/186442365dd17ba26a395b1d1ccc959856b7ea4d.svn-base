	<queryString><![CDATA[SELECT
     blood_stock_detail.`blood_bag_no` AS blood_stock_detail_blood_bag_no,
     blood_stock_main.`expiry_date` AS blood_stock_main_expiry_date,
     blood_stock_main.`collection_date` AS blood_stock_main_collection_date,
     blood_stock_main.`name` AS blood_stock_main_name,
     CONCAT(patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`)as pat_name,
     CONCAT(patient_A.`p_first_name`," ",patient_A.`p_middle_name`," ",patient_A.`p_last_name`)as patissue_name,
     if(patient.`hin_no` is null,"--",patient.`hin_no`) AS patient_hin_no,
     mas_blood_group.`blood_group_name` AS mas_blood_group_blood_group_name,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
     blood_issue_header.`monthly_no` AS blood_issue_header_monthly_no,
     patient_A.`hin_no` AS patient_A_hin_no 
    
FROM
     `blood_stock_main` blood_stock_main RIGHT OUTER JOIN `blood_stock_detail` blood_stock_detail ON blood_stock_main.`stock_id` = blood_stock_detail.`stock_main_id`
     LEFT OUTER JOIN `mas_blood_group` mas_blood_group ON blood_stock_main.`blood_group_id` = mas_blood_group.`blood_group_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON blood_stock_main.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `patient` patient ON blood_stock_main.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `blood_issue_detail` blood_issue_detail ON blood_stock_detail.`id` = blood_issue_detail.`stock_detail_id`
     RIGHT OUTER JOIN `blood_issue_header` blood_issue_header ON blood_issue_detail.`issue_header_id` = blood_issue_header.`id`
     LEFT OUTER JOIN `patient` patient_A ON blood_issue_header.`hin_id` = patient_A.`hin_id`
    
WHERE
     blood_stock_main.`collection_date` between $P{fromDate} and $P{toDate}
     and blood_stock_detail.`blood_issued` = "y"]]></queryString>

