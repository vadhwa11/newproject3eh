	<queryString><![CDATA[select sum(isnull(count1,0)) as c1,sum(isnull(qty,0)) as qty,
sum(isnull(amount,0)) as amt,mmc.main_chargecode_name as main_name ,
(select hospital_name from mas_hospital where hospital_id= main.hospital_id) as hospital_name,
(select address from mas_hospital where hospital_id= main.hospital_id) as hospital_address
from
(
SELECT count(*) as count1,charge_id,hospital_id,sum(isnull(advice_qty,0)) as qty,
sum(isnull(advice_amt,0)) as amount FROM bl_payment_advice_details d,
bl_payment_advice_header b
where b.payment_advice_header_id=d.payment_advice_header_id
and b.cash_advice_amt is not null
and payment_advice_date between $P{fromDate} and $P{toDate}
group by hospital_id,charge_id) as main
left outer join mas_charge_code m on main.charge_id=m.charge_code_id
left outer join mas_main_chargecode mmc on m.main_chargecode_id=mmc.main_chargecode_id
group by mmc.main_chargecode_name,main.hospital_id]]></queryString>

