<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="HesEquipmentAmcDetailsEntry"
		table="hes_equipment_amc_details_entry"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="amc_id"
		>
		<generator class="sequence">
			<param name="sequence">hes_equipment_amc_details_entry_amc_id_seq</param>
			</generator>
		</id>

		<property
			name="SerialNo"
			column="serial_no"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="DateOfInstallation"
			column="date_of_installation"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="AmcWarrentyStartDate"
			column="amc_warrenty_start_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="AmcWarrentyEndDate"
			column="amc_warrenty_end_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="CostOfAmc"
			column="cost_of_amc"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="AdvBillNo"
			column="adv_bill_no"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="AdvBillDate"
			column="adv_bill_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="AdvBillAmount"
			column="adv_bill_amount"
			type="integer"
			not-null="false"
			length="20"
		/>
		<property
			name="BalanceBillNo"
			column="balance_bill_no"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="BalanceBillDate"
			column="balance_bill_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="BalanceBillAmount"
			column="balance_bill_amount"
			type="integer"
			not-null="false"
			length="20"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="PreventiveCycle"
			column="preventive_cycle"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="PreventiveCompletedCycle"
			column="preventive_completed_cycle"
			type="integer"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="Epuipment"
			column="epuipment_id"
			class="HesEquipmentMaster"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="SupplierGroup"
			column="supplier_group_id"
			class="MasStoreSupplierGroup"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>