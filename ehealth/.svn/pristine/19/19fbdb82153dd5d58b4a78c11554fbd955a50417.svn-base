	<queryString><![CDATA[select m.order_no,
     cim.instrument_name,
     m.order_date,
    me.first_name+' '+me.middle_name+' '+me.last_name as order_by,
      me1.first_name+' '+me1.middle_name+' '+me1.last_name as received_by,
     sum(t.qty) AS order_qty,
     m.order_date,
     SUM(rt.qty) as received_qty,
     md.department_name,rm.receipt_date
      from cssd_autoclave_request_t t
left outer join cssd_autoclave_request_m m on m.request_id=t.request_m_id
left outer join cssd_autoclave_receipt_m rm on rm.request_id=m.request_id
left outer join cssd_autoclave_receipt_t rt on rt.receipt_m_id=rm.receipt_id
left outer join mas_employee me on me.employee_id=m.order_by
left outer join mas_employee me1 on me1.employee_id=rm.received_by
left outer join cssd_instrument_master cim on cim.instrument_id=t.instrument_id
left outer join mas_department md on md.department_id=m.department_id
where m.order_date between $P{fromDate} and $P{toDate} and  me1.first_name is not null and me1.middle_name is not null and me1.last_name is not null
group by m.order_no,
     cim.instrument_name,
     m.order_date,me.first_name,me.middle_name,me.last_name,md.department_name,
     me.first_name,me.middle_name,me.last_name,me1.first_name,me1.middle_name,me1.last_name,rm.receipt_date]]></queryString>

