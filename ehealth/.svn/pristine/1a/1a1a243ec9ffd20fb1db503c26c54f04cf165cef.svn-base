<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrLeaveDetailsHistory" table="hr_leave_details_history">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_leave_details_history_id_seq</param>
			</generator>
		</id>

		<property name="Empid" column="empid" type="integer"
			not-null="false" length="10" />
		<property name="FromDate" column="from_date" type="date"
			not-null="false" />
		<property name="ToDate" column="to_date" type="date"
			not-null="false" />
		<property name="NoOfWorkingDays" column="no_of_working_days"
			type="string" not-null="false" length="6" />
		<property name="ApprovedBy" column="approved_by" type="integer"
			not-null="false" length="10" />
		<property name="ApprovedOn" column="approved_on" type="date"
			not-null="false" />
		<property name="JoiningDate" column="joining_date" type="date"
			not-null="false" />
		<property name="Reason" column="reason" type="string"
			not-null="false" length="200" />
		<property name="ContactAddress" column="contact_address"
			type="string" not-null="false" length="100" />
		<property name="ContactPhone" column="contact_phone" type="string"
			not-null="false" length="15" />
		<property name="AppliedOn" column="applied_on" type="date"
			not-null="false" />
		<property name="Suggestion" column="suggestion" type="string"
			not-null="false" length="250" />
		<property name="ModifiedBy" column="modified_by" type="integer"
			not-null="false" length="10" />
		<property name="ModifiedOn" column="modified_on" type="date"
			not-null="false" />
		<property name="DisapprovedReason" column="disapproved_reason"
			type="string" not-null="false" length="250" />
		<property name="AlternateEmailId" column="alternate_email_id"
			type="string" not-null="false" length="50" />
		<property name="HalfDay" column="half_day" type="string"
			not-null="false" length="15" />
		<property name="DeleteReason" column="delete_reason" type="string"
			not-null="false" length="250" />
		<property name="Type" column="type" type="integer" not-null="false"
			length="5" />

		<property name="Status" column="status" type="integer"
			not-null="false" length="5" />
			
			<property
			name="SuffixFromDate"
			column="suffix_from_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="SuffixToDate"
			column="suffix_to_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="PrefixFromDate"
			column="prefix_from_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="PrefixToDate"
			column="prefix_to_date"
			type="date"
			not-null="false"
			length="13"
		/>
	

		<many-to-one name="leaveType" class="HrMasLeaveTypeMediator"
			column="type" update="false" insert="false">
		</many-to-one>

		<many-to-one name="leaveStatus" class="HrMasLeaveStatus"
			column="status" update="false" insert="false">
		</many-to-one>

		<many-to-one name="empId" class="jkt.hms.masters.business.MasEmployee"
			column="empid" update="false" insert="false">
		</many-to-one>

		<many-to-one name="empIdBal" column="balance_id" class="HrEmployeeBalanceNew"
			not-null="false">
		</many-to-one>

	</class>
</hibernate-mapping>