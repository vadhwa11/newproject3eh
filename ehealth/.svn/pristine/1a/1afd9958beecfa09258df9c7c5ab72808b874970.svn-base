<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="Holidaycalendar" table="hr_holidaycalendar">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_holidaycalendar_id_seq</param>
			</generator>
		</id>

		<property name="Title" column="title" type="string" not-null="false"
			length="75" />
		<property name="HolidayDate" column="holiday_date" type="date"
			not-null="false" />
		<property name="Rh" column="rh" type="string" not-null="false"
			length="5" />
		<property name="HolidayListYear" column="holiday_list_year"
			type="string" not-null="false" length="4" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
	</class>
</hibernate-mapping>