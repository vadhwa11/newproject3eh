     physio_requisition_detail.no_of_days as noOfDays,
     physio_requisition_detail.remark as remarks,
     mas_frequency.frequency_name as freqName
    
 from physio_requisition_detail physio_requisition_detail 
left join physio_requisition_header physio_requisition_header 
on physio_requisition_detail.physio_requisition_header_id = physio_requisition_header.physio_requisition_id
 left join patient patient on physio_requisition_header.hin_id = patient.hin_id
