	<queryString><![CDATA[select mst.pvms_no,mst.nomenclature ,
batch_no,expiry_date,sb.cost_price,sum(closing_stock) as qty ,(sb.cost_price * sum(closing_stock)) as total
from store_item_batch_stock as sb  inner join mas_store_item as  mst
on sb.item_id = mst.item_id
where sb.department_id= $P{deptId} and closing_stock > 0 and sb.hospital_id= $P{hospitalId}
group by mst.pvms_no,mst.nomenclature,batch_no ,expiry_date,sb.cost_price]]></queryString>

