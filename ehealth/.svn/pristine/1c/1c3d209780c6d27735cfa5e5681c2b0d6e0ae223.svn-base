	<queryString><![CDATA[select
     visit.age as visit_age,
     CONCAT(patient.p_first_name,' ',patient.p_middle_name,patient.p_last_name) as patientname,
     patient.hin_no as patient_hin_no,
     mas_hospital.hospital_name as hospital_name,
     mas_hospital.address as hospital_address
from
     patient patient inner join visit visit on patient.hin_id = visit.hin_id
      left outer join mas_hospital mas_hospital on patient.hospital_id=mas_hospital.hospital_id
where visit.visit_date between $P{fromDate} and $P{toDate} and patient.hin_no=$P{hinNo} and visit.hospital_id = $P{HOSPITAL_ID}]]></queryString>

