	<queryString><![CDATA[select mas_diet.diet_id as diet_id,mas_diet.diet_name as Diet,
mas_department.department_name as Department,
mas_hospital.hospital_name as hospital_name,mas_hospital.address as address,
patient_diet_indoor_header.dateofcreation as dateofcreation
from patient_diet_indoor_detail patient_diet_indoor_detail
left outer join patient_diet_indoor_header patient_diet_indoor_header on patient_diet_indoor_detail.h_id=patient_diet_indoor_header.id
left outer join mas_department mas_department on mas_department.department_id =patient_diet_indoor_header.department_id
left outer join mas_diet mas_diet on mas_diet.diet_id =patient_diet_indoor_detail.diet_id
left outer join inpatient inpatient on inpatient.inpatient_id=patient_diet_indoor_detail.inpatient_id
left outer join mas_hospital mas_hospital on inpatient.hospital_id=mas_hospital.hospital_id
where patient_diet_indoor_header.department_id=$P{departmentId} and inpatient.ad_status='A' 
and  patient_diet_indoor_header.dateofcreation >=$P{FROM_DATE} and patient_diet_indoor_header.dateofcreation<=$P{TO_DATE}]]></queryString>

