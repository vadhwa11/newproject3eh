	<queryString>
		<![CDATA[SELECT
	dr.disease_id,
	ms.name,
	ms.aadhaar_no,
	(hs.lsg_house_no || ' ' || hs.address || ' ' || hs.street_name || ' ') as house_details,
	pc.post_code_name,
	pc.pin_code,
	hs.contact_no,
	ax.administrative_sex_name,
	(age || ' ' || 'Years') as age,
	diagnosis,
	diagnosis_type,
	date_onset,
	dr.hospital_name as hospital_sought,
	hospitalised,
	investigation_status,
	infection_source,
	maesure_preventive,
	admission_date as planned_date,
	other_diagnosis,
	inpatient_no,
	follow_date,
	diagnosis_date,
	discharge_date,
	patient_condition,
	medicine_system,
	dr.disease_reg_id,
	dr.reg_date,
	mh.hospital_name,
	mht.hospital_name as parent_hospital
FROM
	ph_disease_registration dr
	LEFT OUTER JOIN ph_member_survey ms ON dr.member_id = ms.member_id
	LEFT OUTER JOIN ph_house_survey hs ON ms.house_id = hs.house_hold_id
	LEFT OUTER JOIN mas_post_code pc on hs.pin_code_id = pc.post_code_id
	LEFT OUTER JOIN ph_disease_registration_follow drf ON dr.member_id = cast(drf.member_id as Integer)
	LEFT OUTER JOIN mas_administrative_sex ax ON ms.gender = ax.administrative_sex_id
	LEFT OUTER JOIN mas_hospital mh ON dr.hospital_id = mh.hospital_id
	LEFT OUTER JOIN mas_hospital mht ON mh.parent_institute_id = mht.hospital_id
WHERE
	dr.disease_type = 'cd'
	and  dr."reg_date" between $P{fromDate} and $P{toDate}
	$P!{query}]]>
	</queryString>
