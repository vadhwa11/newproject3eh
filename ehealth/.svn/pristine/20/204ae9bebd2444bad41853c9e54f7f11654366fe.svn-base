	<queryString><![CDATA[select item_id ,pvms_no,nomenclature from mas_store_item where item_id not  in (
select distinct t.item_id from store_ip_issue_m m
inner join store_ip_issue_t t on m.ip_issue_id = t.ip_issue_id
where
m.ip_issue_date between $P{fromDate}  and $P{toDate}  and m.department_id=$P{deptId} 
union
select distinct t.item_id from store_issue_m m
inner join store_issue_t t on m.id = t.issue_m_id
where
m.issue_date between $P{fromDate}  and $P{toDate}  and m.department_id=$P{deptId} 
union
select distinct t.item_id_issue as item_id from store_op_patient_issue_m m
inner join store_op_patient_issue_t t on m.op_issue_id = t.op_issue_id
where
m.issue_date between $P{fromDate}  and $P{toDate}  and m.department_id=$P{deptId}  and m.hospital_id=$P{hospitalId} 
)

;]]></queryString>

