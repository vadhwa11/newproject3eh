<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrAdvance" table="hr_advance">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="advance_id">
			<generator class="sequence">
				<param name="sequence">hr_advance_advance_id_seq</param>
			</generator>
		</id>

		<property name="AdvanceCode" column="advance_code" type="string"
			not-null="false" length="20" />
		<property name="AdvanceDate" column="advance_date" type="date"
			not-null="false" />
		<property name="AdvanceAmount" column="advance_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="RecoveryMode" column="recovery_mode" type="string"
			not-null="false" length="10" />
		<property name="MonthlyDeduction" column="monthly_deduction"
			type="big_decimal" not-null="false" length="11" />
		<property name="RecoveredAmount" column="recovered_amount"
			type="big_decimal" not-null="false" length="11" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="10" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="RecoveryPeriod" column="recovery_period"
			type="java.lang.Float" not-null="false" length="12" />
		<many-to-one name="Hospital" column="hospital_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="Employee" column="employee_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>