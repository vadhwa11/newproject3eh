	<queryString><![CDATA[select mw.ward_name as ward,pms.house_id as house_no,pms.family_id,pms.sum as no_member, pms.name as head,pov.breeding_water_source_positive as container_positive,mh.hospital_name,pov.eliminated_water_source_count as container_eliminated,pov.fever_cases_count as f_case  from (select  pms1.house_id,pms1.family_id, count(*)as sum, pms2.name from ph_member_survey pms1 left outer join (select family_id,name from ph_member_survey pms where hof_status='Y') pms2 on pms2.family_id=pms1.family_id  group by pms1.house_id,pms1.family_id, pms2.name) pms 
left outer join ph_house_survey phs on phs.house_hold_id=pms.house_id 
left outer join  ph_os_vector  pov on phs.house_hold_id=pov.house_id
left outer join  mas_ward mw on mw.ward_id=phs.ward_id 
left outer join  mas_hospital mh on mh.hospital_id=pov.hospital_id

where 
   pov.reg_date between $P{fromDate} and $P{toDate} 
  $P!{qry}]]></queryString>

