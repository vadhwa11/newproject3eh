<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrPayrollProcessDetail" table="hr_payroll_process_detail">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="payroll_process_detail_id">
			<generator class="sequence">
				<param name="sequence">hr_payroll_process_detail_payroll_process_detail_id_seq</param>
			</generator>
		</id>

		<property name="ElementType" column="element_type" type="string"
			not-null="false" length="30" />
		<property name="PayelementAmount" column="payelement_amount"
			type="big_decimal" not-null="false" length="11" />
		<many-to-one name="PayrollProcessHeader" column="payroll_process_header_id"
			class="HrPayrollProcessHeader" not-null="false">
		</many-to-one>
		<many-to-one name="EmpPayElement" column="emp_pay_element_id"
			class="HrEmployeePayElements" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>