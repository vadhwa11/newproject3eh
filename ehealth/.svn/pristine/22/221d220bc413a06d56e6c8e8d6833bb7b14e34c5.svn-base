	<queryString><![CDATA[select c.pvms_no,
       c.nomenclature,
       d.group_name,
       a.tender_no,
       f.department_name,
       e.hospital_name,
       g.item_unit_name,	
       b.tender_qty
from
  store_tender_m  a,
  store_tender_t  b,
  mas_store_item  c,
  mas_store_group d,
  mas_hospital    e,
  mas_department  f,
  mas_store_item_conversion g

where
      a.id = $P{tender_id}
and   d.group_id = $P{group_id}
and   a.department_id = $P{department_id}
and   b.tender_m_id = a.id
and   b.item_id = c.item_id
and   c.group_id = d.group_id
and   e.hospital_id = a.hospital_id
and   f.department_id = a.department_id
and   g.item_conversion_id = c.item_conversion_id
and   b.item_id not in(select Distinct a.item_id from store_tender_comm_bid_m a, store_tender_comm_bid_t b
where a.id = b.comm_bid_id and
a.group_id = $P{group_id} and
a.tender_id = $P{tender_id} and
a.department_id = $P{department_id} and b.lcat<>0)]]></queryString>

