	<queryString><![CDATA[select (p.p_first_name||' '||p_last_name) as pname,p.age,msi.nomenclature,mf.frequency_name,iph.prescription_date,
iph.prescription_time,ipd.dosage,ipd.no_of_days,ipd.total,mm.manufacturer_name,md.department_name,
(me.first_name||' '||me.last_name) as dname,i.date_of_addmission,mh.hospital_name,mh.address
from inpatient_prescription_header iph 
left outer join inpatient_prescription_details ipd on iph.prescription_id=ipd.prescription_id
left outer join patient p on iph.hin_id=p.hin_id
left outer join mas_store_item msi on ipd.item_id=msi.item_id
left outer join mas_frequency mf on ipd.frequency_id=mf.frequency_id
left outer join mas_manufacturer mm on ipd.manufacturer_id=mm.manufacturer_id
left outer join mas_department md on iph.department_id=md.department_id
left outer join inpatient i on iph.hin_id=i.hin_id and iph.inpatient_id=i.inpatient_id 
left outer join mas_employee me on i.doctor_id=me.employee_id
left outer join mas_hospital mh on iph.hospital_id=mh.hospital_id
where iph.status='n' and iph.department_id=$P{ward_name} and iph.prescription_date >=$P{fromDate} and iph.prescription_date <=$P{toDate}
and iph.hospital_id=$P{hospitalId} 
order by iph.prescription_date asc,pname asc]]></queryString>

