	<queryString><![CDATA[SELECT
     mas_district.district_code AS mas_district_district_code,
     mas_district.district_name AS mas_district_district_name,
	CASE WHEN mas_district.status='Y' or mas_district.status='y'  THEN 'Active' else 'Inactive'
 END as status,
     mas_state.state_name AS mas_state_state_name
FROM
     mas_state mas_state INNER JOIN mas_district mas_district ON mas_state.state_id = mas_district.state_id
$P!{qry}
ORDER BY
     mas_district.district_name]]></queryString>

