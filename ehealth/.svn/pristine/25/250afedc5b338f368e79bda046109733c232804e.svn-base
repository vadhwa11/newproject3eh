<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="UserEmpGroup" table="user_emp_group">
		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="user_emp_id">
			<generator class="sequence">
				<param name="sequence">user_emp_group_user_emp_id_seq</param>
			</generator>
		</id>

		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="15" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<many-to-one name="User" column="user_id" class="Users"
			not-null="false">
		</many-to-one>
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="EmpGroup" column="emp_group_id" class="EmpGroups"
			not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>