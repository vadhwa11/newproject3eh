	<queryString><![CDATA[SELECT
mas_allergy_product.product_name,
opd_patient_allergy_t.allergen,
mas_severity_code.severity_name,
opd_patient_allergy_t.from_month,
opd_patient_allergy_t.from_year,
CASE WHEN opd_patient_allergy_t.status='1' THEN 'Activate' else 'Inactivate'
 END as status
FROM
     inpatient inpatient INNER JOIN opd_patient_allergy_m opd_patient_allergy_m ON inpatient.inpatient_id = opd_patient_allergy_m.inpatient_id
     INNER JOIN opd_patient_allergy_t opd_patient_allergy_t ON opd_patient_allergy_m.m_id = opd_patient_allergy_t.opd_patient_allergy
     INNER JOIN PATIENT PATIENT ON PATIENT.HIN_ID = opd_patient_allergy_m.HIN_ID
     INNER JOIN mas_allergy_product mas_allergy_product ON mas_allergy_product.product_id = opd_patient_allergy_t.allergy_id
inner join mas_severity_code  mas_severity_code on mas_severity_code.severity_id= opd_patient_allergy_t.severity_id
WHERE
    inpatient.inpatient_id=$P{inpatientId}]]></queryString>

