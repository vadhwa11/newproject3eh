	<queryString><![CDATA[select bill_no, hin_no, patient_name,age,
((coalesce(bill_amt,0)-coalesce(discount_amt,0))-
round(coalesce(bill_amt,0)-coalesce(discount_amt,0),3))*(-1) as  roundoff,
(select sum(coalesce(amount,0)) from bl_op_bill_details where
 op_bill_header_id = bd.op_bill_header_id )bill_amt, bd.bill_date,
case when bd.discount_amt is null then 0.0 else bd.discount_amt end as discountamt,
case when outstanding is null then 0.0 else outstanding end as onacc,
coalesce(payable_amt,0) as cash1,
(select hospital_name from mas_hospital where
hospital_id = bd.hospital_id) as hospital,
(select Address from mas_hospital where hospital_id = bd.hospital_id) as Address,

case when (select visit_no from visit where visit_id = bd.hin_id) is null
then 0 else (select visit_no from visit where visit_id = bd.hin_id) end as visitno,

(select receipt_header_id from bl_receipt_header where
op_bill_header_id = bd.op_bill_header_id) as rh,

case when (select amount from bl_receipt_details where payment_mode='C'
and receipt_header_id = (select receipt_header_id from bl_receipt_header where
op_bill_header_id = bd.op_bill_header_id)) is null then 0.0 else
(select amount from bl_receipt_details where payment_mode='C' and
receipt_header_id = (select receipt_header_id from bl_receipt_header where
op_bill_header_id = bd.op_bill_header_id)) end as cashamount,

case when (select sum(amount) from bl_receipt_details where
payment_mode in ('Q','R') and receipt_header_id = (select receipt_header_id from bl_receipt_header where
op_bill_header_id = bd.op_bill_header_id)) is null then 0.0
else (select sum(amount) from bl_receipt_details where
payment_mode in ('Q','R')  and receipt_header_id = (select receipt_header_id from bl_receipt_header where
op_bill_header_id = bd.op_bill_header_id)) end as otheracc
from bl_op_bill_header bd 
where bd.bill_date  between $P{fromDate} and $P{toDate} and bd.hospital_id=$P{hospitalId}
$P!{criteria}

order by bill_no , bill_date]]></queryString>

