<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasServiceStatus" table="mas_service_status">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="service_status_id">
			<generator class="sequence">
				<param name="sequence">mas_service_status_service_status_id_seq</param>
			</generator>
		</id>

		<property name="ServiceStatusCode" column="service_status_code"
			type="string" not-null="false" length="8" />
		<property name="ServiceStatusName" column="service_status_name"
			type="string" not-null="false" length="30" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />


		<set name="Patients" inverse="true">
			<key column="service_status_id" />
			<one-to-many class="Patient" />
		</set>

		<set name="MasRanks" inverse="true">
			<key column="service_status_id" />
			<one-to-many class="MasRank" />
		</set>


	</class>
</hibernate-mapping>