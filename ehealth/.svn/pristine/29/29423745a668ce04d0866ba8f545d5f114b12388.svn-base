	<queryString>
		<![CDATA[select department_name,md.department_id,hospital_name
from mas_institute_department mid
left outer join mas_department md on md.department_id = mid.department_id
left outer join mas_hospital mh on mh.hospital_id = mid.institute_id
where department_type_id =1 and visit_applicable = 'y'
and mid.status = 'y' and institute_id = $P{hospitalId} and
CASE
     WHEN ($P{deptId}!=0) THEN md.emp_dept_id = $P{deptId}
     ELSE 0=0
END and
CASE
     WHEN ($P{serviceCentreId}!=0) THEN md.department_id = $P{serviceCentreId}
     ELSE 0=0
END
order by department_name]]>
	</queryString>
