	<queryString><![CDATA[select m.mlc_no,
	(p.p_first_name || ' ' || coalesce(p.p_middle_name,'') || ' ' || coalesce(p.p_last_name,'')) as patientname,
	m.mlc_date,
  (select (hospital_name || ' , ' || coalesce(address,'')) From mas_hospital where hospital_id = m.hospital_id ) as hospitalname,
	p.hin_no,
	p.age,
date_part('month',m.mlc_date) as month_name,
 (select department_name from mas_department where department_id = (select  department_id from visit where visit_no = m.visit_no and hin_id = m.hin_id limit 1 offset 0 )) as department_name
from
mlc_case  m left outer join patient p on m.hin_id = p.hin_id

where m.hospital_id =$P{HOSPITAL_ID} and m.mlc_date between $P{FROM_DATE} and $P{TO_DATE}]]></queryString>

