<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrBonusDetail" table="hr_bonus_detail">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="bonus_detail_id">
			<generator class="sequence">
				<param name="sequence">hr_bonus_detail_bonus_detail_id_seq</param>
			</generator>
		</id>

		<property name="FromDate" column="from_date" type="date"
			not-null="false" />
		<property name="ToDate" column="to_date" type="date"
			not-null="false" />
		<property name="BonusAmount" column="bonus_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="PaidDate" column="paid_date" type="date"
			not-null="false" />
		<property name="DueDate" column="due_date" type="date"
			not-null="false" />
		<property name="Remarks" column="remarks" type="string"
			not-null="false" length="45" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="10" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="Hospital" column="hospital_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="Bonus" column="bonus_id" class="HrMasBonus"
			not-null="false">
		</many-to-one>
		<many-to-one name="Employee" column="employee_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>