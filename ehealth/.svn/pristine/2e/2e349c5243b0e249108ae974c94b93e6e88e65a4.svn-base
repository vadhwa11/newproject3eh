<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrLoanHeader" table="hr_loan_header">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="loan_header_id">
			<generator class="sequence">
				<param name="sequence">hr_loan_header_loan_header_id_seq</param>
			</generator>
		</id>

		<property name="LoanDate" column="loan_date" type="date"
			not-null="false" />
		<property name="LoanPAmount" column="loan_p_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="LoanInterest" column="loan_interest" type="java.lang.Float"
			not-null="false" length="12" />
		<property name="MonthlyInstall" column="monthly_install"
			type="big_decimal" not-null="false" length="15" />
		<property name="LoanCAmount" column="loan_c_amount" type="big_decimal"
			not-null="false" length="17" />
		<property name="LoanPeriod" column="loan_period" type="integer"
			not-null="false" length="10" />
		<property name="DeductFrom" column="deduct_from" type="string"
			not-null="false" length="10" />
		<property name="LoanStatus" column="loan_status" type="string"
			not-null="false" length="8" />
		<property name="LoanInterestDate" column="loan_interest_date"
			type="date" not-null="false" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="10" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastPaymentDate" column="last_payment_date"
			type="date" not-null="false" />
		<property name="BalanceLoan" column="balanceloan" type="big_decimal"
			not-null="false" length="17" />
		<many-to-one name="Loan" column="loan_id" class="HrMasLoan"
			not-null="false">
		</many-to-one>
		<many-to-one name="Employee" column="employee_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="false">
		</many-to-one>

		<set inverse="true" name="HrLoanDetails">
			<key column="loan_header_id" />
			<one-to-many class="HrLoanDetail" />
		</set>



	</class>
</hibernate-mapping>