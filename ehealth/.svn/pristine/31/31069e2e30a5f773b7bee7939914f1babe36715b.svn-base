	</parameter>
	<queryString><![CDATA[select fsnanalysismainquery.* , 
(case  when (percentage >=$P{fast}) then 'f' when ((percentage >$P{non}) 
and (percentage <$P{fast})) then 's' else 'n' end) as status
from ( select firstdata.*,ISNULL(seconddata.qtyconsumed,'0') as qtyconsumed,
((qtyconsumed/qtyactual)*100) as percentage
from ( select f.*,sum(qty) as qtyactual from (select * from (select issue_date as dateissue
,department_id as department_id, to_store as todept, t.item_issued as item_id
,qty_issued  as qty from store_issue_m s left outer join store_issue_t t on s.id = t.issue_m_id
where issue_date between $P{fromDate} and $P{toDate}
union
select b.bill_date as dateissue,35 as department_id,0 as todept,d.item_id as item_id,d.qty as qty
from bl_dispensing_header b
left outer join  bl_dispensing_details d on b.dispensing_header_id = d.dispensing_header_id
where b.bill_date between $P{fromDate} and $P{toDate} 
union
select s.return_date as dateissue,s.from_department_id as department_id,
to_department_id as todept, t.item_id as item_id,t.return_qty as qty
from store_internal_return_m s left outer join  store_internal_return_t t on
s.id = t.return_main_id 
where s.return_date between $P{fromDate} and $P{toDate} 
) as firstsql
inner join
( select item_id as itm,pvms_no,department_id as dept_id,dangerous_drug,controlled_drug,
high_value_drug,essential_drug,nomenclature,item_generic_id,item_type_id,
item_category_id,i.item_conversion_id,manufacturer_id,supplier_id,u.unit_name 
from mas_store_item i
inner join mas_store_item_conversion m on i.item_conversion_id= m.item_conversion_id
inner join mas_store_unit u on m.intermediate_unit_id = u.unit_id
) as secondsql
on firstsql.item_id = secondsql.itm 
where secondsql.dept_id = $P{deptId}  $P!{qry1} 
--$P!{qry}
) as f
group by f.department_id,f.item_id,f.dateissue,f.todept,
f.qty,f.itm,f.pvms_no,f.dept_id,f.dangerous_drug,f.controlled_drug,
f.high_value_drug,f.essential_drug,f.nomenclature,f.item_generic_id,
f.item_type_id,f.item_category_id,f.item_conversion_id,f.manufacturer_id,
f.supplier_id,f.unit_name,f.dateissue
) as firstdata

left outer join
(
select f.*,sum(qty) as qtyconsumed
from (select * from (select issue_date as dateissue ,department_id as department_id,
to_store as todept, t.item_issued as item_id ,qty_issued  as qty from store_issue_m s
left outer join store_issue_t t on s.id = t.issue_m_id
where issue_date between $P{fromDate} and $P{toDate} 
union
select b.bill_date as dateissue,35 as department_id,0 as todept,d.item_id as item_id,d.qty as qty from bl_dispensing_header b
left outer join  bl_dispensing_details d on b.dispensing_header_id = d.dispensing_header_id
where b.bill_date between $P{fromDate} and $P{toDate} 
union
select s.return_date as dateissue,s.from_department_id as department_id,to_department_id as todept,t.item_id as item_id,t.return_qty as qty
 from store_internal_return_m s left outer join  store_internal_return_t t on s.id = t.return_main_id
where s.return_date between $P{fromDate} and $P{toDate}
 ) as firstsql
inner join
(
select item_id as itm,pvms_no,nomenclature,department_id as dept_id,
controlled_drug,high_value_drug,essential_drug,item_generic_id,
item_type_id,dangerous_drug,item_category_id,i.item_conversion_id,
manufacturer_id,supplier_id,u.unit_name from mas_store_item i 

inner join mas_store_item_conversion m on i.item_conversion_id= m.item_conversion_id 
inner join mas_store_unit u on m.intermediate_unit_id = u.unit_id ) as secondsql 
on firstsql.item_id = secondsql.itm where secondsql.dept_id = $P{deptId} $P!{qry1} 
--$P!{qry}
) as f  
group by f.todept,f.item_id,f.dateissue,f.department_id,
f.qty,f.itm,f.pvms_no,f.nomenclature,f.dept_id,f.controlled_drug,
f.high_value_drug,f.essential_drug,f.item_generic_id,f.item_type_id,
f.dangerous_drug,f.item_category_id,f.item_conversion_id,
f.manufacturer_id,f.supplier_id,f.unit_name)
as seconddata on firstdata.item_id = seconddata.item_id) 
as fsnanalysismainquery;]]></queryString>

