	<queryString><![CDATA[select 
   patient.hin_id as hin_id,
   (patient.p_first_name+' '+ISNULL(patient.p_middle_name,'')+' '+ISNULL(patient.p_last_name,'')) as patient_name,
   opd_patient_details.height as height,
   opd_patient_details.weight as weight,
   opd_patient_details.diet_remarks_id as diet_remarks,
   opd_patient_details.diet_patient_history as diet_patient_history,
   opd_patient_details.diet_prescribed_details as diet_prescribed_details,
   (mas_employee.first_name+' '+ISNULL(mas_employee.middle_name,'')+' '+ISNULL(mas_employee.last_name,'')) as doctor,
    mas_hospital.hospital_name as mas_hospital_hospital_name,
    mas_hospital.address as address,
    mas_diet.diet_code as diet_code,
    mas_diet.diet_name as diet_name,
   mas_department.department_name as department_name

from mas_employee mas_employee left outer join opd_patient_details opd_patient_details on mas_employee.employee_id = opd_patient_details.employee_id 
left outer join mas_hospital mas_hospital on mas_employee.hospital_id = mas_hospital.hospital_id
left outer join visit visit on visit.visit_id= opd_patient_details.visit_id
left outer join patient patient on patient.hin_id = visit.hin_id
left outer join mas_diet mas_diet on mas_diet.diet_id = opd_patient_details.diet_id
left outer join mas_department mas_department on mas_department.department_id = mas_employee.department_id

$P!{query}]]></queryString>

