	<queryString><![CDATA[SELECT
     blood_issue_header.`monthly_no` AS blood_issue_header_monthly_no,
     blood_issue_header.`issue_date` AS blood_issue_header_issue_date,
     blood_issue_header.`issue_time` AS blood_issue_header_issue_time,
     CONCAT(mas_employee.`first_name`," ",if(mas_employee.`middle_name` is null, "",mas_employee.`middle_name`)," ",mas_employee.`last_name`)as matched_by,
     CONCAT(mas_employee_A.`first_name`," ",if(mas_employee_A.`middle_name` is null, "",mas_employee_A.`middle_name`)," ",mas_employee_A.`last_name`)as issued_by,
     CONCAT(mas_employee_B.`first_name`," ",if(mas_employee_B.`middle_name` is null, "",mas_employee_B.`middle_name`)," ",mas_employee_B.`last_name`)as received_by,
     if(patient.`hin_no` is null,"--",patient.`hin_no`)AS patient_hin_no,
     CONCAT(patient.`p_first_name`," ",patient.`p_middle_name`," ",patient.`p_last_name`)as pat_name,
     mas_department.`department_name` AS mas_department_department_name,
     blood_stock_detail.`blood_bag_no` AS blood_stock_detail_blood_bag_no,
     blood_stock_detail.`qty` AS blood_stock_detail_qty,
     blood_stock_detail.`blood_issued` AS blood_stock_detail_blood_issued,
     mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
 if( mas_blood_group.`blood_group_name` is null,"--", mas_blood_group.`blood_group_name`)AS mas_blood_group_blood_group_name
   
FROM
     `blood_issue_header` blood_issue_header RIGHT OUTER JOIN `blood_issue_detail` blood_issue_detail ON blood_issue_header.`id` = blood_issue_detail.`issue_header_id`
     LEFT OUTER JOIN `mas_employee` mas_employee ON blood_issue_header.`matched_by` = mas_employee.`employee_id`
     LEFT OUTER JOIN `mas_employee` mas_employee_A ON blood_issue_header.`issued_by` = mas_employee_A.`employee_id`
     LEFT OUTER JOIN `mas_employee` mas_employee_B ON blood_issue_header.`received_by` = mas_employee_B.`employee_id`
     LEFT OUTER JOIN `patient` patient ON blood_issue_header.`hin_id` = patient.`hin_id`
     LEFT OUTER JOIN `mas_department` mas_department ON blood_issue_header.`department_id` = mas_department.`department_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON blood_issue_header.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `mas_blood_group` mas_blood_group ON blood_issue_header.`blood_group_id` = mas_blood_group.`blood_group_id`
     LEFT OUTER JOIN `blood_stock_detail` blood_stock_detail ON blood_issue_detail.`stock_detail_id` = blood_stock_detail.`id`

     WHERE
      blood_issue_header.`issue_date` between $P{fromDate} and  $P{toDate}
and blood_stock_detail.`blood_issued`="y"]]></queryString>

