	<queryString><![CDATA[SELECT
     mas_item_category.item_category_code AS mas_item_category_item_category_code,
     mas_item_category.item_category_name AS mas_item_category_item_category_name,
 CASE
    WHEN mas_item_category.status = 'y' or  mas_item_category.status = 'Y' THEN 'Active'
    ELSE 'Inactive'
 	 END as status,
     mas_store_section.section_name AS mas_store_section_section_name,
dep.department_name
FROM
mas_item_category mas_item_category
left Outer JOIN  mas_store_section mas_store_section     ON mas_store_section.section_id = mas_item_category.section_id
left Outer  JOIN mas_department dep ON dep.department_id = mas_item_category.department_id


$P!{qry}

ORDER BY
     mas_item_category.item_category_code ASC]]></queryString>

