	<queryString><![CDATA[select
     mas_rank.rank_name as mas_rank_rank_name,
     hr_employee_personel_details.date_of_birth as hr_employee_personel_details_date_of_birth,
    (mas_employee.first_name+' '+ISNULL(mas_employee.middle_name,'')+' '+ISNULL(mas_employee.last_name,''))as ename,
     mas_employee.join_date as mas_employee_join_date,
     mas_employee.employee_code as mas_employee_employee_code,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
     hr_employee_experience.exp_yrs as expinyears,
      hr_employee_experience.exp_months as expinmonths,
     mas_employee.personal_details_id as mas_employee_personal_details_id,
     hr_employee_personel_details.id as hr_employee_personel_details_id
from
    mas_employee mas_employee left join hr_employee_personel_details hr_employee_personel_details   on hr_employee_personel_details.id = mas_employee.personal_details_id
     left join mas_hospital mas_hospital on mas_employee.hospital_id = mas_hospital.hospital_id
     left  join mas_rank mas_rank on mas_employee.rank_id = mas_rank.rank_id
     left join hr_employee_experience hr_employee_experience  on hr_employee_experience .experience_id = mas_employee.experience_id
where

      mas_employee.employee_code between $P{fromEmpCode}  and $P{toEmpCode}]]></queryString>

