	<parameter name="inpatient_id" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer('0')]]></defaultValueExpression>
	</parameter>
	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select (case when (SELECT max(m.icd_id) FROM mas_icd m,discharge_icd_code dis WHERE dis.inpatient_id =  $P{inpatient_id} AND m.icd_id = dis.icd_id AND dis.hospital_id=$P{hospitalId} and dis.diagnosis_status  in ('f','p'))IS NOT NULL

then
(SELECT icd.icd_name AS init_diagnosis FROM discharge_icd_code d ,mas_icd icd ,inpatient i
WHERE d.icd_id = icd.icd_id
AND d.inpatient_id =  $P{inpatient_id}
AND i.inpatient_id =  $P{inpatient_id}
AND d.hospital_id=$P{hospitalId}
AND icd.icd_id = (SELECT max(m.icd_id) FROM mas_icd m,discharge_icd_code dis
 WHERE dis.inpatient_id = $P{inpatient_id} AND m.icd_id = dis.icd_id and dis.diagnosis_status  in ('f','p') AND dis.hospital_id=$P{hospitalId}) )

else

 (select init_diagnosis from inpatient where inpatient.inpatient_id =  $P{inpatient_id} AND inpatient.hospital_id=$P{hospitalId})

 end)as init_diagnosis]]></queryString>

