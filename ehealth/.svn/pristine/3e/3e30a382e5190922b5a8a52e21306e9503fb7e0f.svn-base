	<queryString><![CDATA[select hin_id,hin_no,patientname,company_id,sum(bill_amt),sum(pendingamount),address,company_name  from
(
select *,if(bill_amt is null,0,bill_amt)-if(advance_adjustment is null,0,advance_adjustment) as pendingamount from
(select * from
(select hin_id as hinid ,hin_no ,concat(p_first_name,' ',p_middle_name,' ',p_last_name) as patientname,
reg_date,company_id from patient where company_id in(
select company_id from mas_company  mc left join mas_patient_type mpp on mc.patient_type_id=mpp.patient_type_id   where mc.patient_type_id=4 ) and
patient_status in('in patient','out patient')) as frst
inner join
(
select company_id as cmp,company_name,address from mas_company
) as scnd
on frst.company_id = scnd.cmp) as finaldata
left outer join
(select hin_id,final_bill_no as bill_no,gross_amt as bill_amt
,net_amt,final_bill_date as bill_date,settled_amt as advance_adjustment,inpatient_id as patient_name
from bl_final_bill_details
union
select hin_id,bill_no,bill_amt,net_amt,bill_date,advance_adjustment,patient_name
  from bl_op_bill_header
) as finaldata1
on finaldata.hinid = finaldata1.hin_id where finaldata1.bill_date between
$P{fromdate} and $P{todate} and finaldata.company_id!=0 $P!{qry}
) as maindata
group by company_name]]></queryString>

