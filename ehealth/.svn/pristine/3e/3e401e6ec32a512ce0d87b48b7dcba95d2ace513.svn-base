	<queryString><![CDATA[select h.voucher_header_id, ma.acc_desc, 
fmsl.sub_led_desc, h.voucher_no, h.voucher_date, m.hospital_name, m.address, d.narration, 
coalesce(d.dr_amount,null) as dr_amount,
coalesce(d.cr_amount,null)as cr_amount,
h.narration     ,
mcc.cost_center_name   from
fa_voucher_details as d
inner join fa_voucher_header h on d.voucher_header_id=h.voucher_header_id
inner join fa_mas_account ma on d.account_id=ma.acc_id
inner join mas_hospital m on h.hospital_id=m.hospital_id
left join fa_mas_sub_led fmsl on d.sub_led_id=fmsl.sub_led_id
left outer join mas_cost_center mcc on mcc.cost_center_id=d.cost_center_id
where h.voucher_type=$P{voucherType} and h.hospital_id=$P{hospital_id} 
and h.voucher_date = $P{voucherDate}
and h.f_year_id = $P{fYearId}]]></queryString>

