	<parameter name="deptId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="fromDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="toDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select a.item_id,a.nomenclature,a.qty_issued,a.twty_of_qti,a.total,closingQty,(a.total-a.closingQty) as ROL,a.hosp from
(select   t.item_id,i.nomenclature,round(sum(t.qty_issued),0) as qty_issued,
round((sum(t.qty_issued)*0.2),0) as twty_of_qti,
round((sum(t.qty_issued)+sum(t.qty_issued)*0.2),0) as total,
coalesce((select sum(sbt.closing_stock) from  
store_item_batch_stock sbt where sbt.item_id=t.item_id 
and sbt.department_id=$P{deptId} ),0) as closingQty
,mh.hospital_name||' '||mh.address as hosp from store_issue_t t 
left outer join store_issue_m m on m.id=t.issue_m_id
left outer join mas_store_item i on i.item_id=t.item_id
left outer join mas_hospital mh on mh.hospital_id=m.hospital_id 
where m.department_id=$P{deptId} and m.issue_date between $P{fromDate} and  $P{toDate} and m.hospital_id=$P{hospitalId} 
group by t.item_id,i.nomenclature,mh.hospital_name,mh.address ) a
where a.qty_issued > a.closingQty
order by a.nomenclature]]></queryString>

