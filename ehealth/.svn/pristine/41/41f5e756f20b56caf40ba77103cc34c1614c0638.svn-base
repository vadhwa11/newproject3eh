<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PhMembersRelationship"
		table="ph_members_relationship"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="relationship_id"
		>
					<generator class="sequence">
				<param name="sequence">ph_members_relationship_relationship_id_seq</param>
			</generator>
		</id>

		<property
			name="EcrStatus"
			column="ecr_status"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="RelationshipFrom"
			column="relationship_from"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="RegistrationDate"
			column="registration_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="RegistrationTime"
			column="registration_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="FamilyId"
			column="family_id"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="Relation"
			column="relation_id"
			class="MasRelation"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Member1"
			column="member1_id"
			class="PhMemberSurvey"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="RegistrationBy"
			column="registration_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Member2"
			column="member2_id"
			class="PhMemberSurvey"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>