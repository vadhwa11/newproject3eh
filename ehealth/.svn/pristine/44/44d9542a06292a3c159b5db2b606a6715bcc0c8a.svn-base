	<queryString><![CDATA[select a.pa1,a.cde1,a.et1 from
(
select mpe.pay_element_code as cde1,mpe.pay_element_type as et1,ep.pay_amount as pa1,
ep.employee_id as e1
from hr_employee_pay_elements ep
left join hr_mas_pay_element mpe on ep.pay_element_id=mpe.pay_element_id
where month(start_date)=$P{month}  and year(start_date)=$P{year}   and mpe.pay_element_type='addition'
)
as a

left join
mas_employee me on a.e1=me.employee_id


where me.employee_id=$P{empId}]]></queryString>

