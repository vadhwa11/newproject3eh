	<queryString><![CDATA[SELECT
     inpatient.mlc AS inpatient_mlc,
     concat(patient.p_first_name,' ',patient.p_middle_name,' ',patient.p_last_name,',',inpatient.age)as PNameAdd,
     mas_relation.relation_name AS mas_relation_relation_name,
     concat(expiry_details.expiry_time,' ',expiry_details.expiry_date)as DTime,
     mas_icd.icd_name AS mas_icd_icd_name,
     sil_dil_status.condition_status AS sil_dil_status_condition_status
FROM
     inpatient inpatient INNER JOIN expiry_details expiry_details ON inpatient.inpatient_id = expiry_details.inpatient_id
     INNER JOIN patient patient ON inpatient.hin_id = patient.hin_id
     left outer JOIN sil_dil_status sil_dil_status ON inpatient.inpatient_id = sil_dil_status.inpatient_id
     left outer JOIN mas_icd mas_icd ON sil_dil_status.icd_id = mas_icd.icd_id
     left outer JOIN mas_relation mas_relation ON patient.relation_id = mas_relation.relation_id
     
WHERE
     expiry_details.expiry_date = $P{Date}
     and expiry_details.ward_id = $P{Dept_ID}
     and inpatient.hospital_id = $P{hospitalId}]]></queryString>

