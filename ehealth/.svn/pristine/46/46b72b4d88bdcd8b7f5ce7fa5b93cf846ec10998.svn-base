	<parameter name="UHID_No" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="medicoLegalDetailsId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT m.hin_id, 

p.hin_no,p.p_first_name, p.age, mas.hospital_name, 

m.serial_no, m.police_station, m.brought_by, m.brought_by_address, m.requisition_from,m.examination_date,m.history_injury_cause, 
m.history_by_injured, m.injury_details,  m.witnesses_1, m.witnesses_1_father, m.witnesses_1_address, m.witnesses_2, m.witnesses_2_father, 
m.witnesses_2_address, m.physical_examination, m.no_of_additional_sheets,m.id_mark1, m.id_mark2,m.examination_time,
m.patient_status,
a.administrative_sex_name,
rank.rank_name,m.opinion,
district.district_name as place,
m.patient_address,m.injury_type

from medico_legal_details m

LEFT OUTER JOIN patient p ON p.hin_id = m.hin_id
LEFT OUTER JOIN mas_administrative_sex a ON a.administrative_sex_id = p.sex_id
LEFT OUTER JOIN opd_patient_details opd ON opd.id = m.opd_patient_detail_id
LEFT OUTER JOIN mas_employee employee ON employee.employee_id = opd.employee_id
LEFT OUTER JOIN mas_rank rank ON rank.rank_id = employee.rank_id
LEFT OUTER JOIN mas_hospital mas ON mas.hospital_id =opd.hospital_id
LEFT OUTER JOIN mas_district district ON district.district_id = mas.district_id
where  p.hin_no = $P{UHID_No}  and m.medico_legal_details_id=$P{medicoLegalDetailsId}    and opd.hospital_id= $P{hospitalId}
GROUP BY m.hin_id, 
p.hin_no,
p.p_first_name, p.age, 
m.serial_no, m.police_station, m.brought_by, m.brought_by_address, m.requisition_from,m.examination_date,m.history_injury_cause, 
m.history_by_injured, m.injury_details,  m.witnesses_1, m.witnesses_1_father, m.witnesses_1_address, m.witnesses_2, m.witnesses_2_father,
m.witnesses_2_address, m.physical_examination, m.no_of_additional_sheets,m.id_mark1, m.id_mark2,mas.hospital_name, m.examination_time,
a.administrative_sex_name,district.district_name,
m.patient_address,rank.rank_name,m.patient_status,m.opinion,m.injury_type]]></queryString>

