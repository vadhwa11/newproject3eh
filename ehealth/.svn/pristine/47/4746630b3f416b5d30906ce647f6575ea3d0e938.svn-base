<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="BlOpBillDetails" table="bl_op_bill_details">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="op_bill_details_id">
			<generator class="sequence" >
				<param name="sequence">bl_op_bill_details_op_bill_details_id_seq</param>
			</generator>
		</id>

		<property name="Quantity" column="quantity" type="integer"
			not-null="false" length="10" />
		<property name="Rate" column="rate" type="big_decimal"
			not-null="false" length="13" />
		<property name="Amount" column="amount" type="big_decimal"
			not-null="false" length="13" />
		<property name="DiscountPercent" column="discount_percent"
			type="big_decimal" not-null="false" length="9" />
		<property name="DiscountAmt" column="discount_amt" type="big_decimal"
			not-null="false" length="13" />
		<property name="ProportionalDiscountAmount" column="proportional_discount_amount"
			type="big_decimal" not-null="false" length="13" />
		<property name="NetAmt" column="net_amt" type="big_decimal"
			not-null="false" length="13" />
		<property name="BillDate" column="bill_date" type="date"
			not-null="false" />
		<property name="BillTime" column="bill_time" type="string"
			not-null="false" length="10" />
		<property name="RefundedAmt" column="refunded_amt" type="big_decimal"
			not-null="false" length="13" />
		<property name="RefundableStatus" column="refundable_status"
			type="string" not-null="false" length="1" />
		<property name="StdDeductionGen" column="std_deduction_gen"
			type="big_decimal" not-null="false" length="11" />
		<property name="StdDeductionSpl" column="std_deduction_spl"
			type="big_decimal" not-null="false" length="11" />
		<many-to-one name="ChargeCode" column="charge_code_id"
			class="MasChargeCode" not-null="false">
		</many-to-one>
		<many-to-one name="ChangedBy" column="changed_by" class="Users"
			not-null="false">
		</many-to-one>
		<many-to-one name="OpBillHeader" column="op_bill_header_id"
			class="BlOpBillHeader" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>