	<parameter name="deptId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
a.group_id,a.group_name,a.item_id,a.nomenclature,a.hospital_name,a.issuedQty,
round(a.AvgQty,0) AvgQty,a.ClosingStock,(a.AvgQty-a.ClosingStock) ROL,a.costprice,
a.costprice*(a.AvgQty-a.ClosingStock) TotalPrice
from
(
select g.group_id,g.group_name,t.item_id,i.nomenclature,SUM(t.qty_issued) issuedQty,
(SUM(t.qty_issued)/12)*3 AvgQty, 
(select sum(s.closing_stock) from store_item_batch_stock s where s.item_id=t.item_id
and department_id=24) as ClosingStock,
(select (coalesce(final_cost_price,0)/coalesce(received_qty,0))as costprice 
from store_grn_t st where st.item_id=t.item_id order by costprice desc limit 1 offset 0) 
as costprice,
h.hospital_name from store_issue_t t
left outer join store_issue_m m  on m.id=t.issue_m_id
left outer join mas_store_item i on i.item_id=t.item_id
left outer join mas_store_group g on g.group_id=i.group_id
left outer join mas_hospital h on h.hospital_id=m.hospital_id
where m.department_id=$P{deptId} and m.issue_date >= $P{fromdate} and  m.issue_date <= $P{todate} and  m.hospital_id <= $P{hospitalId}
group by  t.item_id,i.nomenclature,g.group_id,g.group_name,h.hospital_name
)a
where a.AvgQty>=a.ClosingStock 
order by a.group_name,a.nomenclature]]></queryString>

