	<queryString><![CDATA[select b.hin_id, bh.visit_id, b.amount,receipt_date as billdate, b.hospital_id,
(select hospital_name from mas_hospital m where m.hospital_id = b.hospital_id) as hname,
p.hin_no,
UPPER((p.p_first_name||' '||coalesce(p.p_middle_name,' ')||' '||coalesce(p.p_last_name,' '))) as patientname,
case when vs.visit_no is null then 0 else vs.visit_no end as visit_no,
case when ip.ad_no is null then '-' else ip.ad_no end as ipno, b.op_bill_header_id,
receipt_no from bl_receipt_header as b
inner join patient p on b.hin_id = p.hin_id
left outer join   inpatient ip on b.inpatient_id=ip.inpatient_id
left outer join bl_op_bill_header bh on b.op_bill_header_id = bh.op_bill_header_id
left outer join visit vs on bh.visit_id = vs.visit_id
where b.amount > 0
and receipt_date between $P{fromdate} and $P{todate} and b.hospital_id=$P{hospitalId}]]></queryString>

