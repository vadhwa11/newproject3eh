	<queryString><![CDATA[SELECT
     mas_department.department_code AS mas_department_department_code,
     mas_department.department_name AS mas_department_department_name,
 CASE
    WHEN mas_department.status = 'Y' or mas_department.status='y' THEN 'Active'
    ELSE 'Inactive'
 	 END  as  mas_department_status,
     mas_department_type.department_type_name AS mas_department_type_department_type_name,
dept_description
FROM
     mas_department_type mas_department_type INNER JOIN mas_department mas_department ON
     mas_department_type.department_type_id = mas_department.department_type_id

  
$P!{qry}
ORDER BY
     mas_department.department_name ASC]]></queryString>

