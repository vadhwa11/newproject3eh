<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PhStudentHealthDetails"
		table="ph_student_health_details"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="ph_student_health_details_id"
		>
		<generator class="sequence">
				<param name="sequence">ph_student_health_details_ph_student_health_details_id_seq</param>
			</generator>
			
		</id>

		<property
			name="Height"
			column="height"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Weight"
			column="weight"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Pulse"
			column="pulse"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Sbp"
			column="sbp"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Dbp"
			column="dbp"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Bmi"
			column="bmi"
			type="big_decimal"
			not-null="false"
			length="11"
		/>
		<property
			name="Age"
			column="age"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="BcgDueDate"
			column="bcg_due_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="BcgImmDate"
			column="bcg_imm_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="OpvODueDate"
			column="opv_o_due_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="OpvOImmDate"
			column="opv_o_imm_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="HepatitiesBoDueDate"
			column="hepatities_bo_due_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="HepatitiesBoImmDate"
			column="hepatities_bo_imm_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Allergy"
			column="allergy"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="AllergyYesText"
			column="allergy_yes_text"
			type="string"
			not-null="false"
			length="100"
		/>
			<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
			<property
			name="SchoolClass"
			column="school_class"
			type="string"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Student"
			column="student_id"
			class="PhStudentRegistration"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="HospitalType"
			column="hospital_type_id"
			class="MasHospitalType"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="District"
			column="district_id"
			class="MasDistrict"
			not-null="false"
		>
		</many-to-one>
	<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>

	</class>	
</hibernate-mapping>