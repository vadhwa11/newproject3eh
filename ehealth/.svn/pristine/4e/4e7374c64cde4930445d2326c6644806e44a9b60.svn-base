<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasSalesType" table="mas_sales_type">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="sales_type_id">
			<generator class="sequence">
				<param name="sequence">mas_sales_type_sales_type_id_seq</param>
			</generator>
		</id>

		<property name="SalesTypeCode" column="sales_type_code" type="string"
			not-null="false" length="8" />
		<property name="SalesTypeName" column="sales_type_name" type="string"
			not-null="false" length="30" />
		<property name="SalesTypeValue" column="sales_type_value"
			type="integer" not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="true" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />


	</class>
</hibernate-mapping>