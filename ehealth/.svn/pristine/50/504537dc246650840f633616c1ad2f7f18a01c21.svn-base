<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PhDiseaseRegistrationScreening"
		table="ph_disease_registration_screening"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="follow_id"
		>
			<generator class="sequence">
				<param name="sequence">ph_disease_registration_screening_follow_id_seq</param>
			</generator>
			
		</id>

		<property
			name="FollowDate"
			column="follow_date"
			type="date"
			not-null="true"
			length="13"
		/>
		<property
			name="FollowTime"
			column="follow_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="DiseaseRegId"
			column="disease_reg_id"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="InstitutionType"
			column="institution_type"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="InstitutionName"
			column="institution_name"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="CheckedStatus"
			column="checked_status"
			type="java.lang.String"
			not-null="false"
			length="10"
		/>
		<property
			name="FollowupType"
			column="followup_type"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="SuspectedNcd"
			column="suspected_ncd"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Height"
			column="height"
			type="big_decimal"
			not-null="false"
			length="6"
		/>
		<property
			name="FollowUpType"
			column="follow_up_type"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="Waist"
			column="waist"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Bmi"
			column="bmi"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="InvestigationName"
			column="investigation_name"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="BloodPreMax"
			column="blood_pre_max"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="BloodPreMin"
			column="blood_pre_min"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="RegDate"
			column="reg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="RegFollowupId"
			column="reg_followup_id"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="Hip"
			column="hip"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="Weight"
			column="weight"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="BloodSugarRbs"
			column="blood_sugar_rbs"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="WaistHipRatio"
			column="waist_hip_ratio"
			type="string"
			not-null="false"
			length="16"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<property
			name="MemberId"
			column="member_id"
			type="java.lang.Long"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>