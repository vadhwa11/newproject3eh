<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="IpdKitIssueHeader"
		table="ipd_kit_issue_header"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="header_id"
		>
			<generator class="sequence">
			<param name="sequence">ipd_kit_issue_header_header_id_seq</param>
			</generator>
		</id>

		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Template"
			column="template_id"
			class="IpdKitIssueMasterTemplateM"
			not-null="false"
		>
		</many-to-one>
      <set inverse="true" name="IpdKitIssueDetails">
			<key column="header_id" />
			<one-to-many class="IpdKitIssueDetails" />
		</set>

	</class>	
</hibernate-mapping>