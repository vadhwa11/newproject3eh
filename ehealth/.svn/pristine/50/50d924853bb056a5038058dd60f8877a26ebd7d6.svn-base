	<queryString><![CDATA[SELECT
   patient.`hin_no` AS hin_no ,op.refer_depart_id,
   patient.`age` AS patient_age,upper(concat(mas_empl.first_name,' ',mas_empl.last_name)) as referc,mas_dept.department_name as refdept,
   visit_date AS patient_add_edit_date,     opd_patient_history.`present_illness` as opd_patient_history_present_illness,
   upper(concat(patient.`p_first_name`,' ',patient.`p_middle_name`,' ',patient.`p_last_name`))as name,
   visit.`token_no`as token_no,
   concat("Dr. ",mas_employee.`first_name`," ",mas_employee.`middle_name`," ",mas_employee.`last_name`)as doctorName,
   administrative_sex_name,
   mas_department.`department_name` as maindepartmentName,
   concat(ifnull(patient.`address`,"")," ",ifnull(mas_block.`block_name`,"")," ",ifnull(mas_district.`district_name`,"")," ",ifnull(mas_state.`state_name`,""))as address,
   patient.`mobile_number` as phone,
   visit.`visit_no` AS visit_visit_no,
   mas_hospital.`hospital_name` AS mas_hospital_hospital_name,
   opd_patient_history.`visit_inpatient_id` AS opd_patient_history_visit_inpatient_id,
   visit.`visit_id` AS visit_visit_id,
   concat(patient.p_first_name,' ',patient.p_middle_name,' ',patient.p_last_name) as patientname,
   mase.`administrative_sex_name` AS mas_administrative_sex_administrative_sex_name,
   patient.`age` AS patient_age,
   patient_prescription_header.`prescription_id` as patient_prescription_header_prescription_id,
   patient_investigation_header.`investigation_header_id` as patient_investigation_header_investigation_header_id,
   patient_allergic_drugs_hd.`patient_allergic_drugs_hd_id` as patient_allergic_drugs_hd_id,
   discharge_icd_code.`discharge_icd_code_id` as discharge_icd_code_id,
   patient_investigation_header.`investigation_header_id` as patient_investigation_header_investigation_header_idR,
patient_prescription_header.`prescription_id` as patient_prescription_header_prescription_idR,
   op.`id` as opd_patient_details_id
FROM
     `patient` patient RIGHT OUTER JOIN `opd_patient_history` opd_patient_history ON patient.`hin_id` = opd_patient_history.`hin_id`
     LEFT OUTER JOIN `mas_hospital` mas_hospital ON patient.`hospital_id` = mas_hospital.`hospital_id`
     LEFT OUTER JOIN `visit` visit ON patient.`hin_id` = visit.`hin_id`
     LEFT OUTER JOIN `patient_investigation_header` patient_investigation_header on visit.`visit_id` = patient_investigation_header.`visit_id`
     LEFT OUTER JOIN  `patient_prescription_header` patient_prescription_header on visit.`visit_id` = patient_prescription_header.`visit_id`
     LEFT OUTER JOIN  `patient_allergic_drugs_hd` patient_allergic_drugs_hd on visit.`visit_id` = patient_allergic_drugs_hd.`visit_id`
     LEFT OUTER JOIN `opd_patient_details` op ON op.`visit_id` = visit.`visit_id`
     LEFT OUTER JOIN `discharge_icd_code` discharge_icd_code on visit.`visit_id` = discharge_icd_code.`visit_id`
     LEFT OUTER JOIN `mas_administrative_sex` mase ON patient.`sex_id` = mase.`administrative_sex_id`
     LEFT OUTER JOIN `mas_block` mas_block ON patient.`block_id` = mas_block.`block_id`
     LEFT OUTER JOIN `mas_district` mas_district ON patient.`district_id` = mas_district.`district_id`
     LEFT OUTER JOIN `mas_state` mas_state ON patient.`state_id` = mas_state.`state_id`
     LEFT OUTER JOIN `mas_employee` mas_employee ON visit.`doctor_id` = mas_employee.`employee_id`
     LEFT OUTER JOIN `mas_department` mas_department ON visit.`department_id` = mas_department.`department_id`
     LEFT OUTER JOIN `mas_department` mas_dept ON op.`refer_depart_id` = mas_dept.`department_id`
     LEFT OUTER JOIN `mas_employee` mas_empl ON op.`refer_dr` = mas_empl.`employee_id`

WHERE
     patient.`hin_no` = $P{hinNo}
and  visit.`visit_no` = $P{visitNo}

ORDER BY
     visit_date DESC,
     visit_time DESC]]></queryString>

