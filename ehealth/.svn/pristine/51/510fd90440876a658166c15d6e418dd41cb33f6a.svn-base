<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class
		name="HrAccruelRecord"
		table="hr_accruel_record"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="accrual_id"
		>
			<generator class="sequence">
				<param name="sequence">hr_accruel_record_accrual_id_seq</param>
			</generator>
		</id>

		<property
			name="Accrued"
			column="accrued"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="Month"
			column="month"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="Year"
			column="year"
			type="string"
			not-null="false"
			length="4"
		/>
		<many-to-one
			name="LeaveType"
			column="leave_type"
			class="HrMasLeaveTypeMediator"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Emp"
			column="emp_id"
			class="jkt.hms.masters.business.MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Balance"
			column="balance_id"
			class="HrEmployeeBalanceNew"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>