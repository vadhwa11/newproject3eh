<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="PrjQueryEntryDoc" table="prj_query_entry_doc">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="query_entry_doc_id">
			<generator class="sequence">
				<param name="sequence">prj_query_entry_doc_query_entry_doc_id_seq</param>
			</generator>
		</id>

		<property name="QueryEntryDocFilenmae" column="query_entry_doc_fileNmae"
			type="string" not-null="false" length="50" />
		<property name="QedDfilename" column="QED_DFilename" type="string"
			not-null="false" length="50" />
		<property name="QueryEntryDocComments" column="query_entry_doc_comments"
			type="string" not-null="false" length="1000" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<many-to-one name="Comapny" column="comapny_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="Project" column="project_id" class="MstrProject"
			not-null="false">
		</many-to-one>
		<many-to-one name="QueryEntry" column="query_entry_id"
			class="PrjQueryEntry" not-null="false">
		</many-to-one>
	</class>
</hibernate-mapping>