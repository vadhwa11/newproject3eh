	<queryString><![CDATA[select b.receipt_no,b.hin_id,b.receipt_type,b.amount,b.receipt_date
,b.hospital_id,b.inpatient_id,bd.payment_mode,bd.cheque_no,
(select concat(p_first_name,'',p_middle_name,'',p_last_name) as patientname from patient where hin_id=b.hin_id) as patientname,
(select hospital_name from mas_hospital where hospital_id =b.hospital_id) as hospitalname,
(case bd.payment_mode when 'c' then 'cash receipt'
           when 'q' then concat('cheque no :',' ',bd.cheque_no,' date : ',bd.cheque_date)   else concat('card no :',' ',bd.cheque_no,' date: ',bd.cheque_date) end) as details,
bd.amount as detailamount,
(select hin_no from patient p where p.hin_id = b.hin_id) as hinno
 from bl_receipt_header b left outer  join  bl_receipt_details bd

on b.receipt_header_id = bd.receipt_header_id where b.receipt_type='adv' and b.receipt_no = $P{receiptNo};
]]></queryString>

