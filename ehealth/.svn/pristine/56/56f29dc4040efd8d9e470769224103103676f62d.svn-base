		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[Select (patient.p_first_name::text || ' ' || coalesce(patient.p_middle_name,'') || ' ' || coalesce(patient.p_last_name,''))as patientname,
mortuary.serial_no,mortuary.mortuary_reg_time,mortuary.mortuary_reg_date,patient.age,gender.administrative_sex_name as sex,mortuary.cause_of_death,mortuary.dead_body_kept_time,
mortuary.dead_body_kept_date,mortuary.initimation_given,mortuary.articles_on_body,mortuary.mortuary_reg_remarks,mortuary.hand_over_time,mortuary.hand_over_date,
mortuary.body_handed_over,mortuary.cremation,mortuary.body_received_date,mortuary.body_received_time,emp.emp_name
    
from
    mortuary_register_details mortuary left join medico_legal_details mlc on mortuary.medico_legal_details_id= mlc.medico_legal_details_id
    left join  patient patient on mlc.hin_id = patient.hin_id
    left join mas_administrative_sex gender on patient.sex_id = gender.administrative_sex_id
    left join mas_employee emp on mortuary.dead_body_received_by = emp.employee_id

    where mlc.patient_status = 'death' and mlc.medico_legal_details_id = $P{medicoLegalDetailsId}]]></queryString>

