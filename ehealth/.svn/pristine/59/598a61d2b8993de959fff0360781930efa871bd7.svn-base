	<queryString><![CDATA[select 
m.police_station,
p.p_first_name,
p.age,reason,relation,
m.crime_no,m.ref_date,
a.administrative_sex_name,
m.patient_address,

district.district_name as place ,mas.hospital_name,
rank.rank_name,e.emp_name,postmortem_date

from medico_legal_details m
left outer join mas_employee e on m.doctor_id=e.employee_id
left outer join opd_patient_details opd on opd.id=m.opd_patient_detail_id
LEFT OUTER JOIN patient p ON p.hin_id = m.hin_id
left outer join mas_administrative_sex  a on a.administrative_sex_id=p.sex_id
left outer join patient_address pa on pa.hin_id=m.hin_id
LEFT OUTER JOIN mas_hospital mas ON mas.hospital_id = p.hospital_id
LEFT OUTER JOIN mas_district district ON district.district_id = mas.district_id
LEFT OUTER JOIN mas_rank rank ON rank.rank_id = e.rank_id

where   p.hin_no = $P{UHID_No}  and m.medico_legal_details_id=$P{medicoLegalDetailsId}    and opd.hospital_id= $P{hospitalId}]]></queryString>

