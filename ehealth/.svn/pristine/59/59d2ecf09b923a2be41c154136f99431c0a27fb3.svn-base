	<parameter name="fromDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="toDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select * from (select charge_slip_no,chg_slp_amt,chg_slp_date,
case when os_amt is null then 0.0 else os_amt end as onacc,
coalesce(discount_amt,0) as discount_amt,
(select hin_no from patient where hin_id =bd.hin_id) as hinno,
(select UPPER((p_first_name||' '||coalesce(p_middle_name,' ')||' '||coalesce(p_last_name,' ')))
from patient where hin_id = bd.hin_id ) as patientname,
(select ad_no from inpatient where inpatient_id = bd.inpatient_id) as inpatientno,
((coalesce(chg_slp_amt,0) - coalesce(discount_amt,0)) - round(coalesce(chg_slp_amt,0) - coalesce(discount_amt,0), 3))*(-1) as roundoff,
(select hospital_name from mas_hospital where hospital_id = bd.hospital_id) as hospital,
(select Address from mas_hospital where hospital_id = bd.hospital_id) as hAdd, 
(select receipt_header_id from bl_receipt_header where charge_slip_main_id = bd.charge_slip_main_id) as rh,

case when (select amount from bl_receipt_details where payment_mode='C'
and receipt_header_id = (select receipt_header_id from bl_receipt_header where
charge_slip_main_id = bd.charge_slip_main_id)) is null then 0.0 else
(select amount from bl_receipt_details where payment_mode='C' 
and receipt_header_id = (select receipt_header_id from bl_receipt_header where
charge_slip_main_id = bd.charge_slip_main_id)) end as cashamount,

case when (select sum(amount) from bl_receipt_details where 
payment_mode in ('Q','R') and receipt_header_id = (select receipt_header_id from bl_receipt_header where
charge_slip_main_id = bd.charge_slip_main_id)) is null then 0.0 else
(select sum(amount) from bl_receipt_details where payment_mode in ('Q','R')
and receipt_header_id = (select receipt_header_id from bl_receipt_header where
charge_slip_main_id = bd.charge_slip_main_id)) end as otheracc

from bl_charge_slip_main bd where chg_slp_date  between $P{fromDate} and $P{toDate} and bd.hospital_id=$P{hospitalId} )
as main where main.inpatientno is not null]]></queryString>

