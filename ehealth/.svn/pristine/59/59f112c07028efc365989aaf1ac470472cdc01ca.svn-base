<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="CntrReqPat" table="cntr_req_pat">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="cp_id">
			<generator class="sequence">
				<param name="sequence">cntr_req_pat_cp_id_seq</param>
			</generator>
		</id>

		<property name="PatientRequired" column="patient_required"
			type="integer" not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<many-to-one name="Company" column="company_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="Prj" column="prj_id" class="MstrProject"
			not-null="false">
		</many-to-one>
		<many-to-one name="Country" column="country_id"
			class="jkt.hms.masters.business.MasCountry" not-null="false">
		</many-to-one>

	</class>
</hibernate-mapping>