	<queryString><![CDATA[select
     reh.result_date as resultdate,
     red.remarks as remarks,
     red.result as result,
     dgm.investigation_name as investigationname,
     msc.sub_chargecode_name as subchargename,
     du.uom_name as unitname,
     mh.hospital_name as hospital,
     md.department_name as department,
     b.patient_name as patinetname,
     b.age as age,
     mad.administrative_sex_name as sex,
     msc.sub_chargecode_name as subcharge,
     mmc.main_chargecode_name as maincharge,
     hd.`patient_type` as patienttype


from

bl_op_bill_header b
left join dg_orderdt dt on b.op_bill_header_id=dt.bill_id
right join dg_orderhd hd on dt.orderhd_id=hd.orderhd_id
left join dg_sample_collection_header sch on hd.orderhd_id=sch.order_id
right join dg_result_entry_header reh on sch.sample_collection_header_id=reh.sample_collection_header_id
left join dg_result_entry_detail red on red.result_entry_id=reh.result_entry_id
left join dg_mas_investigation dgm on red.investigation_id= dgm.investigation_id
left join dg_uom du on dgm.uom_id = du.uom_id
left join mas_administrative_sex mad on  b.sex_id = mad.administrative_sex_id
left join mas_hospital mh on  reh.hospital_id = mh.hospital_id
left join mas_department md on  reh.department_id = md.department_id
left join mas_sub_chargecode msc on  reh.sub_chargecode_id = msc.sub_chargecode_id
left join mas_main_chargecode mmc on  reh.main_chargecode_id = mmc.main_chargecode_id


where

reh.result_date between $P{fromDate} and $P{toDate}

and dt.bill_id !='' or dt.bill_id !=null
]]></queryString>

