	<queryString><![CDATA[select
     concat(hr_payroll_process_header.`year`,"/",hr_payroll_process_header.`month`)as period1,
     hr_payroll_process_header.`net_salary` as net_salary1,
     concat(mas_employee.`first_name`," ",mas_employee.`middle_name`," ",mas_employee.`last_name`)as employee,
     mas_employee.`employee_code` as employee_code,
     concat(hr_payroll_process_header_a.`year`,"/",hr_payroll_process_header_a.`month`)as period2,
     hr_payroll_process_header_a.`net_salary` as hr_payroll_process_header_a_net_salary
from
     `hr_payroll_process_header` hr_payroll_process_header right outer join `mas_employee` mas_employee on hr_payroll_process_header.`employee_id` = mas_employee.`employee_id`
     left outer join `hr_payroll_process_header` hr_payroll_process_header_a on mas_employee.`employee_id` = hr_payroll_process_header_a.`employee_id`
where mas_employee.`department_id` = $P{departmentID}
and   hr_payroll_process_header.`year`    = $P{year1}
and    hr_payroll_process_header_a.`year`  = $P{year2}
and   hr_payroll_process_header.`month`   = $P{month1}

and   hr_payroll_process_header_a.`month` = $P{month2}
]]></queryString>

