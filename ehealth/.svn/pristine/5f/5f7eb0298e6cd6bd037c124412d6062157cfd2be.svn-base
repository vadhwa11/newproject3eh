	<queryString><![CDATA[SELECT
     mas_referral_doctor.doctor_name AS mas_referral_doctor_doctor_name,
   CASE     WHEN mas_referral_doctor.status = 'Y' or mas_referral_doctor.status = 'y' THEN 'Active'     ELSE 'Inactive'
  	 END  as  mas_referral_doctor_status,
     mas_department.department_name AS mas_department_department_name,
