<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="FaBankReconciliationDetails" table="fa_bank_reconciliation_details">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="bank_reconciliation_details_id">
			<generator class="sequence">
				<param name="sequence">fa_bank_reconciliation_detail_bank_reconciliation_details_i_seq</param>
			</generator>
		</id>


		<property name="ChequeNo" column="cheque_no" type="string"
			not-null="false" length="50" />
		<property name="ChequeDate" column="cheque_date" type="date"
			not-null="false" length="13" />
		<property name="VoucherType" column="voucher_type" type="string"
			not-null="false" length="16" />
		<property name="ChequeAmt" column="cheque_amt" type="big_decimal"
			not-null="false" length="13" />
		<property name="ChequeStatus" column="cheque_status" type="string"
			not-null="false" length="500" />
		<property name="ClearingDate" column="clearing_date" type="date"
			not-null="false" length="13" />
			<property name="Narration" column="narration" type="string"
			not-null="false" length="32" />
		<many-to-one name="VoucherHeader" column="voucher_header_id"
			class="FaVoucherHeader" not-null="false">
		</many-to-one>
		<many-to-one name="BankReconciliationHeader" column="bank_reconciliation_header_id"
			class="FaBankReconciliationHeader" not-null="false">
		</many-to-one>



	</class>
</hibernate-mapping>