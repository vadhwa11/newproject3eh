(select administrative_sex_name from mas_administrative_sex mas
where mas.administrative_sex_id = p.sex_id ) as sex_name ,age ,
(select date_of_addmission from inpatient m where ad_no = $P{adNo}) as date_of_addmission,
(select discharge_date from inpatient m where ad_no = $P{adNo}) as discharge_date,
(select ad_no from inpatient m where ad_no = $P{adNo}) as ad_no,
(select hospital_name from mas_hospital m where m.hospital_id = p.hospital_id) as hname
