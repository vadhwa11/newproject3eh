	</parameter>
	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select 

 msi.nomenclature,mm.manufacturer_name,hospital_name,sibs.manufacture_date,sibs.batch_no,sibs.expiry_date,msic.item_unit_name,sum(srt.reserved_stock) as total,sibs.cost_price as unit
from   store_reservation_m  srm left outer join  store_reservation_t srt ON srt.reservation_m_id=srm.reservation_id
left outer join store_item_batch_stock sibs ON  sibs.stock_id = srt.stock_id  
left outer join mas_store_item msi ON msi.item_id = sibs.item_id 
left outer join mas_manufacturer mm ON sibs.manufacturer_id=mm.manufacturer_id
left outer join mas_hospital mh ON mh.hospital_id=sibs.hospital_id
left outer join mas_store_item_conversion msic ON msic.item_conversion_id=msi.item_conversion_id

Where  srt.reservation_till_date  <= $P{fromDate}  and sibs.hospital_id= $P{hospitalId}

GROUP BY msi.item_id ,
msi.nomenclature,mm.manufacturer_name,hospital_name,sibs.manufacture_date,sibs.batch_no,sibs.expiry_date,msic.item_unit_name,sibs.cost_price]]></queryString>

