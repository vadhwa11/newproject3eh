<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrReimbDetail" table="hr_reimb_detail">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="reimb_detail_id">
			<generator class="sequence">
				<param name="sequence">hr_reimb_detail_reimb_detail_id_seq</param>
			</generator>
		</id>

		<property name="ClaimDate" column="claim_date" type="date"
			not-null="false" />
		<property name="ClaimAmount" column="claim_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="Paid" column="paid" type="string" not-null="false"
			length="1" />
		<property name="ReimbDate" column="reimb_date" type="date"
			not-null="false" />
		<property name="ReimbAmount" column="reimb_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="Remarks" column="remarks" type="string"
			not-null="false" length="40" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="10" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="Hospital" column="hospital_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="ReimbHeader" column="reimb_header_id"
			class="HrReimbHeader" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>