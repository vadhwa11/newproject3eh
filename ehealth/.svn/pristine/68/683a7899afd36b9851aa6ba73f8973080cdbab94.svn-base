	<queryString><![CDATA[select main5.main_chargecode_name,
coalesce(main5.op_bill_amt,0) as op_bill_amt,
coalesce(ip_bill_amt,0) as ip_bill_amt ,
(coalesce(op_bill_amt,0)+coalesce(ip_bill_amt,0)) as total_amt
from(
select mmc.main_chargecode_name as main_chargecode_name,
sum(coalesce(ioop_bill.op_bill_amt,0)) as op_bill_amt,
sum(coalesce(ioop_bill.ip_bill_amt,0)) as ip_bill_amt
from
(select charge_code_id, main_chargecode_id, charge_code_name,
coalesce(op_net_amt,0)
as op_bill_amt, coalesce(ip_net_amt,0) as ip_bill_amt,
(coalesce(op_net_amt,0)+ coalesce(ip_net_amt,0)) as total_bill_amt
from (
Select mcc.main_chargecode_id, mcc.charge_code_id, charge_code_name,
coalesce(op_net_amt,0) as op_net_amt,ip_net_amt
from mas_charge_code mcc left outer join
(SELECT bl_op_bill_details.charge_code_id as charge_code_id,
coalesce(sum(bl_op_bill_details.rate*quantity),0) AS op_net_amt,
bl_op_bill_header.hospital_id as  hospital_id
FROM
bl_op_bill_header bl_op_bill_header LEFT OUTER JOIN bl_op_bill_details bl_op_bill_details
ON bl_op_bill_header.op_bill_header_id = bl_op_bill_details.op_bill_header_id
where bl_op_bill_header.bill_date between $P{fromDate} and $P{toDate}
and bl_op_bill_header.bill_time between $P{fromTime} and $P{toTime}
and  bl_op_bill_header.hospital_id= $P{hospitalId}
group by bl_op_bill_details.charge_code_id,bl_op_bill_header.hospital_id) as main1
on mcc.charge_code_id =main1.charge_code_id
left outer join
(SELECT bl_charge_slip_detail.charge_code_id as charge_code_id,
coalesce(sum(bl_charge_slip_detail.rate*quantity),0) AS ip_net_amt,
bl_charge_slip_main.hospital_id as  hospital_id
FROM
bl_charge_slip_main bl_charge_slip_main LEFT OUTER JOIN
bl_charge_slip_detail bl_charge_slip_detail ON 
bl_charge_slip_main.charge_slip_main_id = bl_charge_slip_detail.charge_slip_main_id
where bl_charge_slip_main.chg_slp_date between $P{fromDate} and $P{toDate}
and bl_charge_slip_main.chg_slp_time between $P{fromTime} and $P{toTime}
and bl_charge_slip_main.status='y'
and bl_charge_slip_main.hospital_id= $P{hospitalId}
group by bl_charge_slip_detail.charge_code_id,bl_charge_slip_main.hospital_id) as main2
on mcc.charge_code_id =main2.charge_code_id
) as main4
where main4.op_net_amt is not null OR main4.ip_net_amt is not null
) ioop_bill,mas_main_chargecode mmc
where ioop_bill.main_chargecode_id =mmc.main_chargecode_id
group by mmc.main_chargecode_name) as main5
where main5.op_bill_amt!=0 or main5.ip_bill_amt!=0

union

select 'not given' as charge_code_name,coalesce(op_amt,0) as op_amt,coalesce(ip_amt,0) as ip_amt,
(coalesce(op_amt,0)+coalesce(ip_amt,0)) as tot_amt from (
SELECT 'treat' as charge_code_id, 'TREATMENT PHARMACY' as charge_code_name,
sum(case when inpatient_id is null then bill_amt else 0.0 end) as op_amt,
sum(case when inpatient_id is not null then bill_amt else 0.0 end) as ip_amt 
FROM bl_dispensing_header boh
where bill_date between $P{fromDate} and $P{toDate} 
and bill_time between $P{fromTime} and $P{toTime}
and boh.status='y'
and boh.hospital_id= $P{hospitalId}
) as treat]]></queryString>

