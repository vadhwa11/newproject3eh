	<queryString>
		<![CDATA[select pnl.patient_name,
pnl.age,
sex.administrative_sex_name as gender,
pnl.address,
pc.post_code_name as post_office_name,
pc.pin_code,
pnl.phone_no,
mg.category_name as social_category,
pnl.no_of_skin_lesion,
pnl.no_of_nerves,
pnl.status_skin_smear_test,
pnl.disease_type,
pnl.contact_survey,
pnl.immigrant,
pnl.mode_of_detection,
pnl.type_of_case,
pnl.date_of_first_dose,
pnld.date_of_subsequent_doses as month,
pnld.date_of_subsequent_doses as date,
pnl.end_status,
pnl.deformity_disability,
pnl.site_voluntary_muscle,
pnl.grade_voluntary_muscle,
pnl.date_voluntary_muscle,
pnl.site_vision_test,
pnl.grade_vision_test,
pnl.date_vision_test,
pnl.finding,
pnl.lepra_reaction,
pnl.services_provided
from ph_nationalleprosyeradicationprograme pnl
LEFT OUTER JOIN ph_nationalleprosyeradicationprograme_detail pnld ON pnld.member_id = pnl.member_id
LEFT OUTER JOIN ph_member_survey ms ON pnl.member_id = ms.member_id
LEFT OUTER JOIN mas_category mg ON mg.category_id = ms.social_category
LEFT OUTER JOIN ph_house_survey hs ON ms.house_id = hs.house_hold_id
LEFT OUTER JOIN mas_post_code pc on hs.pin_code_id = pc.post_code_id
LEFT OUTER JOIN mas_hospital mh ON pnl.hospital_id = mh.hospital_id
LEFT OUTER JOIN mas_administrative_sex sex ON ms.gender = sex.administrative_sex_id]]>
	</queryString>
