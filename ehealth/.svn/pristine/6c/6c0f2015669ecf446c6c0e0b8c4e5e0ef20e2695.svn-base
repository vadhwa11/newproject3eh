@import url('tabcontent.css');

body,html {
	height: 100%;
}

body {
	font-family: Tahoma;
	font-size: 11px;
	color: #000000;
	margin-top: 0px;
	margin-right: 0px;
	margin-left: 0px;
	margin-bottom: 0px;
}

:-moz-any-link :focus {
	outline: none;
}

#main {
	background: url(../images/layoutBg.gif) repeat-x top #FFFFFF;
	margin: 0px auto;
	height: auto;
	border-top: 1px dashed #00CCFF;
	height: 100%;
}

#mainIn {
	background: #FFFFFF;
	width: 982px;
	padding: 0px 10px 0px 10px;
	margin: 0px auto;
	margin-bottom: 0px;
	height: auto;
	border-top: 1px dashed #00CCFF;
	height: 100%;
}

span {
	color: #FF0000;
}

form {
	margin: 0px;
	padding: 0px;
}

.clear {
	clear: both;
	overflow: hidden;
	padding: 0px;
	margin: 0px;
	height: 1px;
}

.division {
	clear: both;
	border-bottom: 1px solid #999999;
	overflow: hidden;
	margin: 10px 0px 10px 0px;
}

.paddingTop40 {
	padding-top: 40px;
}

.paddingTop15 {
	padding-top: 15px;
}

.paddLeft30 {
	padding-left: 30px;
}

.paddLeft35 {
	padding-left: 35px;
	float: left;
}

.paddLeft55 {
	padding-left: 55px;
	width: 500px;
	margin: 0px auto;
}

.floatLeft {
	float: left;
}

.floatRight {
	float: right;
}

/***************************************headerTags Starts****************************************************/
h2 {
	color: #058CBC;
	background: url(../images/titleIcon.gif) no-repeat right;
	margin: 0px;
	padding: 0px 20px 0px 10px;
	float: left;
	font: bold 15px/ 30px Tahoma;
}

h3 {
	color: #000000;
	margin: 0px;
	padding: 0px 10px 10px 10px;
	font: bold 20px Tahoma;
}

h4 {
	color: #F27824;
	font: bold 12px/ 20px tahoma;
	margin: 0px;
	padding: 0px 0px 0px 10px;
	float: left;
}

h5 {
	background: url(../images/plusIcon.gif) no-repeat right;
	color: #000000;
	font: bold 12px/ 20px tahoma;
	margin: 0px;
	padding: 0px 30px 0px 10px;
	float: left;
	width: 190px;
	text-align: right;
}

.minus {
	background: url(../images/minusIcon.gif) no-repeat right;
}

/*************************************** headerTags Ends****************************************************/
	/******** Css for Page Header starts here****************/
.header {
	background: url(../images/headerBg.gif) right top #FFFFFF no-repeat;
	height: 103px;
	width: 100%;
	margin: 0px auto;
}

.header span {
	color: #000000;
}

.hdText {
	font: bold 12px/ 22px Tahoma, Verdana, Arial, Helvetica, sans-serif;
	color: #000000;
	float: left;
	width: auto;
	padding: 0px 0px 0px 190px;
}

.hdText a {
	color: #033EFC;
	text-decoration: none;
}

.hdText a:hover {
	color: #FFF;
	text-decoration: underline;
}

.hdText img {
	vertical-align: middle;
}

.hdTextFix {
	width: 200px;
	float: left;
}

/******** Css for Page Header ends here ****************/
	/******** Css for Page Footer starts here ****************/
#footer {
	background: url(../images/footerBg.gif) repeat-x;
	height: 30px;
	width: 100%;
	text-align: center;
	font: normal 11px/ 25px Tahoma, Arial, Helvetica, sans-serif;
	color: #FFFFFF;
	position: fixed;
	clear: both;
	bottom: 0px;
	margin: 0px auto;
	margin-bottom: 0px;
}

#footer a {
	font: bold 11px/ 25px Tahoma, Arial, Helvetica, sans-serif;
	color: #FFFFFF;
	text-decoration: underline;
}

