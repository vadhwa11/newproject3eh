	/****** Css for Table Starts ******/
table {
	width: 976px;
	border: 1px solid #C0C0C0;
	border-right: 1px solid #C0C0C0;
	text-align: center;
}

table.small {
	width: 780px;
	border: 1px solid #C0C0C0;
	border-right: 1px solid #C0C0C0;
	text-align: center;
}

.tableover {
	background-color: #FFD69C !important;
}

table img {
	width: auto;
	border: none;
	height: auto;
	padding: 0px 0px 0px 3px;
}

table .hide {
	display: none;
}

table tr.odd {
	background: #FFF;
	cursor: default;
	color: 000;
}

table tr.odd a {
	color: #000;
	text-decoration: none;
}

table tr.even {
	background: #E1E4E5;
	cursor: default;
	color: #000;
	height: auto;
}

table tr.even a {
	color: #000;
	text-decoration: none;
	display: block;
	height: auto;
}

table th {
	width: auto;
	height: 20px;
	font: bold 11px/ 18px Tahoma;
	color: #FFFFFF;
	background: #036D92;
	border-right: 1px solid #C0C0C0;
	text-align: center;
}

table th input.radioCheck {
	border: none;
	color: #333333;
	font: normal 11px Tahoma;
	width: auto;
	height: 20px;
	float: none;
	margin: 0px 10px 0px 10px;
	padding: 0px;
	vertical-align: middle;
}

table td {
	width: auto;
	height: auto;
	font: normal 11px Tahoma;
