<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="StoreSlowMovingDrugs"
		table="store_slow_moving_drugs"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">store_slow_moving_drugs_id_seq</param>
			</generator>
		</id>

		<property
			name="ItemCode"
			column="item_code"
			type="string"
			not-null="true"
			length="16"
		/>
		<property
			name="ItemName"
			column="item_name"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="12"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Item"
			column="item_id"
			class="MasStoreItem"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>