	<queryString><![CDATA[select
     hr_mas_training.`training_name` as hr_mas_training_training_name,
     hr_mas_training.`fees_charged` as hr_mas_training_fees_charged,
     mas_hospital.`hospital_name` as mas_hospital_hospital_name,
     hr_mas_instructor.`instructor_name` as hr_mas_instructor_instructor_name,
     hr_training_schedule.`training_date` as hr_training_schedule_training_date,
    mas_department.`department_name` as mas_department_department_name
from
    `hr_training_schedule` hr_training_schedule inner join `hr_mas_training` hr_mas_training on hr_training_schedule.`training_id` = hr_mas_training.`training_id`
      left join `mas_department` mas_department on mas_department.department_id =hr_mas_training.department_id
     left join `hr_mas_instructor` hr_mas_instructor on hr_training_schedule.`instructor_id` = hr_mas_instructor.`instructor_id`
     left join `mas_hospital` mas_hospital  on mas_hospital.`hospital_id` = hr_training_schedule.`company_id`

where

	mas_department.`department_id` =  $P{departmentId}]]></queryString>

