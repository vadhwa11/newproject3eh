<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrUpdateLeaveBalanceHistory" table="hr_update_leave_balance_history">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_update_leave_balance_history_id_seq</param>
			</generator>
		</id>

		<property name="HrId" column="hr_id" type="integer" not-null="false"
			length="10" />
		<property name="EmpId" column="emp_id" type="integer"
			not-null="false" length="10" />
		<property name="PreviousLeaveBalance" column="previous_leave_balance"
			type="string" not-null="false" length="6" />
		<property name="NewLeaveBalance" column="new_leave_balance"
			type="string" not-null="false" length="6" />
		<property name="ModifiedAt" column="modified_at" type="timestamp"
			not-null="false" />
		<property name="Leavecounter" column="leavecounter" type="string"
			not-null="false" length="5" />
		<property name="Remarks" column="remarks" type="string"
			not-null="false" length="500" />
		<property name="PrevoiusOnsiteBalance" column="prevoius_onsite_balance"
			type="string" not-null="false" length="6" />
		<property name="NewOnsiteBalance" column="new_onsite_balance"
			type="string" not-null="false" length="6" />
		<property name="Fromdate" column="fromdate" type="date"
			not-null="false" />
		<property name="Todate" column="todate" type="date" not-null="false" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="BalanceAdjustBy" column="balance_adjust_by"
			type="string" not-null="false" length="6" />
		<many-to-one name="LastChgBy" column="last_chg_by"
			class="jkt.hms.masters.business.Users" not-null="false">
		</many-to-one>
		<many-to-one name="Company" column="company_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="users" class="jkt.hms.masters.business.MasEmployee"
			column="hr_id" update="false" insert="false">
		</many-to-one>

		<many-to-one name="EmployeeUpdated" class="jkt.hms.masters.business.MasEmployee"
			column="emp_id" update="false" insert="false">
		</many-to-one>

		<many-to-one name="LeaveType" column="leave_type"
			class="HrMasLeaveTypeMediator" not-null="false">
		</many-to-one>

	</class>
</hibernate-mapping>