	<queryString><![CDATA[select
     visit.`visit_no` as visit_visit_no,
     opd_oncology.`rt` as opd_oncology_rt,
     patient.`hin_no` as patient_hin_no,
     patient.`service_no` as patient_service_no,
     concat(patient.`p_first_name`,patient.`p_middle_name` ,patient.`p_last_name`) as patient_name,
     mas_hospital.`hospital_name` as mas_hospital_hospital_name
from
     `visit` visit right outer join `opd_oncology` opd_oncology on visit.`visit_id` = opd_oncology.`visit_id`
     left outer join `patient` patient on opd_oncology.`hin_id` = patient.`hin_id`
     left outer join `mas_hospital` mas_hospital on opd_oncology.`hospital_id` = mas_hospital.`hospital_id`


where
     opd_oncology.`stage_t` = $P{STAGE_T}
and opd_oncology.`stage_m` = $P{STAGE_M}
and opd_oncology.`stage_n` = $P{STAGE_N}
and opd_oncology.`stage_nor` = $P{STAGE_NOR}

and opd_oncology.`opd_date` between $P{FROM_DATE}and $P{TO_DATE}
]]></queryString>

