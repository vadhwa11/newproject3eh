	<queryString><![CDATA[SELECT frst.*, scnd.nomenclature
FROM(SELECT cast(EXTRACT(year from  bill_date) as varchar) as billyear,
cast(EXTRACT(month from  bill_date)as varchar) as billmonth, item_id,
sum(qty) as qty, sum(amount) as amount,
(cast(sum(cast(qty as float)) AS varchar(10)) || '-' ||
cast(sum(cast(amount as float)) AS varchar(10))) as totalamt
FROM  bl_dispensing_details bd LEFT OUTER JOIN bl_dispensing_header bh 
ON bd.dispensing_header_id  = bh.dispensing_header_id
where bill_date between $P{fmonthyear} and $P{tmonthyear} and bh.hospital_id=$P{hospitalId}
group by EXTRACT(year from  bill_date),EXTRACT(month from  bill_date),item_id
)AS  frst INNER JOIN
(SELECT item_id, nomenclature, mic.item_category_id as itemcatid
FROM  mas_store_item st LEFT JOIN mas_item_category mic 
ON st.item_category_id  = mic.item_category_id  
) AS scnd ON frst.item_id  = scnd.item_id  
$P!{criteria}

UNION

SELECT 'total year' as billyear, 'total month' as billmonth, frst.item_id,
frst.qty, frst.amount,
(cast(sum(cast(frst.qty as float)) AS varchar(10)) || '-' ||
cast(sum(cast(frst.amount as float)) AS varchar(10))) as totalamt,
scnd.nomenclature FROM
(SELECT EXTRACT(year from  bill_date) as billyear, 
EXTRACT(month from  bill_date) as billmonth,item_id,
sum(qty) as qty, sum(amount) as amount,
(cast(sum(cast(qty as float)) AS varchar(10)) || '-' ||
cast(sum(cast(amount as float)) AS varchar(10))) as totalamt
FROM  bl_dispensing_details bd LEFT OUTER JOIN bl_dispensing_header bh
ON bd.dispensing_header_id  = bh.dispensing_header_id 
where bill_date between $P{fmonthyear} and $P{tmonthyear} and bh.hospital_id=$P{hospitalId}
group by EXTRACT(year from  bill_date),EXTRACT(month from  bill_date),item_id
)AS  frst INNER JOIN
(SELECT item_id, nomenclature, mic.item_category_id as itemcatid
FROM  mas_store_item st LEFT JOIN mas_item_category mic ON
st.item_category_id  = mic.item_category_id  
) AS scnd ON frst.item_id = scnd.item_id  
$P!{criteria} 
GROUP BY scnd.nomenclature,frst.item_id, frst.qty, frst.amount]]></queryString>

