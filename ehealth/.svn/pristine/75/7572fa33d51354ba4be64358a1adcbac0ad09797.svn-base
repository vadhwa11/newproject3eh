	<queryString><![CDATA[select t1.id,t1.empid,t1.from_date,t1.to_date,t1.no_of_working_days,t1.approved_by,t1.approved_on,t1.leave_type,t1.joining_date,
t1.reason,t1.contact_address,t1.contact_phone,t1.applied_on,t1.status,t1.suggestion,t1.modified_by,t1.modified_on,t1.company_id,
t1.recommend_status,t2.pre_fromDate,t2.pre_toDate,pre_description,t2.grade_level from
(select * from hr_leave_details where empid = $P{empId}) t1 left outer join
(select hld.empid,grade_level,from_date as pre_fromDate,to_date as pre_toDate,hml.description as pre_description from hr_leave_details hld inner join
mas_employee me on me.employee_id=hld.empid inner join mas_rank mr on mr.rank_id=me.rank_id
inner join mas_grade mg on mg.grade_id=mr.grade_id inner join hr_mas_leave_type_mediator mltm on mltm.id=hld.leave_type inner join
hr_mas_leave_type_new hmltn on hmltn.id=mltm.leave_type_id inner join hr_mas_leave hml on hml.id=hmltn.leave_type  where hld.id not in
(select id from hr_leave_details where empid = $P{empId} order by id desc limit 1) order by hld.id desc limit 1 ) t2 on t1.empid=t2.empid;]]></queryString>

