	<queryString><![CDATA[select 
mas_hospital.hospital_name as hospital_name,
 ipd_hand_take_over.remarks_pending_work as remarks_pending_work,
ipd_hand_take_over.shift_time as shift_time,
(mas_employee.first_name+' '+mas_employee.last_name)as takeBy,
(mas_employee1.first_name+' '+mas_employee1.last_name)as handBy

from
 mas_department mas_department RIGHT JOIN ipd_hand_take_over ipd_hand_take_over ON mas_department.department_id = ipd_hand_take_over.department_id
 left join mas_employee mas_employee on ipd_hand_take_over.take_by=mas_employee.employee_id
 left join mas_employee mas_employee1 on ipd_hand_take_over.hand_by=mas_employee1.employee_id
left join mas_hospital mas_hospital on ipd_hand_take_over.hospital_id=mas_hospital.hospital_id

WHERE
     mas_department.department_name = $P{Ward}
     and ipd_hand_take_over.entry_date >= $P{fromDate}
     and ipd_hand_take_over.entry_date<= $P{toDate}]]></queryString>

