	<queryString><![CDATA[SELECT 
mi.icd_name,
mi.icd_code,
mas.administrative_sex_name,
substring(p.age,1,len(p.age)-6) as age11,
count(dic.icd_id) as total_icd
FROM
discharge_icd_code dic left outer join mas_icd mi on mi.icd_id=dic.icd_id
left outer join visit v on v.visit_id=dic.visit_id
left outer join mas_employee me on me.employee_id=v.doctor_id
left outer join patient p on p.hin_id=dic.hin_id
left outer join mas_administrative_sex mas on p.sex_id=administrative_sex_id
left outer join mas_unit mu on mu.unit_id=p.unit_id
where dic.add_edit_date between $P{fromDate} and $P{toDate} 
and  dic.icd_id is not null 
and mi.icd_code in('I00','I01','I02','I10','I11','I12','I13','I14','I15','I20','I21','I22','I23','I24','I25',
'Q20','Q28','Q22','Q23','Q24','Q25','Q26','Q27',
'I05','I09','I06','I07','I08','I26','I50','I51','I52','E10','E11','J40','J43','J45','T63.0','N17','N18','E66') 
or mi.icd_code like '%I2' or mi.icd_code like '%I3' or mi.icd_code like'%I4' or mi.icd_code like
'%F1' or mi.icd_code like '%S0'
group by mi.icd_name,
mi.icd_code,mas.administrative_sex_name,substring(p.age,1,len(p.age)-6) 
order by mi.icd_code,mas.administrative_sex_name,cast(substring(p.age,1,len(p.age)-6) as int)
;]]></queryString>

