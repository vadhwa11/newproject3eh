	</parameter>
	<queryString><![CDATA[select p.hin_no,p.p_first_name,scd.diag_no,inv.investigation_name,scd.investigation_id,mscc.sub_chargecode_name,mscc.sub_chargecode_code 
from dg_sample_collection_details scd
left outer join  dg_sample_collection_header sch on scd.sample_collection_header_id=sch.sample_collection_header_id
left outer join  mas_sub_chargecode mscc on mscc.sub_chargecode_id=scd.subcharge
left outer join dg_mas_investigation inv on scd.investigation_id=inv.investigation_id
left outer join patient p on p.hin_id=sch.hin_id
where sch.order_id=$P{order_id} and scd.investigation_id=$P{inv_id}]]></queryString>

