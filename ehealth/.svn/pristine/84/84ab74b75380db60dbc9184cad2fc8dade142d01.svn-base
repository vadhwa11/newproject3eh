<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="BlReceiptHeader"
		table="bl_receipt_header"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="receipt_header_id"
		>
			<generator class="sequence"><param name="sequence">bl_receipt_header_receipt_header_id_seq</param></generator>
		</id>

		<property
			name="ReceiptNo"
			column="receipt_no"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="ReceiptType"
			column="receipt_type"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="Amount"
			column="amount"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="ReceiptDate"
			column="receipt_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="ReceiptTime"
			column="receipt_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="CharityAmt"
			column="charity_amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="RoundOff"
			column="round_off"
			type="big_decimal"
			not-null="false"
			length="9"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="RemainingCredit"
			column="remaining_credit"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="TotalRcdAmt"
			column="total_rcd_amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="DispensingHeader"
			column="dispensing_header_id"
			class="BlDispensingHeader"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="ChangedBy"
			column="changed_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="PackageBill"
			column="package_bill_id"
			class="BlPackageBill"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="IpFinalBill"
			column="ip_final_bill_id"
			class="BlFinalBillDetails"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="OpdPatientDetail"
			column="opd_patient_detail_id"
			class="OpdPatientDetails"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="OpBillHeader"
			column="op_bill_header_id"
			class="BlOpBillHeader"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Transfer"
			column="transfer_id"
			class="Transfer"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="ChargeSlipMain"
			column="charge_slip_main_id"
			class="BlChargeSlipMain"
			not-null="false"
		>
		</many-to-one>
		
		<many-to-one
			name="Booking"
			column="booking_id"
			class="BlPaywardBooking"
			not-null="false"
		>
		</many-to-one>

		<set inverse="true" name="BlReceiptDetails">
			<key column="receipt_header_id" />
			<one-to-many class="BlReceiptDetails" />
		</set>

		<set inverse="true" name="FaVoucherHeaders">
			<key column="receipt_header_id" />
			<one-to-many class="FaVoucherHeader" />
		</set>



	</class>	
</hibernate-mapping>