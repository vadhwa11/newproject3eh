<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MasDietMenuItem"
		table="mas_diet_menu_item"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="diet_menu_item_id"
		>
			<generator class="sequence">
				<param name="sequence">mas_diet_menu_item_diet_menu_item_id_seq</param>
			</generator>
		</id>

		<property
			name="MenuType"
			column="menu_type"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="LastChgBy"
			column="last_chg_by"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="DietItems"
			column="diet_items_id"
			class="MasDietItems"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>