	</parameter>
	<parameter name="HOSPITAL_ID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select p.reg_date,
p.hin_no,
p.p_first_name ||' '|| p.p_middle_name ||' '|| p.p_last_name as p_name,
substring(p.age,3,7) as age1,p.age,
s.administrative_sex_name ,mh.hospital_name ||' '|| mh.address as hospital_name
from patient p
left outer join mas_administrative_sex s on s.administrative_sex_id=p.sex_id
left outer join maS_hospital mh on mh.hospital_id=$P{HOSPITAL_ID}
where ( trim(substring(p.age,3,7))='Days' and p.hospital_id=$P{HOSPITAL_ID} and p.reg_date between $P{fromDate} and $P{toDate} )or
( 
trim(substring(p.age,3,7))='Months' and p.hospital_id=$P{HOSPITAL_ID} and p.reg_date between $P{fromDate} and $P{toDate} )
order by p.reg_date,p.hin_no,s.administrative_sex_name]]></queryString>

