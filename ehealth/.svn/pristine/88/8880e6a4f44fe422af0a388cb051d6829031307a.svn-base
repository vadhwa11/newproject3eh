	<queryString><![CDATA[Select SUM(vd.dr_amount) AS dr_amount,SUM(vd.cr_amount) as cr_amount,ma.acc_desc, sg.account_sub_group_name,
mh.hospital_name,mh.address,mb.branch_desc
 from fa_voucher_details vd 
left join fa_voucher_header vh
on vd.voucher_header_id = vh.voucher_header_id 
left join fa_mas_account ma on  ma.acc_id = vd.account_id
left join fa_mas_account_sub_group sg on sg.account_sub_group_id = ma.account_sub_group_id
left join mas_branch mb on mb.branch_id = vh.branch_id
left join mas_hospital mh on mh.hospital_id = vh.hospital_id
where vh.voucher_date  between $P{from_date} and $P{to_date} and mb.branch_id = $P{branchId}
 and mh.hospital_id = $P{hospital_id} GROUP BY ma.acc_desc, sg.account_sub_group_name,
mh.hospital_name,mh.address,mb.branch_desc order by sg.account_sub_group_name]]></queryString>

