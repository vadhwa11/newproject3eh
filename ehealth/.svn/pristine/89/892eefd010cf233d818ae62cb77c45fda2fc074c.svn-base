	<queryString><![CDATA[select date,f.department_id,i.pvms_no,batch_no,f.nomenclature,sum(coalesce(f.opening_qty,0)) as opening_qty,
sum(coalesce(f.return_qty,0)) as return_qty,sum(coalesce(f.received_qty,0)) as received_qty,
sum(coalesce(f.issued_qty,0)) as issued_qty,sum(coalesce(f.department_return,0)) as department_return,
sum(coalesce(f.adjust_qty,0)) as adjust_qty,
(sum(coalesce(f.opening_qty,0))+sum(coalesce(f.received_qty,0))+
sum(coalesce(f.department_return,0))
-sum(coalesce(f.issued_qty,0))-sum(coalesce(f.return_qty,0)))+ sum(coalesce(f.adjust_qty,0)) as final_closing 
from stock_register_final as f
left outer join mas_store_item  i on i.item_id=f.item_id
group by date,f.department_id,batch_no,adjust_qty,f.nomenclature,i.pvms_no
order by date]]></queryString>

