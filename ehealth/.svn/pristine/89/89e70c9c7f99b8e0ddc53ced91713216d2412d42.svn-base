<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasStoreMeScale" table="mas_store_me_scale">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="me_scale_id">
			<generator class="sequence">
				<param name="sequence">mas_store_me_scale_me_scale_id_seq</param>
			</generator>
		</id>

		<property name="MeScale" column="me_scale" type="integer"
			not-null="false" length="10" />
		<property name="MeScaleDescription" column="me_scale_description"
			type="string" not-null="false" length="100" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />


		<set name="StoreMeScaleDetails" inverse="true">
			<key column="me_scale_id" />
			<one-to-many class="StoreMeScaleDetails" />
		</set>

		<set name="StoreGrnMs" inverse="true">
			<key column="me_scale_id" />
			<one-to-many class="StoreGrnM" />
		</set>

		<set name="StoreLoaninMs" inverse="true">
			<key column="me_scale_id" />
			<one-to-many class="StoreLoaninM" />
		</set>


	</class>
</hibernate-mapping>