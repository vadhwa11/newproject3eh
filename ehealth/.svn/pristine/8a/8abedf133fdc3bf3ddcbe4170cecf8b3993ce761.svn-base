package jkt.hms.util;

public class TaperedMedicineUtil {
 private Integer itemId;
 private Integer frequency;
 private String dosage;
 private java.math.BigDecimal dosageCount;
 private Integer duration;
 private java.math.BigDecimal total;
public Integer getItemId() {
	return itemId;
}
public void setItemId(Integer itemId) {
	this.itemId = itemId;
}
public Integer getFrequency() {
	return frequency;
}
public void setFrequency(Integer frequency) {
	this.frequency = frequency;
}
public String getDosage() {
	return dosage;
}
public void setDosage(String dosage) {
	this.dosage = dosage;
}
public java.math.BigDecimal getDosageCount() {
	return dosageCount;
}
public void setDosageCount(java.math.BigDecimal dosageCount) {
	this.dosageCount = dosageCount;
}
public Integer getDuration() {
	return duration;
}
public void setDuration(Integer duration) {
	this.duration = duration;
}
public java.math.BigDecimal getTotal() {
	return total;
}
public void setTotal(java.math.BigDecimal total) {
	this.total = total;
}
 
}
