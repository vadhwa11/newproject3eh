	<queryString><![CDATA[select ptt.hin_no,ptt.full_name,msi.nomenclature, 
pph.prescription_date,ppd.total,
(CASE when ppd.issued_status ='y' then 'Issued' else 'Not issued' end )as status
from patient_prescription_header pph
LEFT OUTER JOIN patient_prescription_details ppd ON (pph.prescription_id=ppd.prescription_id and ppd.prescription_id is not null)
LEFT OUTER JOIN patient ptt ON (ptt.hin_id=pph.hin_id and pph.hin_id is not null)
LEFT OUTER JOIN mas_store_item msi ON (msi.item_id=ppd.item_id and ppd.item_id is not null)
LEFT OUTER JOIN mas_frequency mfq ON (mfq.frequency_id=ppd.frequency_id and ppd.frequency_id is not null)
where pph.hospital_id=$P{hospitalId} 
and pph.prescription_date between $P{fromDate} and $P{toDate} order by pph.prescription_date asc;]]></queryString>

