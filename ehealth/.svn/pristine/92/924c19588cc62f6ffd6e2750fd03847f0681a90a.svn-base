,gatepass.printed_date,gatepass.valid_from,gatepass.valid_to ,gatepass.printed_time
,case when gatepass.valid_from=gatepass.valid_to then '23:59' else gatepass.printed_time end as printed_time_temp
from ipd_gatepass_details gatepass 
inner join inpatient ip on gatepass.inpatient_id=ip.inpatient_id
inner join patient p on ip.hin_id=p.hin_id
inner join mas_department md on ip.department_id=md.department_id
inner join mas_bed mb on ip.bed_id=mb.bed_id
inner join mas_hospital mh on gatepass.hospital_id=mh.hospital_id
inner join mas_pass_type ptype on gatepass.pass_type=ptype.pass_type_id
 where gatepass.gatepass_id=$P{ipdgatepassdetailId} and  gatepass.hospital_id=$P{hospitalId}]]></queryString>

