	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="fromDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="toDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select  msi.item_id , msi.nomenclature,mm.manufacturer_name,mh.hospital_name,sddt.batch_no,sddt.expiry_date,sibs.manufacture_date,sddm.entry_date as defDate,sum(sddt.defect_qty) as total
,sibs.cost_price as unit,sddt.authy_under_declared as resive,msic.item_unit_name

from   store_defective_drug_m  sddm left outer join  store_defective_drug_t sddt ON sddm.defect_m_id=sddt.defect_m_id

left outer join  store_item_batch_stock  sibs ON sddt.batch_no=sibs.batch_no and sddt.item_id=sibs.item_id and sddm.hospital_id=sibs.hospital_id and sddm.department_id=sibs.department_id
left outer join mas_store_item msi ON msi.item_id = sddt.item_id 
left outer join mas_manufacturer mm ON sibs.manufacturer_id=mm.manufacturer_id
left outer join mas_hospital mh ON mh.hospital_id=sddm.hospital_id
left outer join mas_store_item_conversion msic ON msic.item_conversion_id=msi.item_conversion_id
where 
sddm.hospital_id = 1
and sddm.entry_date between $P{fromDate} and $P{toDate}

GROUP BY msi.item_id ,
msi.nomenclature,mm.manufacturer_name,mh.hospital_name,sddt.batch_no,sddt.expiry_date,sibs.manufacture_date,sddm.entry_date ,sddt.defect_qty,sddt.authy_under_declared,msic.item_unit_name
,sibs.cost_price]]></queryString>

