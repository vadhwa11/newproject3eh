<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PhDiseaseRegistration"
		table="ph_disease_registration"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="disease_id"
		>
			<generator class="sequence">
				<param name="sequence">ph_disease_registration_disease_id_seq</param>
			</generator>
		</id>

		<property
			name="DiseaseType"
			column="disease_type"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="RegDate"
			column="reg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="ServerPk"
			column="server_pk"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="MemberId"
			column="member_id"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="DiseaseRegId"
			column="disease_reg_id"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Age"
			column="age"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="DateOnset"
			column="date_onset"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Hospitalised"
			column="hospitalised"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="HospitalName"
			column="hospital_name"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="MedicineSystam"
			column="medicine_systam"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="AdmissionDate"
			column="admission_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Diagnosis"
			column="diagnosis"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="OtherDiagnosis"
			column="other_diagnosis"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="InvestigationStatus"
			column="investigation_status"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="DiagnosisType"
			column="diagnosis_type"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="InfectionSource"
			column="infection_source"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="DiseaseStatus"
			column="disease_status"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Smoking"
			column="smoking"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="Tobacco"
			column="tobacco"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="Alcohol"
			column="alcohol"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="DrugAbuse"
			column="drug_abuse"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="NcdFamilyHistory"
			column="ncd_family_history"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="NcdFamilyDeath"
			column="ncd_family_death"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="UnderTreatment"
			column="under_treatment"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="Exercise"
			column="exercise"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="PhysialActivity"
			column="physial_activity"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="DietFlag"
			column="diet_flag"
			type="string"
			not-null="false"
			length="4"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>