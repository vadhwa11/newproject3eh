	<queryString><![CDATA[select * from(
select * from(
select mainresult1.*,
(select final_bill_no from bl_final_bill_details where inpatient_id = $P{inpatientId}) as fbno,
(select final_bill_date from bl_final_bill_details where inpatient_id = $P{inpatientId}) as fbdate,
case when mainresult2.cashamount is null then 0 else mainresult2.cashamount end as cashamount,
case when mainresult2.receiptamount is null then 0 else mainresult2.receiptamount end as receiptamount,
case when mainresult2.paymentadvice is null then 0 else mainresult2.paymentadvice end as paymentadvice,
case when mainresult2.billonaccount is null then 0 else mainresult2.billonaccount end as billonaccount
from
(
select * from
(select main.hin_id, main.inpatient_id, main.maincategoryname as maincategoryname,
main.amt as detailamt, main.stdgrn as stdgrn, main.stdspl as stdspl,
main.subcategory
from
(select hin_id, inpatient_id, bd.charge_slip_main_id,
coalesce(bd.rate,0)*(coalesce(bd.quantity,0)) as amt,
(coalesce(bd.std_deduction_gen,0)*coalesce(bd.quantity,0))as stdgrn,
(coalesce(std_deduction_spl,0)*coalesce(bd.quantity,0))as stdspl,
bd.hospital_id,
(select main_chargecode_name from mas_main_chargecode where main_chargecode_id =
(select distinct main_chargecode_id from mas_charge_code
where charge_code_id = bd.charge_code_id and bd.status='y')) as maincategoryname,
(select charge_code_name from mas_charge_code where charge_code_id = bd.charge_code_id) as subcategory
from bl_charge_slip_main bh
inner join bl_charge_slip_detail bd
on bh.charge_slip_main_id = bd.charge_slip_main_id
where bh.inpatient_id = $P{inpatientId} and
bh.status='y' )
as main

union all

select hin_id, inpatient_id, 'Medicine' as maincategoryname,
sum(amount+sales_tax_amt) as detailamt,0,0,
(select nomenclature from mas_store_item where item_id =
b.item_id) as subcategoryname from bl_dispensing_header h
left outer join bl_dispensing_details b
on h.dispensing_header_id = b.dispensing_header_id
where inpatient_id = $P{inpatientId} and
h.status='y'
group by hin_id,inpatient_id,hin_no,ad_no,item_id) as mainsql

inner join

(select
(select UPPER((p_first_name||' '||coalesce(p_middle_name,' ')||' '||coalesce(p_last_name,' ')))
from patient where hin_no = inp.hin_no) as patientname,
(select address from patient where hin_no = inp.hin_no) as patientAddress,
inpatient_id as inp,
date_of_addmission as admissiondate,
time_of_addmission as admissiontime,
(select hospital_name from mas_hospital where hospital_id =inp.hospital_id) as hospitalname,
(select address from mas_hospital where hospital_id = inp.hospital_id) as hospitaladdress,
(select department_name from mas_department where department_id =
inp.department_id) as departmentname, inp.ad_status, inp.hin_no,
inp.ad_no, discharge_date,discharge_time,
(select bed_no from mas_bed where bed_id = inp.bed_id) as bedno
from inpatient inp
where inp.inpatient_id =$P{inpatientId}
) as subsql
on mainsql.inpatient_id = subsql.inp
) as mainresult1
left join
(
select finalsql1.*, finalsql2.paymentadvice, finalsql2.billonaccount from
(select mainpayableamount.*, coalesce(mainreceiptamount.cashamount,0) as cashamount from
(
select sum(coalesce(amt,0)) as receiptamount,inpatient_id from
(
select sum(payable_amt) as amt,inpatient_id from bl_dispensing_header b
where inpatient_id = $P{inpatientId} and
status='y'
group by inpatient_id

union all

select sum(receipt_amt) as amt, inpatient_id
from bl_charge_slip_main b
where inpatient_id = $P{inpatientId} and
status='y'
group by inpatient_id
)as mainpayableamount1
where inpatient_id is not null
group by inpatient_id ) as mainpayableamount
left outer join
(
select sum(coalesce(amount,0)) as cashamount,
inpatient_id as inp
from bl_receipt_header
where inpatient_id = $P{inpatientId} and
receipt_type in ('adv')
group by inpatient_id ) as mainreceiptamount
on mainpayableamount.inpatient_id = mainreceiptamount.inp ) as finalsql1

left outer join

(select case when paymentadvise.paymentadvice is null then 0 else
paymentadvise.paymentadvice end as paymentadvice,
paymentadvise.inpatient_id, billamountsql.billonaccount from
(select $P{inpatientId} as inpatient_Id,
coalesce((select sum(coalesce(on_account_amt,0))*(-1)
from bl_payment_advice_header
where inpatient_id = $P{inpatientId}),0)+
coalesce((select sum(coalesce(on_account_amt,0))*(-1)
from bl_pymnt_advice_disp_header
where inpatient_id = $P{inpatientId}),0) +
coalesce((SELECT sum(coalesce(d.net_amt,0))*(-1)
FROM (select * from bl_charge_slip_main b
where inpatient_id = $P{inpatientId} and
status='y') as b,bl_charge_slip_detail d
where b.charge_slip_main_id = d.charge_slip_main_id and d.status='n'),0)
as paymentadvice) as paymentadvise

inner join
(
select sum(mainbill.billonaccount) as billonaccount,
mainbill.inpatient_id from
(
select case when sum(outstanding) is null then 0 else sum(outstanding) end
as billonaccount, inpatient_id from bl_dispensing_header
where inpatient_id = $P{inpatientId} and
status='y'
group by inpatient_id

union all

select case when sum(os_amt) is null then 0 else sum(os_amt) end as billonaccount,
inpatient_id from bl_charge_slip_main b
where inpatient_id = $P{inpatientId} and
status='y'
group by inpatient_id
)as mainbill
where inpatient_id is not null
group by inpatient_id ) as billamountsql
on paymentadvise.inpatient_id = billamountsql.inpatient_id) as finalsql2
on finalsql1.inpatient_id = finalsql2.inpatient_id) as mainresult2
on mainresult1.inpatient_id = mainresult2.inpatient_id
) as finalset1
left outer join
(select sum(coalesce(grossamount,0)) as grossamount,
sum(coalesce(charityamount,0)) as charityamount,
sum(coalesce(netamount,0)) as netamount, inpatient_id as inpid
from
(
select sum(coalesce(bill_amt,0)) as grossamount,
sum(coalesce(discount_amt,0)) as charityamount,
sum(coalesce(net_amt,0)) as netamount, inpatient_id
from bl_dispensing_header
where inpatient_id = $P{inpatientId} and
status='y'
group by inpatient_id

union all

select sum(coalesce(chg_slp_amt,0)) as grossamount,
coalesce(sum(discount_amt),0) as charityamount,
sum(coalesce(net_amt,0)) as netamount,
inpatient_id from bl_charge_slip_main b
where inpatient_id = $P{inpatientId} and
status='y'
group by inpatient_id
) as totalcalsql where inpatient_id is not null
group by inpatient_id ) as finalsetsql2
on finalset1.inpatient_id = finalsetsql2.inpid
) as mainsqlquery
order by mainsqlquery.maincategoryname,mainsqlquery.subcategory]]></queryString>

