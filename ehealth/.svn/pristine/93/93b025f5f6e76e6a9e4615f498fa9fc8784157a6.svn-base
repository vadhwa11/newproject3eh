	<queryString><![CDATA[select (c.department_name+' '+', chafb') department_name , b.ad_no, 
b.date_of_addmission admn_date,
(' at '+ b.time_of_addmission+' hrs') admn_time,
a.expiry_date,
(' at '+a.expiry_time+' hrs') expiry_time,
(d.p_first_name+' '+ISNULL(d.p_middle_name,' ')+' '+ISNULL(d.p_last_name,'')) patient_name, d.service_no,
h.rank_name, (d.s_first_name+' '+ISNULL(d.s_middle_name,' ')+' '+ISNULL(d.s_last_name,' ')) service_person_name,
g.relation_name patient_relation, e.trade_name, f.unit_name, d.age, a.consequence_of, a.d_death_cause_text, a.s_death_cause_text, a.c_death_cause_text, a.remarks, a.id_marks1, a.id_marks2,
d.next_of_kin_name, d.next_of_kin_address, d.next_of_kin_phone_number, i.relation_name next_of_kin_relation 
from expiry_details a, inpatient b, mas_department c, patient d, mas_trade e, mas_unit f, 
mas_relation g, mas_rank h, mas_relation i 
where a.inpatient_id = b.inpatient_id
and b.hin_id = a.hin_id
and a.ward_id = c.department_id
and a.hin_id = d.hin_id
and d.trade_id = e.trade_id
and d.unit_id = f.unit_id
and d.relation_id = g.relation_id
and d.rank_id = h.rank_id
and d.next_of_kin_relation_id = i.relation_id

and b.inpatient_id = $P{inpatientId}]]></queryString>

