<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="DialysisSetup"
		table="dialysis_setup"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="setup_id"
		>
			<generator class="sequence">
			<param name="sequence">dialysis_setup_setup_id_seq</param>
			</generator>
		</id>

		<property
			name="NoOfDoctor"
			column="no_of_doctor"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="FromTime"
			column="from_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="ToTime"
			column="to_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="SlotDuration"
			column="slot_duration"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="PercentageForSlots"
			column="percentage_for_slots"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="BreakFromTime"
			column="break_from_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="MaxNoOfDays"
			column="max_no_of_days"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="MinNoOfDays"
			column="min_no_of_days"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="BeforeTime"
			column="before_time"
			type="string"
			not-null="false"
			length="8"
		/>
		
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Days"
			column="days"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="BreakToTime"
			column="break_to_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="BreakFromTime2"
			column="break_from_time_2"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="BreakToTime2"
			column="break_to_time_2"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="BreakFromTime3"
			column="break_from_time_3"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="BreakToTime3"
			column="break_to_time_3"
			type="string"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="Dept"
			column="dept_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Doctor"
			column="doctor_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>