	<queryString><![CDATA[select store_issue_m.issue_no as issueNo, store_issue_m.issue_date as issueDate,store_issue_t.qty_issued as qty_issued,store_issue_t.batch_no as batch_no,
store_issue_t.qty_request as qty_request,mas_department_from.department_name as departmentFrom,
mas_department_to.department_name as departmentTo,mas_hospital.hospital_name as hospitalName,
mas_hospital.address as address,
(mas_employee_from.first_name||' '||coalesce(mas_employee_from.middle_name,'')||' '||coalesce(mas_employee_from.last_name,'')) as employeeNameFrom,
(mas_employee_to.first_name||' '||coalesce(mas_employee_to.middle_name,'')||' '||coalesce(mas_employee_to.last_name,'')) as employeeNameTo,
store_internal_indent_m.demand_no as demandNo,store_internal_indent_m.demand_date as demandDate,
mas_store_item.nomenclature as itemName,mas_store_item_conversion.item_unit_name as unitName,
store_issue_t.expiry_date as expiry_date
from store_issue_m  store_issue_m 
left outer join  store_issue_t  store_issue_t on  store_issue_t.issue_m_id =store_issue_m.id
left outer join mas_store_item  mas_store_item on store_issue_t.item_id = mas_store_item.item_id
left outer join mas_department mas_department_from on store_issue_m.department_id =mas_department_from.department_id
left outer join mas_department mas_department_to on store_issue_m.to_store =mas_department_to.department_id
left outer join mas_hospital as mas_hospital on store_issue_m.hospital_id=mas_hospital.hospital_id
left outer join mas_employee as mas_employee_from on store_issue_m.request_by =mas_employee_from.employee_id
left outer join mas_employee as mas_employee_to on store_issue_m.issued_by =mas_employee_to.employee_id
left outer join store_internal_indent_m store_internal_indent_m on store_issue_m.request_no = store_internal_indent_m.id
left outer join mas_store_item_conversion mas_store_item_conversion on mas_store_item.item_conversion_id = mas_store_item_conversion.item_conversion_id
where store_issue_m.id =$P{issueId} order by mas_store_item.nomenclature asc]]></queryString>

