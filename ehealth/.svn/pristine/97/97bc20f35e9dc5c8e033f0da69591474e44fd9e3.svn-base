<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="OpdPatientAllergyM" table="opd_patient_allergy_m">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="m_id">
			<generator class="sequence">
				<param name="sequence">opd_patient_allergy_m_m_id_seq</param>
			</generator>
		</id>

		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" length="13" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<many-to-one name="Inpatient" column="inpatient_id" class="Inpatient"
			not-null="false">
		</many-to-one>
		<many-to-one name="LastChgBy" column="last_chg_by" class="Users"
			not-null="false">
		</many-to-one>
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Visit" column="visit_id" class="Visit"
			not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="false">
		</many-to-one>
		<many-to-one name="Hin" column="hin_id" class="Patient"
			not-null="false">
		</many-to-one>
		<many-to-one name="Employee" column="employee_id" class="MasEmployee"
			not-null="false">
		</many-to-one>


		<set name="OpdPatientAllergyTs" inverse="true">
			<key column="opd_patient_allergy" />
			<one-to-many class="OpdPatientAllergyT" />
		</set>


	</class>
</hibernate-mapping>