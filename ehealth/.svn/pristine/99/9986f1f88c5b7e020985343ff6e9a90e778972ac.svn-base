	<queryString><![CDATA[SELECT
     bulk_main.name AS bulk_main_name,
     bulk_main.active_date AS bulk_main_activate_date,
     bulk_main.message AS bulk_main_message,
     bulk_main.activate_time AS bulk_main_activate_time,
     bulk_details.mobile_no AS bulk_details_mobile_no,
     bulk_details.sent_time AS sent_time,
    (case bulk_details.status when 'S' then 'Send' when 'F' then 'Failed' when 'U' then 'Unsend' else 'Cancled' end) bulk_details_status
FROM
     bulk_main bulk_main LEFT OUTER JOIN 
bulk_details bulk_details ON bulk_main.id = bulk_details.group_main_id
where  bulk_details.mobile_no is NOT NULL 
$P!{input_string}]]></queryString>

