<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="StoreMeScaleDetails" table="store_me_scale_details">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">store_me_scale_details_id_seq</param>
			</generator>
		</id>

		<property name="SerialNo" column="serial_no" type="integer"
			not-null="false" length="10" />
		<property name="Qty" column="qty" type="big_decimal"
			not-null="false" length="9" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="false">
		</many-to-one>
		<many-to-one name="MeScale" column="me_scale_id" class="MasStoreMeScale"
			not-null="false">
		</many-to-one>
		<many-to-one name="Item" column="item_id" class="MasStoreItem"
			not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>