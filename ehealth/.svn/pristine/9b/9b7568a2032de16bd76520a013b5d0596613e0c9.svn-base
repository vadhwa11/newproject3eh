	<parameter name="FROM_DATE" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="TO_DATE" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="DEPARTMENT_ID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="EMPLOYEE_ID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="hospitalId" isForPrompting="false" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select distinct
      (patient.p_first_name||' '||COALESCE(patient.p_middle_name,'')||' '||COALESCE(patient.p_last_name,''))as patient_name,
    	mas_employee.emp_name as mas_employee_name,
      (visit.age||'/'||mas_administrative_sex.administrative_sex_name)as age_sex,
      patient_investigation_header.investigation_date as patient_investigation_header_investigation_date,
      patient_investigation_header.investigation_time as patient_investigation_header_investigation_time,
      patient_investigation_details.investigation_header_id as patient_investigation_details_investigation_header_id,
      visit.visit_no as visit_visit_no,
      (visit.age||'/'||mas_administrative_sex.administrative_sex_name)as age_sex,
      patient.service_no as patient_service_no,
      mas_department.department_name as mas_department_department_name,
      mas_hospital.hospital_name as mas_hospital_hospital_name,
      mas_hospital.address as mas_hospital_address,
      (patient.p_first_name||' '||COALESCE(patient.p_middle_name,'')||' '||COALESCE(patient.p_last_name,''))as patient_name,
      patient.hin_no as patient_hin_no,
      (mas_employee.first_name||' '||COALESCE(mas_employee.last_name,'')||' '||COALESCE(mas_employee.middle_name,'')) as  mas_employee_name
from
     patient_investigation_header patient_investigation_header left outer join patient_investigation_details patient_investigation_details on patient_investigation_header.investigation_header_id = patient_investigation_details.investigation_header_id
     left outer join visit visit on patient_investigation_header.visit_id = visit.visit_id
     left outer join patient patient on patient_investigation_header.hin_id = patient.hin_id
     left outer join mas_department mas_department on patient_investigation_header.department_id = mas_department.department_id
     left outer join mas_hospital mas_hospital on patient_investigation_header.hospital_id = mas_hospital.hospital_id
     and mas_hospital.hospital_id = patient.hospital_id
     inner join mas_employee mas_employee on mas_hospital.hospital_id = mas_employee.hospital_id
     and mas_department.department_id = mas_employee.department_id
     and mas_department.department_id = visit.department_id
     and patient.hin_id = visit.hin_id
     left outer join mas_administrative_sex mas_administrative_sex on patient.sex_id = mas_administrative_sex.administrative_sex_id
WHERE
     patient_investigation_header.investigation_date >= $P{FROM_DATE}
     AND patient_investigation_header.investigation_date <= $P{TO_DATE}
     AND patient_investigation_header.department_id = $P{DEPARTMENT_ID}
     AND mas_employee.employee_id = $P{EMPLOYEE_ID}
     AND patient_investigation_header.hospital_Id =$P{hospitalId}]]></queryString>

