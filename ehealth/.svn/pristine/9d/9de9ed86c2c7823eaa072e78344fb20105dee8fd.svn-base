	<queryString><![CDATA[select ip.date_of_addmission,p.hin_no,ip.ad_no,
p.p_first_name  as p_name,
p.age,s.administrative_sex_name,(mh.hospital_name ||' '|| COALESCE(mh.address,'')) as hospital_name 
from inpatient ip
left outer join patient p on p.hin_id=ip.hin_id
