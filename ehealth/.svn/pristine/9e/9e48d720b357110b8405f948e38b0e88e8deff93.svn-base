	<queryString><![CDATA[select sdm.entry_date,sdm.entry_no,sdm.to_place,mh.hospital_name,mh.address,md.department_name,
sdt.expiry_date,msi.nomenclature,sdt.batch_no,manu.manufacturer_name,sdt.defect_qty,
sib.cost_price,(coalesce(sdt.defect_qty,0)*coalesce(sib.cost_price,0)) as value 
from store_defective_drug_m as sdm
left outer join store_defective_drug_t as sdt on sdm.defect_m_id=sdt.defect_m_id
left outer join store_item_batch_stock sib on sdt.item_id=sib.item_id and sdt.batch_no=sib.batch_no and sdm.department_id=sib.department_id
left outer join  mas_manufacturer manu on sdt.manufactured_by=manu.manufacturer_id
left outer join mas_store_item msi on sdt.item_id=msi.item_id
left outer join mas_department md on sdm.department_id=md.department_id
left outer join mas_hospital mh on sdm.hospital_id=mh.hospital_id
where sdm.entry_no=$P{entryNo}
and sdm.department_id=$P{DEPART}
and sdm.hospital_id=$P{hospitalId}]]></queryString>

