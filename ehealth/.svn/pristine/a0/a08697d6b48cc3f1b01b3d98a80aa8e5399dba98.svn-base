<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="AppInvestigationSetup"
		table="app_investigation_setup"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="app_investigation_id"
		>
			<generator class="sequence">
				<param name="sequence">app_investigation_setup_app_investigation_id_seq</param>
			</generator>
		</id>

		<property
			name="FromTime"
			column="from_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="ToTime"
			column="to_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="SlotDuration"
			column="slot_duration"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="BreakFromTime"
			column="break_from_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="BreakToTime"
			column="break_to_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="PercentageForSlots"
			column="percentage_for_slots"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="MaxNoOfDays"
			column="max_no_of_days"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="MinNoOfDays"
			column="min_no_of_days"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Days"
			column="days"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="NoOfEquipments"
			column="no_of_equipments"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="StartToken"
			column="start_token"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="TotalInterval"
			column="total_interval"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="TotalToken"
			column="total_token"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Equipment"
			column="equipment_id"
			class="MasStoreItem"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>