	<queryString>
		<![CDATA[select prn.patient_name,
prn.address,
pc.post_code_name as post_office_name,
pc.pin_code,
prn.phone_no,
prn.age,
sex.administrative_sex_name as gender,
prn.name_of_relative,
prn.address_relative,
prn.mobile_relative,
prn.sputum_taken_on,
prn.name_of_dmc,
prn.lab_no,
prn.result_of_sputum_test,
prn.other_test,
prn.type_patient,
prn.specify_other,
prn.disease_classification,
prn.weight,
prn.name_medical_officer,
prn.institution_medical_officer,
prn.name_of_dots_center,
prn.category_dots,
prn.treatment_starting_date,
prn.followup_sputum_exam,
prn.followup_date_sputum_taken,
prn.name_of_dmc_followup,
prn.lab_no_followup,
prn.rsult_of_sputum_test_followup,
prn.weight_of_patient_followup,
prn.treatment_complication_date,
prn.treatment_outcome
from ph_revisednationaltuberculosis prn
LEFT OUTER JOIN ph_member_survey ms ON prn.member_id = ms.member_id
LEFT OUTER JOIN mas_category mg ON mg.category_id = ms.social_category
LEFT OUTER JOIN ph_house_survey hs ON ms.house_id = hs.house_hold_id
LEFT OUTER JOIN mas_post_code pc on hs.pin_code_id = pc.post_code_id
LEFT OUTER JOIN mas_hospital mh ON prn.hospital_id = mh.hospital_id
LEFT OUTER JOIN mas_administrative_sex sex ON ms.gender = sex.administrative_sex_id
where prn.treatment_starting_date  between $P{fromDate} and $P{toDate}
$P!{query}]]>
	</queryString>
