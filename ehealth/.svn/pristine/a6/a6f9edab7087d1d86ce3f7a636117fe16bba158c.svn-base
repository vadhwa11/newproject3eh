	<queryString><![CDATA[select
bdr.dob_death_date,ax.administrative_sex_name,ms.name,bdr.cause_of_death,hs.address,mh.hospital_name,ms.aadhaar_no,
(select (bdr.dob_death_date-ms.date_of_birth)/365) as age
from ph_birth_death_reg bdr left outer join ph_member_survey ms on bdr.member_id=ms.member_id  
left outer join ph_house_survey hs on hs.house_hold_id=ms.house_id 
left outer join mas_administrative_sex  ax on ax.administrative_sex_id=bdr.gender
left outer join mas_hospital  mh on mh.hospital_id=bdr.hospital_id

where bdr.reg_type = 'death' 
    and  bdr.reg_date between $P{fromDate} and $P{toDate} $P!{query}]]></queryString>

