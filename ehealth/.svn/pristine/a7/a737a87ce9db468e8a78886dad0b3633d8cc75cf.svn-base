<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="OpdTemplateDepartmentWise" table="opd_template_department_wise">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="opd_template_department_wise_id">
			<generator class="sequence">
				<param name="sequence">opd_template_department_wise_opd_template_department_wise_id_seq</param>
			</generator>
		</id>

		<property name="Remarks" column="remarks" type="string"
			not-null="false" length="40" />
		<property name="Result" column="result" type="string"
			not-null="false" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="false">
		</many-to-one>
		<many-to-one name="Visit" column="visit_id" class="Visit"
			not-null="false">
		</many-to-one>
		<many-to-one name="Hin" column="hin_id" class="Patient"
			not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>