	<parameter name="deptId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select msi.pvms_no,msi.nomenclature,msic.item_unit_name,
case msi.ved when '1' then 'Vital'
when '2' then 'Essential'
when '3' then 'Desirable' else '' end as ved_status,
sum(isnull(sgt.unit_rate,0)*isnull(sibs.closing_stock,0))as value,
sum(isnull(sibs.closing_stock,0)) as stockInHand from mas_store_item msi
left outer join mas_store_item_conversion msic on msi.item_conversion_id=msic.item_conversion_id
left outer join store_item_batch_stock sibs on msi.item_id=sibs.item_id and msi.department_id=sibs.department_id
left outer join store_grn_t sgt on sibs.item_id=sgt.item_id and sibs.batch_no=sgt.batch_no
where msi.department_id=$P{deptId}
group by msi.nomenclature,msi.pvms_no,msic.item_unit_name,msi.ved
order by msi.ved,sum(isnull(sgt.unit_rate,0)*isnull(sibs.closing_stock,0)) desc]]></queryString>

