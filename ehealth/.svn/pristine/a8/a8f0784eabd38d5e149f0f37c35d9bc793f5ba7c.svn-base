<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">

<class
		name="HrEmployeeBalanceNew"
		table="hr_employee_balance_new"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">hr_employee_balance_new_id_seq</param>
			</generator>
		</id>

		<property
			name="OpeningBalance"
			column="opening_balance"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="Taken"
			column="taken"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="Earned"
			column="earned"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="ClosingBalance"
			column="closing_balance"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<!-- <property
			name="LastChgBy"
			column="last_chg_by"
			type="string"
			not-null="false"
			length="12"
		/> -->
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="BalanceAdjustedBy"
			column="balance_adjusted"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="AlreadyAvailedPatMat"
			column="already_availed_pat_mat"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="TotalLeaveTaken"
			column="total_leave_taken"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="Accrued"
			column="accrued"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="OpeningBalanceYearly"
			column="opening_balance_yearly"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="ClosingBalanceYearly"
			column="closing_balance_yearly"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="EntitlementAccJoinDate"
			column="entitlement_acc_join_date"
			type="string"
			not-null="false"
			length="6"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="jkt.hms.masters.business.Users"
			not-null="false"
		> 
		</many-to-one>
		<many-to-one
			name="LeaveType"
			column="leave_type"
			class="HrMasLeaveTypeMediator"
			not-null="false"
			lazy="false"
		>
		</many-to-one>
		<many-to-one
			name="Emp"
			column="emp_id"
			class="jkt.hms.masters.business.MasEmployee"
			not-null="false"
			lazy="false"
		>
		</many-to-one>
		<many-to-one
			name="Company"
			column="company_id"
			class="jkt.hms.masters.business.MasHospital"
			not-null="false"
		>
		</many-to-one>

