	<queryString><![CDATA[select 'Servicing' as Servicing ,p.hin_no,h.bill_no,bill_date,bill_time,patient_name as name,
bill_amt,charity ,net_amt,h.authorizer_id,ma.authorizer_name,
(hospital_name+' '+m.Address) as hospital_name
from bl_op_bill_header h
inner join mas_authorizer ma on h.authorizer_id = ma.authorizer_id
left outer join patient p on h.hin_id=p.hin_id
left outer join mas_hospital m on h.hospital_id = m.hospital_id
where charity is not null and charity != 0
and bill_date between $P{fromDate} and $P{todate}

union

select 'Dispensing',p.hin_no,h.bill_no,bill_date,bill_time,patient_name,
bill_amt,charity ,net_amt,h.authorizer_id,ma.authorizer_name,
(hospital_name+' '+m.Address) as hospital_name
from bl_dispensing_header h
inner join mas_authorizer ma on h.authorizer_id = ma.authorizer_id
left outer join mas_hospital m on h.hospital_id = m.hospital_id
left outer join patient p on h.hin_id=p.hin_id
where charity is not null and charity != 0
and bill_date between $P{fromDate} and $P{todate}
order By 4,5,1,2]]></queryString>

