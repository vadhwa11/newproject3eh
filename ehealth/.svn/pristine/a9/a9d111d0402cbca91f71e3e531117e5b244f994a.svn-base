/* ######### CSS for Shade Tabs. Remove if not using ######### */
.tabcontainerDHold {
	float: left;
	width: 700px;
	padding: 0px;
	margin: 1px 0px 0px 5px;
}

.tabcontainerD {
	padding: 10px 0px 10px 0px;
	height: 170px;
	width: 430px;
	float: left;
	border: none;
	margin-top: 15px;
}

.tabcontainerD label {
	background: #FFF;
	font: normal 10px verdana;
	line-height: normal;
	color: #000;
	width: 120px;
	text-align: right;
	height: 25px;
}

.tabcontainerD label.value {
	font: normal 10px/ 15px verdana;
	color: #0350BB;
	width: 80px;
	text-align: center;
	height: 17px;
	padding: 0px;
	/*border:1px solid #C8C8C8;*/
	background: #fff;
}

.tabcontainerD label.value2 {
	font: normal 10px/ 15px verdana;
	color: #0350BB;
	width: 140px;
	text-align: center;
	height: 17px;
	padding: 0px 0px 0px 10px;
	/*border:1px solid #C8C8C8;*/
	background: #fff;
}

.shadetabsD {
	padding: 3px 0;
	margin: 20px 10px 2px 10px;
	font: normal 11px Verdana;
	list-style-type: none;
	text-align: left;
	/*set to left, center, or right to align the menu as desired*/
}

.shadetabsD li {
	display: inline;
	margin: 0px;
}

.shadetabsD li a {
	text-decoration: none;
	position: relative;
	z-index: 1;
	padding: 3px 7px;
	margin-right: 3px;
	border: 1px solid #C4EA8E;
	color: #2d2b2b;
	background: #ffffff url(../images/tabcontent/shadeD.gif) top left
		repeat-x;
	height: 20px;
}

.shadetabsD li a:visited {
	color: #2d2b2b;
}

.shadetabsD li a:hover {
	text-decoration: underline;
	color: #2d2b2b;
}

.shadetabsD li a.selected { /*selected main tab style */
	position: relative;
	top: 0px;
}

* html .shadetabsD li a.selected { /*selected main tab style */
	position: fixed;
	top: 1px;
}

.shadetabsD li a.selected { /*selected main tab style */
	background: url(../images/tabcontent/shadeactiveD.gif) repeat-x;
	height: 20px;
	border: 1px solid #D6EFFF;
}

.shadetabsD li a.selected:hover { /*selected main tab style */
	text-decoration: none;
}

.tabcontentD {
	display: none;
	padding: 0px 5px 0px 5px;
}

* html.tabcontentD {
	display: none;
	padding: 0px;
}

@media print {
	.tabcontentD {
		display: block !important;
	}
}

label.tab {
	text-decoration: none;
	padding: 0px 10px 0px 10px;
	margin-right: 3px;
	border: 1px solid #C4EA8E;
	color: #2d2b2b;
	background: #ffffff url(../images/tabcontent/shadeD.gif) top left
		repeat-x;
	height: 20px;
	text-align: left;
	width: auto;
	font: normal 11px/ 20px verdana;
}

label.tab a {
	text-decoration: none;
	color: #2d2b2b;
}

label.selected { /*selected main tab style */
	background: url(../images/tabcontent/shadeactiveD.gif) repeat-x;
	height: 20px;
	border: 1px solid #D6EFFF;
	text-align: left;
	width: auto;
	font: normal 11px/ 20px verdana;
	padding: 0px 10px 0px 10px;
	margin-right: 3px;
	color: #2d2b2b;
}

label.selected:hover { /*selected main tab style */
	text-decoration: none;
}