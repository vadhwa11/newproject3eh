<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="FaMasVoucherType"
		table="fa_mas_voucher_type"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="voucher_type_id"
		>
			<generator class="sequence">
				<param name="sequence">fa_mas_voucher_type_voucher_type_id_seq</param>
			</generator>
		</id>

		<property
			name="VoucherTypeCode"
			column="voucher_type_code"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="VoucherTypeDesc"
			column="voucher_type_desc"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>

		<set inverse="true" name="FaVoucherHeaders">
			<key column="voucher_type_id" />
			<one-to-many class="FaVoucherHeader" />
		</set>



	</class>	
</hibernate-mapping>