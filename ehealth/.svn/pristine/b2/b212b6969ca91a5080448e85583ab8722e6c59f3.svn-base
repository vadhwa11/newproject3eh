	<queryString><![CDATA[select a.stock_id, b.pvms_no, b.nomenclature,e.unit_name au, a.batch_no, a.expiry_date,
a.closing_stock,a.dispencing_price as rate, (a.closing_stock*a.dispencing_price) as amount
from store_item_batch_stock a
inner join mas_store_item b on a.item_id=b.item_id,
mas_store_item_conversion d,
mas_store_unit e
where b.item_conversion_id = d.item_conversion_id
and d.purchase_unit_id = e.unit_id
and a.department_id = $P{deptId}
and a.hospital_id = $P{hospitalId}
and a.expiry_date <= $P{fromDate}
and a.closing_stock >0 and a.closing_stock is not null


order by a.expiry_date]]></queryString>

