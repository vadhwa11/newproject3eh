	<queryString><![CDATA[select
     concat( mas_employee.`first_name`,mas_employee.`middle_name`, mas_employee.`last_name`) as employee_name,
	 mas_employee.`employee_code` as mas_employee_employee_code,
     mas_hospital.`hospital_name` as mas_hospital_hospital_name,
     mas_department.`department_name` as mas_department_department_name,
     hr_mas_training.`training_name` as hr_mas_training_training_name,
      hr_mas_training_status.`training_status_description` training_status
from
      `hr_training_requisition` hr_training_requisition left join `mas_employee` mas_employee on mas_employee.`employee_id` = hr_training_requisition.`employee_id`

     left join `mas_hospital` mas_hospital on hr_training_requisition.`company_id` = mas_hospital.`hospital_id`

     left join `hr_mas_training` hr_mas_training on hr_mas_training.`training_id` = hr_training_requisition.`training_id`
     left join `mas_department` mas_department on hr_training_requisition.`department_id` = mas_department.`department_id`
      left join `hr_mas_training_status` hr_mas_training_status on hr_mas_training_status.training_status_id =hr_training_requisition.training_status_id
where
 hr_mas_training_status.`training_status_id` =$P{reqStatusId}

$P!{qryString}
]]></queryString>

