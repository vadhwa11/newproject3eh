	<queryString><![CDATA[select
       concat(patient.`p_first_name`,' ',patient.`p_middle_name`,patient.`p_last_name`)as pname,
     mas_village.`village_name` as patient_village,
     patient.`age` as patient_age,
     inpatient.`date_of_addmission` as inpatient_date_of_addmission,
     patient.`hin_no` as patient_hin_no,
     concat(mas_hospital.`hospital_name`,' ',mas_hospital.address) as mas_hospital_hospital_name,
     mas_administrative_sex.`administrative_sex_name` as mas_administrative_sex_administrative_sex_name,
     inpatient.`inpatient_id` as inpatient_inpatient_id,
     patient.`next_of_kin_name` as patient_next_of_kin_name,
     mas_post_code.`post_code_name` as patient_post_code_name,
     mas_department.`department_name` as mas_department_department_name,
    bed_no as inpatient_bed_id
from
     `patient` patient 
     right outer join `inpatient` inpatient on patient.`hin_id` = inpatient.`hin_id`
     left outer join `mas_administrative_sex` mas_administrative_sex on patient.`sex_id` = mas_administrative_sex.`administrative_sex_id`
     inner join `mas_hospital` mas_hospital on inpatient.`hospital_id` = mas_hospital.`hospital_id`
     left outer join `mas_department` mas_department on inpatient.`ad_ward_id` = mas_department.`department_id`
     left outer join `mas_village` mas_village on patient.village_id = mas_village.village_id
     left outer join `mas_post_code` mas_post_code on patient.`post_code_id` = mas_post_code.`post_code_id`
     left outer join mas_bed on inpatient.bed_id=mas_bed.bed_id
where inpatient.ad_status='a'
 Order By inpatient_date_of_addmission, mas_department_department_name]]></queryString>

