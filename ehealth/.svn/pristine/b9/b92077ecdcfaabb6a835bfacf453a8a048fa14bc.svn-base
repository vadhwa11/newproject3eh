	<queryString><![CDATA[SELECT
     ot_specimen_dispatch_entry.entry_no AS ot_specimen_dispatch_entry_entry_no,
     ot_specimen_dispatch_entry.ot_specimen_dispatch_entry_date AS ot_specimen_dispatch_entry_ot_specimen_dispatch_entry_date,
     ot_specimen_dispatch_entry.tissue_organ AS ot_specimen_dispatch_entry_tissue_organ,
     ot_specimen_dispatch_entry.examination_required AS ot_specimen_dispatch_entry_examination_required,
     ot_specimen_dispatch_entry.time_of_dispatch AS ot_specimen_dispatch_entry_time_of_dispatch,
    concat(mas_employee.first_name, mas_employee.last_name,
     mas_employee.middle_name) AS specimenDispatchBy,
    concat(mas_employee_A.first_name,
     mas_employee_A.last_name,
     mas_employee_A.middle_name) AS specimenReceivedBy,
     patient.hin_no AS patient_hin_no,
     concat(patient.p_first_name,
     patient.p_middle_name,
     patient.p_last_name) AS patientName,
     concat(patient.s_first_name,patient.s_middle_name,
     patient.s_last_name) AS servicePersonName,
     mas_hospital.hospital_name AS mas_hospital_hospital_name
FROM
     mas_employee mas_employee INNER JOIN ot_specimen_dispatch_entry ot_specimen_dispatch_entry ON mas_employee.employee_id = ot_specimen_dispatch_entry.specimen_dispatched_by
     INNER JOIN mas_employee mas_employee_A ON ot_specimen_dispatch_entry.specimen_received_by = mas_employee_A.employee_id
     INNER JOIN patient patient ON ot_specimen_dispatch_entry.hin_id = patient.hin_id
     INNER JOIN mas_hospital mas_hospital ON ot_specimen_dispatch_entry.hospital_id = mas_hospital.hospital_id
  where  ot_specimen_dispatch_entry.entry_no = $P{entryNo} and ot_specimen_dispatch_entry.hospital_id=$P{hospitalId}]]></queryString>

