	<queryString><![CDATA[select ic.add_edit_date,ic.add_edit_time,(sc.icd_sub_category_name||' :'||mi.icd_name) as diagName,
(' ['||mi.icd_code||']') as diagCode, 
case ic.z03 when 'y' then 'Yes'
when 'n' then 'No' end as Z03,
case ic.z09 when 'y' then 'Yes'
when 'n' then 'No' end as Z09,
case ic.diagnosis_status when 'p' then 'Provisional'
when 'f' then 'Final' end as diagnosis_status
from discharge_icd_code ic left join mas_icd mi on ic.icd_id=mi.icd_id
left join mas_icd_sub_category sc on mi.icd_sub_category_id=sc.icd_sub_category_id
where ic.hin_id=$P{hin_id} and ic.inpatient_id=$P{inpatient_id}
order by ic.add_edit_time desc]]></queryString>

