<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrEmployeeBalanceHistory" table="hr_employee_balance_history">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_employee_balance_history_id_seq</param>
			</generator>
		</id>

		<property name="OpeningBalance" column="opening_balance"
			type="string" not-null="false" length="6" />
		<property name="Taken" column="taken" type="string" not-null="false"
			length="6" />
		<property name="Earned" column="earned" type="string"
			not-null="false" length="6" />
		<property name="ClosingBalance" column="closing_balance"
			type="string" not-null="false" length="6" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="LeaveType" column="leave_type" class="HrMasLeaveType"
			not-null="false">
		</many-to-one>
		<many-to-one name="Emp" column="emp_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="false">
		</many-to-one>
		<many-to-one name="Company" column="company_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>