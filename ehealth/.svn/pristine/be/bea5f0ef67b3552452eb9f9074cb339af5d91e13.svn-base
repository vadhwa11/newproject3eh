<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="PatientAllergicDrugsHd" table="patient_allergic_drugs_hd">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="patient_allergic_drugs_hd_id">
			<generator class="sequence">
				<param name="sequence">patient_allergic_drugs_hd_patient_allergic_drugs_hd_id_seq</param>
			</generator>
		</id>

		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="false">
		</many-to-one>
		<many-to-one name="Visit" column="visit_id" class="Visit"
			not-null="false">
		</many-to-one>
		<many-to-one name="Hin" column="hin_id" class="Patient"
			not-null="false">
		</many-to-one>


		<set name="PatientAllergicDrugsDts" inverse="true">
			<key column="patient_allergic_drugs_hd_id" />
			<one-to-many class="PatientAllergicDrugsDt" />
		</set>


	</class>
</hibernate-mapping>