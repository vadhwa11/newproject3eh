	<queryString><![CDATA[select i.nomenclature,
po.po_number,
po.po_date,
m.grn_no,
m.grn_date,
t.qty_ordered,t.received_qty
,(t.qty_ordered-t.received_qty) as net,mh.hospital_name 
from store_po_header po
left outer join  store_po_detail pot on po.po_id=pot.po_id 
left outer join mas_hospital mh on mh.hospital_id=po.hospital_id 
left outer join mas_store_item i on i.item_id=pot.item_id
left outer join  store_grn_m m on  m.po_id=po.po_id
left outer join  store_grn_t t on m.grn_master_id=t.grn_master_id
where po.po_id=$P{poId} and po.hospital_id=$P{hospitalId}
$P!{query}]]></queryString>

