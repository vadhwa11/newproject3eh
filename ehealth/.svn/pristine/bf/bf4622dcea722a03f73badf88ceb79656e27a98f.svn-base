patient_wise_mlc patient_wise_mlc
LEFT OUTER JOIN OPD_PATIENT_DETAILS OPD_PATIENT_DETAILS on patient_wise_mlc.OPD_PATIENT_DETAIL_id=OPD_PATIENT_DETAILS.id
