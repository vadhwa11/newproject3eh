	<queryString><![CDATA[select a.acc_desc,sum(coalesce(d.dr_amount,'0.00'))+a.op_balance_dr as sum_dr from fa_voucher_details d
left outer join fa_voucher_header h on h.voucher_header_id=d.voucher_header_id
left outer join fa_mas_account a on a.acc_id=d.account_id
where    d.dr_amount is not null   and d.account_id=$P{account_id} 
and h.voucher_date<$P{fromDate} 
group by d.account_id,a.acc_desc,a.op_balance_dr]]></queryString>

