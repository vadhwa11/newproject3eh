	<queryString><![CDATA[SELECT
     inpatient.ad_no AS VISIT_VISIT_NO,
     PATIENT.SERVICE_NO AS PATIENT_SERVICE_NO,
     PATIENT.HIN_NO AS PATIENT_HIN_NO,
     MAS_STORE_ITEM.NOMENCLATURE AS MAS_STORE_ITEM_NOMENCLATURE,
     MAS_STORE_ITEM.PVMS_NO AS MAS_STORE_ITEM_PVMS_NO,
     PATIENT_PRESCRIPTION_DETAILS.ROUTE AS PATIENT_PRESCRIPTION_DETAILS_R,
     PATIENT_PRESCRIPTION_DETAILS.TOTAL AS PATIENT_PRESCRIPTION_DETAILS_T,
     PATIENT_PRESCRIPTION_DETAILS.NO_OF_DAYS AS NO_OF_DAYS,
     PATIENT_PRESCRIPTION_DETAILS.DOSAGE AS DOSAGE,
     PATIENT_PRESCRIPTION_DETAILS.FREQUENCY_ID AS PATIENT_PRESCRIPTION_DETAILS_F,
     MAS_FREQUENCY.FREQUENCY_NAME AS FREQUENCY_NAME
FROM
     inpatient inpatient INNER JOIN HMS.PATIENT_PRESCRIPTION_HEADER PATIENT_PRESCRIPTION_HEADER ON inpatient.inpatient_id = PATIENT_PRESCRIPTION_HEADER.inpatient_id
     INNER JOIN HMS.PATIENT_PRESCRIPTION_DETAILS PATIENT_PRESCRIPTION_DETAILS ON PATIENT_PRESCRIPTION_HEADER.PRESCRIPTION_ID = PATIENT_PRESCRIPTION_DETAILS.PRESCRIPTION_ID
     INNER JOIN HMS.PATIENT PATIENT ON PATIENT.HIN_ID = inpatient.HIN_ID
     INNER JOIN HMS.MAS_STORE_ITEM MAS_STORE_ITEM ON PATIENT_PRESCRIPTION_DETAILS.ITEM_ID = MAS_STORE_ITEM.ITEM_ID
     INNER JOIN HMS.MAS_FREQUENCY MAS_FREQUENCY ON PATIENT_PRESCRIPTION_DETAILS.FREQUENCY_ID = MAS_FREQUENCY.FREQUENCY_ID
WHERE
    PATIENT_PRESCRIPTION_HEADER.inpatient_id=$P{inpatient_id} 
and PATIENT_PRESCRIPTION_HEADER.prescription_date=$P{caseSheetDate}]]></queryString>

