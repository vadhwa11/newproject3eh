	<queryString><![CDATA[SELECT m.hin_id,  m.requisition_from,m.requisition_date,m.ad_no,
p.hin_no,
p.p_first_name,
m.police_station,m.patient_address, 
mas.hospital_name,m.serial_no,
p.age,
a.administrative_sex_name,
m.opinion,
m.consent,m.history_alcohol_consumption,m.history_of_illness,
m.examination_date,m.examination_time,m.id_mark1,m.id_mark2,m.arrest_date,m.arrest_time,m.alcohol_smell,m.clothing,m.general_disposition,m.speech,m.conjunctiva
,m.pupils,m.self_control,m.memory,m.orientation,m.reaction_time,m.gait,m.pulse,m.bp,m.finger_nose_test,m.rombergs_sign,
m.injury_details,rank.rank_name,district.district_name,reflexes,alcohol_smell,blood_examination,under_arrest
from medico_legal_details m
LEFT OUTER JOIN patient p ON p.hin_id = m.hin_id
LEFT OUTER JOIN mas_administrative_sex a ON a.administrative_sex_id = p.sex_id
LEFT OUTER JOIN opd_patient_details opd ON opd.id = m.opd_patient_detail_id
LEFT OUTER JOIN mas_hospital mas ON mas.hospital_id = opd.hospital_id

LEFT OUTER JOIN mas_employee employee ON employee.employee_id = opd.employee_id
LEFT OUTER JOIN mas_rank rank ON rank.rank_id = employee.rank_id
LEFT OUTER JOIN mas_district district ON district.district_id = mas.district_id

where  p.hin_no = $P{UHID_No} and m.medico_legal_details_id=$P{medicoLegalDetailsId}  and opd.hospital_id= $P{hospitalId}]]></queryString>

