	<parameter name="UHID_No" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<parameter name="medico_legal_details_id" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT m.hin_id, 
p.hin_no,
p.p_first_name,
m.police_station, m.history_injury_cause,
mas.short_name as hospital_name,
p.age,
a.administrative_sex_name,
district.district_name as place,
m.requisition_from,
rank.rank_name,
pad.address

from medico_legal_details m
LEFT OUTER JOIN opd_patient_details opd ON opd.id = m.opd_patient_detail_id
LEFT OUTER JOIN patient p ON p.hin_id = m.hin_id
LEFT OUTER JOIN mas_administrative_sex a ON a.administrative_sex_id = p.sex_id
LEFT OUTER JOIN patient_address pad ON pad.hin_id = m.hin_id
LEFT OUTER JOIN mas_hospital mas ON mas.hospital_id = opd.hospital_id
LEFT OUTER JOIN mas_district district ON district.district_id = mas.district_id
LEFT OUTER JOIN mas_employee employee ON employee.employee_id = opd.employee_id
LEFT OUTER JOIN mas_rank rank ON rank.rank_id = employee.rank_id
where  p.hin_no = $P{UHID_No} and m.medico_legal_details_id=$P{medico_legal_details_id}
GROUP BY m.hin_id, p.hin_no,m.police_station, p.p_first_name,p.age,
m.history_injury_cause,a.administrative_sex_name,pad.address ,mas.short_name,
district.district_name,m.requisition_from,rank.rank_name]]></queryString>

