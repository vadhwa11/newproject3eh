	<queryString><![CDATA[select
     mas_store_item.pvms_no as mas_store_item_pvms_no,
     mas_store_item.nomenclature as mas_store_item_nomenclature,
    (case when  mas_store_item.temprature='null' then '' else coalesce(mas_store_item.temprature,'') end) as mas_store_item_temprature,
     (coalesce(mas_employee.emp_name,''))as ename,
     mas_store_item.last_chg_date as mas_store_item_last_chg_date
from
     mas_employee mas_employee right outer join mas_store_item mas_store_item on   mas_employee.employee_id = mas_store_item.reffred_by   
where
mas_store_item.last_chg_date between $P{fromDate}  and $P{toDate}]]></queryString>

