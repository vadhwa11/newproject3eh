	<queryString><![CDATA[select a.po_number, a.po_date, d.pvms_no, d.nomenclature, f.unit_name, b.quantity_ordered,
coalesce(b.quantity_received,'0') as quantity_received,  (b.quantity_ordered - coalesce(b.quantity_received,'0')) quantity_pending,
(c.supplier_name||' ['||c.supplier_code||']') as supplier_name, ('distributor: '||c.cf_local_distributor_name) as cf_local_distributor_name, c.cf_local_distributor_address1, c.cf_local_distributor_address2
from store_po_header a, store_po_detail b, mas_store_supplier c, mas_store_item d, mas_store_item_conversion e, mas_store_unit f
where a.po_id = b.po_id
and a.supplier_id = c.supplier_id
and b.item_id  = d.item_id
and d.item_conversion_id = e.item_conversion_id
and e.purchase_unit_id = f.unit_id
and a.po_id = $P{poId} 
and a.hospital_id = $P{hospitalId} 
and (b.quantity_ordered - coalesce(b.quantity_received,'0')) !=0

order by a.po_date, a.po_number, d.pvms_no]]></queryString>

