<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrEmployeePersonelDetails" table="hr_employee_personel_details">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_employee_personel_details_id_seq</param>
			</generator>
		</id>

		<property name="DateOfBirth" column="date_of_birth" type="date"
			not-null="false" />
		<property name="PassportNo" column="passport_no" type="string"
			not-null="false" length="30" />
		<property name="PassportIssueDate" column="passport_issue_date"
			type="date" not-null="false" />
		<property name="PassportExpiryDate" column="passport_expiry_date"
			type="date" not-null="false" />
		<property name="PassportIssuePlace" column="passport_issue_place"
			type="string" not-null="false" length="45" />
		<property name="VisaDetails" column="visa_details" type="string"
			not-null="false" length="45" />
		<property name="PanNo" column="pan_no" type="string"
			not-null="false" length="45" />
		<property name="DrivingLicence" column="driving_licence"
			type="string" not-null="false" length="45" />
		<property name="MarriageDate" column="marriage_date" type="date"
			not-null="false" />
		<many-to-one name="Gender" column="gender_id"
			class="jkt.hms.masters.business.MasAdministrativeSex" not-null="false"
			lazy="false">
			<!-- lazy="false" -->
		</many-to-one>
		<many-to-one name="MaritalStatus" column="marital_status_id"
			class="jkt.hms.masters.business.MasMaritalStatus" not-null="false"
			lazy="false">
			<!-- lazy="false" -->
		</many-to-one>
		<one-to-one name="MasEmployee" class="jkt.hms.masters.business.MasEmployee"
			cascade="all" access="property" lazy="false">
			<!-- lazy="false" -->
		</one-to-one>

	</class>
</hibernate-mapping>