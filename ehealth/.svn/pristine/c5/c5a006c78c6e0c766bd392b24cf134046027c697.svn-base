<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="UserManager" table="hr_user_manager">
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_user_manager_id_seq</param>
			</generator>
		</id>

		<property name="EmpId" column="emp_id" type="integer"
			not-null="false" length="11" />
		<property name="ManagerId" column="manager_id" type="integer"
			not-null="false" length="11" />
		<many-to-one name="users" class="jkt.hms.masters.business.MasEmployee"
			column="emp_id" update="false" insert="false" lazy="false">
		</many-to-one>

		<many-to-one name="managers" class="jkt.hms.masters.business.MasEmployee"
			column="manager_id" update="false" insert="false" lazy="false">
		</many-to-one>

	</class>
</hibernate-mapping>