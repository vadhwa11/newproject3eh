	<queryString><![CDATA[select main.*,
(select hospital_name from mas_hospital where hospital_id = main.hospital_id)
as hospitalname
from (select (case when (op_bill_header_id is not null) then
'Bill Servicing' when (dispensing_header_id is not null) then
'Bill Dispensing' else 'Receipt' end) as billtype,
hin_Id, receipt_no as brno, amount as brnetamount, receipt_date as brreceiptdate,
receipt_time as brreceiptteime,
(select user_name from users where user_id = changed_by) as brchangedname,
changed_by as brchangedby, hospital_id,
CASE WHEN charity_amt  is null THEN 0 ELSE charity_amt END as brcharityamt,
(select (case when (op_bill_header_id is not null) 
then CASE WHEN (select outstanding from bl_op_bill_header where
op_bill_header_id = bh.op_bill_header_id) is null then 0 
ELSE (select outstanding from bl_op_bill_header where
op_bill_header_id = bh.op_bill_header_id)END 
when (dispensing_header_id is not null) then
CASE WHEN (select outstanding from bl_dispensing_header where
dispensing_header_id = bh.dispensing_header_id) is null then 0 
ELSE (select outstanding from bl_dispensing_header
where dispensing_header_id = bh.dispensing_header_id) END else 0 END ))
as broutstandingamt,
CASE WHEN (select distinct payment_mode from bl_receipt_details where
receipt_header_id = bh.receipt_header_id and receipt_header_id is not null and
payment_mode='C')= 'C' THEN 
(select sum(amount) from bl_receipt_details where
receipt_header_id = bh.receipt_header_id and receipt_header_id is not null and
payment_mode = 'C') ELSE 0 END as brcashamt,
CASE WHEN (select distinct payment_mode from bl_receipt_details where
receipt_header_id = bh.receipt_header_id and payment_mode='Q') = 'Q' THEN
(select sum(amount) from bl_receipt_details where
receipt_header_id = bh.receipt_header_id and receipt_header_id
is not null and payment_mode='Q') ELSE 0 END as brchqamt,
CASE WHEN (select distinct payment_mode from bl_receipt_details where
receipt_header_id = bh.receipt_header_id and payment_mode='R') = 'R' THEN
(select sum(amount) from bl_receipt_details where
receipt_header_id = bh.receipt_header_id and receipt_header_id
is not null and payment_mode = 'R') ELSE 0 END as brcrdamt

from bl_receipt_header bh

union

select 'refund' as billtype, hin_Id, refund_no as brno, total_refund_amt*(-1) as brnetamount,
refund_date as brreceiptdate, refund_time as brreceiptteime,
(select user_name from users where user_id =
last_chg_by) as brchangedname,
last_chg_by as brchangedby, hospital_id,
CASE WHEN charity_amt is null THEN 0 ELSE charity_amt END * (-1) as brcharityamt,
0 as broutstandingamt,
CASE WHEN (select distinct payment_mode from bl_refund_details where
refund_header_id = bh.refund_header_id and payment_mode = 'C') = 'C' THEN
(select sum(refund_amount) from bl_refund_details where
refund_header_id = bh.refund_header_id and payment_mode = 'C')*(-1) ELSE 0 END
as brcashamt,
CASE WHEN (select distinct payment_mode from bl_refund_details where
refund_header_id = bh.refund_header_id and payment_mode = 'Q')='Q' THEN
(select sum(refund_amount) from bl_refund_details where
refund_header_id = bh.refund_header_id and payment_mode = 'Q')*(-1)
ELSE 0 END as brchqamt,
CASE WHEN (select distinct payment_mode from bl_refund_details where
refund_header_id = bh.refund_header_id and payment_mode = 'R') = 'R' THEN
(select sum(refund_amount) from bl_refund_details where
refund_header_id = bh.refund_header_id and payment_mode='R')*(-1)
ELSE 0 END as brcrdamt

from bl_refund_header bh
where refund_date between $P{fromDate} and $P{toDate} and bh.hospital_id=$P{hospitalId}
) as main
where main.brreceiptdate between $P{fromDate} and $P{toDate} and main.hospital_id=$P{hospitalId}
and main.brchangedby = case when $P{changedBy}=0 then main.brchangedby else $P{changedBy} end
Order By main.brchangedby]]></queryString>

