	<parameter name="EmpId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
     (mas_employee.first_name+ISNULL(mas_employee.middle_name,'')+ISNULL(mas_employee.last_name,''))as ename,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
     mas_employee.join_date as mas_employee_join_date,
     mas_rank.rank_name as mas_rank_rank_name,
     hr_employee_balance_new.earned as hr_employee_balance_new_earned,
     hr_employee_balance_new.taken as hr_employee_balance_new_taken,
     hr_employee_balance_new.opening_balance as hr_employee_balance_new_opening_balance,
     hr_mas_leave_type_new.allowed_days as hr_mas_leave_type_new_allowed_days,
     hr_mas_leave.description as hr_mas_leave_description,
    (hr_mas_leave_type_new.allowed_days + hr_employee_balance_new.opening_balance - convert(int, hr_employee_balance_new.taken))as balance
from
     mas_hospital mas_hospital right outer join mas_employee mas_employee on mas_hospital.hospital_id = mas_employee.hospital_id
     left outer join mas_rank mas_rank on mas_employee.rank_id = mas_rank.rank_id
     right outer join hr_employee_balance_new hr_employee_balance_new on mas_employee.employee_id = hr_employee_balance_new.emp_id
     left outer join hr_mas_leave_type_mediator hr_mas_leave_type_mediator on hr_employee_balance_new.leave_type = hr_mas_leave_type_mediator.id
     left outer join hr_mas_leave_type_new hr_mas_leave_type_new on hr_mas_leave_type_mediator.leave_type_id = hr_mas_leave_type_new.id
     left outer join hr_mas_leave hr_mas_leave on hr_mas_leave_type_new.leave_type = hr_mas_leave.id
where

     employee_id = $P{EmpId}]]></queryString>

