	<queryString><![CDATA[select mo.ot_name,mh.hospital_name as mas_hospital_hospital_name,ob.surgery_date,mcc.charge_code_name,(pt.p_first_name+ ' ' +pt.p_last_name)as patientName,pt.age,ob.surgery_time,ob.surgery_end_time,md.department_name,(me.first_name+' '+me.last_name)as surgenName,(me1.first_name+' '+me1.last_name)as bookedBy from ot_booking ob 
left outer join mas_charge_code mcc on ob.charge_code_id=mcc.charge_code_id 
left outer join patient pt on pt.hin_id=ob.hin_id
left outer join opd_surgery_header osh on osh.opd_surgery_id=ob.opd_sursery_header_id
left outer join mas_department md on md.department_id=osh.prescribed_department_id
left outer join ot_book_surgeon obs on obs.booking_id=ob.booking_id
left outer join mas_employee me on me.employee_id=obs.employee_id
left outer join mas_employee me1 on me1.employee_id=ob.booked_by
left outer join mas_ot mo on mo.ot_id=ob.ot_id 
left outer join mas_hospital mh on mh.hospital_id=ob.hospital_id
where ob.ot_booking_status='y' and ob.surgery_date between $P{fromDate} and $P{toDate} 
$P!{query} ]]></queryString>

