<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrEmployeeInvestment" table="hr_employee_investment">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_employee_investment_id_seq</param>
			</generator>
		</id>

		<property name="InvAmount" column="inv_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="InvDate" column="inv_date" type="date"
			not-null="false" />
		<property name="DocumentSubmitted" column="document_submitted"
			type="string" not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="45" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<many-to-one name="InvYear" column="inv_year"
			class="jkt.hrms.masters.business.HrMasFinancialYear" not-null="false">
		</many-to-one>
		<many-to-one name="Emp" column="emp_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="false">
		</many-to-one>
		<many-to-one name="SecInvest" column="sec_invest_id"
			class="HrMasItaxSecInvestment" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>