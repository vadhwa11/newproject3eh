	<queryString><![CDATA[select
     
     patient.hin_no as patient_hin_no,
     patient.service_no as patient_service_no,
     bl_dispensing_header.bill_no,
     bl_dispensing_header.bill_date,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
     mas_hospital.address as mas_hospital_address,
     patient.age as patient_age,
     (patient.p_first_name||' '||COALESCE(patient.p_middle_name,'')||' '||COALESCE(patient.p_last_name,''))as patient_name,
     item.nomenclature,
     item.cost_price,
     bl_dispensing_details.qty,
     bl_dispensing_details.sales_tax_amt,
     bl_dispensing_details.net_amt,
     bl_dispensing_details.amount
     
     
from
     bl_dispensing_header bl_dispensing_header 
     left outer join bl_dispensing_details  bl_dispensing_details on bl_dispensing_header.dispensing_header_id = bl_dispensing_details.dispensing_header_id
     left outer join patient patient on bl_dispensing_header.hin_id = patient.hin_id
     left outer join mas_hospital mas_hospital on bl_dispensing_header.hospital_id = mas_hospital.hospital_id
     left outer join mas_store_item item on item.item_id = bl_dispensing_details.item_id
where
     bl_dispensing_header.bill_no = $P{billNo} 

     and patient.hin_no = $P{hinNo}]]></queryString>

