	<queryString><![CDATA[select * from
(select b.hospital_id, b.hin_id, b.refund_date as billdate, total_refund_amt as totalrefundamount,
p.hin_no, (p.p_first_name||' '||p.p_middle_name||' '||p.p_last_name) as patientname
,0 as visit_no,
(select hospital_name from mas_hospital m where m.hospital_id = b.hospital_id) as hname,
coalesce(ip.ad_no,'0') as ipno from bl_refund_header b
inner join patient p on b.hin_id = p.hin_id
left outer join   inpatient ip on b.hin_id=ip.hin_id ) as main
where billdate between $P{fromdate} and $P{todate} and hospital_id=$P{hospitalId}]]></queryString>

