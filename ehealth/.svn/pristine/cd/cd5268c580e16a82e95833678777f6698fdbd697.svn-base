<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="MasCourse" table="mas_course">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="course_id">
			<generator class="sequence">
				<param name="sequence">mas_course_course_id_seq</param>
			</generator>
		</id>

		<property name="CourseName" column="course_name" type="string"
			not-null="false" length="45" />
		<property name="CourseCode" column="course_code" type="string"
			not-null="true" length="45" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="45" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="45" />
		<many-to-one name="Hospital" column="hospital_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>