	<parameter name="to_date" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
fvh.voucher_header_id,
fvh.voucher_no,
fvh.voucher_date,
(select op_balance_dr from fa_mas_account  where acc_id=$P{accountId}) as dr_amt,
(select op_balance_cr from fa_mas_account  where acc_id=$P{accountId}) as cr_amt,
(select acc_desc from fa_mas_account where acc_id=$P{accountId}) as main_ACC,
(select acc_desc from fa_mas_account where acc_id=$P{accountId}) as main_ACC,
(  
    case fvh.voucher_type     when 'PU'then 'Purchase'
                            when 'PV' then 'Payment'
                            when 'SR' then 'Sales Return'
                            when 'PR' then 'Purchase Return'
                            when 'JV' then 'Journal'
                            when 'RV' then 'Receipt Voucher'
                            
end) as voucher_types,
m.hospital_name,
coalesce(a.dr_amount,0) as dr_amount,
coalesce(a.cr_amount,0)as cr_amount,
fma.acc_desc
from (select fvd.* from fa_voucher_details fvd
where voucher_header_id in( select voucher_header_id as a from fa_voucher_header where voucher_header_id in (select voucher_header_id from fa_voucher_details where account_id=$P{accountId} )) and account_id!=$P{accountId} ) a
left outer join fa_mas_account fma on fma.acc_id=a.account_id
left outer join fa_voucher_header fvh on fvh.voucher_header_id=a.voucher_header_id
left join mas_hospital m on m.hospital_id=fvh.hospital_id
where
fvh.hospital_id=$P{hospital_id} 
and fvh.voucher_date between  $P{from_date} and $P{to_date}]]></queryString>

