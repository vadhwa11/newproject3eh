	<queryString><![CDATA[SELECT patient.hin_no as hin_no,
     visit_A.visit_no AS visit_A_visit_no,
     patient.service_no AS patient_service_no,
     (patient.p_first_name||''||coalesce(patient.p_middle_name,'')||''||coalesce(patient.p_last_name,''))AS patient_name,
     patient.age AS patient_age,
     visit_A.visit_date AS visit_A_visit_date,
     patient.date_of_birth AS patient_date_of_birth,
     mas_hospital.hospital_name AS mas_hospital_hospital_name,
     opd_oph_retinal_header.right_eye AS opd_oph_retinal_header_right_eye,
     opd_oph_retinal_header.left_eye AS opd_oph_retinal_header_left_eye,
     opd_oph_retinal_details.eye AS opd_oph_retinal_details_eye,
     opd_oph_retinal_details.power AS opd_oph_retinal_details_power,
     opd_oph_retinal_details.duration AS opd_oph_retinal_details_duration,
     opd_oph_retinal_details.no_of_spots AS opd_oph_retinal_details_no_of_spots,
     opd_oph_retinal_details.laser_type AS opd_oph_retinal_details_laser_type,
     opd_oph_retinal_details.doctor_name AS opd_oph_retinal_details_doctor_name,
     opd_oph_retinal_details.sign AS opd_oph_retinal_details_sign
FROM
     mas_hospital mas_hospital RIGHT OUTER JOIN opd_oph_retinal_header opd_oph_retinal_header ON mas_hospital.hospital_id = opd_oph_retinal_header.hospital_id
     LEFT OUTER JOIN visit visit_A ON opd_oph_retinal_header.visit_id = visit_A.visit_id
     LEFT OUTER JOIN patient patient ON opd_oph_retinal_header.hin_id = patient.hin_id
     LEFT OUTER JOIN opd_oph_retinal_details opd_oph_retinal_details ON opd_oph_retinal_header.oph_retinal_header_id = opd_oph_retinal_details.oph_retinal_header_id

WHERE
     visit_A.visit_no = $P{Visit_No}
     And patient.hin_no = $P{Hin_No}]]></queryString>

