	<queryString><![CDATA[select distinct hospital.hospital_name as hospital_name, m.issue_no as IssueNo, 
(patient.p_first_name || COALESCE(patient.p_middle_name,'')|| COALESCE(patient.p_last_name,'')) as PatientName,
item.nomenclature as ItemName,t.qty_issued as QtyIssued
from store_issue_m m

left outer join store_issue_t t on m.id = t.issue_m_id
left outer join visit visit on t.visit_id = visit.visit_id
left outer join patient patient on visit.hin_id = patient.hin_id
left outer join mas_hospital hospital on m.hospital_id = hospital.hospital_id
left outer join mas_store_item item on item.item_id = t.item_id
 where  m.issue_date between $P{fromDate} and $P{toDate} and m.department_id = $P{deptId} and hospital.hospital_id=$P{hospitalId}

--group by hospital.hospital_name]]></queryString>

