<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="OpdDiagnosticRecord"
		table="opd_diagnostic_record"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="diagnostic_record_id"
		>
			<generator class="sequence">
			<param name="sequence">opd_diagnostic_record_diagnostic_record_id_seq</param>
			</generator>
		</id>

		<property
			name="MedicalHistory"
			column="medical_history"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="ChiefComplaint"
			column="chief_complaint"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="EdentulousnessMaxillary"
			column="edentulousness_maxillary"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="EdentulousnessMandibular"
			column="edentulousness_mandibular"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="ProsthesisMaxillary"
			column="prosthesis_maxillary"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="ProsthesisMandibular"
			column="prosthesis_mandibular"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="PreviousMaxillary"
			column="previous_maxillary"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="PreviousMandibular"
			column="previous_mandibular"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="RootStumps"
			column="root_stumps"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="RootStumpsValue"
			column="root_stumps_value"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="MobileTeeth"
			column="mobile_teeth"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="MobileTeethValue"
			column="mobile_teeth_value"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="Plaque"
			column="plaque"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="PlaqueValue"
			column="plaque_value"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="Calculus"
			column="calculus"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="CalculusValue"
			column="calculus_value"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="OralLesions"
			column="oral_lesions"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="OralLesionsValue"
			column="oral_lesions_value"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="Defects"
			column="defects"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="DefectsValue"
			column="defects_value"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="MouthOpening"
			column="mouth_opening"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="MouthOpeningValue"
			column="mouth_opening_value"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="PreprostheticProcedures"
			column="preprosthetic_procedures"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="PreprostheticProceduresValue"
			column="preprosthetic_procedures_value"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="MinorsurgicalProcedures"
			column="minorsurgical_procedures"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="MinorsurgicalProceduresValue"
			column="minorsurgical_procedures_value"
			type="string"
			not-null="false"
			length="128"
		/>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Visit"
			column="visit_id"
			class="Visit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
	

		<set name="OpdPreAssessmentClinicDentals" inverse="true">
			<key column="diagnostic_record_id"/>
			<one-to-many class="OpdPreAssessmentClinicDental"/>
		</set>


	</class>	
</hibernate-mapping>