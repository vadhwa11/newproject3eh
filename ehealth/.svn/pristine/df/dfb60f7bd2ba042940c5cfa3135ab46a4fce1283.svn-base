	<queryString><![CDATA[select sum(isnull(qty,0)) as qty,item_id,nomenclature

from
(

SELECT 'Issue' as billType,d.item_id,
qty as qty,nomenclature
FROM bl_dispensing_header b ,bl_dispensing_details d
inner join store_item_batch_stock msb on d.batch_id=msb.stock_id,mas_store_item msi
where b.dispensing_header_id=d.dispensing_header_id
and d.item_id=msi.item_id
and b.bill_date between $P{fromDate} and $P{toDate}
and d.item_id=case when $P{itemId}=0 then d.item_id else $P{itemId} end
union
SELECT 'Refund' as billType,
d.item_id,advice_qty*(-1) as advice_qty ,nomenclature
FROM bl_pymnt_advice_disp_header b ,bl_pymnt_advice_disp_details d
inner join store_item_batch_stock msb on d.batch_id=msb.stock_id ,mas_store_item msi,patient p
where b.pymnt_advice_disp_header_id=d.pymnt_advice_disp_header_id
and d.item_id=msi.item_id
and p.hin_id=b.hin_id
and b.payment_advice_date between $P{fromDate} and $P{toDate}
and d.item_id=case when $P{itemId}=0 then d.item_id else $P{itemId} end
) as main
group by item_id,nomenclature]]></queryString>

