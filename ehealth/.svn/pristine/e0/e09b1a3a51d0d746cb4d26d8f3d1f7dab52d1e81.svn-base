	<queryString><![CDATA[select bd.hospital_id,bd.hin_id,bd.inpatient_id,
(select hin_no from patient where hin_id = bd.hin_id) as hinno,0 as refunddiscount,
bd.refund_no,bd.refund_date,bd.total_refund_amt,bd.remarks,
(case hd.payment_mode when 'c' then 'Cash Receipt'
           when 'q' then concat('Cheque No :',' ',hd.cheque_no,' Date : ',hd.cheque_date)
            else concat('Card No :',' ',hd.cheque_no,' Date: ',hd.cheque_date) end) as details,
hd.refund_amount,
(select concat(p_first_name,'',p_middle_name,'',p_last_name) as patientname
from patient where hin_id=bd.hin_id) as patientname,
(select hospital_name from mas_hospital where hospital_id =bd.hospital_id) as hospitalname

 from bl_refund_header bd left outer join

 bl_refund_details hd on bd.refund_header_id = hd.refund_header_id where bd.refund_no = $P{refundno}]]></queryString>

