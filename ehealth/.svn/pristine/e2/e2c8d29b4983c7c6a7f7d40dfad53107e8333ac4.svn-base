	<queryString><![CDATA[select
     hr_employee_balance_new.opening_balance as hr_employee_balance_new_opening_balance,
     hr_employee_balance_new.taken as hr_employee_balance_new_taken,
     hr_employee_balance_new.leave_type as hr_employee_balance_new_leave_type,
     mas_employee.employee_code as mas_employee_employee_code,
     mas_employee.first_name as mas_employee_first_name,
     mas_employee.last_name as mas_employee_last_name,
     mas_employee.middle_name as mas_employee_middle_name,
     hr_mas_leave.description as hr_mas_leave_description,
    (cast(hr_employee_balance_new.opening_balance as integer) - cast(hr_employee_balance_new.taken as integer)) as bal 
from
     mas_employee mas_employee right outer join hr_employee_balance_new hr_employee_balance_new on mas_employee.employee_id = hr_employee_balance_new.emp_id,
    hr_mas_leave_type_mediator hr_mas_leave_type_mediator right outer join hr_leave_details hr_leave_details on hr_mas_leave_type_mediator.id = hr_leave_details.leave_type
     left outer join hr_mas_leave_type_new hr_mas_leave_type_new on hr_mas_leave_type_mediator.leave_type_id = hr_mas_leave_type_new.id
     left outer join hr_mas_leave hr_mas_leave on hr_mas_leave_type_new.leave_type = hr_mas_leave.id

where 


mas_employee.pe_no between $P{penFirst}  and $P{penSecond}]]></queryString>

