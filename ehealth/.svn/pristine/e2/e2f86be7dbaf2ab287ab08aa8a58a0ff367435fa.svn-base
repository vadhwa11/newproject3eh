	<queryString><![CDATA[select a.pa2,a.cde2,a.et2 from
(
select mpe.pay_element_code as cde2,mpe.pay_element_type as et2,ep.pay_amount as pa2,
ep.employee_id as e2
from hr_employee_pay_elements ep
left join hr_mas_pay_element mpe on ep.pay_element_id=mpe.pay_element_id
where month(start_date)=$P{month}  and year(start_date)=$P{year}  and mpe.pay_element_type='deduction'
)
as a

left join
mas_employee me on a.e2=me.employee_id


where me.employee_id=$P{empId} 
]]></queryString>

