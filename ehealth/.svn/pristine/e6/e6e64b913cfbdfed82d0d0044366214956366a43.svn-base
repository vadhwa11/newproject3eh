<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="BlChargeSlipDetail"
		table="bl_charge_slip_detail"
	>
		<meta attribute="sync-DAO">false</meta>
	<id name="Id" type="integer" column="charge_slip_detail_id">
			<generator class="sequence">
				<param name="sequence">bl_charge_slip_detail_charge_slip_detail_id_seq</param>
			</generator>
			</id>

		<property
			name="Quantity"
			column="quantity"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Amt"
			column="amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="DiscountPercent"
			column="discount_percent"
			type="big_decimal"
			not-null="false"
			length="5"
		/>
		<property
			name="DiscountAmt"
			column="discount_amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="ProportionalDiscountAmount"
			column="proportional_discount_amount"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="NetAmt"
			column="net_amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Rate"
			column="rate"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="RefundedAmt"
			column="refunded_amt"
			type="big_decimal"
			not-null="false"
			length="13"
		/>
		<property
			name="RefundableStatus"
			column="refundable_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="StdDeductionGen"
			column="std_deduction_gen"
			type="big_decimal"
			not-null="false"
			length="11"
		/>
		<property
			name="StdDeductionSpl"
			column="std_deduction_spl"
			type="big_decimal"
			not-null="false"
			length="11"
		/>
		<property
			name="InPkgFlag"
			column="in_pkg_flag"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="ChargeCode"
			column="charge_code_id"
			class="MasChargeCode"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="ChargeSlipMain"
			column="charge_slip_main_id"
			class="BlChargeSlipMain"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>