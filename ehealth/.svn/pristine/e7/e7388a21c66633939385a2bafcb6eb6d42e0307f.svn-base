<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="BlPaywardBooking"
		table="bl_payward_booking"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="booking_id"
		>
			<generator class="sequence"><param name="sequence">bl_payward_booking_booking_id_seq</param></generator>
		</id>

		<property
			name="CurrentWaitingList"
			column="current_waiting_list"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="NumberOfDays"
			column="number_of_days"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Amount"
			column="amount"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="BookingStatus"
			column="booking_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="BookingDate"
			column="booking_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="BookingTime"
			column="booking_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="ApproveDate"
			column="approve_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="ApproveTime"
			column="approve_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="AllotmentDate"
			column="allotment_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="AllotmentTime"
			column="allotment_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="BedAllotmentDate"
			column="bed_allotment_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="BedAllotmentTime"
			column="bed_allotment_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="AmountPaid"
			column="amount_paid"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="AmountAdjusted"
			column="amount_adjusted"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="ChargeRate"
			column="charge_rate"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<many-to-one
			name="ChargeCode"
			column="charge_code_id"
			class="MasChargeCode"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Priority"
			column="priority_id"
			class="BlPriority"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Transfer"
			column="transfer_id"
			class="Transfer"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Payward"
			column="payward_id"
			class="MasDepartment"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="OpdPatientDetails"
			column="opd_patient_details_id"
			class="OpdPatientDetails"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="RoomType"
			column="room_type_id"
			class="MasRoomType"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>