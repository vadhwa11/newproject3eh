<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrEmployeeOtherEarning" table="hr_employee_other_earning">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_employee_other_earning_id_seq</param>
			</generator>
		</id>

		<property name="IncomeAmount" column="income_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="IncomeDate" column="income_date" type="date"
			not-null="false" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="45" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="Emp" column="emp_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="false">
		</many-to-one>
		<many-to-one name="IncomeCode" column="income_code"
			class="HrMasItaxIncomeCode" not-null="false">
		</many-to-one>
		<many-to-one name="FinancialYear" column="financial_year"
			class="HrMasFinancialYear" not-null="false">
		</many-to-one>
		<many-to-one name="CheckCode" column="check_code" class="HrMasItaxCheckCode"
			not-null="false">
		</many-to-one>

	</class>
</hibernate-mapping>