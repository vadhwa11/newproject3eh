<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="DgOrderhd" table="dg_orderhd">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="orderhd_id">
			<generator class="sequence">
				<param name="sequence">dg_orderhd_orderhd_id_seq</param>
			</generator>
		</id>

		<property name="OrderDate" column="order_date" type="date"
			not-null="false" length="13" />
			
		<property name="OrderTime" column="order_time" type="string"
			not-null="false" length="10" />
			
		<property name="TestType" column="test_type" type="string"
			not-null="false" length="10" />
			
		<property name="ClinicalNote" column="clinical_note" type="string"
			not-null="false" length="200" />
			
		<property name="ProvisionalDiag" column="provisional_diag"
			type="string" not-null="false" length="30" />
			
		<property name="OrderNo" column="order_no" type="string"
			not-null="false" length="15" />
			
		<property name="PatientType" column="patient_type" type="string"
			not-null="false" length="2" />
			
		<property name="OrderStatus" column="order_status" type="string"
			not-null="false" length="1" />
			
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" length="13" />
			
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
			
		<property name="NetAmount" column="net_amount" type="big_decimal"
			not-null="false" length="9" />
			
		<property name="BillChargeSlpNo" column="bill_charge_slp_no"
			type="string" not-null="false" length="45" />
			
		<property name="RoutineUrgentStatus" column="routine_urgent_status"
			type="string" not-null="false" length="1" />
			
		<property name="UrgentRemarks" column="urgent_remarks" type="string"
			not-null="false" length="50" />
			
			<property name="RefVisitId" column="ref_visit_id"
			type="integer" not-null="false" />
			
			<property name="SmearNo" column="smear_no" type="string"
			not-null="false" length="32" />
			
		<property
			name="PrescribedByNurse"
			column="prescribed_by_nurse"
			type="string"
			not-null="false"
			length="1"
		/>
			
		<many-to-one name="Inpatient" column="inpatient_id" class="Inpatient"
			not-null="false">
			
		</many-to-one>
		<many-to-one name="LastChgBy" column="last_chg_by" class="Users"
			not-null="false">
			
		</many-to-one>
		
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		
		<many-to-one name="BulkOrder" column="bulk_order_id"
			class="DgBulkOrderRequisition" not-null="false">
		</many-to-one>
		
		<many-to-one name="Visit" column="visit_id" class="Visit"
			not-null="false">
		</many-to-one>
		
		<many-to-one name="PrescribedBy" column="prescribed_by"
			class="MasEmployee" not-null="false">
		</many-to-one>
		
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="false">
		</many-to-one>
		
		<many-to-one name="InvestigationRequestionNo" column="investigation_requestion_no"
			class="PatientInvestigationHeader" not-null="false">
		</many-to-one>
		
		<many-to-one name="Hin" column="hin_id" class="Patient"
			not-null="false">
		</many-to-one>

		<set inverse="true" name="DgOrderdts">
			<key column="orderhd_id" />
			<one-to-many class="DgOrderdt" />
		</set>

		<set inverse="true" name="DgSampleCollectionHeaders">
			<key column="order_id" />
			<one-to-many class="DgSampleCollectionHeader" />
		</set>



	</class>
</hibernate-mapping>