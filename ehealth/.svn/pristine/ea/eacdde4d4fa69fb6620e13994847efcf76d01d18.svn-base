	<queryString><![CDATA[select h.doner_name as dname,s. administrative_sex_name as gender,h. rh_factor,h.age as age, 
b.blood_group_name as bloodGroup, h.state_id as state,hos.hospital_name as hospitalName
from blood_donation_entry_header h
left outer join blood_assessment_entry_m a on h.donation_id =a. donation_id
left outer join mas_administrative_sex s on h.sex_id=s.administrative_sex_id 
left outer join mas_blood_group b on b.blood_group_id=h.blood_group_id 
left outer join mas_hospital hos on hos.hospital_id=h.hospital_id 
where   a.entry_m_id = $P{assestment_m_id}  ORDER BY  h.donation_id DESC
LIMIT 1 ;]]></queryString>

