<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasItemType" table="mas_item_type">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="item_type_id">
			<generator class="sequence">
				<param name="sequence">mas_item_type_item_type_id_seq</param>
			</generator>
		</id>

		<property name="ItemTypeCode" column="item_type_code" type="string"
			not-null="false" length="8" />
		<property name="ItemTypeName" column="item_type_name" type="string"
			not-null="false" length="30" />
		<property name="Status" column="status" type="string"
			not-null="true" length="1" />
		
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<many-to-one
			name="Group"
			column="group_id"
			class="MasStoreGroup"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>

		<set name="MasStoreItems" inverse="true">
			<key column="item_type_id" />
			<one-to-many class="MasStoreItem" />
		</set>


	</class>
</hibernate-mapping>