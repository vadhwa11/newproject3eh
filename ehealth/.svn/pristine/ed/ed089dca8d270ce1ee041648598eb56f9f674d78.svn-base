	<queryString><![CDATA[select a.department_id,a.po_number, a.po_date, d.supplier_name, Concat('Dist:[',d.cf_local_distributor_name,']') dist_name,d.cf_local_distributor_address1,d.cf_local_distributor_address2,concat('Phone: ',d.local_phone_no) phone_no,
h.department_name, j.tender_qty, m.unit_name, b.quantity_ordered,
b.disp_type, b.mdq_value, b.rate_per_mdq, n.unit_name,
f.received_qty, f.free_qty, b.cancelled, concat(k.nomenclature, '[',k.pvms_no,']') item,
o.supplier_name re_supplier_name,c.tender_no,
(case when mb.brand_name is null
       then concat('Brand Name:',cbt.brand_name)
        else concat('Brand Name:',mb.brand_name) end ) as brand_name,
b.amount as Amount from store_po_detail b
left outer join store_po_header a on a.po_id = b.po_id
left outer join store_tender_m c on a.tender_id = c.id
left outer join mas_store_supplier d on a.supplier_id = d.supplier_id
left outer join store_grn_m e on e.po_id = a.po_id
left outer join store_grn_t f on (e.grn_master_id = f.grn_master_id and b.item_id = f.item_id)
left outer join mas_department h on a.department_id = h.department_id
left outer join store_tender_t i on (c.id = i.tender_m_id and b.item_id = i.item_id)
left outer join store_tender_proposal j on j.tender_id = a.tender_id and j.item_id = b.item_id
and j.department_id = a.department_id
left outer join mas_store_item k on k.item_id = b.item_id
left outer join mas_store_item_conversion l on l.item_conversion_id = k.item_conversion_id
left outer join mas_store_unit m on m.unit_id = l.purchase_unit_id
left outer join mas_store_unit n on n.unit_id = l.intermediate_unit_id
left outer join mas_store_supplier o on o.supplier_id = b.re_supplier_id
left outer join store_tender_comm_bid_m cbm on cbm.tender_id = c.id and cbm.item_id = k.item_id
and cbm.item_id = b.item_id
left outer join store_tender_comm_bid_t cbt on cbt.comm_bid_id = cbm.id and cbt.supplier_id = a.supplier_id
left outer join mas_store_brand mb on mb.item_id = k.item_id and mb.brand_id = b.brand_id
$P!{query} ]]></queryString>

