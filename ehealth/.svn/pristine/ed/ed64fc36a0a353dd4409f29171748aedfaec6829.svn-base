<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="WardRemarks"
		table="ward_remarks"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="sequence">
			<param name="sequence">ward_remarks_id_seq</param>
			</generator>
		</id>

		<property
			name="RemarksDate"
			column="remarks_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="RemarksTime"
			column="remarks_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
<many-to-one
			name="Doctor"
			column="doctor_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>

	</class>	
</hibernate-mapping>