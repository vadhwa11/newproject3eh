<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="IpdHandTakeOver"
		table="ipd_hand_take_over"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="hand_take_id"
		>
			<generator class="sequence">
				<param name="sequence">ipd_hand_take_over_hand_take_id_seq</param>
			</generator>
		</id>

		<property
			name="EntryNo"
			column="entry_no"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="EntryDate"
			column="entry_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="ShiftTime"
			column="shift_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="RemarksPendingWork"
			column="remarks_pending_work"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="EntryTime"
			column="entry_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="WardBedTransferRequired"
			column="ward_bed_transfer_required"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="TakeBy"
			column="take_by"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="HandBy"
			column="hand_by"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="RequestStatus"
			column="request_status"
			class="MmMasRequestStatus"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="AuthorisedBy"
			column="authorised_by"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="FromDepartment"
			column="from_department"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>