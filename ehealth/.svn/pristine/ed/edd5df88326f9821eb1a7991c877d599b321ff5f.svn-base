	</parameter>
	<queryString><![CDATA[select main.* , case when submain.cash is null then 0 else submain.cash end as cash ,
case when submain.other is null then 0 else submain.other end as other from
(
select boh.op_bill_header_id, hin_id ,hin_no, bill_no, bill_amt, bod.bill_date,
bod.amount, boh.discount,
(case when bod.discount_amt is null then 0 else bod.discount_amt end +
case when bod.proportional_discount_amount is null then 0 else bod.proportional_discount_amount end) as charity,
bod.net_amt,
(select charge_code_name from mas_charge_code m where m.charge_code_id = bod.charge_code_id ) as description
from bl_op_bill_header boh , bl_op_bill_details bod
where boh.op_bill_header_id = bod.op_bill_header_id
and  boh.hin_no = $P{hinNo} and boh.hospital_id=$P{hospitalId}) as main  

left outer join
(
select op_bill_header_id as oph_id, case when bd.payment_mode = 'c' then bd.amount else 0 end as cash,
case when bd.payment_mode != 'c' then bd.amount else 0 end as other
from bl_receipt_header bh , bl_receipt_details bd
where bh.receipt_header_id = bd.receipt_header_id
and op_bill_header_id is not null
) as submain
on  main.op_bill_header_id = submain.oph_id]]></queryString>

