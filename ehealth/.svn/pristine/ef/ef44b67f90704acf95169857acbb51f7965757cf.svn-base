	<queryString><![CDATA[select dmi.investigation_name,
(select count(*) as c1 from dg_result_entry_header h,
dg_result_entry_detail d
where  h.result_entry_id=d.result_entry_id
and d.charge_code_id=t.investigation_id
and result_date between $P{fromDate} and $P{toDate} and h.hospital_id=$P{hospitalId}

) as total,
(select count(*) as c1 from dg_result_entry_header h,
dg_result_entry_detail d
where  h.result_entry_id=d.result_entry_id
and d.charge_code_id=t.investigation_id
and d.result='Positive'
and result_date between $P{fromDate} and $P{toDate} and h.hospital_id=$P{hospitalId}
) as postive,
(select count(*) as c1 from dg_result_entry_header h,
dg_result_entry_detail d
where  h.result_entry_id=d.result_entry_id
and d.charge_code_id=t.investigation_id
and inpatient_id is not null
and result_date between $P{fromDate} and $P{toDate} and h.hospital_id=$P{hospitalId}
) as IPD,
(select count(*) as c1 from dg_result_entry_header h,
dg_result_entry_detail d
where  h.result_entry_id=d.result_entry_id
and d.charge_code_id=t.investigation_id
and inpatient_id is null
and result_date between $P{fromDate} and $P{toDate} and h.hospital_id=$P{hospitalId}
) as OPD

 from dg_mas_investigation_report_template t,dg_mas_investigation dmi
where t.investigation_id=dmi.investigation_id]]></queryString>

