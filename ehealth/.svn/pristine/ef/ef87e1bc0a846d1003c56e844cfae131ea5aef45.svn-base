	<queryString><![CDATA[select 
	finaldataset.*,
	mh.department_name as tostore,
	(select (hospital_name||' '||address) from mas_hospital where hospital_id = finaldataset.hospital_id) as hospitalname
 from
	(
	select 
		demand_no,
		demand_date,
		to_store,
		finalmain.status,
		finalmain.item_id,
		qty_request,
		namerequester,
		finalmain.hospital_id ,
		msi.pvms_no,
		msi.nomenclature 
	from
		(
		select 
			main.*,
			(emp.first_name||' '||emp.middle_name||'  '||emp.last_name) as namerequester 
		from
			(
			select 
				siim.department_id  ,
				siim.demand_no,
				siim.demand_date,
				siim.to_store,
				siim.approved_by,
				siim.requested_by,
				(case when siim.status = 'Intra Indent Waiting' then 'Intra Indent Waiting' 
				when siim.status = 'intra indent reject' then 'Intra indent reject'
				when siim.status = 'intra indent approved' then 'Intra indent approved'
				when siim.status = 'issue' then 'issue'
				else 'Not Given' end ) as status,
				siit.item_id,
				siit.stock_in_hand,
				siit.qty_request,
				siim.hospital_id
			from store_internal_indent_m siim
			left outer join store_internal_indent_t siit on siim.id = siit.internal_id
			) as main
			inner join mas_employee emp on main.requested_by = emp.employee_id
		) as finalmain
		inner join mas_store_item  msi on finalmain.item_id =msi.item_id
	) as finaldataset
inner join mas_department mh on finaldataset.to_store = mh.department_id
where mh.department_id =$P{departmentId} and finaldataset.hospital_id =$P{hospitalId} 
and finaldataset.status ='Intra Indent Waiting';]]></queryString>

