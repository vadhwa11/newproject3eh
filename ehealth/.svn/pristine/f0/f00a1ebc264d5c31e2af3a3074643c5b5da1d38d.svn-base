<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrParameter" table="hr_parameter">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_parameter_id_seq</param>
			</generator>
		</id>

		<property name="Fullday" column="fullday" type="java.lang.Float"
			not-null="false" length="12" />
		<property name="Halfday" column="halfday" type="java.lang.Float"
			not-null="false" length="12" />
		<property name="Overtime" column="overtime" type="java.lang.Float"
			not-null="false" length="12" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="8" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />


	</class>
</hibernate-mapping>