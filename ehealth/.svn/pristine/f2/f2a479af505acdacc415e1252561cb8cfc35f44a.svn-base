	<queryString><![CDATA[SELECT
     store_issue_t.qty_issued AS store_issue_t_qty_issued,
     store_issue_m.issue_no AS store_issue_m_issue_no,
     store_issue_m.issue_date AS store_issue_m_issue_date,
     mas_department.department_name AS mas_department_department_name,
     mas_department1.department_name AS mas_department_department_name_to,
     mas_store_unit.unit_name AS mas_store_item_conversion_item_unit_name,
     mas_store_item.pvms_no AS mas_store_item_pvms_no,
     mas_store_item.nomenclature AS mas_store_item_nomenclature,
     mas_store_item.item_id AS mas_store_item_item_id,
     mas_hospital.hospital_name AS mas_hospital_hospital_name,
     mas_store_brand.brand_name as brand_name,
store_issue_t.batch_no
FROM
     store_issue_m store_issue_m LEFT OUTER JOIN store_issue_t store_issue_t ON store_issue_m.id = store_issue_t.issue_m_id
     LEFT OUTER JOIN mas_department mas_department ON store_issue_m.department_id = mas_department.department_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON store_issue_m.hospital_id = mas_hospital.hospital_id
     LEFT OUTER JOIN mas_store_item mas_store_item ON store_issue_t.item_id = mas_store_item.item_id
     LEFT OUTER JOIN mas_store_item_conversion mas_store_item_conversion ON mas_store_item.item_conversion_id = mas_store_item_conversion.item_conversion_id
     LEFT OUTER JOIN mas_store_unit mas_store_unit ON mas_store_item_conversion.purchase_unit_id = mas_store_unit.unit_id
     LEFT OUTER JOIN mas_store_brand mas_store_brand ON store_issue_t.brand_id = mas_store_brand.brand_id
     left outer join mas_department mas_department1 ON store_issue_m.to_store = mas_department1.department_id
     left outer join store_internal_indent_m siit on store_issue_m.request_no=siit.id
WHERE
     store_issue_m.issue_type = 'i'
and  store_issue_t.issued = 'y'
and issue_no=case when $P{issue_no}='' then issue_no else $P{issue_no} end
AND store_issue_m.department_id = $P{DEPART}
and siit.indent_flag=$P{indent_flag}
AND issue_date between  $P{FROM_DATE} AND  $P{TO_DATE} $P!{query}
order by mas_store_item.nomenclature]]></queryString>

