	<queryString><![CDATA[SELECT
     ipd_output.date AS ipd_output_date,
     sum(cast(ipd_output.urine as int))as sumUrine,
     sum(cast(ipd_output.stool as int ))as sumstool,
    sum(cast(ipd_output.vom as int ))as sumVom,
    sum(cast(ipd_output.asp as int)) as sumAsp
FROM
     ipd_intake_output_chart ipd_intake_output_chart
     LEFT OUTER JOIN ipd_output ipd_output ON ipd_intake_output_chart.intakeoutput_id = ipd_output.intakeoutput_id
WHERE
     ipd_intake_output_chart.ad_no = $P{adNo}
     and ipd_intake_output_chart.hospital_id = $P{hospitalId}
     and ipd_intake_output_chart.date between $P{fromDate} and $P{toDate}

group by  ipd_output.date]]></queryString>

