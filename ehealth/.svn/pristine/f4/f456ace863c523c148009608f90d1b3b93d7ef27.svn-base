	<queryString><![CDATA[select mb.branch_desc, h.voucher_no,h.voucher_type, ma.acc_desc, fmsl.sub_led_desc as sub_led_desc, h.voucher_date, m.hospital_name, m.address, h.narration,
isnull(d.dr_amount,0) as dr_amount,isnull( d.cr_amount,0)as cr_amount,ma1.acc_desc as acc_desc1,
isnull(fmsl1.sub_led_desc,' ') as sub_led_desc1,
case when fapo.opening_dr=fapo.opening_proc_cr and fapo.opening_dr=fapo.opening_proc_dr 
then fapo.opening_dr else  isnull(fapo.opening_proc_dr ,0) end as Debit,
case when fapo.opening_cr=fapo.opening_proc_cr and fapo.opening_cr=fapo.opening_proc_dr 
then fapo.opening_cr else  isnull(fapo.opening_proc_cr ,0)  end as Credit
from fa_voucher_header h
left outer join  fa_voucher_details d  on h.voucher_header_id=d.voucher_header_id
left outer join fa_mas_account_sub_group fmsg on h.account_sub_group_id=fmsg.account_sub_group_id

left outer join fa_mas_account ma on d.account_id=ma.acc_id
left outer join fa_mas_account ma1 on d.account_id_for_report=ma1.acc_id

left outer join mas_hospital m on h.hospital_id=m.hospital_id
left outer join mas_branch mb on mb.branch_id = h.branch_id
left outer join fa_mas_sub_led fmsl on d.sub_led_id=fmsl.sub_led_id
left outer join fa_mas_sub_led fmsl1 on d.sub_led_id_for_report=fmsl1.sub_led_id
left outer join fa_account_proc_opening fapo on d.account_id=fapo.account_id or d.sub_led_id=fapo.sub_led_id 

where h.account_sub_group_id in (1,2) and d.account_id=$P{accountId}
and h.voucher_date between $P{from_date} and $P{to_date} and h.hospital_id=$P{hospital_id}
and mb.branch_id = $P{branchId} order by h.voucher_date]]></queryString>

