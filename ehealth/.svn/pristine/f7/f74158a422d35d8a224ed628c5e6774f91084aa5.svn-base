<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrLoanDetail" table="hr_loan_detail">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="loan_detail_id">
			<generator class="sequence">
				<param name="sequence">hr_loan_detail_loan_detail_id_seq</param>
			</generator>
		</id>

		<property name="BalanceLoan" column="balance_loan" type="big_decimal"
			not-null="false" length="17" />
		<property name="InstallDate" column="install_date" type="date"
			not-null="false" />
		<property name="InstallAmount" column="install_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="InterestPaid" column="interest_paid" type="big_decimal"
			not-null="false" length="11" />
		<property name="PPaid" column="p_paid" type="big_decimal"
			not-null="false" length="11" />
		<property name="Remark" column="remark" type="string"
			not-null="false" length="60" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="8" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="LoanHeader" column="loan_header_id"
			class="HrLoanHeader" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>