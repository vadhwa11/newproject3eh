	<queryString><![CDATA[select mh.hospital_name,mh.address,count(dred.investigation_id) as total,dmi.investigation_name,
(case count(dred.investigation_id)  when 0 then NULL  else count(dred.investigation_id)  end) as totalCount,
(case count(dred.investigation_id)  when 0 then NULL  else msc.sub_chargecode_name  end) as subC
 from dg_result_entry_header dreh left outer join mas_sub_chargecode msc on dreh.sub_chargecode_id=msc.sub_chargecode_id
left outer join dg_result_entry_detail dred on dred.result_entry_id=dreh.result_entry_id left outer join dg_mas_investigation dmi on dmi.investigation_id=dred.investigation_id
left outer join mas_hospital mh on mh.hospital_id=dreh.hospital_id
$P!{query}
group by dmi.investigation_name,msc.sub_chargecode_name,mh.hospital_name,mh.address]]></queryString>

