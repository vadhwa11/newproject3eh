	<queryString><![CDATA[select    
     ipd_intake.date as ipd_intake_date,
     sum(cast (ipd_intake.intake as int))as "sum(ipd_intake.intake)",
     sum(cast (ipd_intake.iv as int)) as "sum(ipd_intake.iv)"
from
     ipd_intake_output_chart ipd_intake_output_chart
     left outer join ipd_intake ipd_intake on ipd_intake_output_chart.intakeoutput_id = ipd_intake.intakeoutput_id
where
     ipd_intake_output_chart.ad_no = $P{adNo}
     and ipd_intake_output_chart.hospital_id = $P{hospitalId}
     and ipd_intake_output_chart.date between $P{fromDate} and $P{toDate}

group by  ipd_intake.date]]></queryString>

