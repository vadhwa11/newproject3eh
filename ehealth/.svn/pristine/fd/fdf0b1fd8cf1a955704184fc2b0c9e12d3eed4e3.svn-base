	<queryString><![CDATA[select v.visit_date,p.hin_no,p.p_first_name ||' '|| p.p_middle_name ||' '|| p.p_last_name as p_name,
substring(p.age,3,7) as age1,p.age,s.administrative_sex_name,mh.hospital_name ||' '|| mh.address as hospital_name 
from visit v
left outer join patient p on p.hin_id=v.hin_id
left outer join mas_administrative_sex s on s.administrative_sex_id=p.sex_id
left outer join mas_hospital mh on mh.hospital_id=$P{HOSPITAL_ID}
where v.hospital_id=$P{HOSPITAL_ID} and v.visit_date between $P{fromDate} and $P{toDate}
and trim(substring(p.age,3,7))='Days' and v.visit_no>1]]></queryString>

