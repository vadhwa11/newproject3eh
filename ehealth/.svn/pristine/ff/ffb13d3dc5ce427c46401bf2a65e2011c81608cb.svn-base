<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PrqQuotationItemDetails"
		table="prq_quotation_item_details"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="item_details_id"
		>
			<generator class="sequence">
			 <param name="sequence">prq_quotation_item_details_item_details_id_seq</param>
			</generator>
		</id>

		<property
			name="Rate"
			column="rate"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="Quantity"
			column="quantity"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="ReqQty"
			column="req_qty"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="ApprovedQty"
			column="approved_qty"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="Amount"
			column="amount"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="DiscountPercent"
			column="discount_percent"
			type="big_decimal"
			not-null="false"
			length="5"
		/>
		<property
			name="NetAmount"
			column="net_amount"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="ExciseDutyType"
			column="excise_duty_type"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="ExciseDutyAmount"
			column="excise_duty_amount"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="Tax"
			column="tax"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="Vat"
			column="vat"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="TotalAmount"
			column="total_amount"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="TechSpecs"
			column="tech_specs"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="NewItemRate"
			column="new_item_rate"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="NewQty"
			column="new_qty"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="NewAmount"
			column="new_amount"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="NewDiscountOnRate"
			column="new_discount_on_rate"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="NewNetAmount"
			column="new_net_amount"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="VendorFlag"
			column="vendor_flag"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="NewExciseDuty"
			column="new_excise_duty"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="NewVat"
			column="new_vat"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="NewTax"
			column="new_tax"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="NewTotalAmount"
			column="new_total_amount"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="NewExciseDutyType"
			column="new_excise_duty_type"
			type="string"
			not-null="false" 
			length="8"
		/>
		<property
			name="LCategory"
			column="l_category"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="VendorDetails"
			column="vendor_details_id"
			class="PrqQuotationVendorDetails"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Item"
			column="item_id"
			class="MasStoreItem"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="ItemStatus"
			column="item_status"
			class="MmMasRequestStatus"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="ManufacturedBy"
			column="manufactured_by"
			class="MasManufacturer"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>