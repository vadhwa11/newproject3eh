<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrArrearSalary" table="hr_arrear_salary">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="arrear_salary_id">
			<generator class="sequence">
				<param name="sequence">hr_arrear_salary_arrear_salary_id_seq</param>
			</generator>
		</id>

		<property name="FromDate" column="from_date" type="date"
			not-null="false" />
		<property name="ToDate" column="to_date" type="date"
			not-null="false" />
		<property name="ArrearDays" column="arrear_days" type="java.lang.Float"
			not-null="false" length="12" />
		<property name="Remark" column="remark" type="string"
			not-null="false" length="40" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="10" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="PaymentDate" column="payment_date" type="date"
			not-null="false" />
		<many-to-one name="Hospital" column="hospital_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="Employee" column="employee_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>