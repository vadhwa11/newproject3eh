<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrItaxCalculate" table="hr_itax_calculate">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_itax_calculate_id_seq</param>
			</generator>
		</id>

		<property name="TotalEarning" column="total_earning" type="big_decimal"
			not-null="true" length="11" />
		<property name="TotalDeduction" column="total_deduction"
			type="big_decimal" not-null="true" length="11" />
		<property name="MonthlyItax" column="monthly_itax" type="big_decimal"
			not-null="false" length="10" />
		<property name="TotalTax" column="total_tax" type="big_decimal"
			not-null="false" length="11" />
		<many-to-one name="FinYear" column="fin_year_id" class="HrMasFinancialYear"
			not-null="true">
		</many-to-one>
		<many-to-one name="Employee" column="employee_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="true">
		</many-to-one>


	</class>
</hibernate-mapping>