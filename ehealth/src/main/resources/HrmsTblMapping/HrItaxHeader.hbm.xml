<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrItaxHeader" table="hr_itax_header">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_itax_header_id_seq</param>
			</generator>
		</id>

		<property name="FMonth" column="f_month" type="integer"
			not-null="true" length="10" />
		<property name="Earning" column="earning" type="big_decimal"
			not-null="false" length="11" />
		<property name="Deduction" column="deduction" type="big_decimal"
			not-null="false" length="11" />
		<property name="NetIncome" column="net_income" type="big_decimal"
			not-null="false" length="11" />
		<property name="Itax" column="itax" type="big_decimal"
			not-null="false" length="11" />
		<many-to-one name="FYear" column="f_year" class="HrMasFinancialYear"
			not-null="true">
		</many-to-one>
		<many-to-one name="Employee" column="employee_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="true">
		</many-to-one>
		<set name="ITaxDetailsSet" inverse="true">
			<key column="itax_header_id" />
			<one-to-many class="HrItaxDetails" />
		</set>


	</class>
</hibernate-mapping>