<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrMasBonus" table="hr_mas_bonus">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="bonus_id">
			<generator class="sequence">
				<param name="sequence">hr_mas_bonus_bonus_id_seq</param>
			</generator>
		</id>

		<property name="BonusCode" column="bonus_code" type="string"
			not-null="false" length="40" />
		<property name="Description" column="description" type="string"
			not-null="false" length="45" />
		<property name="BonusType" column="bonus_type" type="string"
			not-null="false" length="10" />
		<property name="PaymentFrequency" column="payment_frequency"
			type="string" not-null="false" length="10" />
		<property name="FromDate" column="from_date" type="date"
			not-null="false" />
		<property name="ToDate" column="to_date" type="date"
			not-null="false" />
		<property name="DueDate" column="due_date" type="date"
			not-null="false" />
		<property name="FixedAmount" column="fixed_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="MaxBasic" column="max_basic" type="big_decimal"
			not-null="false" length="11" />
		<property name="BonusRate" column="bonus_rate" type="java.lang.Float"
			not-null="false" length="12" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="10" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="Taxable" column="taxable" type="string"
			not-null="false" length="1" />
		<many-to-one name="Hospital" column="hospital_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="Grade" column="grade_id"
			class="jkt.hms.masters.business.MasGrade" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>