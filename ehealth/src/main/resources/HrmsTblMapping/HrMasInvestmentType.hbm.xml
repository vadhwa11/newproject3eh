<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrMasInvestmentType" table="hr_mas_investment_type">
		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_mas_investment_type_id_seq</param>
			</generator>
		</id>
		<property name="InvestmentCode" column="investment_code"
			type="string" not-null="false" length="3" />
		<property name="InvestmentDescription" column="investment_description"
			type="string" not-null="false" length="45" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="45" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />


	</class>
</hibernate-mapping>