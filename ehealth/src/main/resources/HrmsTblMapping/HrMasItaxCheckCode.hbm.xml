<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrMasItaxCheckCode" table="hr_mas_itax_check_code">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_mas_itax_check_code_id_seq</param>
			</generator>
		</id>

		<property name="CheckCode" column="check_code" type="string"
			not-null="false" length="4" />
		<property name="CheckCodeDesc" column="check_code_desc" type="string"
			not-null="false" length="45" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="45" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />


		<set name="HrEmployeeOtherEarnings" inverse="true">
			<key column="id" />
			<one-to-many class="HrEmployeeOtherEarning" />
		</set>


	</class>
</hibernate-mapping>