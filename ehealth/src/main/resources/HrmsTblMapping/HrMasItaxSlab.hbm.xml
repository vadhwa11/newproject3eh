<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrMasItaxSlab" table="hr_mas_itax_slab">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_mas_itax_slab_id_seq</param>
			</generator>
		</id>

		<property name="LowerLimit" column="lower_limit" type="big_decimal"
			not-null="false" length="11" />
		<property name="UpperLimit" column="upper_limit" type="big_decimal"
			not-null="false" length="11" />
		<property name="TaxRate" column="tax_rate" type="big_decimal"
			not-null="false" length="4" />
		<property name="Citizen" column="citizen" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="FinancialYear" column="financial_year"
			class="HrMasFinancialYear" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>