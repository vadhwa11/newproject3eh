<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrMasLoan" table="hr_mas_loan">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="loan_id">
			<generator class="sequence">
				<param name="sequence">hr_mas_loan_loan_id_seq</param>
			</generator>
		</id>

		<property name="LoanCode" column="loan_code" type="string"
			not-null="false" length="30" />
		<property name="LoanDescription" column="loan_description"
			type="string" not-null="false" length="45" />
		<property name="MaxAmount" column="max_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="InterestPercent" column="interest_percent"
			type="java.lang.Float" not-null="false" length="12" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="8" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="Hospital" column="hospital_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>