<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrMasSurcharge" table="hr_mas_surcharge">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_mas_surcharge_id_seq</param>
			</generator>
		</id>

		<property name="SurchargeCode" column="surcharge_code" type="string"
			not-null="false" length="4" />
		<property name="SurchargeDescription" column="surcharge_description"
			type="string" not-null="false" length="45" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="45" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />

		<set inverse="true" name="HrMasItaxSurcharges">
			<key column="id" />
			<one-to-many class="HrMasItaxSurcharge" />
		</set>



	</class>
</hibernate-mapping>