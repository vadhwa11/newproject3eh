<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrPayrollProcessHeader" table="hr_payroll_process_header">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="payroll_process_header_id">
			<generator class="sequence">
				<param name="sequence">hr_payroll_process_header_payroll_process_header_id_seq</param>
			</generator>
		</id>

		<property name="Year" column="year" type="integer" not-null="false"
			length="10" />
		<property name="Month" column="month" type="integer"
			not-null="false" length="10" />
		<property name="TotalDays" column="total_days" type="integer"
			not-null="false" length="10" />
		<property name="LwpDays" column="lwp_days" type="integer"
			not-null="false" length="10" />
		<property name="SalDays" column="sal_days" type="integer"
			not-null="false" length="10" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="10" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LoanInstallment" column="loan_installment"
			type="big_decimal" not-null="false" length="15" />
		<property name="AdvanceInstallment" column="advance_installment"
			type="big_decimal" not-null="false" length="15" />
		<property name="BonusAmount" column="bonus_amount" type="big_decimal"
			not-null="false" length="15" />
		<property name="Basic" column="basic" type="big_decimal"
			not-null="false" length="11" />
		<property name="ArrearPayelementAmt" column="arrear_payelement_amt"
			type="big_decimal" not-null="false" length="11" />
		<property name="ArrearDays" column="arrear_days" type="java.lang.Float"
			not-null="false" length="12" />
		<property name="ArrearSalaryAmt" column="arrear_salary_amt"
			type="big_decimal" not-null="false" length="15" />
		<property name="TotalMonthDays" column="total_month_days"
			type="integer" not-null="false" length="10" />
		<property name="Flag" column="flag" type="string" not-null="false"
			length="5" />
		<property name="NetSalary" column="net_salary" type="big_decimal"
			not-null="false" length="17" />
		<property name="TotalEarning" column="total_earning" type="big_decimal"
			not-null="false" length="15" />
		<property name="TotalDeduction" column="total_deduction"
			type="big_decimal" not-null="false" length="15" />
		<property name="TotalReimb" column="total_reimb" type="big_decimal"
			not-null="false" length="15" />
		<many-to-one name="Hospital" column="hospital_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="jkt.hms.masters.business.MasDepartment" not-null="false">
		</many-to-one>
		<many-to-one name="Employee" column="employee_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="false">
		</many-to-one>

		<set inverse="true" name="HrPayrollProcessDetails">
			<key column="payroll_process_header_id" />
			<one-to-many class="HrPayrollProcessDetail" />
		</set>



	</class>
</hibernate-mapping>