<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="HrPerformanceReviewRatings" table="hr_performance_review_ratings">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="rating_id">
			<generator class="sequence">
				<param name="sequence">hr_performance_review_ratings_rating_id_seq</param>
			</generator>
		</id>

		<property name="RatingCode" column="rating_code" type="string"
			not-null="false" length="10" />
		<property name="RatingDesc" column="rating_desc" type="string"
			not-null="false" length="45" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="45" />
		<property name="LastChgDate" column="last_chg_date" type="string"
			not-null="false" length="45" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="45" />
		<many-to-one name="Company" column="company_id" class="MasHospital"
			not-null="true">
		</many-to-one>


		<set name="HrEmployeePerformanceReviewsByOverallAssesment"
			inverse="true">
			<key column="rating_id" />
			<one-to-many class="HrEmployeePerformanceReview" />
		</set>

		<set name="HrEmployeePerformanceReviewsByAssesmentBehaviour"
			inverse="true">
			<key column="rating_id" />
			<one-to-many class="HrEmployeePerformanceReview" />
		</set>

		<set name="HrEmployeePerformanceReviewsByAssesmentPerformanceGoals"
			inverse="true">
			<key column="rating_id" />
			<one-to-many class="HrEmployeePerformanceReview" />
		</set>


	</class>
</hibernate-mapping>