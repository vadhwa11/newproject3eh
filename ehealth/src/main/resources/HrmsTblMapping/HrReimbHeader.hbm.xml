<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrReimbHeader" table="hr_reimb_header">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="reimb_header_id">
			<generator class="sequence">
				<param name="sequence">hr_reimb_header_reimb_header_id_seq</param>
			</generator>
		</id>

		<property name="Paymode" column="paymode" type="string"
			not-null="false" length="25" />
		<property name="Remark" column="remark" type="string"
			not-null="false" length="40" />
		<property name="MaxAmount" column="max_amount" type="big_decimal"
			not-null="false" length="11" />
		<property name="TotalReimbAmt" column="total_reimb_amt" type="big_decimal"
			not-null="false" length="11" />
		<property name="ReimbStatus" column="reimb_status" type="string"
			not-null="false" length="10" />
		<property name="FromDate" column="from_date" type="date"
			not-null="false" />
		<property name="ToDate" column="to_date" type="date"
			not-null="false" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="10" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="Hospital" column="hospital_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>
		<many-to-one name="Reimb" column="reimb_id" class="HrMasReimbersement"
			not-null="false">
		</many-to-one>
		<many-to-one name="Employee" column="employee_id"
			class="jkt.hms.masters.business.MasEmployee" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>