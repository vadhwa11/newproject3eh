<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.masters.business">
	<class name="HrMasLeaveStatus" table="hr_mas_leave_status">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">hr_mas_leave_status_id_seq</param>
			</generator>
		</id>

		<property name="StatusDesc" column="status_desc" type="string"
			not-null="false" length="25" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="Company" column="company_id"
			class="jkt.hms.masters.business.MasHospital" not-null="false">
		</many-to-one>


		<set name="LeaveDetails" inverse="true" lazy="true">
			<key column="id" />
			<one-to-many class="HrLeaveDetails" />
		</set>

		<set name="LeaveDetailsHistories" inverse="true" lazy="true">
			<key column="id" />
			<one-to-many class="HrLeaveDetailsHistory" />
		</set>


	</class>
</hibernate-mapping>