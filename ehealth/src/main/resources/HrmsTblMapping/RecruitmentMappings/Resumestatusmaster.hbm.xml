<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hrms.recruitment.masters.business">
	<class name="Resumestatusmaster" table="hr_resumestatusmaster">
		<id name="Id" type="integer" column="status_id">
			<generator class="sequence">
				<param name="sequence">hr_resumestatusmaster_status_id_seq</param>
			</generator>
		</id>

		<property name="StatusDesc" column="status_desc" type="string"
			not-null="false" length="50" />


	</class>
</hibernate-mapping>