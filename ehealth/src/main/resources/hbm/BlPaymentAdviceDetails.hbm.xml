<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="BlPaymentAdviceDetails" table="bl_payment_advice_details">
		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="payment_advice_details_id">
			<generator class="sequence">
				<param name="sequence">bl_payment_advice_details_payment_advice_details_id_seq</param>
			</generator>
		</id>

		<property name="AdviceQty" column="advice_qty" type="integer"
			not-null="false" length="10" />
		<property name="AdviceAmt" column="advice_amt" type="big_decimal"
			not-null="false" length="13" />
		<property name="AdviceCharityAmt" column="advice_charity_amt"
			type="big_decimal" not-null="false" length="13" />
		<property name="Refunded" column="refunded" type="string"
			not-null="false" length="1" />
		<many-to-one name="Charge" column="charge_id" class="MasChargeCode"
			not-null="false">
		</many-to-one>
		<many-to-one name="PaymentAdviceHeader" column="payment_advice_header_id"
			class="BlPaymentAdviceHeader" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>