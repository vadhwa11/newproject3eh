<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="BloodAssessmentEntryT"
		table="blood_assessment_entry_t"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="entry_t_id"
		>
			<generator class="sequence">
			<param name="sequence">blood_assessment_entry_t_entry_t_id_seq</param>
			</generator>
		</id>

		<property
			name="AssessmentResult"
			column="assessment_result"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="200"
		/>
		<many-to-one
			name="Assessment"
			column="assessment_id"
			class="MasAssessment"
			not-null="false"
		>
		</many-to-one>
		
		<many-to-one
			name="EntryM"
			column="entry_m_id"
			class="BloodAssessmentEntryM"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>