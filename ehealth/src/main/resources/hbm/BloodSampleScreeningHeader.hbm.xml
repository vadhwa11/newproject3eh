<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="BloodSampleScreeningHeader"
		table="blood_sample_screening_header"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="screening_test_id"
		>
			<generator class="sequence">
			<param name="sequence">blood_sample_screening_header_screening_test_id_seq</param>
			</generator>
		</id>

		<property
			name="SampleTestNo"
			column="sample_test_no"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="SampleTestDate"
			column="sample_test_date"
			type="timestamp"
			not-null="false"
			length="29"
		/>
		<property
			name="SampleTestTime"
			column="sample_test_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="FitBloodIssue"
			column="fit_blood_issue"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="LastChgBy"
			column="last_chg_by"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="timestamp"
			not-null="false"
			length="29"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="BloodIssue"
			column="blood_issue"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Compatibility"
			column="compatibility"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="MajorRsDc"
			column="major_rs_dc"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="MinorRsDc"
			column="minor_rs_dc"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="ReactionId"
			column="reaction_id"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="ResultEntryStatus"
			column="result_entry_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="SampleTestBy"
			column="sample_test_by"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="CrossMatchBy"
			column="cross_match_by"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="SampleCollection"
			column="sample_collection_id"
			class="BloodSampleCollection"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>

		<set inverse="true" name="BloodSampleScreeningDetails">
			<key column="screening_test_id" />
			<one-to-many class="BloodSampleScreeningDetail" />
		</set>

		<set inverse="true" name="BloodIssueHeaders">
			<key column="screening_test_id" />
			<one-to-many class="BloodIssueHeader" />
		</set>



	</class>	
</hibernate-mapping>