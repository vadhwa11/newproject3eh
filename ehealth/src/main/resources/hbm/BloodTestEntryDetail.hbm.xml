<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="BloodTestEntryDetail" table="blood_test_entry_detail">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">blood_test_entry_detail_id_seq</param>
			</generator>
		</id>

		<property name="Result" column="result" type="string"
			not-null="false" length="100" />
		<many-to-one name="TestHeader" column="test_header_id"
			class="BloodTestEntryHeader" not-null="false">
		</many-to-one>
		<many-to-one name="Investigation" column="investigation_id"
			class="DgMasInvestigation" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>