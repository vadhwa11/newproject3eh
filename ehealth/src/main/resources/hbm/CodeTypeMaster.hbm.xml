<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="CodeTypeMaster" table="code_type_master">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="code_type_master_id">
			<generator class="sequence">
				<param name="sequence">code_type_master_code_type_master_id_seq</param>
			</generator>
		</id>

		<property name="HospitalId" column="hospital_id" type="integer"
			not-null="false" length="10" />
		<property name="CodeTypeMasterCode" column="code_type_master_code"
			type="string" not-null="false" length="50" />
		<property name="CodeTypeMasterDescription" column="code_type_master_description"
			type="string" not-null="false" length="50" />
		<property name="VariableType" column="variable_type" type="string"
			not-null="false" length="30" />
		<property name="AddEditById" column="add_edit_by_id" type="integer"
			not-null="false" length="10" />
		<property name="AddEditDateTime" column="add_edit_date_time"
			type="timestamp" not-null="false" />


	</class>
</hibernate-mapping>