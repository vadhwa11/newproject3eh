<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="DgCollectionCenter" table="dg_collection_center">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="collection_center_id">
			<generator class="sequence">
				<param name="sequence">dg_collection_center_collection_center_id_seq</param>
			</generator>
		</id>

		<property name="CollectionCenterCode" column="collection_center_code"
			type="string" not-null="false" length="8" />
		<property name="CollectionCenterName" column="collection_center_name"
			type="string" not-null="false" length="30" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
	<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>

		<set name="DgSampleCollectionDetails" inverse="true">
			<key column="collection_center_id" />
			<one-to-many class="DgSampleCollectionDetails" />
		</set>


	</class>
</hibernate-mapping>