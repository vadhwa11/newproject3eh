<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="Diagnosispro"
		table="diagnosispro"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="sno"
		>
			<generator class="sequence"/>
		</id>

		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
		/>
		<property
			name="Signssymptoms"
			column="signssymptoms"
			type="string"
			not-null="false"
		/>
		<property
			name="Diseaseprogression"
			column="diseaseprogression"
			type="string"
			not-null="false"
		/>
		<property
			name="Diagnostictestresults"
			column="diagnostictestresults"
			type="string"
			not-null="false"
		/>
		<property
			name="Associateddiseasesruleouts"
			column="associateddiseasesruleouts"
			type="string"
			not-null="false"
		/>
		<property
			name="Diseasemechanismclassification"
			column="diseasemechanismclassification"
			type="string"
			not-null="false"
		/>
		<property
			name="Treatment"
			column="treatment"
			type="string"
			not-null="false"
		/>


	</class>	
</hibernate-mapping>