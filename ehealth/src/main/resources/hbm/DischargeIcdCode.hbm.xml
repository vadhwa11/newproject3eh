<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="DischargeIcdCode" table="discharge_icd_code">
		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="discharge_icd_code_id">
			<generator class="sequence">
				<param name="sequence">discharge_icd_code_discharge_icd_code_id_seq</param>
			</generator>
		</id>

		<property name="AddEditDate" column="add_edit_date" type="date"
			not-null="false" length="13" />
		<property name="AddEditTime" column="add_edit_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="Z03" column="z03" type="string" not-null="false"
			length="1" />
		<property name="Z09" column="z09" type="string" not-null="false"
			length="1" />
		<property name="DiagnosisStatus" column="diagnosis_status"
			type="string" not-null="false" length="1" />
		<property name="ComorbidityStatus" column="comorbidity_status"
			type="string" not-null="false" length="8" />
		<property name="SinceMonth" column="since_month" type="string"
			not-null="false" length="8" />
		<property name="SinceYear" column="since_year" type="string"
			not-null="false" length="8" />
		<property name="ComorbidityRemark" column="comorbidity_remark"
			type="string" not-null="false" length="200" />
		<many-to-one name="Inpatient" column="inpatient_id" class="Inpatient"
			not-null="false">
		</many-to-one>
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Visit" column="visit_id" class="Visit"
			not-null="false">
		</many-to-one>
		<many-to-one name="Hin" column="hin_id" class="Patient"
			not-null="false">
		</many-to-one>
		<many-to-one name="AddEditBy" column="add_edit_by_id"
			class="Users" not-null="false">
		</many-to-one>
		<many-to-one name="Icd" column="icd_id" class="MasIcd"
			not-null="false">
		</many-to-one>
		<many-to-one name="OpdPatientDetails" column="opd_patient_details_id"
			class="OpdPatientDetails" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>