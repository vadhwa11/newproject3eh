<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="Drugdetails" table="DRUGDETAILS">

		<id name="Id" type="string" column="SNO">
			<generator class="native" />
		</id>
		<property name="Genericname" column="GENERICNAME" type="string"
			not-null="false" length="1000" />
		<property name="DrugClass" column="CLASS" type="string"
			not-null="false" length="1000" />
		<property name="Indicationdosage" column="INDICATIONDOSAGE"
			type="string" not-null="false" />
		<property name="Indicationdosage1" column="INDICATIONDOSAGE1"
			type="string" not-null="false" />
		<property name="Administration" column="ADMINISTRATION" type="string"
			not-null="false" />
		<property name="Overdosage" column="OVERDOSAGE" type="string"
			not-null="false" />
		<property name="Contraindications" column="CONTRAINDICATIONS"
			type="string" not-null="false" />
		<property name="Specialprecautions" column="SPECIALPRECAUTIONS"
			type="string" not-null="false" />
		<property name="Adversedrugreactions" column="ADVERSEDRUGREACTIONS"
			type="string" not-null="false" />
		<property name="Druginteractions" column="DRUGINTERACTIONS"
			type="string" not-null="false" />
		<property name="Pregnancycategory" column="PREGNANCYCATEGORY"
			type="string" not-null="false" />
		<property name="Storage" column="STORAGE" type="string"
			not-null="false" length="1000" />
		<property name="Mechanismaction" column="MECHANISMACTION"
			type="string" not-null="false" />
		<property name="Atcclassification" column="ATCCLASSIFICATION"
			type="string" not-null="false" length="1000" />
		<property name="Atcclassificationdetails" column="ATCCLASSIFICATIONDETAILS"
			type="string" not-null="false" />
	</class>
</hibernate-mapping>