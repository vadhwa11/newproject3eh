<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="EmployeeCategory" table="employee_category">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="employee_category_id">
			<generator class="sequence">
				<param name="sequence">employee_category_employee_category_id_seq</param>
			</generator>
		</id>

		<property name="EmployeeCategoryName" column="employee_category_name"
			type="string" not-null="false" length="20" />
		<property name="StatusId" column="status_id" type="integer"
			not-null="false" length="10" />


	</class>
</hibernate-mapping>