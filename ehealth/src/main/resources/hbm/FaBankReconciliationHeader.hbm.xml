<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="FaBankReconciliationHeader" table="fa_bank_reconciliation_header">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="bank_reconciliation_header_id">
			<generator class="sequence">
				<param name="sequence">fa_bank_reconciliation_header_bank_reconciliation_header_id_seq</param>
			</generator>
		</id>


		<property name="ReconciliationDate" column="reconciliation_date"
			type="date" not-null="false" length="13" />
		<property name="DrBlncAsPerCompany" column="dr_blnc_as_per_company"
			type="big_decimal" not-null="false" length="13" />
		<property name="CrBlncAsPerCompany" column="cr_blnc_as_per_company"
			type="big_decimal" not-null="false" length="13" />
		<property name="DrBlncAsPerBank" column="dr_blnc_as_per_bank"
			type="big_decimal" not-null="false" length="13" />
		<property name="CrBlncAsPerBank" column="cr_blnc_as_per_bank"
			type="big_decimal" not-null="false" length="13" />
		<property name="DiffAmt" column="diff_amt" type="big_decimal"
			not-null="false" length="13" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" length="13" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<many-to-one name="LastChgBy" column="last_chg_by" class="Users"
			not-null="false">
		</many-to-one>
		<many-to-one name="Account" column="account_id" class="FaMasAccount"
			not-null="false">
		</many-to-one>
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="false">
		</many-to-one>

		<set inverse="true" name="FaBankReconciliationDetails">
			<key column="bank_reconciliation_header_id" />
			<one-to-many class="FaBankReconciliationDetails" />
		</set>



	</class>
</hibernate-mapping>