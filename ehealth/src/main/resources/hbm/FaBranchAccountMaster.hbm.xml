<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="FaBranchAccountMaster" table="fa_branch_account_master">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="branch_account_id">
			<generator class="sequence">
				<param name="sequence">fa_branch_account_master_branch_account_id_seq</param>
			</generator>
		</id>

		<property name="OpBalanceDr" column="op_balance_dr" type="big_decimal"
			not-null="false" length="17" />
		<property name="OpBalanceCr" column="op_balance_cr" type="big_decimal"
			not-null="false" length="17" />
		<property name="YtdBalanceDr" column="ytd_balance_dr" type="big_decimal"
			not-null="false" length="17" />
		<property name="YtdBalanceCr" column="ytd_balance_cr" type="big_decimal"
			not-null="false" length="17" />
		<property name="ClBalanceDr" column="cl_balance_dr" type="big_decimal"
			not-null="false" length="17" />
		<property name="ClBalanceCr" column="cl_balance_cr" type="big_decimal"
			not-null="false" length="17" />
		<many-to-one name="Account" column="account_id" class="FaMasAccount"
			not-null="false">
		</many-to-one>
		<many-to-one name="SubLedger" column="sub_ledger_id"
			class="FaMasSubLed" not-null="false">
		</many-to-one>
		<many-to-one name="Branch" column="branch_id" class="MasBranch"
			not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>