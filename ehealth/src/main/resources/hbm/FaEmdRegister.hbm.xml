<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="FaEmdRegister"
		table="fa_emd_register"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="emd_register_id"
		>
			<generator class="sequence">
				<param name="sequence">fa_emd_register_emd_register_id_seq</param>
			</generator>
		</id>

		<property
			name="TenderNo"
			column="tender_no"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Date"
			column="date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="SoNo"
			column="so_no"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="Organization"
			column="organization"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="EmdAmount"
			column="emd_amount"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="FromDate"
			column="from_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="ToDate"
			column="to_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="Bank"
			column="bank_id"
			class="MasBankMaster"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Location"
			column="location_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>