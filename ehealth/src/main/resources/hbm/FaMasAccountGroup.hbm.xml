<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="FaMasAccountGroup"
		table="fa_mas_account_group"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="java.lang.Integer"
			column="account_group_id"
		>
			<generator class="sequence">
				<param name="sequence">fa_mas_account_group_account_group_id_seq</param>
			</generator>
		</id>

		<property
			name="AccountGroupCode"
			column="account_group_code"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="AccountGroupDesc"
			column="account_group_desc"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="YtdAmountDr"
			column="ytd_amount_dr"
			type="big_decimal"
			not-null="false"
			length="17"
		/>
		<property
			name="YtdAmountCr"
			column="ytd_amount_cr"
			type="big_decimal"
			not-null="false"
			length="17"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="OpBalanceDr"
			column="op_balance_dr"
			type="big_decimal"
			not-null="false"
			length="17"
		/>
		<property
			name="OpBalanceCr"
			column="op_balance_cr"
			type="big_decimal"
			not-null="false"
			length="17"
		/>
		<property
			name="ClBalanceDr"
			column="cl_balance_dr"
			type="big_decimal"
			not-null="false"
			length="17"
		/>
		<property
			name="ClBalanceCr"
			column="cl_balance_cr"
			type="big_decimal"
			not-null="false"
			length="17"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="FYear"
			column="f_year_id"
			class="MasStoreFinancial"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>

		<set inverse="true" name="FaMasAccountSubGroups">
			<key column="account_group_id" />
			<one-to-many class="FaMasAccountSubGroup" />
		</set>



	</class>	
</hibernate-mapping>