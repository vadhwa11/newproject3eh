<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="FaMasFdRegister"
		table="fa_mas_fd_register"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">fa_mas_fd_register_id_seq</param>
			</generator>
		</id>

		<property
			name="FdrNo"
			column="fdr_no"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="DateOfIssue"
			column="date_of_issue"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="DateOfMaturity"
			column="date_of_maturity"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Amount"
			column="amount"
			type="big_decimal"
			not-null="false"
			length="14"
		/>
		<property
			name="RateOfInterest"
			column="rate_of_interest"
			type="big_decimal"
			not-null="false"
			length="5"
		/>
		<property
			name="NoOfDays"
			column="no_of_days"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="Bank"
			column="bank_id"
			class="MasBankMaster"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Location"
			column="location_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>