<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="FaVoucherDetails" table="fa_voucher_details">
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="voucher_details_id">
			<generator class="sequence">
				<param name="sequence">fa_voucher_details_voucher_details_id_seq</param>
			</generator>
		</id>

		<property name="DrAmount" column="dr_amount" type="big_decimal"
			not-null="false" length="17" />
		<property name="CrAmount" column="cr_amount" type="big_decimal"
			not-null="false" length="17" />
		<property name="Narration" column="narration" type="string"
			not-null="false" length="150" />
		<property name="PayMode" column="pay_mode" type="string"
			not-null="false" length="45" />
		<property name="Reconcile" column="reconcile" type="string"
			not-null="false" length="1" />
		<property name="ClearDate" column="clear_date" type="date"
			not-null="false" length="13" />
		<many-to-one name="CostCenter" column="cost_center_id"
			class="MasCostCenter" not-null="false">
		</many-to-one>
		<many-to-one name="Account" column="account_id" class="FaMasAccount"
			not-null="false">
		</many-to-one>
		<many-to-one name="AccountIdForReport" column="account_id_for_report"
			class="FaMasAccount" not-null="false">
		</many-to-one>
		<many-to-one name="SubLed" column="sub_led_id" class="FaMasSubLed"
			not-null="false">
		</many-to-one>
		<many-to-one name="VoucherHeader" column="voucher_header_id"
			class="FaVoucherHeader" not-null="false">
		</many-to-one>
		<many-to-one name="SubLedIdForReport" column="sub_led_id_for_report"
			class="FaMasSubLed" not-null="false">
		</many-to-one>

		<many-to-one name="AdvAcc" column="adv_acc_id" class="FaMasAccount"
			not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>