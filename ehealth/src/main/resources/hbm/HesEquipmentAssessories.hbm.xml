<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="HesEquipmentAssessories"
		table="hes_equipment_assessories"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="assessory_id">
			<generator class="sequence">
				<param name="sequence">hes_equipment_assessories_assessory_id_seq</param>
			</generator>

		</id>

		<property
			name="AssessoryName"
			column="assessory_name"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="SerialNo"
			column="serial_no"
			type="string"
			not-null="false"
			length="150"
		/>
		<property
			name="Quantity"
			column="quantity"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="ModelNo"
			column="model_no"
			type="string"
			not-null="false"
			length="150"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="WarrantyStartDate"
			column="warranty_start_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="WarrantyEndDate"
			column="warranty_end_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Item"
			column="item_id"
			class="MasStoreItem"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Equipment"
			column="equipment_id"
			class="HesEquipmentMaster"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>