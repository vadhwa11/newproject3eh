<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="HesEquipmentMaster"
		table="hes_equipment_master"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="equipment_id"
		>
			<generator class="sequence"><param name="sequence">hes_equipment_master_equipment_id_seq</param></generator>
		</id>

		<property
			name="EquipmentName"
			column="equipment_name"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="ModelName"
			column="model_name"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="SerialNo"
			column="serial_no"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Make"
			column="make"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="WarrantyDetails"
			column="warranty_details"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="TechnicalSpecifications"
			column="technical_specifications"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="ContactNoSalesService"
			column="contact_no_sales_service"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="EntryNo"
			column="entry_no"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="WarrentyStartDate"
			column="warrenty_start_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="WarrentyEndDate"
			column="warrenty_end_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="DateOfInstallation"
			column="date_of_installation"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="SupplyOrderNo"
			column="supply_order_no"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="SourceOfSupply"
			column="source_of_supply"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="PreventiveCycle"
			column="preventive_cycle"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="PreventiveCompletedCycle"
			column="preventive_completed_cycle"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="RejectionDetail"
			column="rejection_detail"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="ReplacementValue"
			column="replacement_value"
			type="big_decimal"
			not-null="true"
			length="14"
		/>
		<property
			name="WarrentyStatus"
			column="warrenty_status"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="PreventiveStatus"
			column="preventive_status"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="ChecklistStatus"
			column="checklist_status"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="EquipmentDetailDate"
			column="equipment_detail_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="WarrantyType"
			column="warranty_type"
			type="string"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Supplier"
			column="supplier_id"
			class="MasStoreSupplier"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Item"
			column="item_id"
			class="MasStoreItem"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Manufacturer"
			column="manufacturer_id"
			class="MasManufacturer"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="GrnT"
			column="grn_t_id"
			class="StoreGrnT"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="EquipStatus"
			column="equip_status"
			class="MmMasRequestStatus"
			not-null="false"
		>
		</many-to-one>

		<set inverse="true" name="HesEquipmentAssessoriess">
			<key column="assessory_id" />
			<one-to-many class="HesEquipmentAssessories" />
		</set>



	</class>	
</hibernate-mapping>