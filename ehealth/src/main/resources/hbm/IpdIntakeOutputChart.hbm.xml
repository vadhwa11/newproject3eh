<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="IpdIntakeOutputChart"
		table="ipd_intake_output_chart"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="intakeoutput_id"
		>
			<generator class="sequence">
			<param name="sequence">ipd_intake_output_chart_intakeoutput_id_seq</param>
			</generator>
		</id>

		<property
			name="AdNo"
			column="ad_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Treatment"
			column="treatment"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="Date"
			column="date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Time"
			column="time"
			type="string"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>

		<set inverse="true" name="IpdIntakes">
			<key column="intakeoutput_id" />
			<one-to-many class="IpdIntake" />
		</set>

		<set inverse="true" name="IpdTemperatures">
			<key column="intakeoutput_id" />
			<one-to-many class="IpdTemperature" />
		</set>

		<set inverse="true" name="IpdOutputs">
			<key column="intakeoutput_id" />
			<one-to-many class="IpdOutput" />
		</set>



	</class>	
</hibernate-mapping>