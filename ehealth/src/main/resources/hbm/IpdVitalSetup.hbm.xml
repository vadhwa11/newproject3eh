<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="IpdVitalSetup"
		table="ipd_vital_setup"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="vital_setup_id"
		>
			<generator class="sequence">
			<param name="sequence">ipd_vital_setup_vital_setup_id_seq</param>
			</generator>
		</id>

		<property
			name="VitalName"
			column="vital_name"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="StopVital"
			column="stop_vital"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Frequency"
			column="frequency_id"
			class="MasFrequency"
			not-null="false"
		>
		</many-to-one>
	

		<set name="IpdVitalcareHeaders" inverse="true">
			<key column="vital_setup_id"/>
			<one-to-many class="IpdVitalcareHeader"/>
		</set>


	</class>	
</hibernate-mapping>