<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="LaborRoom"
		table="labor_room"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="labor_room_id"
		>
				<generator class="sequence">
				<param name="sequence">labor_room_labor_room_id_seq</param>
			</generator>
		</id>

		<property
			name="LaborRoomDate"
			column="labor_room_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="MaternalPulseRate"
			column="maternal_pulse_rate"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="RespRate"
			column="resp_rate"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="LabRoomBp"
			column="lab_room_bp"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="LungBases"
			column="lung_bases"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="KneeJerk"
			column="knee_jerk"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="FetalHeart"
			column="fetal_heart"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Nst"
			column="nst"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="Contraction"
			column="contraction"
			type="string"
			not-null="false"
			length="150"
		/>
		<property
			name="LabRoomIo"
			column="lab_room_io"
			type="string"
			not-null="false"
			length="150"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="timestamp"
			not-null="false"
			length="29"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="LaborRoomType"
			column="labor_room_type"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="LaborRoomTime"
			column="labor_room_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="OxyinfoRate"
			column="oxyinfo_rate"
			type="string"
			not-null="false"
			length="150"
		/>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Employee"
			column="employee_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>