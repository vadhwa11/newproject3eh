<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="LrPulseBp"
		table="lr_pulse_bp"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="lr_pulse_bp_id"
		>
			<generator class="sequence"><param name="sequence">lr_pulse_bp_lr_pulse_bp_id_seq</param></generator>
		</id>

		<property
			name="PulseBpDate"
			column="pulse_bp_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="PulseBpTime"
			column="pulse_bp_time"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="PulseBeat"
			column="pulse_beat"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="PulseMin"
			column="pulse_min"
			type="integer"
			not-null="false"
			length="5"
		/>
		<property
			name="BpSystolic"
			column="bp_systolic"
			type="integer"
			not-null="false"
			length="5"
		/>
		<property
			name="BpDiastolic"
			column="bp_diastolic"
			type="integer"
			not-null="false"
			length="5"
		/>
		<property
			name="LaborRoomType"
			column="labor_room_type"
			type="integer"
			not-null="false"
			length="5"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="timestamp"
			not-null="false"
			length="29"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Employee"
			column="employee_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>