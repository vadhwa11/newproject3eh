<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MasBedStatus"
		table="mas_bed_status"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="bed_status_id"
		>
			<generator class="sequence">
				<param name="sequence">mas_bed_status_bed_status_id_seq</param>
			</generator>
		</id>

		<property
			name="BedStatusCode"
			column="bed_status_code"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="BedStatusName"
			column="bed_status_name"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>

		<set inverse="true" name="MasBeds">
			<key column="bed_status_id" />
			<one-to-many class="MasBed" />
		</set>



	</class>	
</hibernate-mapping>