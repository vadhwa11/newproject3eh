<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasBranch" table="mas_branch">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="branch_id">
			<generator class="sequence">
				<param name="sequence">mas_branch_branch_id_seq</param>
			</generator>
		</id>

		<property name="BranchCode" column="branch_code" type="string"
			not-null="false" length="50" />
		<property name="BranchDesc" column="branch_desc" type="string"
			not-null="false" length="50" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="50" />
		<property name="LastChgDate" column="last_chg_date" type="timestamp"
			not-null="false" length="23" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="50" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="HospitalId" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="false">
		</many-to-one>
	</class>
</hibernate-mapping>