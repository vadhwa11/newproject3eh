<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasBulkMain" table="mas_bulk_main">
		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">mas_bulk_main_id_seq</param>
			</generator>
		</id>
		<property name="GroupCode" column="group_code" type="string"
			not-null="false" length="8" />
		<property name="GroupType" column="group_type" type="string"
			not-null="false" length="1" />

		<property name="GroupName" column="group_name" type="string"
			not-null="false" length="64" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<many-to-one name="LastChgBy" column="last_chg_by" class="Users"
			not-null="false">
		</many-to-one>
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" length="13" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="8" />
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>

	</class>
</hibernate-mapping>