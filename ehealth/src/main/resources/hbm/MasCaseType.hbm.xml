<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasCaseType" table="mas_case_type">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="case_type_id">
			<generator class="sequence">
				<param name="sequence">mas_case_type_case_type_id_seq</param>
			</generator>
		</id>

		<property name="CaseTypeCode" column="case_type_code" type="string"
			not-null="false" length="8" />
		<property name="CaseTypeName" column="case_type_name" type="string"
			not-null="false" length="30" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />

<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<set name="Inpatients" inverse="true">
			<key column="case_type_id" />
			<one-to-many class="Inpatient" />
		</set>

		<set name="Visits" inverse="true">
			<key column="case_type_id" />
			<one-to-many class="Visit" />
		</set>


	</class>
</hibernate-mapping>