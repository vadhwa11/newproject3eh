<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasCostCenter" table="mas_cost_center">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="cost_center_id">
			<generator class="sequence">
				<param name="sequence">mas_cost_center_cost_center_id_seq</param>
			</generator>
		</id>

		<property name="CostCenterCode" column="cost_center_code"
			type="string" not-null="false" length="8" />
		<property name="CostCenterName" column="cost_center_name"
			type="string" not-null="false" length="30" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />


		<set name="MasEmployees" inverse="true">
			<key column="cost_center_id" />
			<one-to-many class="MasEmployee" />
		</set>

		<set name="MasDepartments" inverse="true">
			<key column="cost_center_id" />
			<one-to-many class="MasDepartment" />
		</set>


	</class>
</hibernate-mapping>