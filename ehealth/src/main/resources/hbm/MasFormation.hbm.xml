<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasFormation" table="mas_formation">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="formation_id">
			<generator class="sequence">
				<param name="sequence">mas_formation_formation_id_seq</param>
			</generator>
		</id>

		<property name="FormationCode" column="formation_code" type="string"
			not-null="false" length="8" />
		<property name="FormationName" column="formation_name" type="string"
			not-null="false" length="50" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<many-to-one name="ServiceType" column="service_type_id"
			class="MasServiceType" not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>