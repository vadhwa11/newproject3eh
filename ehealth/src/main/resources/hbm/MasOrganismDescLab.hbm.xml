<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasOrganismDescLab" table="mas_organism_desc_lab">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="organism_desc_id">
			<generator class="sequence">
				<param name="sequence">mas_organism_desc_lab_organism_desc_id_seq</param>
			</generator>
		</id>

		<property name="OrganismDescCode" column="organism_desc_code"
			type="string" not-null="false" length="8" />
		<property name="OrganismDescName" column="organism_desc_name"
			type="string" not-null="false" length="30" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<many-to-one name="OrganismGroup" column="organism_group_id"
			class="MasOrganismLab" not-null="true">
		</many-to-one>


	</class>
</hibernate-mapping>