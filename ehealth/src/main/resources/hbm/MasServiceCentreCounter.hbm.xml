<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MasServiceCentreCounter"
		table="mas_service_centre_counter"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="service_centre_counter_id"
		>
			<generator class="sequence">
				<param name="sequence">mas_service_centre_counter_service_centre_counter_id_seq</param>
			</generator>
		</id>

		<property
			name="CounterNo"
			column="counter_no"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<set inverse="true" name="Users">
			<key column="user_id" />
			<one-to-many class="Users" />
		</set>

	</class>	
</hibernate-mapping>