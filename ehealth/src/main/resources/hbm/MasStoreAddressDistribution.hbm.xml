<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="MasStoreAddressDistribution" table="mas_store_address_distribution">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">mas_store_address_distribution_id_seq</param>
			</generator>
		</id>

		<property name="AddressDescr" column="address_descr" type="string"
			not-null="false" length="30" />
		<property name="AddressLine1" column="address_line_1" type="string"
			not-null="false" length="45" />
		<property name="AddressLine2" column="address_line_2" type="string"
			not-null="false" length="45" />
		<property name="AddressLine3" column="address_line_3" type="string"
			not-null="false" length="45" />
		<property name="AddressLine4" column="address_line_4" type="string"
			not-null="false" length="45" />


	</class>
</hibernate-mapping>