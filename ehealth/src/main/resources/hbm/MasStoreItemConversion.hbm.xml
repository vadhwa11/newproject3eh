<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MasStoreItemConversion"
		table="mas_store_item_conversion"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="item_conversion_id"
		>
			<generator class="sequence">
				<param name="sequence">mas_store_item_conversion_item_conversion_id_seq</param>
			</generator>
		</id>

		<property
			name="ItemUnitName"
			column="item_unit_name"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="ConversionFactor1"
			column="conversion_factor1"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ConversionFactor2"
			column="conversion_factor2"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Formula"
			column="formula"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="PurchaseUnit"
			column="purchase_unit_id"
			class="MasStoreUnit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="IssueUnit"
			column="issue_unit_id"
			class="MasStoreUnit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="IntermediateUnit"
			column="intermediate_unit_id"
			class="MasStoreUnit"
			not-null="false"
		>
		</many-to-one>

		<set inverse="true" name="MasStoreItems">
			<key column="item_conversion_id" />
			<one-to-many class="MasStoreItem" />
		</set>



	</class>	
</hibernate-mapping>