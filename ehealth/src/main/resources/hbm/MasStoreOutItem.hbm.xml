<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MasStoreOutItem"
		table="mas_store_out_item"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="item_id"
		>
			<generator class="sequence">
			<param name="sequence">mas_store_out_item_item_id_seq</param>
			</generator>
		</id>

		<property
			name="Pvms"
			column="pvms"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Nomenclature"
			column="nomenclature"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="DangerousDrug"
			column="dangerous_drug"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="ItemTypeId"
			column="item_type_id"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="ItemCategoryId"
			column="item_category_id"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="ItemConversionId"
			column="item_conversion_id"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="CostPrice"
			column="cost_price"
			type="integer"
			not-null="false"
		/>
		<property
			name="ControlledDrug"
			column="controlled_drug"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="RateContractItem"
			column="rate_contract_item"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="GroupId"
			column="group_id"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="CreatedOn"
			column="created_on"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="ReffredBy"
			column="reffred_by"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="CommonName"
			column="common_name"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="DispUnit"
			column="disp_unit"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="ADispQty"
			column="a_disp_qty"
			type="integer"
			not-null="false"
		/>
		<many-to-one
			name="CreatedBy"
			column="created_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>