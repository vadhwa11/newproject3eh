<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MasSystemDiagnosis"
		table="mas_system_diagnosis"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="system_diagnosis_id"
		>
			<generator class="sequence">
				<param name="sequence">mas_system_diagnosis_system_diagnosis_id_seq</param>
			</generator>
		</id>

		<property
			name="SystemDiagnosisCode"
			column="system_diagnosis_code"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="SystemDiagnosisName"
			column="system_diagnosis_name"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="GroupId"
			column="group_id"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>