<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="MasTaluk"
		table="mas_taluk"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="taluk_id"
		>
			<generator class="sequence">
				<param name="sequence">mas_taluk_taluk_id_seq</param>
			</generator>
		</id>

		<property
			name="TalukCode"
			column="taluk_code"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="TalukName"
			column="taluk_name"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="State"
			column="state_id"
			class="MasState"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="District"
			column="district_id"
			class="MasDistrict"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>