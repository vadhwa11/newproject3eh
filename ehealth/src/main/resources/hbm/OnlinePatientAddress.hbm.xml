		<property
			name="NationalityId"
			column="nationality_id"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="City"
			column="city"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="PoliceStation"
			column="police_station"
			type="string"
			not-null="false"
			length="32"
