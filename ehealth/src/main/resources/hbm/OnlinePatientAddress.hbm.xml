			length="10"
		/>
		<property
			name="HouseNo"
			column="house_no"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="StreetRoad"
			column="street_road"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="AreaSector"
			column="area_sector"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="LandMark"
			column="land_mark"
			type="string"
			not-null="false"
			length="32"
		/>
