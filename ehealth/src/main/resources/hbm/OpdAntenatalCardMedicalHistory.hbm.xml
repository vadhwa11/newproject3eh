<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="OpdAntenatalCardMedicalHistory"
		table="opd_antenatal_card_medical_history"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="opd_antenatal_card_medical_history_id"
		>
			<generator class="sequence">
				<param name="sequence">opd_antenatal_card_pregnancy_opd_antenatal_card_pregnancy_i_seq</param>
			</generator>
		</id>

		<property
			name="Comorbidity"
			column="comorbidity"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="Years"
			column="years"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="Months"
			column="months"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="ChronicDisease"
			column="chronic_disease"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="OtherDisease"
			column="other_disease"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="LastChgBy"
			column="last_chg_by"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		
		<property
			name="Infection"
			column="infection"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="InfectionYears"
			column="infection_years"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="InfectionMonths"
			column="infection_months"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="InfectionRemarks"
			column="infection_remarks"
			type="string"
			not-null="false"
			length="300"
		/>			
		<property
			name="InfectionOthers"
			column="infection_others"
			type="string"
			not-null="false"
			length="300"
		/>
		
		<property
			name="PastSurgeryName"
			column="past_surgery_name"
			type="string"
			not-null="false"
			length="300"
		/>
		
		<property
			name="PastSurgeryYears"
			column="past_surgery_years"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="PastSurgeryHospital"
			column="past_surgery_hospital"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="PastSurgeryRemarks"
			column="past_surgery_remarks"
			type="string"
			not-null="false"
			length="300"
		/>
		<many-to-one
			name="OpdAntenatalCard"
			column="opd_antenatal_card_id"
			class="OpdAntenatalCard"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Visit"
			column="visit_id"
			class="Visit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>