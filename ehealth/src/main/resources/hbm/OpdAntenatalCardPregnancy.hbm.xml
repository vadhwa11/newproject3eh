<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="OpdAntenatalCardPregnancy"
		table="opd_antenatal_card_pregnancy"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="opd_antenatal_card_pregnancy_id"
		>
			<generator class="sequence">
				<param name="sequence">opd_antenatal_card_medical_hi_opd_antenatal_card_medical_hi_seq</param>
			</generator>
		</id>

		<property
			name="Year"
			column="year"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="Age"
			column="age"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="PregnancyOutcome"
			column="pregnancy_outcome"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="PlaceDelivery"
			column="place_delivery"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="DeliveryOutcome"
			column="delivery_outcome"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="Sex"
			column="sex"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="BirthWeight"
			column="birth_weight"
			type="java.lang.Double"
			not-null="false"
			length="10"
		/>
		<property
			name="Antenatal"
			column="antenatal"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="IntraPartum"
			column="intra_partum"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="PostPartum"
			column="post_partum"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="BloodTransfusion"
			column="blood_transfusion"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgBy"
			column="last_chg_by"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		
		<property
			name="PlaceOfDeliveryOthersValue"
			column="place_of_delivery_others_value"
			type="string"
			not-null="false"
			length="128"
		/>
		
		<property
			name="PregnancyOutcomePreTermValue"
			column="pregnancy_outcome_pre_term_value"
			type="string"
			not-null="false"
			length="128"
		/>
		
		<property
			name="NoOfPregnancy"
			column="no_of_pregnancy"
			type="int"
			not-null="false"
			length="12"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="PreviousGestationalAge"
			column="previous_gestational_age"
			type="string"
			not-null="false"
			length="32"
		/>
		<many-to-one
			name="OpdAntenatalCard"
			column="opd_antenatal_card_id"
			class="OpdAntenatalCard"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Visit"
			column="visit_id"
			class="Visit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>