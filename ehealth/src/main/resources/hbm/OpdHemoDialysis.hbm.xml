<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="OpdHemoDialysis"
		table="opd_hemo_dialysis"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="hemo_dialysis_id"
		>
			<generator class="sequence">
				<param name="sequence">opd_hemo_dialysis_hemo_dialysis_id_seq</param>
			</generator>
		</id>

		<property
			name="DialysisDate"
			column="dialysis_date"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="DialysisHdNo"
			column="dialysis_hd_no"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="DialysisAccess1"
			column="dialysis_access1"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="DialysisAccess2"
			column="dialysis_access2"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="PreHdWeight"
			column="pre_hd_weight"
			type="big_decimal"
			not-null="false"
			length="10"
		/>
		<property
			name="PostHdWeight"
			column="post_hd_weight"
			type="big_decimal"
			not-null="false"
			length="10"
		/>
		<property
			name="WeightGain"
			column="weight_gain"
			type="big_decimal"
			not-null="false"
			length="10"
		/>
		<property
			name="HeparinDose"
			column="heparin_dose"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="BloodFlowRate"
			column="blood_flow_rate"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="DialysateFlowRate"
			column="dialysate_flow_rate"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="KtV"
			column="kt_v"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Urr"
			column="urr"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Uf"
			column="uf"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="NextDialysisDate"
			column="next_dialysis_date"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="NextDialysisTime"
			column="next_dialysis_time"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="EventsComplications"
			column="events_complications"
			type="string"
			not-null="false"
			length="300"
		/>
		
		<property
			name="VitalDateTime"
			column="vital_date_time"
			type="string"
			not-null="false"
			length="25"
		/>
		<property
			name="VitalBp"
			column="vital_bp"
			type="string"
			not-null="false"
			length="10"
		/>
		
		<property
			name="VitalPulse"
			column="vital_pulse"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		
		<property
			name="VitalTemperature"
			column="vital_temperature"
			type="big_decimal"
			not-null="false"
			length="10"
		/>
		
		<many-to-one
			name="OpdPatientDetails"
			column="opd_patient_details"
			class="OpdPatientDetails"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>