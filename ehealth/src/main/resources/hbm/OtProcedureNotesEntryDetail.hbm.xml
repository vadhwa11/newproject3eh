<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="OtProcedureNotesEntryDetail"
		table="ot_procedure_notes_entry_detail"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">ot_procedure_notes_entry_detail_id_seq</param>
			</generator>
		</id>

		<many-to-one
			name="Employee"
			column="employee_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="OtProcedureHeader"
			column="ot_procedure_header_id"
			class="OtProcedureNotesEntryHeader"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>