<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="OtSpecimenDispatchEntry"
		table="ot_specimen_dispatch_entry"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="ot_specimen_dispatch_entry_id"
		>
			<generator class="sequence">
				<param name="sequence">ot_specimen_dispatch_entry_ot_specimen_dispatch_entry_id_seq</param>
			</generator>
		</id>

		<property
			name="EntryNo"
			column="entry_no"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="OtSpecimenDispatchEntryDate"
			column="ot_specimen_dispatch_entry_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="TissueOrgan"
			column="tissue_organ"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ClinicalNotes"
			column="clinical_notes"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ExaminationRequired"
			column="examination_required"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="TimeOfDispatch"
			column="time_of_dispatch"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="SpecimenReceivedBy"
			column="specimen_received_by"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Visit"
			column="visit_id"
			class="Visit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="SpecimenDispatchedBy"
			column="specimen_dispatched_by"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>