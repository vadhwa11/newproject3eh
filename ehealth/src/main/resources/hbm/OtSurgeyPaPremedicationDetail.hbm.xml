<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="OtSurgeyPaPremedicationDetail"
		table="ot_surgey_pa_premedication_detail"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="surgey_pa_premedication_detail_id"
		>
			<generator class="sequence">
				<param name="sequence">ot_surgey_pa_premedication_seq</param>
			</generator>
		</id>

		<property
			name="SurgeyPaPremedicationDetailType"
			column="surgey_pa_premedication_detail_type"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="SurgeyPaPremedicationDetailDosa"
			column="surgey_pa_premedication_detail_dosa"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="SurgeyPaPremedicationDetailRoute"
			column="surgey_pa_premedication_detail_route"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="PreMedicationTime"
			column="pre_medication_time"
			type="string"
			not-null="false"
			length="10"
		/>
		
		
		
		
		
		
		
		<many-to-one
			name="Item"
			column="item_id"
			class="MasStoreItem"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="OtPostAnaesthesiaProcedure"
			column="ot_post_anaesthesia_procedure_id"
			class="OtPostAnaesthesiaProcedure"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>