<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PatientDetentionRegister"
		table="patient_detention_register"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="patient_detention_register_id"
		>
			<generator class="sequence"/>
		</id>

		<property
			name="DetentionRegisterDate"
			column="detention_register_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Remark"
			column="remark"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="DetainedFrom"
			column="detained_from"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="DetainedTo"
			column="detained_to"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Treatment"
			column="treatment"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Fromtime"
			column="fromtime"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Totime"
			column="totime"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Requisitiondate"
			column="requisitiondate"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Reviewat"
			column="reviewat"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Visit"
			column="visit_id"
			class="Visit"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>