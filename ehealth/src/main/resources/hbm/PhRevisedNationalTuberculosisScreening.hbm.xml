<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="PhRevisedNationalTuberculosisScreening"
		table="ph_revised_national_tuberculosis_screening"
	>
		<meta attribute="sync-DAO">false</meta>
<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="sequence">
				<param name="sequence">ph_revised_national_tuberculosis_screening_id_seq</param>
			</generator>
		</id>
		<property
			name="AadharNo"
			column="aadhar_no"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="PastHistoryTb"
			column="past_history_tb"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="FamilyMemberTb"
			column="family_member_tb"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="CoughMorethanTwoWeeks"
			column="cough_morethan_two_weeks"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="BloodStainedSputum"
			column="blood_stained_sputum"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="CheckedStatus"
			column="checked_status"
			type="java.lang.String"
			not-null="false"
			length="10"
		/>
		<property
			name="Fever"
			column="fever"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="LossOfWeight"
			column="loss_of_weight"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="HistoryOfDiabetesMellitus"
			column="history_of_diabetes_mellitus"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Copd"
			column="copd"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Smoking"
			column="smoking"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Alcohol"
			column="alcohol"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="TypeOfInstitution"
			column="type_of_institution"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="NameOfInstitution"
			column="name_of_institution"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="LastChangeDate"
			column="last_change_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="UniqueRegId"
			column="unique_reg_id"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="RegType"
			column="reg_type"
			type="string"
			not-null="false"
			length="30"
		/>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="LastChangeBy"
			column="last_change_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Member"
			column="member_id"
			class="PhMemberSurvey"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>