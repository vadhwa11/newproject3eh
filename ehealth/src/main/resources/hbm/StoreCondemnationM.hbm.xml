<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="StoreCondemnationM" table="store_condemnation_m">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">store_condemnation_m_id_seq</param>
			</generator>
		</id>

		<property name="CondemnationNo" column="condemnation_no"
			type="string" not-null="true" length="15" />
		<property name="CondemnationDate" column="condemnation_date"
			type="date" not-null="true" />
		<property name="WorkOrderDate" column="work_order_date" type="date"
			not-null="false" />
		<property name="Status" column="status" type="string"
			not-null="true" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="true" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="true" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="true" length="12" />
		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="true">
		</many-to-one>
		<many-to-one name="WorkOrder" column="work_order_id"
			class="StoreWorkOrderM" not-null="false">
		</many-to-one>
		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="true">
		</many-to-one>


		<set name="StoreCondemnationTs" inverse="true">
			<key column="id" />
			<one-to-many class="StoreCondemnationT" />
		</set>


	</class>
</hibernate-mapping>