<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="StoreOpPatientIssueM"
		table="store_op_patient_issue_m"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="op_issue_id"
		>
			<generator class="sequence">
				<param name="sequence">store_op_patient_issue_m_op_issue_id_seq</param>
			</generator>
		</id>

		<property
			name="IssueType"
			column="issue_type"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="IssueNo"
			column="issue_no"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="IssueDate"
			column="issue_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="PrescriptionNo"
			column="prescription_no"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="LastChgDate"
			column="last_chg_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="LastChgTime"
			column="last_chg_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="ServiceNo"
			column="service_no"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="IssueToDate"
			column="issue_to_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="TypeOfIssue"
			column="type_of_issue"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="LastChgBy"
			column="last_chg_by"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Emp"
			column="emp_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Department"
			column="department_id"
			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Prescription"
			column="prescription_id"
			class="PatientPrescriptionHeader"
			not-null="false"
		>
		</many-to-one>

		<set inverse="true" name="StoreOpPatientIssueTs">
			<key column="op_issue_id" />
			<one-to-many class="StoreOpPatientIssueT" />
		</set>



	</class>	
</hibernate-mapping>