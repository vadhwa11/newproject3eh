<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class
		name="StorePoDetail"
		table="store_po_detail"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="po_detail_id"
		>
			<generator class="sequence"><param name="sequence">store_po_detail_po_detail_id_seq</param></generator>
		</id>

		<property
			name="QuantityOrdered"
			column="quantity_ordered"
			type="big_decimal"
			not-null="false"
			length="9"
		/>
		<property
			name="QuantityReceived"
			column="quantity_received"
			type="big_decimal"
			not-null="false"
			length="9"
		/>
		<property
			name="FreeQuantity"
			column="free_quantity"
			type="big_decimal"
			not-null="false"
			length="9"
		/>
		<property
			name="UnitRate"
			column="unit_rate"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="Amount"
			column="amount"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="DiscountAmount"
			column="discount_amount"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="TaxAmount"
			column="tax_amount"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="Notes"
			column="notes"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="TaxPercent"
			column="tax_percent"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="DiscountPercent"
			column="discount_percent"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="FreeItem"
			column="free_item"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="SerialNo"
			column="serial_no"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="BrandId"
			column="brand_id"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="DispType"
			column="disp_type"
			type="string"
			not-null="false"
			length="75"
		/>
		<property
			name="MdqValue"
			column="mdq_value"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="RatePerMdq"
			column="rate_per_mdq"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="Mrp"
			column="mrp"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="OtherTaxes"
			column="other_taxes"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="Cancelled"
			column="cancelled"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="Remarks"
			column="remarks"
			type="string"
			not-null="false"
			length="250"
		/>
		<property
			name="TaxamtMdq"
			column="taxamt_mdq"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="ItemStatus"
			column="item_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="ReSupplyOrder"
			column="re_supply_order"
			class="StorePoHeader"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Po"
			column="po_id"
			class="StorePoHeader"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Item"
			column="item_id"
			class="MasStoreItem"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="ReSupplier"
			column="re_supplier_id"
			class="MasStoreSupplier"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Manufacturer"
			column="manufacturer_id"
			class="MasManufacturer"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="ItemDetails"
			column="item_details_id"
			class="PrqQuotationItemDetails"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>