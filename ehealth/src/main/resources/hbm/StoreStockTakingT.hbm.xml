<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="StoreStockTakingT" table="store_stock_taking_t">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">store_stock_taking_t_id_seq</param>
			</generator>
		</id>

		<property name="SrNo" column="sr_no" type="integer" not-null="false"
			length="10" />
		<property name="BatchNo" column="batch_no" type="string"
			not-null="false" length="32" />
		<property name="ExpiryDate" column="expiry_date" type="date"
			not-null="false" />
		<property name="ComputedStock" column="computed_stock" type="big_decimal"
			not-null="false" length="12" />
		<property name="StoreStockService" column="store_stock_service"
			type="big_decimal" not-null="false" length="12" />
		<property name="StoreStockDefective" column="store_stock_defective"
			type="big_decimal" not-null="false" length="12" />
		<property name="Remarks" column="remarks" type="string"
			not-null="false" length="50" />
		<property name="StockSurplus" column="stock_surplus" type="big_decimal"
			not-null="false" length="12" />
		<property name="StockDeficient" column="stock_deficient"
			type="big_decimal" not-null="false" length="12" />
		<property name="CostPrice" column="cost_price" type="big_decimal"
			not-null="false" length="12" />
			<property name="ItemAvailabilityStatus" column="item_availability_status" type="string"
			not-null="false" length="1" />
			
			<property name="ItemCondition" column="item_condition" type="string"
			not-null="false" length="16" />
			<property name="NonAvailabilityReason" column="non_availability_reason" type="string"
			not-null="false" length="16" />
			
		<many-to-one name="StockTakingM" column="stock_taking_m_id"
			class="StoreStockTakingM" not-null="false">
		</many-to-one>
		<many-to-one name="Brand" column="brand_id" class="MasStoreBrand"
			not-null="false">
		</many-to-one>
		<many-to-one name="Item" column="item_id" class="MasStoreItem"
			not-null="false">
		</many-to-one>
		
		<many-to-one name="Asset" column="asset_id" class="PrqAssetDetails"
			not-null="false">
		</many-to-one>
		<many-to-one name="ItemBatchStock" column="store_item_batch_stock_id"
			class="StoreItemBatchStock" not-null="false">
		</many-to-one>

	</class>
</hibernate-mapping>