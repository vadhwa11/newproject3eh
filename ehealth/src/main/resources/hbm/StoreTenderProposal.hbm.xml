<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="StoreTenderProposal" table="store_tender_proposal">
		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">store_tender_proposal_id_seq</param>
			</generator>
		</id>

		<property name="QtyInMmf" column="qty_in_mmf" type="big_decimal"
			not-null="false" length="16" />
		<property name="TenderQty" column="tender_qty" type="big_decimal"
			not-null="false" length="16" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="false" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="false" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="false" length="10" />
		<property name="Status" column="status" type="string"
			not-null="false" length="1" />

		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="true">
		</many-to-one>

		<many-to-one name="Department" column="department_id"
			class="MasDepartment" not-null="true">
		</many-to-one>

		<many-to-one name="Item" column="item_id" class="MasStoreItem"
			not-null="true">
		</many-to-one>
		<many-to-one name="TenderId" column="tender_id" class="StoreTenderM"
			not-null="false">
		</many-to-one>

		<many-to-one name="Po" column="po_id" class="StorePoHeader"
			not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>