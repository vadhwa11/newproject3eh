<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="TemplateApplication" table="template_application">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="temp_app_id">
			<generator class="sequence">
				<param name="sequence">template_application_temp_app_id_seq</param>
			</generator>
		</id>

		<property name="Status" column="status" type="string"
			not-null="false" length="1" />
		<property name="LastChgBy" column="last_chg_by" type="string"
			not-null="true" length="12" />
		<property name="LastChgDate" column="last_chg_date" type="date"
			not-null="true" />
		<property name="LastChgTime" column="last_chg_time" type="string"
			not-null="true" length="12" />
		<many-to-one name="Button" column="button_id" class="MasButtonForm"
			not-null="false">
		</many-to-one>
		<many-to-one name="Template" column="template_id" class="MasTemplate"
			not-null="true">
		</many-to-one>
		<many-to-one name="App" column="app_id" class="MasApplication"
			not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>