			name="PatientCondition"
			column="patient_condition"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ListDate"
			column="list_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="ListTime"
			column="list_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="TransferType"
			column="transfer_type"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="TransferReason"
			column="transfer_reason"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="RequestStatus"
			column="request_status"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="PaywardBookingStatus"
			column="payward_booking_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<many-to-one
			name="Inpatient"
			column="inpatient_id"
			class="Inpatient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hospital"
			column="hospital_id"
			class="MasHospital"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="ToWard"
			column="to_ward_id"
			class="MasDepartment"
			not-null="false"
