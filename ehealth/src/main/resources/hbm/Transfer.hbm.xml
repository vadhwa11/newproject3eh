			type="integer"
			column="transfer_id"
		>
			<generator class="sequence"><param name="sequence">transfer_transfer_id_seq</param></generator>
		</id>

		<property
			name="TransferNo"
			column="transfer_no"
			type="java.lang.Integer"
			not-null="false"
			length="10"
		/>
		<property
			name="AdNo"
			column="ad_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="DateOfTransfer"
			column="date_of_transfer"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="TimeOfTransfer"
			column="time_of_transfer"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="AddEditDate"
			column="add_edit_date"
			type="date"
			not-null="false"
			length="13"
		/>
		<property
			name="AddEditTime"
			column="add_edit_time"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="AdStatus"
			column="ad_status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="List"
			column="list"
			type="string"
			not-null="false"
			length="20"
		/>
