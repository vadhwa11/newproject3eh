			class="MasDepartment"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Hin"
			column="hin_id"
			class="Patient"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="AddEditBy"
			column="add_edit_by_id"
			class="Users"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="AuthorizedBy"
			column="authorized_by_id"
			class="MasEmployee"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>