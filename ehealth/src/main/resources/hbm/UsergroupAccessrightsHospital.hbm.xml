<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="jkt.hms.masters.business">
	<class name="UsergroupAccessrightsHospital" table="usergroup_accessrights_hospital">
		<meta attribute="sync-DAO">false</meta>
		<id name="Id" type="integer" column="id">
			<generator class="sequence">
				<param name="sequence">usergroup_accessrights_hospital_id_seq</param>
			</generator>
		</id>

		<many-to-one name="Hospital" column="hospital_id" class="MasHospital"
			not-null="false">
		</many-to-one>
		<many-to-one name="Access" column="access_id" class="AccessRights"
			not-null="false">
		</many-to-one>
		<many-to-one name="Group" column="group_id" class="UserGroups"
			not-null="false">
		</many-to-one>


	</class>
</hibernate-mapping>