	<queryString><![CDATA[select msi.item_id,msic.item_unit_name,msi.nomenclature,coalesce(taa.cost_price,taa.cost_price) as cost,
coalesce(taa.closing,0) as closing,(coalesce(taa.total_issued,0)*coalesce(taa.cost_price,0)) as vale,
taa.cum_cons as cum,taa.class as class,role_in_total as per,taa.total_issued,taa.fromDate,
taa.toDate
from temp_abc_analysis taa left outer join mas_store_item msi 
on taa.item_id=msi.item_id and taa.department_id=msi.department_id
left outer join mas_store_item_conversion msic on msi.item_conversion_id=msic.item_conversion_id
where taa.department_id=$P{deptId}
order by class,cum_cons asc]]></queryString>

