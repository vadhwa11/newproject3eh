	<parameter name="fromDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="toDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select * from (
select sqldata.* ,( case when (percent1 >= 80.0  ) then 'A'
                when (percent1 >= 60.0 ) and (percent1 <= 79.9 ) then 'B'
                else  'C' end ) as abclevel

      from (
select hname,
(select charge_code_code from mas_charge_code h where  h.charge_code_id = mainsql.charge_code_id ) as chargecode,
(select charge_code_name from mas_charge_code h where  h.charge_code_id = mainsql.charge_code_id ) as chargename,
qty,amt, (((amt)/(
select sum(amt) from (
select sum(sd.amt) as amt,
(select main_chargecode_id from mas_charge_code h where  h.charge_code_id = sd.charge_code_id
and  h.main_chargecode_id in (1,2)) as mainchargecode
from bl_charge_slip_detail sd left outer join bl_charge_slip_main sm on
sd.charge_slip_main_id = sm.charge_slip_main_id
where sm.chg_slp_date between $P{fromDate} and $P{toDate} and sm.hospital_id = $P{hospitalId}
group by sd.charge_code_id ) as mainsql
where mainchargecode is not null


))*100) as percent1,discount,
(select charge from mas_charge_code h where  h.charge_code_id = mainsql.charge_code_id ) as rate

from (
select
(select hospital_name from mas_hospital h where status ='y' and h.hospital_id = sd.hospital_id ) as hname,
sd.charge_code_id,sum(sd.quantity) as qty,
sum(sd.amt) as amt ,sum(sd.amt-sd.net_amt) as discount
from bl_charge_slip_detail sd left outer join bl_charge_slip_main sm on
sd.charge_slip_main_id = sm.charge_slip_main_id
where sm.chg_slp_date between $P{fromDate} and $P{toDate} and sm.hospital_id = $P{hospitalId}
group by sd.charge_code_id,sd.hospital_id ) as mainsql ) as sqldata ) as mailsqldata
order by abclevel]]></queryString>

