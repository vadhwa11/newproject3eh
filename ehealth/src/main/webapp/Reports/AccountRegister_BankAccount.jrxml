	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select * from
(select main.*, pn.hin_no, 
UPPER((pn.p_first_name||' '||coalesce(pn.p_middle_name,' ')||' '||coalesce(p_last_name,' '))) as patientname,
inpatient_no from
(select vst.*,coalesce(vs.visit_no,0) as visit_no from
(select frst.*,scn.visit_id from
(select op_bill_header_id ,hin_id, receipt_type, coalesce(brh.amount,0) as amount,
brh.receipt_date, hospital_id,
(select hospital_name from mas_hospital m where m.hospital_id = brh.hospital_id) as hname,
case when brh.inpatient_id is null then '-' else 
(select ad_no from inpatient where inpatient_id=brh.inpatient_id) end as inpatient_id,
brd.payment_mode from bl_receipt_header brh inner join
bl_receipt_details brd on brh.receipt_header_id = brd.receipt_header_id
where brd.payment_mode in('Q','R')) as frst
left outer join
bl_op_bill_header scn
on frst.op_bill_header_id = scn.op_bill_header_id) as vst
left join visit vs on vst.visit_id = vs.visit_id) as main
inner join patient pn
on pn.hin_id = main.hin_id) as main
where receipt_date between $P{fromdate} and $P{todate} and hospital_id=$P{hospitalId}]]></queryString>

