	</parameter>
	<queryString><![CDATA[select * from
(
select 'D' as type, b.hin_id, 0 as visit_id, discount_amt as charityamount, 
bill_date as billdate, b.hospital_id,
(select hospital_name from mas_hospital m where m.hospital_id = b.hospital_id) as hname,
p.hin_no, UPPER((p.p_first_name||' '||coalesce(p.p_middle_name,' ')||' '||coalesce(p.p_last_name,' '))) as patientname,
0 as visit_no, 
case when ip.ad_no is null then '-' else ip.ad_no end as ipno
from bl_dispensing_header b inner join patient p on b.hin_id = p.hin_id
left outer join inpatient ip on b.inpatient_id = ip.inpatient_id
where  b.patient_type_id not in (2,8)

union

select 'C' as type, b.hin_id, 0 as visit_id, discount_amt as charityamount,
chg_slp_date as billdate, b.hospital_id,
(select hospital_name from mas_hospital m where m.hospital_id = b.hospital_id) as hname,
p.hin_no,
UPPER((p.p_first_name||' '||coalesce(p.p_middle_name,' ')||' '||coalesce(p.p_last_name,' '))) as patientname,
0 as visit_no, case when ip.ad_no is null then '-' else ip.ad_no end as ipno
from bl_charge_slip_main b inner join patient p on b.hin_id = p.hin_id
left outer join inpatient ip on b.inpatient_id = ip.inpatient_id
where b.patient_type_id not in (2,8)

union

select 'S' as type, b.hin_id, b.visit_id, discount_amt as charityamount,
bill_date as billdate, b.hospital_id,
(select hospital_name from mas_hospital m where m.hospital_id = b.hospital_id) as hname,
p.hin_no,
UPPER((p.p_first_name||' '||coalesce(p.p_middle_name,' ')||' '||coalesce(p.p_last_name,' '))) as patientname,
case when vs.visit_no is null then 0 else vs.visit_no end as visit_no, '-' as ipno
from bl_op_bill_header b inner join patient p on b.hin_id = p.hin_id
left outer join visit vs on b.visit_id = vs.visit_id
where b.patient_type_id not in (2,8)

union

select 'F' as type, b.hin_id, 0, discount_amt as charityamount, last_chg_date as billdate,
b.hospital_id,
(select hospital_name from mas_hospital m where m.hospital_id = b.hospital_id) as hname,
p.hin_no,
UPPER((p.p_first_name||' '||coalesce(p.p_middle_name,' ')||' '||coalesce(p.p_last_name,' '))) as patientname,
0 as visit_no,
case when ip.ad_no is null then '-' else ip.ad_no end as ipno
from bl_final_bill_details b inner join patient p on b.hin_id = p.hin_id
left outer join inpatient ip on b.inpatient_id=ip.inpatient_id
where settled_amt is not null and b.status='s'
and b.patient_type_id not in (2,8)
) as mainsql
where charityamount > 0
and billdate between $P{fromdate} and $P{todate} and hospital_id=$P{hospitalId}]]></queryString>

