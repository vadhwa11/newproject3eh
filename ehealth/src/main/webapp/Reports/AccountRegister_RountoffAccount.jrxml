	<queryString><![CDATA[select main.hin_no, main.patient_name, main.bill_date, main.bill_no, 
main.type, main.amount*(-1) as amount from (
SELECT b.hin_no, patient_name, bill_date, b.bill_no,'OP Service' as type,
sum((coalesce(bill_amt,0)-coalesce(discount_amt,0))-round(coalesce(bill_amt,0)-coalesce(discount_amt,0),3)) as amount 
FROM bl_op_bill_header b,patient p where b.hin_id = p.hin_id
and bill_date  between $P{fromdate} and $P{todate} and b.hospital_id=$P{hospitalId}
group by bill_date,b.bill_no,b.hin_no,patient_name

union

SELECT b.hin_no,
UPPER((p.p_first_name||' '||coalesce(p.p_middle_name,' ')||' '||coalesce(p.p_last_name,' '))) as pname,
bill_date, b.bill_no, 
case when inpatient_id is null then 'OP Dispensing' else 'IP Dispensing' end as type,
sum((coalesce(bill_amt,0)-coalesce(discount_amt,0))-round(coalesce(bill_amt,0)-coalesce(discount_amt,0),3)) as amount
FROM bl_dispensing_header b, patient p where b.hin_id = p.hin_id
and bill_date between $P{fromdate} and $P{todate}  and b.hospital_id=$P{hospitalId}
group by bill_date,b.bill_no,b.hin_no,p.p_first_name,
p.p_middle_name,p.p_last_name,inpatient_id

union

SELECT p.hin_no, 
UPPER((p.p_first_name||' '||coalesce(p.p_middle_name,' ')||' '||coalesce(p.p_last_name,' '))) as pname,
chg_slp_date, cast(charge_slip_no as varchar), 'IP service' as type,
sum((coalesce(chg_slp_amt,0)-coalesce(discount_amt,0))-round(coalesce(chg_slp_amt,0)-coalesce(discount_amt,0),3)) as amount
FROM bl_charge_slip_main b,patient p where b.hin_id = p.hin_id
and chg_slp_date between $P{fromdate} and $P{todate}  and b.hospital_id=$P{hospitalId}
group by chg_slp_date,charge_slip_no,p.hin_no,p.p_first_name,
p.p_middle_name,p.p_last_name

union

SELECT p.hin_no,
UPPER((p.p_first_name||' '||coalesce(p.p_middle_name,' ')||' '||coalesce(p.p_last_name,' '))) as pname,
last_chg_date, final_bill_no, 'Final Bill' as type, sum(coalesce(round_off,0)) as amount
FROM bl_final_bill_details b,patient p where p.hin_id = b.hin_id
and last_chg_date  between $P{fromdate} and $P{todate}  and b.hospital_id=$P{hospitalId}
group by last_chg_date, final_bill_no, p.hin_no,p.p_first_name,
p.p_middle_name,p.p_last_name
) as main
where main.amount != 0 ]]></queryString>

