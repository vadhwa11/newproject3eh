		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select *, 
CASE WHEN net_amt  < 0  THEN CAST(ROUND((CASE WHEN net_amt is null THEN 0.0
ELSE net_amt END + CASE WHEN settled_amt is null  THEN 0.0
ELSE settled_amt END),0) AS INTEGER) ELSE
CAST(ROUND((CASE WHEN net_amt  is null  THEN 0.0 ELSE net_amt END - 
CASE WHEN settled_amt is null THEN 0.0 ELSE settled_amt END),0) AS INTEGER)
END as pendingamount
from
(select * from
(select final_bill_details_id, hin_id as hd, inpatient_id, gross_amt, discount_amt, net_amt,
hospital_id, settled_amt, final_bill_date
from bl_final_bill_details) as frst
inner join
(select ip.ad_no, ip.hin_no, ip.hin_id, discharge_date, b.bed_no,
UPPER((p.p_first_name||' '||coalesce(p.p_middle_name,' ')||' '||coalesce(p.p_last_name,' '))) as patientname
from inpatient ip
inner join patient p on p.hin_id = ip.hin_id
left outer join mas_bed b on ip.bed_id=b.bed_id) as snd
on frst.hd=snd.hin_id) as fnaldata
left join (select hin_id as hin,total_refund_amt,final_bill_id from bl_refund_header) as fdata
on fnaldata.final_bill_details_id = fdata.final_bill_id
where fnaldata.final_bill_date between $P{fromDate} and $P{toDate} and hospital_id=$P{hospitalId}]]></queryString>

