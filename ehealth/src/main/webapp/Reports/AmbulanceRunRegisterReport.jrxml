	<queryString><![CDATA[select
     ambulance_run_register.ambulance_run_date as run_date,
 ambulance_run_register.ambulance_run_time as time,
     ambulance_run_register.from_smc as source,
     ambulance_run_register.to_destination as destination,
      ambulance_run_register.patient_name as patient_name,
       patient.hin_no as hin_no,
      hosp.hospital_name as hospital_name,
    ambulance_run_register.charge as charge,
     ambulance_run_register.distance as rate,
 ambulance_run_register.driver_name as driver,
      masAmbulance.ambulance_no as ambulance_no

from
     ambulance_register ambulance_run_register left join patient patient on ambulance_run_register.hin_id = patient.hin_id
     left join mas_ambulance masAmbulance on masAmbulance.ambulance_id=ambulance_run_register.ambulance_id
     left join mas_hospital hosp on hosp.hospital_id = ambulance_run_register.hospital_id
where ambulance_run_date between $P{fromDate} and $P{toDate}
and ambulance_run_register.hospital_id=$P{hospitalId}]]></queryString>

