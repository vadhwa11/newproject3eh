	<queryString><![CDATA[select a.op_balance_dr,d.cheque_amt,h.account_id as account_id,me.first_name,mh.hospital_name,a.acc_desc
,case when d.cheque_no!='Interest given' then  d.cheque_no else '' end as cheque_no,d.cheque_date,
case when d.cheque_no='Interest given' then  d.cheque_no else '' end as narration
 from fa_bank_reconciliation_details  d 
left outer join  fa_bank_reconciliation_header h 
on h.bank_reconciliation_header_id=d.bank_reconciliation_header_id
left outer join fa_mas_account a on a.acc_id=h.account_id
left outer join mas_hospital mh on mh.hospital_id=h.hospital_id
--left outer join account_main_transac amt on amt.account_id=h.account_id
left outer join users u on u.user_id=h.last_chg_by
left outer join mas_employee me on me.employee_id=u.employee_id
  --WHERE cheque_status in('1','2')
  where h.hospital_id=$P{hospitalId} and d.cheque_status in('1','2') 
and h.reconciliation_date between $P{fromDate} and $P{toDate}]]></queryString>

