	<queryString>
		<![CDATA[select main.* ,if(main.actual_collected_amt is null,0,main.actual_collected_amt) as camount,
if(h.DrugNo1=null,"",concat("DL No.          ",DrugNo1)) as drug1,
if(h.DrugNo2=null,"",concat("               ",DrugNo2)) as drug2,
if(h.TIN=null    ,"",concat(" TIN/ST NO.  ",TIN)) as TIN,
if(if(main.actual_collected_amt is null,0,main.actual_collected_amt-main.net_amt)<0,0,if(main.actual_collected_amt is null,0,main.actual_collected_amt-main.net_amt)) as returnamt,
sibs.expiry_date,sibs.batch_no,sibs.dispencing_price,sibs.cost_price,sibs.expiry_date from
(select tbl1.* from
(select b.dispensing_header_id,if(b.discount_amt is null,0.00,b.discount_amt) as discount_amt,b.hin_id,UPPER(b.patient_name),UPPER(next_of_kin_name),b.age,b.hin_no,b.bill_no,b.bill_amt,b.round_off,b.net_amt,
b.payable_amt,UPPER(b.consultant_name) as consultant_name ,
hospital_name  as hospitalname,
mh.address  as hospitaladdress
,b.bill_date,b.bill_time,b.adv_adjustment,
nomenclature as itemname,bd.batch_id,bd.qty,bd.item_id,
bd.amount as tamt,bd.net_amt as tamount,ifnull(b.actual_collected_amt,0) as actual_collected_amt,
(case  when (pt.patient_type_code ='COM') then 'Company' when (pt.patient_type_code ='PRJ') then 'Project' else patient_type_name end) as comlabel,
(case  when (pt.patient_type_code ='COM' or pt.patient_type_code = 'PRJ') then b.outstanding else '' end) as comaccamt,
(case  when (pt.patient_type_code ='COM' or pt.patient_type_code = 'PRJ') then 'Company Account' else '' end) as comacc,
if(b.discount_amt is null,'',(case  when (pt.patient_type_code ='STF' or pt.patient_type_code = 'DEP') then 'Staff Health Scheme' else 'Charity2' end))
      as dt,if(b.company_id is null,'',mc.company_name) as com,
registration_type,if(b.inpatient_id is null,'Out Patient Bill','In Patient Bill') as type,
u.user_name,ifnull(i1.ad_no,"--") as ad_no,a1.administrative_sex_name,b.hospital_id
 from (select * from bl_dispensing_header where bill_no = $P{billNo} ) b inner  join
bl_dispensing_details bd on b.dispensing_header_id=bd.dispensing_header_id
inner join patient p on b.hin_id= p.hin_id
left outer  join inpatient i1 on b.inpatient_id=i1.inpatient_id
inner join mas_patient_type pt on b.patient_type_id = pt.patient_type_id
left outer join mas_company mc on b.company_id = mc.company_id
left join mas_hospital mh on b.hospital_id = mh.hospital_id
left  join mas_administrative_sex a1 on p.sex_id = a1.administrative_sex_id
left join mas_store_item ms on bd.item_id = ms.item_id
left  join users u on b.changed_by=u.user_id
) as tbl1 )
as main
inner join hospitalparameters h on main.hospital_id=h.hospital_id
inner join store_item_batch_stock sibs on main.batch_id = sibs.stock_id
and  sibs.item_id = main.item_id]]>
	</queryString>
