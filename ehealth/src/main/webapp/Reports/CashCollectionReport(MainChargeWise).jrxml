	</parameter>
	<queryString><![CDATA[select main.hospital_name,mcc.main_chargecode_name,sum(main.amt) as total_amt,sum(main.unit) as total_unit ,main.mccid from 
(select bd.bill_date as chg_slp_date,bh.hospital_id as hid,amount as amt,
quantity as unit,
mc.charge_code_name
as subchargecodename,bd.charge_code_id,mc.main_chargecode_id as mccid,
mh.hospital_name
from
bl_op_bill_details bd left outer join bl_op_bill_header bh on
bd.op_bill_header_id = bh.op_bill_header_id
left join mas_charge_code mc on bd.charge_code_id=mc.charge_code_id
left join mas_hospital mh on bh.hospital_id = mh.hospital_id
where bd.charge_code_id is not null and bh.hospital_id is not null
and bd.bill_date between $P{fromDate} and $P{toDate}
union all
select bm.chg_slp_date,bm.hospital_id  as hid,bd.amt as amt,bd.quantity as unit,
mc.charge_code_name
  as subchargecodename,bd.charge_code_id,mc.main_chargecode_id as mccid,
mh.hospital_name
 from bl_charge_slip_main bm inner join bl_charge_slip_detail bd
on bm.charge_slip_main_id = bd.charge_slip_main_id
left join mas_charge_code mc on bd.charge_code_id=mc.charge_code_id
left join mas_hospital mh on bm.hospital_id = mh.hospital_id
where bm.chg_slp_date between $P{fromDate} and $P{toDate}) as main

left join mas_main_chargecode mcc on main.mccid = mcc.main_chargecode_id
group by main.mccid,mcc.main_chargecode_name,main.hospital_name order by mcc.main_chargecode_name
;]]></queryString>

