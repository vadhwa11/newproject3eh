	<queryString><![CDATA[select frst.*, scnd.nomenclature from
(select cast(EXTRACT(year From bill_date) as varchar) as billyear, item_id,
sum(qty) as qty ,sum(amount) as amount,
(cast(sum(cast(qty as float)) AS varchar(10)) || '-' ||
cast(sum(cast(amount as float)) AS varchar(10))) as totalamt
from bl_dispensing_details bd left outer join
bl_dispensing_header bh on bd.dispensing_header_id = bh.dispensing_header_id
where EXTRACT(year From bill_date) between $P{fyear} and $P{tyear} and bh.hospital_id=$P{hospitalId}
group by EXTRACT(year From bill_date), item_id) as frst
inner join
(select item_id, nomenclature, mic.item_category_id as itemcatid 
from mas_store_item st left join mas_item_category mic on
st.item_category_id = mic.item_category_id) as scnd
on frst.item_id = scnd.item_id
$P!{criteria}

union

select 'total' as billyear, frst.item_id, frst.qty, frst.amount,
(cast(sum(cast(qty as float)) AS varchar(10)) || '-' ||
cast(sum(cast(amount as float)) AS varchar(10))) as totalamt, scnd.nomenclature
from
(select EXTRACT(year From bill_date) as billyear, item_id, sum(qty) as qty,
sum(amount) as amount,
(cast(sum(cast(qty as float )) AS varchar(10)) || '-' ||
cast(sum(cast(amount as float)) AS varchar(10))) as totalamt
from bl_dispensing_details bd left outer join
bl_dispensing_header bh on bd.dispensing_header_id = bh.dispensing_header_id
where EXTRACT(year From  bill_date) between $P{fyear} and $P{tyear} and bh.hospital_id=$P{hospitalId}
group by EXTRACT(year From  bill_date), item_id) as frst
inner join
(select item_id,nomenclature,mic.item_category_id as itemcatid
from mas_store_item st left join mas_item_category mic on st.item_category_id = mic.item_category_id) as scnd
on frst.item_id = scnd.item_id
$P!{criteria}
group by scnd.nomenclature,frst.item_id,frst.qty,frst.amount]]></queryString>

