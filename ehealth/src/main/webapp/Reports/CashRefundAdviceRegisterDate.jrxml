	<queryString><![CDATA[select
p.hin_no,
UPPER((p_first_name||' '||coalesce(p_middle_name,' ')||' '||coalesce(p_last_name,' '))) as pname,
bd.last_chg_date,
coalesce(bd.person_collected_amt,'Final Settlement') as person_collected_amt,
bd.refund_no,
bd.refund_date,
u.user_name,
bd.total_refund_amt,
bd.last_chg_by,
(case when bd.inpatient_id is null then 'OP' else 'IP' end) as IPOP,
(case when bd.final_bill_id is not null then 'Final Bill' else
(case when bd.pymnt_adv_serv_id is not null then 'Servicing' else
(case when bd.pymnt_adv_disp_id is not null then 'Dispensing' else '' end) end)  end)
as byType,
(select hospital_name from mas_hospital where hospital_id =bd.hospital_id) as hospitalname
from bl_refund_header bd
left outer join patient p on bd.hin_id = p.hin_id
left outer join users u on bd.last_chg_by=u.user_id
where bd.refund_date between $P{fromDate} and $P{toDate} and bd.hospital_id=$P{hospitalId}]]></queryString>

