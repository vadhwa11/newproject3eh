	<queryString><![CDATA[select voucher_no,voucher_date,cheque_no,h.narration,coalesce(d.dr_amount,'0.00')as dr,coalesce(d.cr_amount,'0.00') as cr,
a.acc_desc as cr_acc,h.issue_date,a1.acc_desc as dr_acc,mh.hospital_name,s.sub_led_desc
 from fa_voucher_details d
left outer join fa_voucher_header h on h.voucher_header_id=d.voucher_header_id
left outer join fa_mas_account a on a.acc_id=d.account_id
left outer join fa_mas_account a1 on a1.acc_id=d.account_id_for_report
left outer join fa_mas_sub_led s on s.sub_led_id=d.sub_led_id
left outer join mas_hospital mh on mh.hospital_id=h.hospital_id
where h.cheque_no is not null and h.voucher_date between $P{fromDate} and $P{toDate} and h.hospital_id=$P{hospitalId}]]></queryString>

