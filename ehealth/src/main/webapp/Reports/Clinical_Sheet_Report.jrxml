	<queryString><![CDATA[select 
h.hospital_name,
p.age,
ip.ad_no,
ip.date_of_addmission,
ip.discharge_date,
p.p_first_name as pa,
mas.administrative_sex_name as gender,
ic.icd_name as diagnosis_name
from ipd_intake_output_chart a

inner join mas_hospital h on a.hospital_id=h.hospital_id 
left outer join inpatient ip on ip.hin_id=a.hin_id 
left outer join patient p on p.hin_id=ip.hin_id
left outer join mas_administrative_sex mas on mas.administrative_sex_id=p.sex_id
left outer join mas_icd ic on ic.icd_id=ip.diagnosis_id
where a.ad_no=$P{ad_no}
and a.hospital_id=$P{hospitalId}]]></queryString>

