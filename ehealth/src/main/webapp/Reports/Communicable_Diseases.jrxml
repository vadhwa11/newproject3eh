	<queryString><![CDATA[SELECT
     ms."name",
     dr."age",
     ms."gender",
     ax."administrative_sex_name",
     ml."locality_name",
     hs."lsg_house_no",
     dr."date_onset",
     dr."admission_date",
     dr."disease_status",
     dr."medicine_systam",
     dr."hospital_name"
FROM
     "ph_disease_registration" dr LEFT OUTER JOIN "ph_member_survey" ms ON dr."member_id" = ms."member_id"
     LEFT OUTER JOIN "ph_house_survey" hs ON ms."house_id" = hs."house_hold_id"
     LEFT OUTER JOIN "mas_administrative_sex" ax ON ms."gender" = ax."administrative_sex_id"
     LEFT OUTER JOIN "ph_mas_locality" ml ON hs."locality_id" = ml."locality_id"
     LEFT OUTER JOIN "mas_hospital" ho ON dr."hospital_id" = ho."hospital_id"
WHERE
     dr."disease_type" = 'cd'
     and  dr."reg_date" between $P{fromDate} and $P{toDate} $P!{query}]]></queryString>

