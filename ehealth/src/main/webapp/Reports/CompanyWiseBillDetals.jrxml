	<queryString><![CDATA[select 1 as seq,p.hin_no,opb.company_id,'Out Patient Servicing' as type,case when opb.status='n' then 'Cancel' else ' ' end as status,
(p.p_first_name||' '||coalesce(p.p_middle_name,'')||' '||coalesce(p.p_last_name,'')) as pname,
opb.bill_no,coalesce(opb.net_amt,0) as net_amt,opb.bill_date,coalesce(opb.discount_amt,0) as discount_amt,coalesce(opb.payable_amt,0) as payable_amt,coalesce(opb.outstanding,0) as outstanding,
opb.op_bill_header_id,opd.charge_code_id,mcd.charge_code_name,opd.quantity,coalesce(opd.net_amt,0) as opd_net_amt,
coalesce(opd.discount_amt, 0) as opd_discopunt_amt,com.company_name,m.hospital_name,m.address
from patient p
right join bl_op_bill_header opb on opb.hin_id=p.hin_id
left outer join bl_op_bill_details opd on opb.op_bill_header_id=opd.op_bill_header_id
left outer join mas_charge_code mcd on opd.charge_code_id=mcd.charge_code_id
left join mas_company com on  opb.company_id=com.company_id
left join mas_hospital m on opb.hospital_id=m.hospital_id
where opb.company_id is not null
and opb.bill_date between $P{fromBillDate} and $P{toBillDate}
and opb.hin_no=(case when $P{hin_no}='' then p.hin_no else $P{hin_no} end )
and opb.company_id=(case when $P{company_id}=0 then opb.company_id else $P{company_id} end )
and opb.patient_type_id=1

union
select 2 as seq,p.hin_no,csm.company_id,'In Patient Servicing' as type,case when csm.status='n' then 'Cancel' else ' ' end  as status,
(p.p_first_name||' '||coalesce(p.p_middle_name,'')||' '||coalesce(p.p_last_name,''))as pname,
cast(csm.charge_slip_no as varchar) ,coalesce(csm.net_amt,0) as net_amt,csm.chg_slp_date ,coalesce(csm.discount_amt,0) as discount_amt,coalesce(csm.receipt_amt,0) as payable_amt,coalesce(csm.os_amt,0) as outstanding,
csm.charge_slip_main_id as op_bill_header_id,csd.charge_code_id,mcd.charge_code_name,csd.quantity,
csd.net_amt as opd_net_amt,csd.discount_amt,com.company_name,m.hospital_name,m.address
from patient p
right join bl_charge_slip_main csm on csm.hin_id =p.hin_id
left outer join bl_charge_slip_detail csd on csm.charge_slip_main_id=csd.charge_slip_main_id
left outer join mas_charge_code mcd on csd.charge_code_id=mcd.charge_code_id
left join mas_company com on  csm.company_id=com.company_id
left join mas_hospital m on csm.hospital_id=m.hospital_id
where csm.company_id is not null 
and csm.chg_slp_date between $P{fromBillDate} and $P{toBillDate}
and p.hin_no=(case when $P{hin_no}='' then p.hin_no else $P{hin_no} end )
and csm.company_id=(case when $P{company_id}=0 then csm.company_id else $P{company_id} end)
and csm.patient_type_id=1

union all
select 3 as seq,p.hin_no,bdh.company_id,
(case when bdh.inpatient_id is null then '0' else 'Out Patient Dispensing' end ) as type,(case when bdh.status='n' then 'Cancel' else '' end ) as status,
(p.p_first_name||' '||coalesce(p.p_middle_name,'')||' '||coalesce(p.p_last_name,'')) as pname,
bdh.bill_no,coalesce(bdh.net_amt,0) as net_amt,bdh.bill_date,coalesce(bdh.discount_amt,0) as discount_amt,coalesce(bdh.payable_amt,0) as payable_amt,coalesce(bdh.outstanding,0) as outstanding,
bdh.dispensing_header_id,msi.item_id,msi.nomenclature,bdd.qty as quantity,
(case when bdd.net_amt is null then 0 else bdd.net_amt end ) as bdd_bill_amt,
(case when bdd.discount_amt is null then 0 else bdd.discount_amt end ) as discopunt_amt,
com.company_name,m.hospital_name,m.address
from patient p
right join bl_dispensing_header bdh on bdh.hin_id=p.hin_id
left outer join bl_dispensing_details bdd on bdh.dispensing_header_id=bdd.dispensing_header_id
left outer join mas_store_item msi on bdd.item_id=msi.item_id
left join mas_company com on  bdh.company_id=com.company_id
left join mas_hospital m on bdh.hospital_id=m.hospital_id
where bdh.company_id is not null
and bdh.bill_date between $P{fromBillDate} and $P{toBillDate}
and bdh.hin_no=(case when $P{hin_no}='' then bdh.hin_no else $P{hin_no} end)
and bdh.company_id=(case when $P{company_id}=0 then bdh.company_id else $P{company_id} end)
and bdh.patient_type_id=1

union all
select 4 as seq,p.hin_no,bdh.company_id,
(case when bph.inpatient_id is null then '0' else 'Out Patient Advice Dispensing' end ) as type,(case when bdh.status='n' then 'Cancel' else '' end ) as status,
(p.p_first_name||' '||coalesce(p.p_middle_name,'')||' '||coalesce(p.p_last_name,'')) as pname,
bph.payment_advice_no,coalesce(bph.total_advice_amt,0) as net_amt,bph.payment_advice_date,coalesce(bph.total_advice_charity_amt,0) as discount_amt,coalesce(bph.cash_advice_amt,0) as payable_amt,coalesce(bph.on_account_amt,0) as outstanding,
bph.pymnt_advice_disp_header_id,msi.item_id,msi.nomenclature,bpd.advice_qty as quantity,
(case when bpd.advice_amt is null then 0 else bpd.advice_amt end ) as bdd_bill_amt,
(case when bpd.advice_charity_amt is null then 0 else bpd.advice_charity_amt end ) as discopunt_amt,
com.company_name,m.hospital_name,m.address
from patient p
right join bl_pymnt_advice_disp_header bph on bph.hin_id=p.hin_id
left outer join bl_pymnt_advice_disp_details bpd
on bph.pymnt_advice_disp_header_id = bpd.pymnt_advice_disp_header_id
left  join bl_dispensing_header bdh on bph.bill_dispensing_id=bdh.dispensing_header_id
and bph.hin_id=bdh.hin_id
left outer join mas_store_item msi on bpd.item_id=msi.item_id
left join mas_company com on  bdh.company_id=com.company_id
left join mas_hospital m on bdh.hospital_id=m.hospital_id
where bdh.company_id is not null
and bph.payment_advice_date between  $P{fromBillDate} and $P{toBillDate}
and bdh.patient_type_id=1
and bdh.hin_no=(case when $P{hin_no}='' then bdh.hin_no else $P{hin_no} end)
and bdh.company_id=(case when $P{company_id}=0 then bdh.company_id else $P{company_id} end)

union all
select 5 as seq,p.hin_no,opb.company_id,'Out Patient Advice Servicing' as type,
