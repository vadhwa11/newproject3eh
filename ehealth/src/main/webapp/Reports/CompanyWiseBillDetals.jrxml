(p.p_first_name||' '||coalesce(p.p_middle_name,'')||' '||coalesce(p.p_last_name,'')) as pname,
bph.payment_advice_no,coalesce(bph.total_advice_amt,0) as net_amt,
bph.payment_advice_date,coalesce(bph.total_advice_charity_amt,0) as discount_amt,
coalesce(bph.cash_advice_amt,0) as payable_amt,coalesce(bph.on_account_amt,0) as outstanding,
bph.payment_advice_header_id,bpd.charge_id,mcd.charge_code_name,bpd.advice_qty,
coalesce(bpd.advice_amt ,0) as opd_net_amt,coalesce(bpd.advice_charity_amt ,0) as opd_discopunt_amt,com.company_name,m.hospital_name,m.address
from patient p
right join bl_payment_advice_header bph on bph.hin_id=p.hin_id
inner join bl_op_bill_header opb on bph.bill_id=opb.op_bill_header_id
inner join bl_payment_advice_details bpd
on bph.payment_advice_header_id= bpd.payment_advice_header_id
inner join mas_charge_code mcd on bpd.charge_id=mcd.charge_code_id
inner join mas_company com on  opb.company_id=com.company_id
inner join mas_hospital m on opb.hospital_id=m.hospital_id
where opb.company_id is not null
and bph.payment_advice_date between $P{fromBillDate} and $P{toBillDate}
and p.hin_no=(case when $P{hin_no}='' then p.hin_no else $P{hin_no} end)
and opb.company_id=(case when $P{company_id}=0 then opb.company_id else $P{company_id} end)
and opb.patient_type_id=1

union all
select 6 as seq,p.hin_no,csm.company_id,'In Patient Advice Servicing' as type,(case when csm.status='n' then 'Cancel' else '' end ) as status,
(p.p_first_name||' '||coalesce(p.p_middle_name,'0')||' '||coalesce(p.p_last_name,'0'))as pname,
cast(csm.charge_slip_no as varchar) ,sum(coalesce(csd.net_amt,0)) as net_amt,csm.chg_slp_date ,
sum(coalesce(csd.discount_amt,0)) as discount_amt,0 as payable_amt,sum(coalesce(csd.net_amt,0)) as outstanding,
csm.charge_slip_main_id as op_bill_header_id,csd.charge_code_id,mcd.charge_code_name,csd.quantity,csd.net_amt as opd_net_amt,
csd.discount_amt,com.company_name,m.hospital_name,m.address

from patient p
right join bl_charge_slip_main csm on csm.hin_id =p.hin_id
left outer join bl_charge_slip_detail csd on csm.charge_slip_main_id=csd.charge_slip_main_id
left outer join mas_charge_code mcd on csd.charge_code_id=mcd.charge_code_id
left join mas_company com on  csm.company_id=com.company_id
left join mas_hospital m on csm.hospital_id=m.hospital_id
where csm.company_id is not null
and csm.chg_slp_date between $P{fromBillDate} and $P{toBillDate}
and p.hin_no=(case when $P{hin_no}='' then p.hin_no else $P{hin_no} end)
and csm.company_id=(case when $P{company_id}=0 then csm.company_id else $P{company_id} end)
and csd.status='n' and csm.patient_type_id=1
group by csd.charge_slip_main_id,p.hin_no,csm.company_id,
csm.status,p.p_first_name,p.p_middle_name,p.p_last_name,csm.charge_slip_no,csm.chg_slp_date,
csm.charge_slip_main_id,csd.charge_code_id,mcd.charge_code_name, csd.quantity,csd.net_amt,csd.discount_amt,
com.company_name,m.hospital_name,m.address
union all
select 5 as seq,p.hin_no,opb.company_id,'In Patient Advice Servicing' as type,
case when opb.status='n' then 'Cancel' else '' end  as status,
(p.p_first_name||' '||coalesce(p.p_middle_name,'0')||' '||coalesce(p.p_last_name,'0')) as pname,
bph.payment_advice_no,coalesce(bph.total_advice_amt,0) as net_amt,
bph.payment_advice_date,coalesce(bph.total_advice_charity_amt,'0') as discount_amt,
coalesce(bph.cash_advice_amt,0) as payable_amt,coalesce(bph.on_account_amt,0) as outstanding,
bph.payment_advice_header_id,bpd.charge_id,mcd.charge_code_name,bpd.advice_qty,
coalesce(bpd.advice_amt,0) as opd_net_amt,coalesce(bpd.advice_charity_amt,0) as opd_discopunt_amt,
com.company_name,m.hospital_name,m.address
from patient p
right join bl_payment_advice_header bph on bph.hin_id=p.hin_id
inner join bl_charge_slip_main opb on bph.charge_slip_main_id=opb.charge_slip_main_id
inner join bl_payment_advice_details bpd
on bph.payment_advice_header_id= bpd.payment_advice_header_id
inner join mas_charge_code mcd on bpd.charge_id=mcd.charge_code_id
inner join mas_company com on  opb.company_id=com.company_id
inner join mas_hospital m on opb.hospital_id=m.hospital_id
where opb.company_id is not null
and bph.payment_advice_date between $P{fromBillDate} and $P{toBillDate}
and p.hin_no=(case when $P{hin_no}='' then p.hin_no else $P{hin_no} end )
and opb.inpatient_id is not null
and opb.company_id=(case when $P{company_id}=0 then opb.company_id else $P{company_id} end)
and opb.patient_type_id=1
]]></queryString>

