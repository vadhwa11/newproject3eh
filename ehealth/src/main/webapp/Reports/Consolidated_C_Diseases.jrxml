	<queryString><![CDATA[SELECT
     count(dr.disease_type)as confirmed,
     count(dr.disease_type)as ,
     ( SELECT count(*) FROM "ph_birth_death_reg" WHERE cause_of_death = 'Communicable diseases' ),
     ,as death_dec
FROM
     "ph_disease_registration" dr LEFT OUTER JOIN "ph_member_survey" ms ON dr."member_id" = ms."member_id"
     RIGHT OUTER JOIN "mas_hospital" ho ON dr."hospital_id" = ho."hospital_id"
     RIGHT OUTER JOIN "dbr" dbr ON dr."member_id" = dbr."member_id"
WHERE
     dr.disease_type = 'cd']]></queryString>

