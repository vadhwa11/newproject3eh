	<queryString><![CDATA[select
     sum(store_op_patient_issue_t.`qty_issued`)as store_op_patient_issue_t_qty_issued,
     mas_store_item.`nomenclature` as mas_store_item_nomenclature,
     mas_store_item.`pvms_no` as mas_store_item_pvms_no,
     mas_store_item_generic.`generic_name` as mas_store_item_generic_generic_name
from
     `store_op_patient_issue_m` store_op_patient_issue_m inner join `store_op_patient_issue_t` store_op_patient_issue_t on store_op_patient_issue_m.`op_issue_id` = store_op_patient_issue_t.`op_issue_id`
     inner join `mas_store_item` mas_store_item on store_op_patient_issue_t.`item_id_issue` = mas_store_item.`item_id`
     inner join `mas_store_item_generic` mas_store_item_generic on mas_store_item.`item_generic_id` = mas_store_item_generic.`item_generic_id`


where
 store_op_patient_issue_m.`issue_date` between $P{FROM_DATE}and $P{TO_DATE}
  and store_op_patient_issue_m.`department_id` = $P{DEPART}
  and mas_store_item_generic.`item_generic_id` = $P{GENERIC}



group by   mas_store_item.`item_id`]]></queryString>

