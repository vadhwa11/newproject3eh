	<queryString><![CDATA[select vh.voucher_no, 
vh.voucher_date,  
vh.voucher_type, 
vh.narration, 
vh.f_year_id, 
vh.hospital_id,
fma.acc_desc,
fmsl.sub_led_desc,
vd.dr_amount, 
vd.cr_amount, 
mh.hospital_name as hospital_name, 
mh.address as hospital_address 

from fa_voucher_details vd

inner join fa_voucher_header `vh` on vd.voucher_header_id=vh.voucher_header_id
inner join mas_hospital `mh` on mh.hospital_id=vh.hospital_id
left outer join fa_mas_sub_led `fmsl` on vd.sub_led_id=fmsl.sub_led_id
left outer join fa_mas_account `fma` on vd.account_id=fma.acc_id
where vh.hospital_id=$P{hospitalId} and vh.voucher_date=$P{select_date}]]></queryString>

