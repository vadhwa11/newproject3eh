	<queryString><![CDATA[select p.ad_no, main.inpatient_id, final_bill_no, gross_amt, final_bill_date,
p.department_id, main.hin_id,
(select department_name from mas_department where department_id = p.department_id) as departmentname,
(select hospital_name from mas_hospital m where m.hospital_id = p.hospital_id) as hname
from
(
select hin_id, inpatient_id, final_bill_no, gross_amt, final_bill_date
from bl_final_bill_details where final_bill_date between $P{fromdate} and $P{todate} and bl_final_bill_details.hospital_id=$P{hospitalId}) as main
left outer join inpatient p  on main.inpatient_id = p.inpatient_id
where department_id != 0 and department_id =
case when $P{department_id}=0 then department_id else $P{department_id} end
order by departmentname,final_bill_no,final_bill_date]]></queryString>

