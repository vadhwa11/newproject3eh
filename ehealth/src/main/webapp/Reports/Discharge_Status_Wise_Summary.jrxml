	<queryString><![CDATA[select count(discharge_id) as discharge,date_of_discharge,department_name ,
ds.discharge_status_name,mh.hospital_name as hospital
from discharge d
left outer join mas_discharge_status ds on d.discharge_status_id = ds.discharge_status_id
left outer join mas_department mas on  d.ward_id =mas.department_id
left outer join mas_hospital mh on d.hospital_id = mh.hospital_id

where d.date_of_discharge between $P{from_date} and $P{to_date}
and  d.ward_id = case when $P{ward_id}=0 then d.ward_id else $P{ward_id} end
and  d.discharge_status_id = case when $P{status}=0 then d.discharge_status_id else $P{status} end
and d.hospital_id=$P{hospitalId}
group by date_of_discharge,
department_name,
ds.discharge_status_name,
mh.hospital_name

order by date_of_discharge,
mas.department_name]]></queryString>

