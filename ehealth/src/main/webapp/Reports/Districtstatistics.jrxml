	<queryString><![CDATA[select mv.village_name,
md.district_name as district,
count(p.hin_id) as patient_no 
from patient p
left outer join patient_address pa ON p.hin_id = pa.hin_id
left outer join mas_village mv on mv.village_id = pa.village
left outer join mas_taluk mt on mt.taluk_id = mv.taluk_id
left outer join mas_district md on md.district_id = mt.district_id
where p.hospital_id=$P{HOSPITAL_ID} and p.reg_date between  $P{fromDate} and $P{toDate}
group by md.district_name,mv.village_name
order by district_name,village_name]]></queryString>

