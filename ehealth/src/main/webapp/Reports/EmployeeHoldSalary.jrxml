	<queryString><![CDATA[select
     mas_employee.employee_id as mas_employee_employee_id,
     (mas_employee.first_name+' '+ISNULL(mas_employee.middle_name,'')+' '+ISNULL(mas_employee.last_name,''))as ename,
     mas_employee.join_date as mas_employee_join_date,
     mas_employee.employee_code as mas_employee_employee_code,
     hr_mas_location.location_name,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
     hr_payroll_process_header.year as hr_payroll_process_header_year,
     hr_payroll_process_header.month as hr_payroll_process_header_month,
     hr_payroll_process_header.net_salary as hr_payroll_process_header_net_salary,
     mas_emp_status.emp_status_name as mas_emp_status_emp_status_name
from
     mas_hospital mas_hospital inner join hr_mas_location hr_mas_location on mas_hospital.hospital_id = hr_mas_location.company_id
     inner join mas_employee mas_employee on mas_hospital.hospital_id = mas_employee.hospital_id
     inner join hr_payroll_process_header hr_payroll_process_header on mas_hospital.hospital_id = hr_payroll_process_header.hospital_id
     and mas_employee.employee_id = hr_payroll_process_header.employee_id
     inner join mas_emp_status mas_emp_status on mas_employee.employee_status_id = mas_emp_status.emp_status_id
where
     hr_payroll_process_header.year = $P{year}
     and hr_payroll_process_header.month = $P{month}]]></queryString>

