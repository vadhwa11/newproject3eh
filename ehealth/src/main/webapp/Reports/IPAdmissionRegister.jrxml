	<queryString>
		<![CDATA[select Distinct on (inpatient.inpatient_id)
     inpatient.inpatient_id,inpatient.ad_no as inpatient_ad_no,inpatient.dependent_name as dependent_Name,
	 patient.hin_no as Reg_no,
     masrelation.relation_name as relation_name,
     inpatient.date_of_addmission as inpatient_date_of_addmission,
     inpatient.diagnosis_id as inpatient_diagnosis_id,
     mas_department.department_name as mas_department_department_name,

     (coalesce(patient.p_first_name,'') || ' ' || coalesce(patient.p_middle_name,'') || ' ' || coalesce(patient.p_last_name,''))as patientname,
     (CASE WHEN EXTRACT(YEAR FROM (age(now(),date_of_birth))) != 0 THEN CONCAT(EXTRACT(YEAR FROM (age(now(),date_of_birth))),' Y')
WHEN EXTRACT(MONTH FROM (age(now(),date_of_birth))) != 0 THEN CONCAT(EXTRACT(MONTH FROM (age(now(),date_of_birth))),' M')
WHEN EXTRACT(DAY FROM (age(now(),date_of_birth))) != 0 THEN CONCAT(EXTRACT(DAY FROM (age(now(),date_of_birth))),' D') END) AS age,
     patient.sex_id as patient_sex_id,
     left(administrative_sex_name,1) as gender,
     mas_hospital.hospital_name as hospital_name,
    mas_hospital.address as hospital_address,
     mas_bed.bed_no as bedno,time_of_addmission,
      visit.total_hospital_visit as total_hospital_visit ,inpatient.address,ad_status

from inpatient inpatient

     left outer join  patient patient  on inpatient.hin_id=patient.hin_id
     left outer join visit visit on patient.hin_id = visit.hin_id

     left outer join mas_relation masrelation on patient.relation_id=masrelation.relation_id
     left outer join mas_administrative_sex mas_administrative_sex on patient.sex_id = mas_administrative_sex.administrative_sex_id
     left outer join mas_department mas_department on inpatient.department_id = mas_department.department_id
     left outer join mas_hospital mas_hospital on inpatient.hospital_id=mas_hospital.hospital_id
     left outer join mas_bed mas_bed on inpatient.bed_id=mas_bed.bed_id
     left outer join opd_patient_details opd on opd.id = inpatient.opd_patient_details_id
     left outer join visit vt on opd.visit_id = vt.visit_id

where
    inpatient.hospital_id = $P{HOSPITAL_ID} and (cast(inpatient.date_of_addmission as date) + cast(time_of_addmission as time))  between (cast($P{fromDate} as date) + cast($P{fromTime} as time)) and (cast($P{toDate} as date) + cast($P{toTime} as time))
$P!{stringVariable}

order  by  inpatient.inpatient_id,inpatient.date_of_addmission,time_of_addmission]]>
	</queryString>
