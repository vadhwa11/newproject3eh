	<queryString><![CDATA[select
     concat(p_first_name,p_middle_name,p_last_name)as pname,
     address,
     patient.`hin_no` as patient_hin_no,
     bl_receipt_header.`receipt_no` as bl_receipt_header_receipt_no,
     bl_receipt_header.`amount` as bl_receipt_header_amount,
