	<queryString><![CDATA[select distinct
patient_diet_indoor_detail.bed_id  as bed_id,mas_bed.bed_no,
(case  when (patient_diet_indoor_header.time ='M') then 'Morning' 
when (patient_diet_indoor_header.time ='E') then 'Evening' 
else '' end) as timetype,
patient.hin_id  as hin_id,
(patient.p_first_name+' '+ISNULL(patient.p_middle_name,' ')+' '+ISNULL(patient.p_last_name,' ')) as patient_name,
 mas_hospital.hospital_name as hospital_name,mas_hospital.address as address,
(mas_employee.first_name+' '+ISNULL(mas_employee.middle_name,' ')+' '+ISNULL(mas_employee.last_name,' ')) as employee_name,
mas_department.department_name as department_name,
mas_diet.diet_name as diet_name,
patient_diet_indoor_detail.morning_tea_bread as morning_tea_bread, patient_diet_indoor_detail.eggs as eggs,
patient_diet_indoor_detail.lunch as lunch,patient_diet_indoor_detail.evening_tea as evening_tea,patient_diet_indoor_detail.banana as banana,patient_diet_indoor_detail.dinner as dinner,
patient_diet_indoor_detail.remarks as remark
from  inpatient inpatient left outer join patient patient on inpatient.hin_id = patient.hin_id
left outer join mas_hospital mas_hospital on inpatient.hospital_id = mas_hospital.hospital_id
left outer join mas_employee mas_employee on inpatient.doctor_id = mas_employee.employee_id
left outer join patient_diet_indoor_detail patient_diet_indoor_detail on patient_diet_indoor_detail.inpatient_id = inpatient.inpatient_id
left outer join patient_diet_indoor_header patient_diet_indoor_header on patient_diet_indoor_detail.h_id = patient_diet_indoor_header.id
left outer join mas_bed on patient_diet_indoor_detail.bed_id=mas_bed.bed_id
left outer join mas_diet mas_diet on patient_diet_indoor_detail.diet_id = mas_diet.diet_id
left outer join mas_department mas_department on patient_diet_indoor_header.department_id = mas_department.department_id
$P!{query}]]></queryString>

