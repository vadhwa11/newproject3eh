	<queryString><![CDATA[select m.id,m.issue_no,m.issue_date,m.department_id,t.item_id,
(select pvms_no from mas_store_item where item_id=t.item_id) as itemcode,
(select nomenclature from mas_store_item where item_id=t.item_id) as itemname,
sum(t.qty_issued) as qty,t.expiry_date,sum(t.cost_price) as rate,sum((t.qty_issued*t.cost_price)) as totalamount
 from store_issue_m m
inner join store_issue_t t
on m.id = t.issue_m_id where t.issued='y' and m.issue_date between $P{fromDate}  and $P{toDate}  and m.department_id=$P{deptId}   and m.hospital_id=$P{hospitalId}
group by m.id,m.issue_no,m.issue_date,m.department_id,t.item_id,t.expiry_date;]]></queryString>

