	<queryString><![CDATA[select pvms_no,nomenclature,ss.section_name,it.group_name,mm.manufacturer_name,br.manufacturer_id,
ig.generic_name,ic.item_unit_name
from mas_store_item st left join mas_store_section ss on st.section_id = ss.section_id
left join mas_store_group it on st.group_id = it.group_id
left join mas_store_item_generic ig on st.item_generic_id = ig.item_generic_id
left join mas_store_item_conversion ic on st.item_conversion_id = ic.item_conversion_id
left join mas_store_brand br on st.item_id=br.item_id
left join mas_manufacturer mm on br.manufacturer_id = mm.manufacturer_id
where st.status ='y' $P!{query} 
and st.section_id= case when $P{sectionId}=0 then st.section_id else $P{sectionId} end 
and st.group_id=case when $P{groupId}=0 then st.group_id else $P{groupId} end
and br.manufacturer_id=case  when $P{manufactureId}=0 then br.manufacturer_id else $P{manufactureId} end
order by ss.section_name,mm.manufacturer_name,it.group_id,st.pvms_no,st.nomenclature]]></queryString>

