	<queryString><![CDATA[select h.voucher_header_id,h.voucher_no,h.voucher_date,
a.acc_desc,coalesce(d.dr_amount,null) as dr_voucher
,coalesce(d.cr_amount,null) as cr_voucher,coalesce(amt.cl_balance_dr,null) as dr_transac
,
case when amt.cl_balance_dr>0 then cast(round(amt.cl_balance_dr,0) as int)
when amt.cl_balance_cr>0 then cast(round(amt.cl_balance_cr,0) as int) else '0' end as final_amount
,mh.hospital_name,coalesce(d.dr_amount,null)-coalesce(d.cr_amount,null)
 from fa_voucher_details d 
left outer join fa_voucher_header h on h.voucher_header_id=d.voucher_header_id
left outer join fa_mas_account a on a.acc_id=d.account_id
left outer join account_main_transac amt on amt.account_id=d.account_id
left outer join mas_hospital mh on mh.hospital_id=h.hospital_id
where h.hospital_id=$P{hospitalId} and h.voucher_date between $P{fromDate} and $P{toDate}
order by h.voucher_no,h.voucher_date asc]]></queryString>

