	<queryString><![CDATA[select
     hr_loan_header.loan_date as hr_loan_header_loan_date,
     hr_loan_header.monthly_install as hr_loan_header_monthly_install,
     hr_loan_header.loan_interest as hr_loan_header_loan_interest,
     hr_loan_header.loan_p_amount as hr_loan_header_loan_p_amount,
     hr_loan_detail.install_amount as hr_loan_detail_install_amount,
     hr_mas_loan.loan_description as hr_mas_loan_loan_description,
     (mas_employee.first_name+' '+ISNULL(mas_employee.middle_name,'')+' '+ISNULL(mas_employee.last_name,''))as ename,
     hr_loan_header.loan_period as hr_loan_header_loan_period,
     mas_employee.employee_code as mas_employee_employee_code,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
     hr_mas_loan.hospital_id as hr_mas_loan_hospital_id,
     mas_employee.hospital_id as mas_employee_hospital_id
from
      hr_loan_detail hr_loan_detail left join hr_loan_header hr_loan_header on hr_loan_detail.loan_header_id = hr_loan_header.loan_header_id
      left join hr_mas_loan hr_mas_loan on hr_mas_loan.loan_id = hr_loan_header.loan_id left join
      mas_employee mas_employee on hr_loan_header.employee_id= mas_employee.employee_id  left join
      mas_hospital mas_hospital on mas_employee.hospital_id = mas_hospital.hospital_id
where
     mas_employee.employee_code  between $P{fromEmpCode}  and  $P{toEmpCode}]]></queryString>

