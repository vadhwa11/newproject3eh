	<queryString><![CDATA[SELECT
		     assembly.code AS assembly_code,
		     assembly.name AS assembly_name,
		       CASE WHEN assembly.status='Y'  or  assembly.status='y' THEN 'Active' else 'Inactive' end as status,
		     district.district_name AS district_name,
parliyament.name as p_name
		FROM
		     ph_mas_parliyament_assembly assembly
		    LEFT OUTER JOIN mas_district  district ON district.district_id = assembly.district_id
					    LEFT OUTER JOIN ph_mas_parliyament_assembly  parliyament ON assembly.parliyament_assembly_id = parliyament.parliyament_assembly_id
		where assembly.category='A' $P!{qry}
		ORDER BY
		     assembly.name ASC]]></queryString>

