     mas_store_brand.brand_code AS mas_store_brand_brand_code,
     mas_store_item.nomenclature AS mas_store_item_nomenclature,
 CASE     WHEN mas_store_brand.status = 'Y' or mas_store_brand.status = 'y' THEN 'Active'     ELSE 'Inactive'
  	 END  as  mas_store_brand_status
