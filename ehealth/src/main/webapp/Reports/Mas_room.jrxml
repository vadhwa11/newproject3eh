	<queryString><![CDATA[SELECT
     mas_room.room_code AS mas_room_room_code,
    CASE WHEN mas_room.status='Y' or mas_room.status='y' then 'Active' else 'Inactive' end AS mas_room_status,
     mas_room_type.room_type_name AS mas_room_type_room_type_name,
     mas_department.department_name AS mas_department_department_name
FROM
     mas_room_type mas_room_type RIGHT OUTER JOIN mas_room mas_room ON mas_room_type.room_type_id = mas_room.room_type_id
     LEFT OUTER JOIN mas_department mas_department ON mas_room.department_id = mas_department.department_id
