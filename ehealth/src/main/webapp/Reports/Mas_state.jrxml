	<queryString><![CDATA[SELECT
     mas_state.state_code AS mas_state_state_code,
     mas_state.state_name AS mas_state_state_name,
     mas_state.country_id AS mas_state_country_id,
  CASE WHEN mas_state.status='Y' OR mas_state.status='y' THEN 'Active' else 'Inactive' end as status,
     mas_country.country_name AS mas_country_country_name
FROM
     mas_country mas_country INNER JOIN mas_state mas_state ON mas_country.country_id = mas_state.country_id
$P!{qry}
ORDER BY
     mas_state.state_name]]></queryString>

