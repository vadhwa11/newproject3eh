	<queryString><![CDATA[SELECT
     mas_store_item_conversion.item_unit_name AS mas_store_item_conversion_item_unit_name,
     mas_store_item_conversion.conversion_factor1 AS mas_store_item_conversion_conversion_factor1,
     mas_store_item_conversion.conversion_factor2 AS mas_store_item_conversion_conversion_factor2,
     mas_store_item_conversion.item_conversion_id AS mas_store_item_conversion_item_conversion_id,
     mas_store_unit.unit_name AS mas_store_unit_unit_name,
 CASE
    WHEN  mas_store_item_conversion.status = 'y' or  mas_store_item_conversion.status = 'Y' THEN 'Active'
    ELSE 'Inactive'
 	 END as status
FROM
     mas_store_unit mas_store_unit INNER JOIN mas_store_item_conversion mas_store_item_conversion ON mas_store_unit.unit_id = mas_store_item_conversion.purchase_unit_id
     AND mas_store_unit.unit_id = mas_store_item_conversion.issue_unit_id
     AND mas_store_unit.unit_id = mas_store_item_conversion.intermediate_unit_id
$P!{qry}]]></queryString>

