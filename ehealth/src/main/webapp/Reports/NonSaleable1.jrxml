	<queryString><![CDATA[select  maindata.*,secondmain.*,sitem.nomenclature as itemname,
(select department_name from mas_department where
department_id=maindata.department_id) as departmentname,
(select department_name from mas_department where
department_id=maindata.to_store) as tostorename,
(maindata.qty_issued*secondmain.mrp) as totalmrp,
((maindata.qty_issued*maindata.amount)-(maindata.qty_issued*secondmain.mrp))
as margin,

(select  concat(p_first_name ,'', p_middle_name , '' , p_last_name)
as patientname from patient where hin_id=maindata.hin_id) as patientname from

(

select m.department_id,'' as to_store ,m.hin_id,t.batch_no,
t.item_id_issue as

item_id,t.qty_issued,t.amount,(t.qty_issued*t.amount) as totalcost from
store_op_patient_issue_t t inner join store_op_patient_issue_m m on
t.op_issue_id=m.op_issue_id
and m.issue_date between $P{fromDate}and $P{toDate}

union

select m.department_id ,'' as to_store
,m.hin_id,t.batch_no,t.item_id,t.qty_issued,t.amount,(t.qty_issued*t.amount)
as

totalcost from
store_ip_issue_t t inner join store_ip_issue_m m on t.ip_issue_id =
m.ip_issue_id
and m.ip_issue_date between $P{fromDate}and $P{toDate}

union

select m.department_id ,to_store ,'' as hin_id ,t.batch_no,
t.item_id,t.qty_issued,t.cost_price,(t.cost_price*t.qty_issued)

as totalcost
from store_issue_t t inner join store_issue_m m on t.issue_m_id = m.id
and m.issue_date between $P{fromDate}and $P{toDate}

) as maindata inner join

(select s.department_id,s.item_id,s.batch_no,s.mrp,s.dispencing_price
 from store_item_batch_stock s  ) as secondmain

inner join mas_store_item sitem on  sitem.item_id=secondmain.item_id

where maindata.department_id = secondmain.department_id and
maindata.department_id =$P{deptId}
and maindata.item_id = secondmain.item_id and maindata.batch_no =
secondmain.batch_no
and sitem.saleble =$P{salebaleType}

order by maindata.item_id
]]></queryString>

