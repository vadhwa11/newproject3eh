	<queryString><![CDATA[SELECT
	mdp.department_name,
        mh.hospital_name hospital_name,
     ich.ad_no AS ipdcaredetail_ad_no,     
	ptt.p_first_name AS patientName,
      mnc.nursing_name AS mas_nursing_care_nursing_name,
      ich.date_of_care as ipdcaredetail_date_of_care,
      icd.care_date ,
      icd.care_time as ipdcaredetail_care1_time,
mh.hospital_name as mas_hospital_hospital_name 
	
FROM ipd_care_header ich 
     LEFT OUTER JOIN ipd_care_details icd ON ich.care_header_id = icd.care_header_id
     LEFT OUTER JOIN patient ptt ON ich.hin_id=ptt.hin_id
     LEFT OUTER JOIN nursingcare_setup ncs ON ich.nursingcare_setup_id = ncs.nursingcare_setup_id
     LEFT OUTER JOIN mas_nursing_care mnc ON ncs.nursing_id = mnc.nursing_id
     LEFT OUTER JOIN mas_department mdp ON mdp.department_id=ich.department_id
     LEFT OUTER JOIN mas_hospital mh ON mh.hospital_id=ich.hospital_id
   
WHERE
     ptt.hin_no = $P{hinNo}
     and mdp.department_id = $P{Ward}
     and ich.hospital_id=$P{hospitalId}
     and ich.date_of_care between $P{fromDate}   and  $P{toDate}  
 $P!{qry}]]></queryString>

