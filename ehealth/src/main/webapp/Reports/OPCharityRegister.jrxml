	<queryString><![CDATA[select main.*,if(main.department is null,'other',main.department) as maindept from
(
select bd.hin_id,bd.hin_no,bd.patient_name,bd.bill_no,bd.bill_amt,if(bd.discount_amt is null,0,discount_amt) as discount_amt,bd.hospital_id,bd.bill_date,bd.visit_id,
'servicing' as billtype, null as inpatient,
(select visit_no from visit where visit_id=bd.visit_id) as visitno,
(select hospital_name from mas_hospital where hospital_id = bd.hospital_id) as hospital,
(select department_name from mas_Department where department_id =
(select department_id from visit where visit_id = bd.visit_id )) as department
 from bl_op_bill_header bd

union
select bd.hin_id,bd.hin_no,bd.patient_name,bd.bill_no,bd.bill_amt,if(bd.discount_amt is null,0,discount_amt) as discount_amt,bd.hospital_id,bd.bill_date,0 as visit_id,
'dispensing' as billtype,null as inpatient,
null as visitno,
(select hospital_name from mas_hospital where hospital_id = bd.hospital_id) as hospital,
(select department_name from mas_department where department_id =
(select department_id from inpatient where hin_id = bd.hin_id )) as department
from bl_dispensing_header bd where inpatient_id is  null)as main
where main.bill_date between $P{fromDate}and $P{toDate}and discount_amt > 0

order by main.department,main.bill_no,main.bill_date]]></queryString>

