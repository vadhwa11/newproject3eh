	<queryString>
		<![CDATA[select distinct on (visit.visit_id) visit.visit_id,patient.hin_no as patient_hin_no,(patient.p_first_name::text || ' ' || coalesce(patient.p_last_name,''))as patientname,
(CASE WHEN EXTRACT(YEAR FROM (age(visit.visit_date,patient.date_of_birth))) != 0 THEN CONCAT(EXTRACT(YEAR FROM (age(visit.visit_date,patient.date_of_birth))),' Y')
WHEN EXTRACT(MONTH FROM (age(visit.visit_date,patient.date_of_birth))) != 0 THEN CONCAT(EXTRACT(MONTH FROM (age(visit.visit_date,patient.date_of_birth))),' M')
WHEN EXTRACT(DAY FROM (age(visit.visit_date,patient.date_of_birth))) != 0 THEN CONCAT(EXTRACT(DAY FROM (age(visit.visit_date,patient.date_of_birth))),' D') END) AS patient_age,
visit.visit_date as patient_reg_date,visit.visit_time,
visit.visit_no as visit_visit_no,
mas_department.department_name as department_name,
left(administrative_sex_name,1) as gender,
mas_hospital.hospital_name as hospital_name,
patient.next_of_kin_name as relative_name,(mas_employee.first_name::text || ' ' || coalesce(mas_employee.last_name,'')) as doctor_name,
mas_complaint.complaint_name as complaint,mas_case_type.case_type_name as case_type,
(case  when (visit.visit_no =1) then 'New' else 'Re Visit' end) as visit_type,
(case  when (visit.visit_no =1) then 'New' end) as new_type,
(case  when (visit.visit_no >1) then 'Re Visit' end) as new_re_type,
total_hospital_visit as slno,token_no,creation_source

from
      visit visit left outer join  patient patient on visit.hin_id = patient.hin_id
     left outer join mas_department mas_department on visit.department_id = mas_department.department_id
     left outer join mas_administrative_sex mas_administrative_sex on patient.sex_id = mas_administrative_sex.administrative_sex_id
     left outer join mas_hospital mas_hospital on mas_hospital.hospital_id=$P{HOSPITAL_ID}
     LEFT OUTER JOIN opd_patient_details opd ON opd.visit_id = visit.visit_id
     LEFT OUTER JOIN mas_employee mas_employee ON opd.employee_id = mas_employee.employee_id
     LEFT OUTER JOIN mas_complaint mas_complaint ON visit.complaint_id = mas_complaint.complaint_id
     LEFT OUTER JOIN mas_case_type mas_case_type ON visit.case_type_id = mas_case_type.case_type_id

 $P!{qry}]]>
	</queryString>
