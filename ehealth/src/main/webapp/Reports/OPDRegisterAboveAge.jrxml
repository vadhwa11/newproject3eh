	</parameter>
	<parameter name="fromAge" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="toAge" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select patient.hin_no as patient_hin_no,(patient.p_first_name+' '+ISNULL(patient.p_last_name,''))as patientname,
patient.age as patient_age,patient.reg_date as patient_reg_date,visit.visit_no as visit_visit_no,
mas_department.department_name as department_name,patient.address as patient_address,
mas_administrative_sex.administrative_sex_name as mas_administrative_sex_administrative_sex_name,
(mas_hospital.hospital_name+','+ISNULL(mas_hospital.address,'')) as hospital_name,
patient.next_of_kin_name as relative_name,(mas_employee.first_name+' '+ISNULL(mas_employee.last_name,'')) as doctor_name,
mas_complaint.complaint_name as complaint,mas_case_type.case_type_name as case_type,
(case  when (visit.visit_no =1) then 'New' else 'Re Visit' end) as visitType,

case when ($P{userId} > 0 )   then (select count(visit.visit_no) from visit visit where visit_no=1 
and visit.visit_date between $P{fromDate} and $P{toDate} and  visit.add_edit_by_id=$P{userId})
else (select count(visit.visit_no) from visit visit where visit_no=1 
and visit.visit_date between $P{fromDate} and $P{toDate} ) end as new_total ,

case when ($P{userId} > 0 ) then (select count(visit.visit_no) from visit visit where visit.visit_no!=1
and visit.visit_date between $P{fromDate} and $P{toDate} and  visit.add_edit_by_id=$P{userId}) 
else (select count(visit.visit_no) from visit visit where visit_no!=1 
and visit.visit_date between $P{fromDate} and $P{toDate})end as visit_total,

case when ($P{userId} > 0 ) then (select count(visit.visit_no) from visit visit where visit.visit_date
between $P{fromDate} and $P{toDate} and  visit.add_edit_by_id=$P{userId}) 
else (select count(visit.visit_no) from visit visit where visit.visit_date between $P{fromDate} and $P{toDate})
 end as total_visit_patient,
     mas_village.village_name as village_name
from
      visit visit 
left outer join  patient patient on visit.hin_id = patient.hin_id
left outer join mas_state s on patient.state_id=s.state_id
     left outer join mas_department mas_department on visit.department_id = mas_department.department_id
     left outer join mas_administrative_sex mas_administrative_sex on patient.sex_id = mas_administrative_sex.administrative_sex_id
     left outer join mas_hospital mas_hospital on patient.hospital_id=mas_hospital.hospital_id
     LEFT OUTER JOIN mas_employee mas_employee ON visit.doctor_id = mas_employee.employee_id
     LEFT OUTER JOIN mas_complaint mas_complaint ON visit.complaint_id = mas_complaint.complaint_id
     LEFT OUTER JOIN mas_case_type mas_case_type ON visit.case_type_id = mas_case_type.case_type_id
     LEFT OUTER JOIN mas_village mas_village ON patient.village_id = mas_village.village_id
where visit.visit_date between $P{fromDate} and $P{toDate}

and (rtrim(SUBSTRING(patient.age,1,CHARINDEX(' ',patient.age))) > $P{fromAge}) and (ltrim(substring(patient.age,(CHARINDEX(' ', patient.age)),8)) = 'Years')
and (rtrim(SUBSTRING(patient.age,1,CHARINDEX(' ',patient.age))) < $P{toAge}) and (ltrim(substring(patient.age,(CHARINDEX(' ', patient.age)),8)) = 'Years')

$P!{qry}
order by visit.visit_date,patient.p_first_name, patient.age  asc]]></queryString>

