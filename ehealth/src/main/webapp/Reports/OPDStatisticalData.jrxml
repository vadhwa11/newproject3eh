		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="to_date" isForPrompting="true" class="java.util.Date"/>
	<parameter name="from_date" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[select td.temp_date ,
(select hospital_name from mas_hospital where hospital_id = $P{hospital_id}) hospital_name,
(select count(visit_no) from visit t where t.visit_no = 1 and t.visit_date = td.temp_date) as noofnewpatient,
(select count(visit_no) from visit t where t.visit_no != 1 and t.visit_date = td.temp_date) as noofoldpatient,
(select count(t.op_bill_header_id) from bl_op_bill_header t,bl_op_bill_details d where t.op_bill_header_id = d.op_bill_header_id and d.charge_code_id not in(302,303,304) and t.status = 'y' and t.bill_date = td.temp_date  ) as noofservising,
(select count(t.charge_slip_main_id) from bl_charge_slip_main t,bl_charge_slip_detail d where t.charge_slip_main_id = d.charge_slip_main_id and d.charge_code_id not in(302,303,304) and t.status = 'y' and t.chg_slp_date = td.temp_date  ) as noofIpservising,
(select count(dispensing_header_id) from bl_dispensing_header t where t.inpatient_id is null and t.status = 'y' and  t.bill_date = td.temp_date) as noofdispencing,
(select count(dispensing_header_id) from bl_dispensing_header t where t.inpatient_id is not null and t.status = 'y' and  t.bill_date = td.temp_date) as noofipdispencing
from temp_date td;]]></queryString>

