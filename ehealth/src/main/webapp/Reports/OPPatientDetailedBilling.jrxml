	<queryString><![CDATA[select hin_id, hin_no , UPPER((p_first_name||' '||coalesce(p_middle_name,' ')||' '||coalesce(p_last_name,' '))) as pname,
(select administrative_sex_name from mas_administrative_sex mas
where mas.administrative_sex_id = p.sex_id ) as sex_name ,age ,
(select max(visit_no) from visit v where v.hin_id = p.hin_id ) as visitno,
(select max(visit_date) from visit v where v.hin_id = p.hin_id ) as visitdate,
(select hospital_name from mas_hospital m where m.hospital_id = p.hospital_id) as hname
from patient p where hin_no = case when $P{hinNo}=' ' then hin_no else $P{hinNo} end 
and patient_status ='out patient'
and p.hospital_id=$P{hospitalId}]]></queryString>

