		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
	(patient.p_first_name::text || ' ' || coalesce(patient.p_middle_name,'') || ' ' || coalesce(patient.p_last_name,''))as patientname,patient.age,
	(medico_legal_details.accomp_by_name::text || ' ' || coalesce(medico_legal_details.accomp_by_address,''))as accompanied_by,
medico_legal_details.accomp_by_name,
medico_legal_details.accomp_by_address,
	consent,crime_no,police_station,serial_no,ref_date,
	requisition_from,requisition_date,received_time,received_date,
	exam_comm_date,exam_comm_time,id_mark1,id_mark2,
	medico_legal_details.height,medico_legal_details.weight,build,hair,exam_comm_date,exam_comm_time,opinion,reason_for_conclusion,
employee.emp_name,mlc_material_objects.material_objects,examination_date
,examination_time,breasts,last_menstrual_date,
s.administrative_sex_name,examination_place,material_objects_preserved,preservative_used,
sample_paking,sealed,history_of_illness,findings,examination_required,
district.district_name,rank.rank_name,mas.hospital_name,emp.first_name,
postmortem_certificate_no,postmorem_time,postmortem_date,concluded_at,Cause_death,postmortem_date,postmortem_ser_no
from
     medico_legal_details  medico_legal_details 
	left outer join patient patient on patient.hin_id = medico_legal_details.hin_id
	left outer join mas_employee employee on employee.employee_id = medico_legal_details.doctor_id
	left outer join mlc_material_objects  mlc_material_objects  on mlc_material_objects.medico_legal_details_id = medico_legal_details.medico_legal_details_id
LEFT OUTER JOIN mas_hospital mas ON mas.hospital_id = patient.hospital_id
LEFT OUTER JOIN mas_district district ON district.district_id = mas.district_id
left outer join mas_occupation Occupation on patient.occupation_id=occupation.occupation_id
left outer join mas_administrative_sex s on patient.sex_id=s.administrative_sex_id
LEFT OUTER JOIN patient_wise_mlc pw ON pw.hin_id = patient.hin_id
LEFT OUTER JOIN opd_patient_details opd ON opd.id = pw.opd_patient_detail_id
LEFT OUTER JOIN mas_employee emp ON emp.employee_id = opd.employee_id
LEFT OUTER JOIN mas_rank rank ON rank.rank_id = employee.rank_id

where medico_legal_details.medico_legal_details_id=$P{medicoLegalDetailsId}]]></queryString>

