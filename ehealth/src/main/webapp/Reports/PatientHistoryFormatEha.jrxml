	</parameter>
	<queryString><![CDATA[SELECT
     opd_patient_history.present_complaint_history,
 opd_patient_history.past_illness_history,

     opd_patient_history.personal_past_history AS opd_patient_history_personal_past_history,
     patient.hin_no AS patient_hin_no,
     visit.visit_no AS visit_visit_no,
     mas_hospital.hospital_name AS mas_hospital_hospital_name,
     mas_hospital.address AS mas_hospital_address,
     opd_patient_history.visit_inpatient_id AS opd_patient_history_visit_inpatient_id,
     visit.visit_id AS visit_visit_id,
     opd_patient_history.present_illness AS opd_patient_history_present_illness,
     opd_patient_history.present_complain AS opd_patient_history_present_complain,
     opd_patient_history.personal_present_history AS opd_patient_history_personal_present_history,
     opd_patient_history.family_present_history AS opd_patient_history_family_present_history,
     opd_patient_history.ip_op_pac_status AS opd_patient_history_ip_op_pac_status,
	(patient.p_first_name||' '||COALESCE(patient.p_middle_name,'')||' '||COALESCE(patient.p_last_name,'')) as patientname,
     mas_administrative_sex.administrative_sex_name AS mas_administrative_sex_administrative_sex_name,
	(COALESCE(mas_marital_status.marital_status_name,'--'))AS mas_marital_status_marital_status_name,
     patient.age AS patient_age, opd_patient_history.family_past_history

FROM
opd_patient_history opd_patient_history LEFT OUTER JOIN 
visit visit on visit.visit_id=opd_patient_history.visit_inpatient_id left outer join  
patient patient   ON patient.hin_id = opd_patient_history.hin_id 
   LEFT OUTER JOIN mas_hospital mas_hospital ON opd_patient_history.hospital_id = mas_hospital.hospital_id
     LEFT OUTER JOIN mas_administrative_sex mas_administrative_sex ON patient.sex_id = mas_administrative_sex.administrative_sex_id
     LEFT OUTER JOIN mas_marital_status mas_marital_status ON patient.marital_status_id = mas_marital_status.marital_status_id
     
where
     visit.visit_no = $P{visitNo} 
     and patient.hin_no = $P{hinNo} 
     and opd_patient_history.hospital_id=$P{hospitalId}]]></queryString>

