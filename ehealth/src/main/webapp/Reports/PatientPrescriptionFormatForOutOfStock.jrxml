	<queryString><![CDATA[select   mas_store_item.pvms_no as mas_store_item_pvms_no,
     mas_store_item.nomenclature as mas_store_item_nomenclature,
     ipd.dosage as patient_prescription_details_dosage,
     ipd.frequency_id as patient_prescription_details_frequency_id,
     ipd.no_of_days as patient_prescription_details_no_of_days,
     ipd.total as patient_prescription_details_total,
      visit.visit_date as visit_visit_date,
     visit.visit_no as visit_visit_no,
     patient.age as patient_age,
     mas_department.department_name as mas_department_department_name,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
     mas_hospital.address as mas_hospital_address,
     (patient.p_first_name||' '||COALESCE(patient.p_middle_name,'')||' '||COALESCE(patient.p_last_name,''))as patient_name,
     patient.hin_no as patient_hin_no,
     (mas_employee.first_name||' '||COALESCE(mas_employee.middle_name,'')||' '||COALESCE(mas_employee.last_name,''))as mas_employee_name
      from inpatient_prescription_details ipd 
inner join inpatient_prescription_header iph on iph.prescription_id = ipd.prescription_id
left outer join patient patient on iph.hin_id = patient.hin_id
left outer join inpatient inpatient on inpatient.hin_id = patient.hin_id
 left outer join mas_store_item mas_store_item on ipd.item_id = mas_store_item.item_id
 left outer join visit visit on iph.visit_id = visit.visit_id
       left outer join mas_department mas_department on iph.department_id = mas_department.department_id
     left outer join mas_hospital mas_hospital on iph.hospital_id = mas_hospital.hospital_id
     left outer join mas_employee mas_employee on visit.doctor_id = mas_employee.employee_id
where  patient.hin_no =$P{hinNo} and  iph.hospital_id =$P{hospitalId}   and  inpatient.ad_no =$P{ad_no} and 
(ipd.item_id not in(select item_id from store_item_batch_stock where hospital_id=iph.hospital_id and expiry_date>=current_date and closing_stock>0 ))]]></queryString>

