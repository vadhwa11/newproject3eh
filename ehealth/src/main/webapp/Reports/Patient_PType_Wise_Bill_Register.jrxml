	</parameter>
	<queryString><![CDATA[select mpt.patient_type_name,count(*) as total,mh.hospital_name,mh.address as address ,main.main_chargecode_name
from dg_orderdt dtl
left outer join dg_orderhd main1 on main1.orderhd_id=dtl.orderhd_id
left outer join mas_charge_code charge on charge.charge_code_id=dtl.charge_code_id
left outer join mas_main_chargecode main on main.main_chargecode_id=charge.main_chargecode_id
left outer join patient pt on pt.hin_id=main1.hin_id 
left outer join mas_patient_type mpt on mpt.patient_type_id=pt.patient_type_id
left outer join mas_hospital mh on mh.hospital_id=pt.hospital_id
where charge.main_chargecode_id=$P{mainChargeId}
and main1.order_date between $P{fromDate} and $P{toDate}
group by mpt.patient_type_name,mh.hospital_name,mh.address,main.main_chargecode_name]]></queryString>

