	<queryString><![CDATA[select   sum(issuet.qty_issued) as qtyIssued,
item.nomenclature as ItemName,hospital.hospital_name as HospitalName,
(patient.p_first_name || COALESCE(patient.p_middle_name,' ')|| COALESCE(patient.p_last_name,'')) as PatientName
,issuem.issue_no, u.login_name as userName
from store_issue_m issuem 
 left outer join store_issue_t issuet on issuem.id= issuet.issue_m_id 
 left outer join mas_store_item item on item.item_id = issuet.item_id 
 left outer join mas_hospital hospital on hospital.hospital_id = issuem.hospital_id
 left outer join  visit visit on issuet.visit_id = visit.visit_id 
 left outer join patient patient on visit.hin_id = patient.hin_id
left outer join users u on u.user_id = issuem.last_chg_by
where issuem.issue_date between $P{fromDate}  and $P{toDate}  and issuem.department_id=$P{deptId} and hospital.hospital_id=$P{hospitalId}
$P!{query}
 group by item.nomenclature,hospital.hospital_name,patient.p_first_name,
	patient.p_middle_name,patient.p_last_name,issuem.issue_no,u.login_name]]></queryString>

