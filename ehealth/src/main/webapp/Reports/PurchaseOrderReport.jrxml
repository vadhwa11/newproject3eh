	<queryString><![CDATA[select manisql.*,m.pvms_no,m.nomenclature from

(
select main.*,coalesce(mainsub.closingstock,0) as closingstock from
(select m.po_id,m.po_number,t.item_id,sum(t.quantity_ordered) as reqqty
 from store_po_header m , store_po_detail t
 where m.po_id = t.po_id
and m.po_id = $P{poId} and m.hospital_id=$P{hospitalId}
group by m.po_id,m.po_number,t.item_id ) as main

  left outer join

(select item_id,sum(closing_stock) as closingstock
 from store_item_batch_stock
 where  expiry_date > current_date
 group by item_id ) as mainsub

on main.item_id = mainsub.item_id ) as manisql , mas_store_item as m


where manisql.item_id = m.item_id]]></queryString>

