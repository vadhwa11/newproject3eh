	<queryString><![CDATA[select * from (select 
	main.item_id,
	main.pvms_no,
	main.nomenclature,
	main.rol ,
	submain.closingstock
from
	(	select 
			item_id,
			pvms_no,
			nomenclature,
			rol 
		from mas_store_item
	) as main ,
	(	select 
			item_id,
			sum(closing_stock) as  closingstock 
		from store_item_batch_stock s where expiry_date >current_timestamp group by item_id  
	) as submain
	where main.item_id = submain.item_id
	and cast(submain.closingstock as decimal) < cast(main.rol as decimal)
	union
	select 
		item_id,
		pvms_no,
		nomenclature,
		rol ,
		0 as closingstock 
	from mas_store_item m
	where item_id not in ( select distinct item_id from store_item_batch_stock )) a
	order by a.nomenclature]]></queryString>

