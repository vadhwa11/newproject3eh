	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select receipt_no, hin_id, amount, receipt_date, inpatient_id,
coalesce(charity_amt,0.0) as charityamt,
coalesce(round_off,0.0) as roundoff,
(select UPPER((p_first_name||' '||coalesce(p_middle_name,' ')||' '||coalesce(p_last_name,' ')))
from patient where hin_id = bd.hin_id ) as patientname,
(case receipt_type when 'adv' then 'advanced' else 'final settlement' end ) as receipt_type,
(select hospital_name from mas_hospital where hospital_id = bd.hospital_id) as hospital,
(select coalesce(ad_no,'-') from inpatient where ad_status = 'A' and
inpatient_id = bd.inpatient_id) as inpatientno,
coalesce((select  visit_no from visit where visit_id = bd.hin_id),0) as visitno,
coalesce((select amount from bl_receipt_details where payment_mode = 'C' and
receipt_header_id = bd.receipt_header_id),0.0) as cashamount,
coalesce((select amount from bl_receipt_details where payment_mode = 'Q' and
receipt_header_id=bd.receipt_header_id),0.0) as chaqueamount,
coalesce((select amount from bl_receipt_details where payment_mode = 'R' and
receipt_header_id=bd.receipt_header_id),0.0) as creditcardamount,
(select hin_no from patient where hin_id = bd.hin_id) as hinno
from bl_receipt_header bd where receipt_type in ('fs','adv')
and receipt_date  between $P{fromDate} and $P{toDate}  and bd.hospital_id=$P{hospitalId}
order by receipt_no, receipt_date]]></queryString>

