	<queryString><![CDATA[select a.*,b.*,c.*,d.*,ee.*,ff.*,g.*,h.*,i.*,j.*,k.*,l.*,m.*,
(mas_employee4.first_name+' '+ISNULL(mas_employee4.middle_name,'')+' '+ISNULL(mas_employee4.last_name,''))as empname,mas_employee4.employee_code,mas_hospital4.hospital_name,
e.*,f.*


 from
(select pay_amount as hra,hep.employee_id as e1 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=7 and month(start_date) = $P{month}  and year(start_date) = $P{year}  and year(start_date) = $P{year} )as a

left join

(select pay_amount as sa,hep.employee_id as e2 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=14 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as b on a.e1=b.e2


left join

(select pay_amount as pay,hep.employee_id as e3 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=15 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as c on a.e1=c.e3

left join

(select pay_amount as conv,hep.employee_id as e4 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=16  and month(start_date) = $P{month}  and year(start_date) = $P{year} )as d on a.e1=d.e4

left join

(select pay_amount as medical,hep.employee_id as e5 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=17 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as ee on a.e1=ee.e5

left join

(select pay_amount as leavetravel,hep.employee_id as e6 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=18 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as ff on a.e1=ff.e6

left join

(select pay_amount as meal,hep.employee_id as e7 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=19 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as g on a.e1=g.e7

left join

(select pay_amount as furniture,hep.employee_id as e8 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=20 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as h on a.e1=h.e8

left join

(select pay_amount as pf,hep.employee_id as e9 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=21 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as i on a.e1=i.e9

left join

(select pay_amount as it,hep.employee_id as e10 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=22 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as j on a.e1=j.e10

left join

(select pay_amount as gratuity,hep.employee_id as e11 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=23 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as k on a.e1=k.e11

left join

(select pay_amount as superannuation,hep.employee_id as e12 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=24 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as l on a.e1 = l.e12

left join

(select pay_amount as lunch,hep.employee_id as e13 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=25 and month(start_date) = $P{month}  and year(start_date) = $P{year} )as m on a.e1 = m.e13



left join mas_employee mas_employee4 on a.e1=mas_employee4.employee_id
left join mas_hospital mas_hospital4 on mas_employee4.hospital_id = mas_hospital4.hospital_id
left join (select basic_pay,employee_id as e5 from hr_employee_pay_structure hr_employee_pay_structure4
) as e on e.e5 = mas_employee4.employee_id
left join (select month,year,net_salary,total_earning,total_deduction,total_reimb,loan_installment,advance_installment,bonus_amount,arrear_payelement_amt,arrear_salary_amt,employee_id as e6
from hr_payroll_process_header hr_payroll_process_header4
) as f on f.e6 = mas_employee4.employee_id
where
     mas_employee4.employee_id = $P{empId} 
         and f.year = $P{year} 
and f.month = $P{month} 
;]]></queryString>

