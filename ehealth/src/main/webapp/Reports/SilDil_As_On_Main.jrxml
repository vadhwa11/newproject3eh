	<queryString><![CDATA[select
   
     inpatient.`ad_no` as inpatient_ad_no,
     inpatient.`condition_status` as sil_or_dil,
    concat(mas_hospital.`hospital_name`,' ',mas_hospital.`address`)as hospital_name
from
     `patient` patient inner join `inpatient` inpatient on patient.`hin_id` = inpatient.`hin_id`
      LEFT OUTER JOIN `mas_hospital` mas_hospital ON patient.`hospital_id` = mas_hospital.`hospital_id`
where
      inpatient.`condition_status` <>'normal' and inpatient.`condition_status` <>'dead'
$P!{stringVariable}

group  by inpatient.`condition_status`]]></queryString>

