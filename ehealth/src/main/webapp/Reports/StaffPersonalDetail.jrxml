		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<parameter name="toEmpCode" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<parameter name="empStatusId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
     mas_employee.employee_code as mas_employee_employee_code,
    (mas_employee.first_name+' '+ISNULL(mas_employee.middle_name,'')+' '+ISNULL(mas_employee.last_name,''))as ename,
     mas_employee.join_date as mas_employee_join_date,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
     hr_employee_pay_structure.basic_pay as hr_employee_pay_structure_basic_pay,
     mas_rank.rank_name as mas_rank_rank_name,
     hr_employee_personel_details.date_of_birth as hr_employee_personel_details_date_of_birth,
     mas_emp_status.emp_status_name as mas_emp_status_emp_status_name,
     mas_emp_status.emp_status_id as mas_emp_status_emp_status_id
from
     mas_hospital mas_hospital right outer join mas_employee mas_employee on mas_hospital.hospital_id = mas_employee.hospital_id
     left outer join hr_employee_pay_structure hr_employee_pay_structure on mas_employee.employee_id = hr_employee_pay_structure.employee_id
     left outer join mas_rank mas_rank on mas_employee.rank_id = mas_rank.rank_id
     left outer join hr_employee_personel_details hr_employee_personel_details on mas_employee.personal_details_id = hr_employee_personel_details.id
     inner join mas_emp_status mas_emp_status on mas_employee.employee_status_id = mas_emp_status.emp_status_id
where
     mas_employee.employee_code between $P{fromEmpCode} and  $P{toEmpCode}  
     and mas_emp_status.emp_status_id = $P{empStatusId}]]></queryString>

