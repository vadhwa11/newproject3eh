	<queryString><![CDATA[select  date,department_id,item_id,batch_no,nomenclature,sum(coalesce(f.opening_qty,0)) as opening_qty,
sum(coalesce(f.return_qty,0)) as return_qty,sum(coalesce(f.received_qty,0)) as received_qty,
sum(coalesce(f.issued_qty,0)) as issued_qty,sum(coalesce(f.cum_qty,0)) as cunsumptionQty,
sum(coalesce(f.department_return,0)) as department_return,sum(coalesce(f.adjust_qty,0)) as adjust_qty,
(sum(coalesce(f.opening_qty,0))+sum(coalesce(f.received_qty,0))+
sum(coalesce(f.department_return,0))
-sum(coalesce(f.issued_qty,0))-sum(coalesce(f.return_qty,0))+sum(coalesce(f.adjust_qty,0))-sum(coalesce(f.cum_qty,0))) as final_closing 
from stock_register_final as f 
where f.date between $P{startDate} and $P{endDate} and item_id=$P{itemId} and batch_No=$P{batchNo}
group by date,department_id,item_id,batch_no,nomenclature
order by date]]></queryString>

