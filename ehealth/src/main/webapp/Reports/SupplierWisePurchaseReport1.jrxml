	<queryString><![CDATA[select
main2nd.*,
sup.supplier_name ,
itm.pvms_no,
itm.nomenclature
from
(
select
main.department_id ,
main.supplier_id ,
main.item_id ,
coalesce(main.purqty,'0') as purqty,
coalesce(main.puramount,'0') as puramount ,
coalesce(main.retqty,'0') as retqty ,
coalesce(main.retamount,'0') as retamount ,
coalesce(issue.sqty,'0') sqty ,
coalesce(issue.samount,'0') as samount,
((coalesce(main.purqty,'0') - coalesce(main.retqty,'0')) - coalesce(issue.sqty,'0')) as netqty
from
(
(
select
receivedmain.department_id ,
receivedmain.supplier_id ,
receivedmain.item_id ,
receivedmain.purqty,
receivedmain.puramount ,
returnmain.retqty,
returnmain.retamount
from
(
select
m.department_id,
m.supplier_id ,
t.item_id ,
sum(t.received_qty) as purqty ,
sum(t.amount_value) as puramount
from store_grn_m m , store_grn_t t where m.grn_master_id = t.grn_master_id
group by m.department_id,m.supplier_id ,t.item_id
) as receivedmain left join
(select m.department_id,m.supplier_id ,t.item_id,sum(t.return_qty) as
retqty ,
sum(t.return_amount) as retamount
from store_grn_return_m m , store_grn_return_t t
where m.grn_return_id = t.grn_return_id
group by m.supplier_id ,t.item_id,m.department_id) as returnmain

on receivedmain.department_id = returnmain.department_id
and receivedmain.supplier_id = returnmain.supplier_id
and receivedmain.item_id = returnmain.item_id ) )as main


left join
(


select department_id ,item_id , sum(sqty) as sqty ,sum(sprice) as
sprice,sum(samount) as samount
from

(select m.department_id ,t.item_id, sum(t.qty_issued) as sqty ,
sum(t.cost_price) as sprice , sum(t.qty_issued * t.cost_price ) as samount
from store_issue_m m , store_issue_t t where m.id = t.issue_m_id
and m.issue_date between  $P{fromDate}  and $P{toDate} 
and m.hospital_id=$P{hospitalId}
group by m.department_id , t.item_id
union
select m.department_id ,t.item_id,sum(t.qty_issued) as sqty ,sum(t.rate)
as sprice , sum(t.amount) as samount from store_ip_issue_m m ,
store_ip_issue_t t where m.ip_issue_id = t.ip_issue_id
and m.ip_issue_date between  $P{fromDate}  and $P{toDate} 
and m.hospital_id=$P{hospitalId}
group by m.department_id ,t.item_id
union
select m.department_id ,t.item_id_issue as item_id,sum(t.qty_issued) as
sqty ,sum(t.rate) as sprice , sum(t.amount) as samount
from store_op_patient_issue_m m , store_op_patient_issue_t t
where m.op_issue_id = t.op_issue_id
and m.issue_date between  $P{fromDate}  and $P{toDate} 
and m.hospital_id=$P{hospitalId}
group by m.department_id ,t.item_id_issue ) as salesmain
group by salesmain.department_id ,salesmain.item_id


) as issue

on main.department_id = issue.department_id
and main.item_id = issue.item_id
where main.department_id = $P{deptId} 

) as main2nd ,mas_store_supplier sup , mas_store_item itm
where main2nd.supplier_id = sup.supplier_id and main2nd.item_id = itm.item_id
$P!{query}  order by sup.supplier_name ,itm.item_id]]></queryString>

