	<queryString><![CDATA[select
     concat(hr_training_schedule.`training_date`,' ',hr_training_schedule.`training_time`)as date,
     hr_training_schedule.`training_id` as hr_training_schedule_training_id,
     hr_training_schedule.`training_date` as hr_training_schedule_training_date,
     hr_training_schedule.`training_time` as hr_training_schedule_training_time,
     hr_training_schedule.`class_size` as hr_training_schedule_class_size,
     hr_training_schedule.`duration` as hr_training_schedule_duration,
     hr_training_schedule.`remarks` as hr_training_schedule_remarks,
     hr_training_schedule.`training_start_date` as hr_training_schedule_training_start_date,
     hr_training_schedule.`training_end_date` as hr_training_schedule_training_end_date,
     hr_mas_training.`training_name` as hr_mas_training_training_name,
     hr_mas_instructor.`instructor_name` as hr_mas_instructor_instructor_name,
      mas_hospital.`hospital_name` as mas_hospital_hospital_name

from
     `hr_training_schedule` hr_training_schedule
     left join `hr_mas_training` hr_mas_training on hr_training_schedule.`training_id` = hr_mas_training.`training_id`
      left join `hr_mas_instructor` hr_mas_instructor on hr_training_schedule.`instructor_id` = hr_mas_instructor.`instructor_id`
left join `mas_hospital` mas_hospital on  hr_training_schedule.`company_id` = mas_hospital.`hospital_id`

where
 hr_training_schedule.`training_start_date` between  $P{fromDate}and $P{toDate}

      and hr_training_schedule.`training_end_date` between  $P{fromDate}and $P{toDate}
]]></queryString>

