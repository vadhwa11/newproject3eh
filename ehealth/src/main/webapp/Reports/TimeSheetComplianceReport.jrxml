	<queryString><![CDATA[select
     count(*)as total,
     tbltimesheet.`entrydate` as tbltimesheet_entrydate,
     tbltimesheet.`status` as tbltimesheet_status,
     concat(mas_employee.`first_name`," ",mas_employee.`last_name`)as ename,
	mas_employee.`employee_code` as empcode,
     mas_employee.`department_id` as mas_employee_department_id,
     mas_employee.`hospital_id` as mas_employee_hospital_id,
     mas_employee.`employee_id` as mas_employee_employee_id,
     tbltimesheet.`hrsworked` as tbltimesheet_hrsworked,
     mas_department.`department_name` as mas_department_department_name
from
     `mas_employee` mas_employee left outer join `tbltimesheet` tbltimesheet on mas_employee.`employee_id` = tbltimesheet.`emp_id`
     right outer join `mas_department` mas_department on mas_employee.`department_id` = mas_department.`department_id`
where
     mas_employee.`department_id` = $P{dept}
     and tbltimesheet.`entrydate`between $P{fromDate}and $P{toDate}
group by tbltimesheet.`emp_id`,     tbltimesheet.`status`

order by mas_employee.`employee_code` asc ;]]></queryString>

