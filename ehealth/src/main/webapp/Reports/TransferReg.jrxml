	<queryString><![CDATA[select
     transfer.`date_of_transfer` as transfer_date_of_transfer,
     transfer.`time_of_transfer` as transfer_time_of_transfer,
     patient.`service_no` as patient_service_no,
     concat(patient.`s_first_name`," ",patient.`s_middle_name`," ",patient.`s_last_name`) as s_name,
     transfer.`ad_no` as transfer_ad_no,
     mas_rank.`rank_name` as mas_rank_rank_name,
     mas_relation.`relation_name` as mas_relation_relation_name,
     mas_dep1.`department_name` as from_dept,
    mas_dep2.`department_name` as to_dept
from
     `patient` patient left outer join `transfer` transfer on patient.`hin_id` = transfer.`hin_id`
     left  outer join `mas_relation` mas_relation on patient.`relation_id` = mas_relation.`relation_id`
     left outer join `mas_rank` mas_rank on patient.`rank_id` = mas_rank.`rank_id`
     left outer join `mas_department` mas_dep1 on transfer.`from_ward_id` = mas_dep1.`department_id`
     left outer join `mas_department` mas_dep2 on transfer.`to_ward_id` = mas_dep2.`department_id`
where  transfer.`date_of_transfer` between $P{fromDate} and $P{toDate}  $P!{stringVariable}


order by concat(from_dept,transfer_date_of_transfer)]]></queryString>

