	<queryString><![CDATA[Select hmfy.financial_year as financialYear,masg.account_sub_group_name, ma.acc_desc, ma.op_balance_dr as ma_op_balance_dr, ma.op_balance_cr as ma_op_balance_cr, ma.cl_balance_dr as ma_cl_balance_dr, ma.cl_balance_cr as ma_cl_balance_cr,
 masg.op_balance_dr, masg.op_balance_cr, masg.cl_balance_dr, masg.cl_balance_cr, mas_hospital.hospital_name 
as hospital_name, mas_hospital.address as hospital_address

from fa_mas_account_sub_group masg

left outer join fa_mas_account ma on ma.account_sub_group_id=masg.account_sub_group_id 

left outer join mas_hospital mas_hospital on mas_hospital.hospital_id=masg.hospital_id 

left outer join hr_mas_financial_year hmfy on masg.f_year_id=hmfy.id  

 where (masg.op_balance_dr>'0' or masg.op_balance_cr>'0' or masg.cl_balance_dr>0 or masg.cl_balance_cr>'0') and
(ma.op_balance_dr>'0' or ma.op_balance_cr>'0' or ma.cl_balance_dr>0 or ma.cl_balance_cr>'0') and
hmfy.financial_year='2010-11' and masg.status='y' and
masg.hospital_id='1' order by masg.account_sub_group_code;]]></queryString>

