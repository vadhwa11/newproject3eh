	<queryString><![CDATA[select mainsql.*, mainsubsql.* from

(select grn_date,item_id,batch_no,expiry_date,invoice_date,
(select manufacturer_name from mas_manufacturer mm where mm.manufacturer_id = t.manufacturer_id ) as manufacturername,
manufacturer_id,coalesce(final_cost_price,'0') as final_cost_price
,coalesce(mrp,'0') as mrp , (coalesce(final_cost_price,'0')-coalesce(mrp,'0')) as variance1
from store_grn_m m right outer join store_grn_t t on m.grn_master_id = t.grn_master_id
where m.hospital_id=$P{hospitalId} and  grn_date between $P{fromDate} and $P{toDate} ) as mainsql
left outer join
(select item_id,pvms_no,nomenclature,item_type_id,item_category_id,supplier_id,
(select supplier_name from mas_store_supplier mss where mss.supplier_id = msi.supplier_id ) as suppliername ,
(select item_type_name from mas_item_type mtt where mtt.item_type_id = msi.item_type_id ) as itemtypename,
(select item_category_name from mas_item_category mic where mic.item_category_id = msi.item_category_id ) as itemcategoryname
from mas_store_item msi ) as mainsubsql
on mainsql.item_id = mainsubsql.item_id
$P!{qry}
order by supplier_id,pvms_no ;]]></queryString>

