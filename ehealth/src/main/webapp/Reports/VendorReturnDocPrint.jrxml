     left outer join `mas_employee` mas_employee on store_grn_return_m.`return_by` = mas_employee.`employee_id`
     left outer join `mas_employee` mas_employee_a on store_grn_return_m.`approved_by` = mas_employee_a.`employee_id`
where
     store_grn_return_m.`grn_return_id` = $P{RETURN_ID}

 and store_grn_return_m.`department_id` = $P{DEPART}
