	<queryString><![CDATA[select
     mas_store_supplier.supplier_code as mas_store_supplier_supplier_code,
     mas_store_supplier.supplier_name as mas_store_supplier_supplier_name,
     mas_store_supplier.phoneno as mas_store_supplier_phoneno,
     mas_store_supplier.mobileno as mas_store_supplier_mobileno,
     mas_store_supplier.emailid as mas_store_supplier_emailid,
     (local_address1||' '||local_address2||' '||local_city) as address1,
case  when local_pin_code=0 then 0 else local_pin_code end   as mas_store_supplier_local_pin_code,
     mas_store_supplier.tin_no as mas_store_supplier_tin_no,
     mas_store_supplier.faxnumber as mas_store_supplier_faxnumber,
     mas_store_supplier_type.supplier_type_name as mas_store_supplier_type_supplier_type_name,
     mas_manufacturer.manufacturer_name as mas_manufacturer_manufacturer_name
from
     mas_store_supplier_type mas_store_supplier_type right outer join mas_store_supplier mas_store_supplier 
on mas_store_supplier_type.supplier_type_id = mas_store_supplier.supplier_type_id
     left outer join mas_store_vendor_wise_manufacturer  mas_store_vendor_wise_manufacturer 
on mas_store_supplier.supplier_id = mas_store_vendor_wise_manufacturer.supplier_id
     left outer join mas_manufacturer mas_manufacturer on mas_store_vendor_wise_manufacturer.manufacturer_id = mas_manufacturer.manufacturer_id

  $P!{query}
Order by mas_store_supplier_supplier_name]]></queryString>

