      store_grn_m.grn_date between $P{fromDate} and $P{toDate} and
      store_grn_m.department_id = $P{deptId} and store_grn_m.status = 'o' and store_grn_m.hospital_id=$P{hospitalId}
order by mas_store_supplier.supplier_name]]></queryString>

