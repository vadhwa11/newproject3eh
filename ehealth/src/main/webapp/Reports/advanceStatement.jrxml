	<queryString><![CDATA[select
    (mas_employee.first_name+''+ISNULL(mas_employee.middle_name,'')+''+ISNULL(mas_employee.last_name,''))as empname,
     hr_advance.advance_code as hr_advance_advance_code,
     hr_advance.advance_date as hr_advance_advance_date,
     hr_advance_detail.recovered_amount as hr_advance_detail_recovered_amount,
     hr_advance.monthly_deduction as hr_advance_monthly_deduction,
     hr_advance.advance_amount as hr_advance_advance_amount,
     mas_hospital.hospital_name as mas_hospital_hospital_name
from
     hr_advance hr_advance inner join hr_advance_detail hr_advance_detail on hr_advance.advance_id = hr_advance_detail.advance_id
     inner join mas_employee mas_employee on hr_advance.employee_id = mas_employee.employee_id
     inner join mas_hospital mas_hospital on hr_advance.hospital_id = mas_hospital.hospital_id
     and mas_hospital.hospital_id = mas_employee.hospital_id
     and mas_hospital.hospital_id = hr_advance_detail.hospital_id
where

    mas_employee.employee_code between $P{fromEmpCode} and $P{toEmpCode}]]></queryString>

