     app_investigation_setup.no_of_equipments as app_investigation_setup_no_of_equipments,
     mas_department.department_name as mas_department_department_name,
     app_equipment_master.equipment_name as app_equipment_master_equipment_name,
     mas_hospital.hospital_name as mas_hospital_hospital_name
from
     mas_department right outer join  app_investigation_setup on mas_department.department_id = app_investigation_setup.department_id
     left outer join  app_equipment_master on app_investigation_setup.equipment_id = app_equipment_master.equipment_id
     left outer join  mas_hospital on app_investigation_setup.hospital_id = mas_hospital.hospital_id



