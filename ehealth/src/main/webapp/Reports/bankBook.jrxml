	<parameter name="hospital_id" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="from_date" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="accountId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="to_date" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
fvh.voucher_header_id,
fvh.voucher_no,
fvh.voucher_date,
(select op_balance_dr from fa_mas_account  where acc_id=$P{accountId}) as dr_amt,
(select op_balance_cr from fa_mas_account  where acc_id=$P{accountId}) as cr_amt,
(select acc_desc from fa_mas_account where acc_id=$P{accountId}) as main_ACC,
(  
    case fvh.voucher_type     when 'PU'then 'Purchase'
                            when 'PV' then 'Payment'
                            when 'SR' then 'Sales Return'
                            when 'PR' then 'Purchase Return'
                            when 'JV' then 'Journal'
                            when 'RV' then 'Receipt Voucher'
                            
end) as voucher_types,
m.hospital_name,
(case fvh.voucher_type  when 'PV' then coalesce(fvd.dr_amount,0) else 0 end) as dr_amount,
(case fvh.voucher_type  when 'RV' then coalesce(fvd.cr_amount,0) else 0 end) as cr_amount,
fma.acc_desc
from fa_voucher_details fvd 
left outer join fa_mas_account fma on fma.acc_id=fvd.account_id
left outer join fa_voucher_header fvh on fvh.voucher_header_id=fvd.voucher_header_id
left join mas_hospital m on m.hospital_id=fvh.hospital_id
where fvd.account_id=$P{accountId} and fvh.hospital_id=$P{hospital_id} and
fvh.voucher_date between $P{from_date} and $P{to_date} and  (fvh.voucher_type='PV' or fvh.voucher_type='RV')
and (((case fvh.voucher_type  when 'PV' then coalesce(fvd.dr_amount,0) else 0 end)!=0) or 
((case fvh.voucher_type  when 'RV' then coalesce(fvd.cr_amount,0) else 0 end)!=0))]]></queryString>

