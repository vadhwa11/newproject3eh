	<queryString><![CDATA[select pt.hin_no,h.bill_no,h.bill_date,sum(coalesce(d.net_amt,null))as net_amt,sum(coalesce(h.discount_amt,null)) as discount_amt,ma.authorizer_name,mh.hospital_name,h.remarks from bl_op_bill_details d
left outer join  bl_op_bill_header h on h.op_bill_header_id=d.op_bill_header_id
