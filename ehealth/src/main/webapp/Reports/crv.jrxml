	<queryString><![CDATA[select 
m.grn_no,m.grn_date,m.grn_amount,t.item_id,msi.nomenclature as item_name,
mss.supplier_name,mmu.manufacturer_name,t.unit_rate,
coalesce(t.received_qty,0) as qty,
coalesce(free_qty,0) as free,
coalesce(t.final_cost_price,0) as cost_price,
coalesce(amount_value,0)  as amt
from store_grn_m as m,store_grn_t as t,mas_store_supplier as mss,
mas_store_item  as msi,mas_manufacturer as mmu
where m.grn_master_id=t.grn_master_id
and t.manufacturer_id=mmu.manufacturer_id
and m.supplier_id=mss.supplier_id
and msi.item_id=t.item_id
and grn_no=$P{grn_no}
and m.department_id=$P{dept_id}
and m.hospital_id=$P{hospitalId}
Order By m.grn_no,m.grn_date;]]></queryString>

