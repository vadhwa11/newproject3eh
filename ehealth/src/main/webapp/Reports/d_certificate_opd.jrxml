	<queryString><![CDATA[select 

  expiry_details.expiry_details_id as serial_no,expiry_details.expiry_date,expiry_details.expiry_time,expiry_details.d_death_cause_text,
      (patient1.p_first_name::text || ' ' || coalesce(patient1.p_middle_name,'') || ' ' || coalesce(patient1.p_last_name,'')) as patient_name,
    
     patient1.age as age,mas_administrative_sex.administrative_sex_name as administrative_sex_name,
     case when cast(SUBSTRING(patient1.age, 1, 2) as integer) >=1 then patient1.age else ' ' end as age1,
     case when SUBSTRING(patient1.age, 4, 8) = 'Days' then patient1.age else ' ' end as age2,
     case when SUBSTRING(patient1.age, 4, 9) = 'Months' then patient1.age else ' ' end as age3
     from patient patient1  
     left outer join expiry_details expiry_details on patient1.hin_id=expiry_details.hin_id 
     left outer join mas_administrative_sex mas_administrative_sex on patient1.sex_id=mas_administrative_sex.administrative_sex_id

     left outer join patient patient2  on patient2.hin_id =expiry_details.hin_id
where patient1.hin_id=$P{hinId}
]]></queryString>

