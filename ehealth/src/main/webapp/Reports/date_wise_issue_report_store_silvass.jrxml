	<queryString><![CDATA[select   stg.group_name,t.item_id,i.nomenclature,sum(t.qty_issued) as qty_issued,
(select (final_cost_price/received_qty)as costprice from store_grn_t st where st.item_id=t.item_id order by costprice desc limit 1 offset 0) as costprice,mh.hospital_name||' '||mh.address as hosp from store_issue_t t 
left outer join store_issue_m m on m.id=t.issue_m_id
left outer join mas_store_item i on i.item_id=t.item_id
left outer join mas_hospital mh on mh.hospital_id=m.hospital_id 
left outer join mas_store_group stg on stg.group_id=i.group_id
where m.department_id=$P{deptId} and m.issue_date between $P{fromDate} and $P{toDate} and i.group_id=$P{groupId} and m.hospital_id=$P{hospitalId}
group by t.item_id,i.nomenclature,mh.hospital_name,mh.address,stg.group_name
order by i.nomenclature]]></queryString>

