	<queryString><![CDATA[SELECT
     store_internal_indent_m.demand_no AS store_internal_indent_m_demand_no,
     mas_department.department_name AS mas_department_department_name,
     mas_store_item.pvms_no AS mas_store_item_pvms_no,
     mas_store_item.nomenclature AS mas_store_item_nomenclature,
     store_internal_indent_t.qty_request AS store_internal_indent_t_qty_request
FROM
     store_internal_indent_m store_internal_indent_m 
     LEFT OUTER JOIN store_internal_indent_t store_internal_indent_t ON store_internal_indent_m.id = store_internal_indent_t.internal_id
     LEFT OUTER JOIN mas_store_item mas_store_item ON store_internal_indent_t.item_id = mas_store_item.item_id
     LEFT OUTER JOIN mas_department mas_department ON store_internal_indent_m.to_store = mas_department.department_id
where store_internal_indent_m.demand_no = $P{demand_no} and store_internal_indent_m.department_id = $P{deptId}
 and  store_internal_indent_m.hospital_id=$P{hospitalId}
order by mas_store_item.nomenclature]]></queryString>

