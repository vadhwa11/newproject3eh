	<parameter name="QUERY" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
     maschargecode.charge_code_name as mas_charge_code_charge_code_name,
     massubchargecode.sub_chargecode_name as mas_sub_chargecode_sub_chargecode_name,
     mashospital.hospital_name as mas_hospital_hospital_name,
     mashospital.Address as mas_hospital_Address,
     masdepartment.department_name as mas_department_department_name,
     patient.hin_no as patient_hin_no,
    (COALESCE(patient.p_first_name,' ')||COALESCE(patient.p_middle_name,'  ')||COALESCE(patient.p_last_name,'  '))as patientname,
     patient.age as patient_age,
     masadministrativesex.administrative_sex_name as mas_administrative_sex_administrative_sex_name,
     maschargecode.charge_code_code as mas_charge_code_charge_code_code,
     dgorderdt.order_qty as dg_orderdt_order_qty,
     visit.visit_id as visit_visit_id,
      dgorderhd.order_no,
     dgorderhd.clinical_note as dg_orderhd_clinical_note,
      dgorderhd.order_no as order_no,dgorderdt.amount
from
     dg_orderhd dgorderhd left outer join mas_hospital mashospital on dgorderhd.hospital_id = mashospital.hospital_id
     left outer join mas_department masdepartment on dgorderhd.department_id = masdepartment.department_id
     left outer join patient patient on dgorderhd.hin_id = patient.hin_id
     right outer join dg_orderdt dgorderdt on dgorderhd.orderhd_id = dgorderdt.orderhd_id
     left outer join visit visit on dgorderhd.visit_id = visit.visit_id
     left outer join mas_charge_code maschargecode on dgorderdt.charge_code_id = maschargecode.charge_code_id
     left outer join mas_sub_chargecode massubchargecode on dgorderdt.sub_chargeid = massubchargecode.sub_chargecode_id
     left outer join mas_administrative_sex masadministrativesex on patient.sex_id = masadministrativesex.administrative_sex_id
     left outer join inpatient inpatient on dgorderhd.inpatient_id = inpatient.inpatient_id

$P!{QUERY}

order by

     mas_sub_chargecode_sub_chargecode_name asc]]></queryString>

