	mashospital.address as address,
  msdept.department_id as department_name,
     masmainchargecode.main_chargecode_name as mas_main_chargecode_main_chargecode_name
from
     mas_department msdept right outer join dg_result_entry_header dgresultentryheader on msdept.department_id = dgresultentryheader.department_id
     left outer join dg_result_entry_detail dgresultentrydetail on dgresultentryheader.result_entry_id = dgresultentrydetail.result_entry_id
