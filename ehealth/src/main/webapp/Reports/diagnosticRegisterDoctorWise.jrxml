     left outer join mas_sub_chargecode massubchargecode on dgresultentryheader.sub_chargecode_id = massubchargecode.sub_chargecode_id
     left outer join mas_hospital mashospital on dgresultentryheader.hospital_id = mashospital.hospital_id
     left outer join mas_main_chargecode masmainchargecode on dgresultentryheader.main_chargecode_id = masmainchargecode.main_chargecode_id
     left outer join dg_orderhd dgorderhd on dgsamplecollectionheader.order_id = dgorderhd.orderhd_id

$P!{qry}
group by msdept.department_id, dgresultentryheader.result_date, dgorderhd.patient_type,massubchargecode.sub_chargecode_name, mashospital.hospital_name,masmainchargecode.main_chargecode_name,mashospital.address]]></queryString>

