		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select h.hospital_name,h.address ,b.donation_no,b.doner_name,b.state_id,s.administrative_sex_name,

(st.state_name||' '||COALESCE(ta.taluk_name,'')||' '||COALESCE(b.house_apt_no,'')||' '||COALESCE(po.post_code_name,'')||' '||COALESCE(b.pin_code,''))as full_address,
age,b.mob_no,bg.blood_group_name,b.rh_factor,b.mob_no,
DATE(b.date_of_birth) AS date_of_birth1
from blood_donation_entry_header b
left outer join  mas_administrative_sex s on b.sex_id=s.administrative_sex_id
left outer join  mas_hospital h on b.hospital_id=h.hospital_id
left outer join  mas_blood_group bg on b.blood_group_id=bg.blood_group_id
left outer join  mas_state st on b.state_id=st.state_id
left outer join  mas_taluk ta on ta.taluk_id=ta.taluk_id
left outer join  mas_post_code po on po.post_code_id=b.post_office
 where  b.donation_id = $P{donorRegSerialNo} Limit 1;]]></queryString>

