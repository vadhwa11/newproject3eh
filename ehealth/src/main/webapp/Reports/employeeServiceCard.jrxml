	<queryString><![CDATA[select
     concat(mas_employee.`first_name`," ",mas_employee.`middle_name`," ",mas_employee.`last_name`)as employee,
     mas_employee.`employee_code` as mas_employee_employee_code,
     mas_department.`department_name` as mas_department_department_name,
     hr_employee_personel_details.`date_of_birth` as hr_employee_personel_details_date_of_birth,
     mas_administrative_sex.`administrative_sex_name` as mas_administrative_sex_administrative_sex_name,
     hr_employee_personel_details.`pan_no` as hr_employee_personel_details_pan_no,
     mas_employee.`permanent_address` as mas_employee_permanent_address,
     mas_employee.`residential_address` as mas_employee_residential_address,
     mas_rank.`rank_name` as mas_rank_rank_name,
     mas_employee.`bank_account_number` as mas_employee_bank_account_number,
     mas_employee.`email` as mas_employee_email,
     mas_employee.`join_date` as mas_employee_join_date,
     mas_employee.`tel_no_residence` as mas_employee_tel_no_residence,
     mas_employee.`tel_no_office` as mas_employee_tel_no_office,
     mas_hospital.`hospital_name` as mas_hospital_hospital_name,
     hr_mas_employee_education.`qualification_id` as hr_mas_employee_education_qualification_id,
     mas_qualification.`qualification_name` as mas_qualification_qualification_name,
     mas_course.`course_name` as mas_course_course_name,
     mas_employee.`confirmation_due_date` as mas_employee_confirmation_due_date,
     hr_payroll_process_header.`year` as hr_payroll_process_header_year,
     hr_payroll_process_header.`month` as hr_payroll_process_header_month,
     hr_payroll_process_header.`net_salary` as hr_payroll_process_header_net_salary,
     mas_rank_a.`rank_name` as mas_rank_a_rank_name
from
     `mas_hospital` mas_hospital right outer join `mas_employee` mas_employee on mas_hospital.`hospital_id` = mas_employee.`hospital_id`
     left outer join `mas_department` mas_department on mas_employee.`department_id` = mas_department.`department_id`
     left outer join `hr_employee_personel_details` hr_employee_personel_details on mas_employee.`personal_details_id` = hr_employee_personel_details.`id`
     left outer join `mas_rank` mas_rank on mas_employee.`rank_id` = mas_rank.`rank_id`
     left outer join `hr_mas_employee_education` hr_mas_employee_education on mas_employee.`employee_id` = hr_mas_employee_education.`employee_id`
     left outer join `hr_payroll_process_header` hr_payroll_process_header on mas_employee.`employee_id` = hr_payroll_process_header.`employee_id`
     left outer join `mas_rank` mas_rank_a on hr_payroll_process_header.`rank_id` = mas_rank_a.`rank_id`
     left outer join `mas_qualification` mas_qualification on hr_mas_employee_education.`qualification_id` = mas_qualification.`qualification_id`
     left outer join `mas_course` mas_course on hr_mas_employee_education.`course_id` = mas_course.`course_id`
     left outer join `mas_administrative_sex` mas_administrative_sex on hr_employee_personel_details.`gender_id` = mas_administrative_sex.`administrative_sex_id`
where
     mas_employee.`employee_id` = $P{empId}


group by hr_payroll_process_header.`rank_id`
]]></queryString>

