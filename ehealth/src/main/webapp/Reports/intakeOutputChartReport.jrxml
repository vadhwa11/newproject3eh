	<parameter name="hinNo" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select    
  (patient.p_first_name+' '+ISNULL(patient.p_last_name,' '))as patientname,
ipd_temperature.time as ipd_temperature_time,
   ipd_temperature.temperature as temperature,
	ipd_temperature.pulse as pulse,
	ipd_temperature.respiration as respiration,
    ipd_temperature.bp as ipd_temperature_bp,
    ipd_temperature.spo2 as spo2,
	ipd_intake.time as ipd_intake_time,
     ipd_intake.intake as ipd_intake_intake,
     ipd_intake.iv as ipd_intake_iv,
    ipd_intake.oral as ipd_intake_oral,
    ipd_output.time as ipd_output_time,
   ipd_output.urine as ipd_output_urine,
  ipd_output.stool as ipd_output_stool,
    (vom+'/'+asp)as vomasp,
     inpatient.age as inpatient_age,
     inpatient.ad_no as inpatient_ad_no,
     inpatient.date_of_addmission as inpatient_date_of_addmission,
     mas_diagnosis_conclusion.diagnosis_conclusion_name as mas_diagnosis_conclusion_diagnosis_conclusion_name,
     patient.hin_no as patient_hin_no,
     inpatient.bed_id as bed_id,
ipd_intake_output_chart.treatment as treatment,
mas_department.department_name as wardName,
mas_administrative_sex.administrative_sex_name as gender,
mas_hospital.hospital_name as hos_name
     
from
     patient patient inner join inpatient inpatient on patient.hin_id = inpatient.hin_id
     left join ipd_intake_output_chart ipd_intake_output_chart on patient.hin_id = ipd_intake_output_chart.hin_id
     left join ipd_intake ipd_intake on ipd_intake_output_chart.intakeoutput_id = ipd_intake.intakeoutput_id
     left join ipd_output ipd_output on ipd_intake_output_chart.intakeoutput_id = ipd_output.intakeoutput_id
     left join ipd_temperature ipd_temperature on ipd_intake_output_chart.intakeoutput_id = ipd_temperature.intakeoutput_id
     left join mas_diagnosis_conclusion mas_diagnosis_conclusion on inpatient.diagnosis_id = mas_diagnosis_conclusion.diagnosis_conclusion_id
left join mas_department mas_department on inpatient.ad_ward_id = mas_department.department_id
left join mas_administrative_sex mas_administrative_sex on patient.sex_id = mas_administrative_sex.administrative_sex_id
left join mas_hospital mas_hospital on ipd_intake_output_chart.hospital_id=mas_hospital.hospital_id

where
     patient.hin_no = $P{hinNo}
     and inpatient.ad_no = $P{adNo}

     and ipd_intake_output_chart.date between $P{fromDate} and $P{toDate}
]]></queryString>

