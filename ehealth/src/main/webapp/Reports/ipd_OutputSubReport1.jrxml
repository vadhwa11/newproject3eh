	<queryString><![CDATA[SELECT
     ipd_output.date AS ipd_output_date,
    sum(cast(ipd_output.drain as int)) as sumDrain,
      sum(cast(ipd_output.girth as int )) as sumGirth,
     sum(cast(ipd_output.blood_sugar as int )) as sumSugar,
     sum(cast(ipd_output.insulin as int))as sumInsuline
   
FROM
     ipd_intake_output_chart ipd_intake_output_chart
     LEFT OUTER JOIN ipd_output ipd_output ON ipd_intake_output_chart.intakeoutput_id = ipd_output.intakeoutput_id
WHERE
     ipd_intake_output_chart.ad_no = $P{adNo} and ipd_intake_output_chart.hin_id = $P{hin_id}

group by  ipd_output.date]]></queryString>

