	<queryString><![CDATA[select 
msi.nomenclature,mm.manufacturer_name,hospital_name,sibs.manufacture_date,sibs.batch_no,
sibs.expiry_date,SUM(sibs.closing_stock) as stock

  from store_item_batch_stock sibs left outer join mas_store_item msi ON sibs.item_id=msi.item_id
left outer join mas_manufacturer mm ON sibs.manufacturer_id=mm.manufacturer_id
left outer join mas_hospital mh ON mh.hospital_id=sibs.hospital_id


 where  sibs.hospital_id = $P{hospitalId} and  expiry_date between  $P{fromDate} and $P{toDate}  

 GROUP BY msi.nomenclature,mm.manufacturer_name,hospital_name,sibs.manufacture_date,sibs.batch_no,sibs.expiry_date,sibs.closing_stock]]></queryString>

