	</parameter>
	<queryString><![CDATA[select
  
    patient.age as patient_age,
     patient.date_of_birth as dob,
     visit_date,
hin_no,
     hosp.hospital_name as mas_hospital_hospital_name,
       (patient.p_first_name||coalesce(patient.p_middle_name,'')||coalesce(patient.p_last_name,''))as patientname,
     mas.administrative_sex_name as mas_administrative_sex_administrative_sex_name,
coalesce(mv.village_name, '') || coalesce(md.district_name, '') || coalesce(ms.state_name, '') as address,
opdsd.*,presenting_complaints,
CASE WHEN opdsd.hypertension='y'  THEN 'Yes' else 'No' end as hypertension_y,
CASE WHEN opdsd.diabetes='y'  THEN 'Yes' else 'No' end as diabetes_y,
CASE WHEN opdsd.tb='y'  THEN 'Yes' else 'No' end as tb_y,
CASE WHEN opdsd.cardiac='y'  THEN 'Yes' else 'No' end as cardiac_y,
CASE WHEN opdsd.hepatic='y'  THEN 'Yes' else 'No' end as hepatic_y,
CASE WHEN opdsd.renal='y'  THEN 'Yes' else 'No' end as renal_y,
CASE WHEN opdsd.past_history_others='y'  THEN 'Yes' else 'No' end as past_history_others_y,
msg.*
from
     patient patient right outer join 
     opd_dermatology_leprosy_proforma opdsd on patient.hin_id = opdsd.hin_id
    	left outer join opd_patient_details opd on opd.id=opdsd.opd_patient_details
     left outer join visit visit ON patient.hin_id = visit.hin_id
     left outer join mas_hospital hosp on hosp.hospital_id = opd.hospital_id
     left outer join mas_administrative_sex mas on patient.sex_id = mas.administrative_sex_id
	 left outer join opd_dermatological_leprosy_examination msg on opdsd.leprosy_proforma_id=msg.leprosy_proforma_id
left join patient_address pa on pa.hin_id = patient.hin_id and pa.address_type_id = 
CASE
    WHEN (EXISTS(select address from patient_address where address_type_id = 2)) THEN 2
    WHEN (EXISTS(select address from patient_address where address_type_id = 1)) THEN 1
    ELSE 4
END
left join mas_village mv on mv.village_id = pa.village
left join mas_district md on md.district_id = pa.district 
left join mas_state ms on ms.state_id = pa.state_id
where
	
 	opdsd.opd_patient_details = $P{opdPatientDetailId} and opd.hospital_id=$P{hospitalId}]]></queryString>

