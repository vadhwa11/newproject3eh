     mas_ot.ot_name AS mas_ot_distribution_ot_id,
     mas_ot_distribution.validity_start_date AS mas_ot_distribution_validity_start_date,
  mas_department.department_name AS mas_department_department_name,
 	CASE
    WHEN mas_ot_distribution.status = 'y'  or mas_ot_distribution.status = 'Y' THEN 'Active'
    ELSE 'Inactive'
 	 END  as  mas_ot_distribution_status
   
