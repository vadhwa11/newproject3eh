	<queryString><![CDATA[select d.result_entry_id,dsm.investigation_name,
case 
when d.sub_investigation_id is null then '' 
when d.sub_investigation_id is not null then sm.sub_investigation_name
else '' end as subInvestigationName
,uom_name,d.remarks,ms.sample_description,
case when v.normalValue!='0' then v.normalValue else v.min_normal_value||'-'||v.max_normal_value end as normalValue,
case when d.fixed_id!='0' then fv.fixed_value else d.result end as result,('[ '||cc.charge_code_name||' ]') as charge_code_name
from dg_result_entry_header dreh
left outer join dg_result_entry_detail d on dreh.result_entry_id=d.result_entry_id
left outer join dg_sample_collection_header dsch on dreh.sample_collection_header_id=dsch.sample_collection_header_id
left outer join dg_orderhd dgo on dsch.order_id=dgo.orderhd_id
left join visit visit on dgo.visit_id = visit.visit_id
left join dg_mas_investigation dsm on d.investigation_id=dsm.investigation_id
left join dg_sub_mas_investigation sm on d.sub_investigation_id=sm.sub_investigation_id
left join dg_normal_value v on d.normal_id=v.normal_id
left join dg_fixed_value fv on d.fixed_id=fv.fixed_id
left join dg_uom u on d.uom_id=u.uom_id
left join mas_sample ms on d.sample_id=ms.sample_id
left join mas_charge_code cc on d.charge_code_id=cc.charge_code_id
where visit.visit_id=$P{visitId} 
order by dsm.test_order_no asc]]></queryString>

