	<queryString><![CDATA[SELECT
     inpatient.inpatient_id AS VISIT_VISIT_ID,
     inpatient.ad_no AS VISIT_VISIT_NO,
     PATIENT.HIN_ID AS PATIENT_HIN_ID,
     PATIENT.SERVICE_NO AS PATIENT_SERVICE_NO,
     PATIENT.HIN_NO AS PATIENT_HIN_NO,
     PATIENT_INVESTIGATION_HEADER.HIN_ID AS PATIENT_INVESTIGATION_HEADER_H,
     PATIENT_INVESTIGATION_HEADER.VISIT_ID AS PATIENT_INVESTIGATION_HEADER_V,
     MAS_CHARGE_CODE.CHARGE_CODE_NAME AS MAS_CHARGE_CODE_CHARGE_CODE_NA,
     PATIENT_INVESTIGATION_DETAILS.QUANTITY AS PATIENT_INVESTIGATION_QTY,
     PATIENT_INVESTIGATION_DETAILS.CLINICAL_NOTES AS CLINICAL_NOTES,
    ( case when PATIENT_INVESTIGATION_DETAILS.refer_to_mh='n' then '' else 'Yes' end) AS refer_to_mh
FROM
     HMS.PATIENT PATIENT INNER JOIN HMS.inpatient inpatient ON PATIENT.HIN_ID = inpatient.HIN_ID
     INNER JOIN HMS.PATIENT_INVESTIGATION_HEADER PATIENT_INVESTIGATION_HEADER ON inpatient.inpatient_id = PATIENT_INVESTIGATION_HEADER.inpatient_id
     INNER JOIN HMS.PATIENT_INVESTIGATION_DETAILS PATIENT_INVESTIGATION_DETAILS ON PATIENT_INVESTIGATION_HEADER.INVESTIGATION_HEADER_ID = PATIENT_INVESTIGATION_DETAILS.INVESTIGATION_HEADER_ID
     INNER JOIN HMS.MAS_CHARGE_CODE MAS_CHARGE_CODE ON PATIENT_INVESTIGATION_DETAILS.CHARGE_CODE_ID = MAS_CHARGE_CODE.CHARGE_CODE_ID

where PATIENT_INVESTIGATION_HEADER.inpatient_id=$P{inpatient_id} 
and PATIENT_INVESTIGATION_HEADER.investigation_date=$P{caseSheetDate}]]></queryString>

