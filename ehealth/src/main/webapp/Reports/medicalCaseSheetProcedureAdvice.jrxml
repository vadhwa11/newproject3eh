	<queryString><![CDATA[SELECT VISIT.VISIT_NO AS visitNo,
     PATIENT.SERVICE_NO AS SERVICE_NO,
     PATIENT.HIN_NO AS HIN_NO,
    nvl2(procedure_id,mas_nursing_care.nursing_name,procedure_name) as procedureName,
     procedure_details.REMARKS as remarks
 from procedure_details procedure_details 
left join procedure_header procedure_header 
on procedure_details.procedure_header_id = procedure_header.procedure_header_id
 left join patient patient on procedure_header.hin_id = patient.hin_id
left join visit visit on procedure_header.visit_id = visit.visit_id
left join mas_nursing_care mas_nursing_care on procedure_details.procedure_id = mas_nursing_care.nursing_id
where
visit.visit_id=$P{visit_id}]]></queryString>

