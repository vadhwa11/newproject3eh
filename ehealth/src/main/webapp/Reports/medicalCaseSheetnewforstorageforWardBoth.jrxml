	<queryString><![CDATA[SELECT
     (patient.p_first_name || ' ' || patient.p_middle_name|| ' ' ||patient.p_last_name)AS patient_name,
      patient.sex_id AS patient_sex_id,
     patient.age AS patient_age,
     mas_administrative_sex.administrative_sex_name AS administrative_sex_name,
     patient.relation_id AS patient_relation_id,
     mas_relation_A.relation_name AS A_relation_name,
     patient.hin_no AS patient_hin_no,
     mas_rank.rank_name AS mas_rank_rank_name,
     (patient.s_first_name|| ' ' ||patient.s_middle_name|| ' ' ||patient.s_last_name)AS service_name,
    mas_marital_status.MARITAL_STATUS_NAME as marital_status,
     mas_blood_group.blood_group_name as bloodgroup,
     patient.unit_id AS patient_unit_id,
    patient.date_of_birth as dob,
     mas_unit.unit_name AS mas_unit_unit_name,
     patient.service_type_id AS patient_service_type_id,
     mas_service_type.service_type_name AS service_type_name,
     patient.trade_id AS patient_trade_id,
     mas_trade.trade_name AS mas_trade_trade_name,
     inpatient.hin_id AS inpatient_hin_id,
     inpatient.date_of_addmission AS inpatient_visit_date,
     inpatient.ad_no as adNo,
     OPD_PATIENT_DETAILS.initial_diagnosis AS working_diagnosis,
     patient.rank_id AS patient_rank_id,
     mas_hospital.hospital_name AS hospital_name,
     mas_hospital.address AS hospital_address,
     patient.service_no as serviceNo,
     mas_occupation.occupation_name as occupation,
     CATEGORY.CATEGORIES AS CATEGORY_NAME,
    OPD_PATIENT_DETAILS.BP AS OPD_PATIENT_DETAILS_BP,
     OPD_PATIENT_DETAILS.PLAN AS OPD_PATIENT_DETAILS_PLAN,
     OPD_PATIENT_DETAILS.AFMS_DESC AS OPD_PATIENT_DETAILS_AFMS_DESC,
     OPD_PATIENT_DETAILS.NEXT_VISIT_DATE AS OPD_PATIENT_DETAILS_NEXT_VISIT,
     OPD_PATIENT_DETAILS.CONSULTATION_DATE AS OPD_PATIENT_DETAILS_CONSULTATI,
     OPD_PATIENT_DETAILS.CONSULTATION_TIME AS OPD_PATIENT_DETAILS_CONSULTATI,
     OPD_PATIENT_DETAILS.REFERRED_DEPT AS OPD_PATIENT_DETAILS_REFERRED_D,
     OPD_PATIENT_DETAILS.INITIAL_DIAGNOSIS AS OPD_PATIENT_DETAILS_INITIAL_DI,
     OPD_PATIENT_DETAILS.OPD_DATE AS OPD_PATIENT_DETAILS_OPD_DATE,
     OPD_PATIENT_DETAILS.OPD_TIME AS OPD_PATIENT_DETAILS_OPD_TIME,
     OPD_PATIENT_DETAILS.ON_EXAMINATION AS OPD_PATIENT_DETAILS_ON_EXAMINA,
     OPD_PATIENT_DETAILS.REFERED_DOCTARS AS OPD_PATIENT_DETAILS_REFERED_DO,
     OPD_PATIENT_DETAILS.DISPOSAL AS OPD_PATIENT_DETAILS_DISPOSAL,
     OPD_PATIENT_DETAILS.DAYS AS OPD_PATIENT_DETAILS_DAYS,
     OPD_PATIENT_DETAILS.RETURNFROMHOSPITAL AS OPD_PATIENT_DETAILS_RETURNFROM,
     OPD_PATIENT_DETAILS.HOSPNAME AS OPD_PATIENT_DETAILS_HOSPNAME,
     OPD_PATIENT_DETAILS.DOA AS OPD_PATIENT_DETAILS_DOA,
     OPD_PATIENT_DETAILS.DOD AS OPD_PATIENT_DETAILS_DOD,
     OPD_PATIENT_DETAILS.GPE_EXAMINATION AS OPD_PATIENT_DETAILS_GPE_EXAMIN,
     OPD_PATIENT_DETAILS.SYSTAMICEXAM AS OPD_PATIENT_DETAILS_SYSTAMICEX,
     OPD_PATIENT_DETAILS.HEIGHT AS OPD_PATIENT_DETAILS_HEIGHT,
     OPD_PATIENT_DETAILS.WEIGHT AS OPD_PATIENT_DETAILS_WEIGHT,
     OPD_PATIENT_DETAILS.PULSE AS OPD_PATIENT_DETAILS_PULSE,
     OPD_PATIENT_DETAILS.TEMPERATURE AS OPD_PATIENT_DETAILS_TEMPERATUR,
     OPD_PATIENT_DETAILS.RR AS OPD_PATIENT_DETAILS_RR,
     OPD_PATIENT_DETAILS.BMI AS OPD_PATIENT_DETAILS_BMI,
     OPD_PATIENT_DETAILS.PHY_STATUS AS OPD_PATIENT_DETAILS_PHY_STATUS,
     OPD_PATIENT_DETAILS.ADVICEONDISCHARGE AS OPD_PATIENT_DETAILS_ADVICEONDI,
     OPD_PATIENT_DETAILS.PASTDIAGNOSIS AS OPD_PATIENT_DETAILS_PASTDIAGNO,
     OPD_PATIENT_DETAILS.PASTDISPOSAL AS OPD_PATIENT_DETAILS_PASTDISPOS,
     OPD_PATIENT_DETAILS.DAYSPHY AS OPD_PATIENT_DETAILS_DAYSPHY,
     OPD_PATIENT_DETAILS.THREPYTYPEID AS OPD_PATIENT_DETAILS_THREPYTYPE,
     OPD_PATIENT_DETAILS.DURATIONPHY AS OPD_PATIENT_DETAILS_DURATIONPH,
     OPD_PATIENT_DETAILS.MH_RUN AS OPD_PATIENT_DETAILS_MH_RUN,
     OPD_PATIENT_DETAILS.ideal_weight AS ideal_weight,
    opd_patient_history.present_complain as complaint,
    opd_patient_history.pastmedicalhistory as pastmedicalhistory,
    opd_patient_history.family_past_History as familyhistory,
   opd_patient_history.riskfactor as riskfactor,
 OPD_PATIENT_DETAILS.initial_diagnosis as workingDiagnosis ,
inpatient.inpatient_id as inpatient_id
 
FROM   OPD_PATIENT_DETAILS OPD_PATIENT_DETAILS 
 LEFT OUTER JOIN inpatient inpatient ON OPD_PATIENT_DETAILS.inpatient_id = inpatient.inpatient_id
 LEFT OUTER JOIN patient patient on inpatient.hin_id=patient.hin_id
left join
     mas_administrative_sex mas_administrative_sex  ON mas_administrative_sex.administrative_sex_id = patient.sex_id
     LEFT OUTER JOIN mas_relation mas_relation ON patient.relation_id = mas_relation.relation_id
     LEFT OUTER JOIN mas_relation mas_relation_A ON patient.relation_id = mas_relation_A.relation_id
     LEFT OUTER JOIN mas_rank mas_rank ON patient.rank_id = mas_rank.rank_id
     LEFT OUTER JOIN mas_unit mas_unit ON patient.unit_id = mas_unit.unit_id
     LEFT OUTER JOIN mas_service_type mas_service_type ON patient.service_type_id = mas_service_type.service_type_id
     LEFT OUTER JOIN mas_trade mas_trade ON patient.trade_id = mas_trade.trade_id
    left join mas_occupation mas_occupation on patient.occupation_id = mas_occupation.occupation_id
  left join mas_marital_status mas_marital_status on patient.marital_status_id = mas_marital_status.marital_status_id
    left join mas_blood_group mas_blood_group on patient.BLOOD_GROUP_ID = mas_blood_group.BLOOD_GROUP_ID
    
     LEFT OUTER JOIN mas_hospital mas_hospital ON OPD_PATIENT_DETAILS.hospital_id = mas_hospital.hospital_id
     LEFT OUTER JOIN CATEGORY CATEGORY ON patient.category_id = CATEGORY.CATEGORYID
     
     left join opd_patient_history opd_patient_history on OPD_PATIENT_DETAILS.Id = opd_patient_history.opd_patient_details_id
WHERE
    
   inpatient.ad_no = $P{adNo}
     And mas_hospital.hospital_id=$P{hospitalId}
And OPD_PATIENT_DETAILS.OPD_DATE in (select opd_date from temp_case_sheet where hin_no=$P{hinNo}
and hin_id=$P{hinId})
and OPD_PATIENT_DETAILS.OPD_time =$P{caseSheetTime}
order by OPD_PATIENT_DETAILS.OPD_DATE asc,OPD_PATIENT_DETAILS.OPD_time asc]]></queryString>

