	<queryString><![CDATA[select
     (mas_employee.first_name+' '+ISNULL(mas_employee.middle_name,'')+' '+ISNULL(mas_employee.last_name,''))as ename,
     mas_employee.employee_code as mas_employee_employee_code,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
     mas_employee.bank_account_code as mas_employee_bank_account_code,
     mas_employee.bank_account_number as mas_employee_bank_account_number,
     hr_payroll_process_header.year as hr_payroll_process_header_year,
     hr_payroll_process_header.month as hr_payroll_process_header_month,
     hr_payroll_process_header.net_salary as hr_payroll_process_header_net_salary,
     hr_payroll_process_header.employee_id as hr_payroll_process_header_employee_id,
     mas_employee.bank_code as mas_employee_bank_code
from
     mas_employee mas_employee inner join hr_payroll_process_header hr_payroll_process_header on mas_employee.employee_id = hr_payroll_process_header.employee_id
     inner join mas_hospital mas_hospital on hr_payroll_process_header.hospital_id = mas_hospital.hospital_id
where
    mas_employee.employee_code between $P{fromempcode} and  $P{toempcode}
    and hr_payroll_process_header.year = $P{year}
    and hr_payroll_process_header.month = $P{month}]]></queryString>

