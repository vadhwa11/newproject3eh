	<parameter name="opdvid" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
     visit.visit_no as visit_no,
     patient.age as patient_age,
     visit.visit_date as visit_date,
     hosp.hospital_name as mas_hospital_hospital_name,
     patient.hin_no as patient_hin_no,
     opdsd.sp_values2,
     opdsd.sp_values,
     (patient.p_first_name||coalesce(patient.p_middle_name,'')||coalesce(patient.p_last_name,''))as patientname,
     mas.administrative_sex_name as mas_administrative_sex_administrative_sex_name,
	msp.sp_parameter_name,
	mst.template_name,msg.sp_group_name,msg.display
from
     patient patient right outer join 
     opd_speciality_details opdsd on patient.hin_id = opdsd.hin_id
     left outer join visit visit on opdsd.visit_id = visit.visit_id
     left outer join mas_hospital hosp on opdsd.hospital_id = hosp.hospital_id
     left outer join mas_administrative_sex mas on patient.sex_id = mas.administrative_sex_id
     left outer join mas_specialty_template mst on opdsd.sp_template_id = mst .template_id
     left outer join mas_speciality_parameter msp on opdsd.sp_parameter_id = msp.sp_parameter_id
 left outer join mas_speciality_group msg on opdsd.sp_group_id = msg.sp_group_id

where

 	opdsd.visit_id = $P{opdvid}

order by opdsd.opd_speciality_details_id]]></queryString>

