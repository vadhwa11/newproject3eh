	<queryString><![CDATA[SELECT patient.hin_no as hin_no,
     opd_oncosurgery_case_sheet.fanc_no AS opd_oncosurgery_case_sheet_fanc_no,
     opd_oncosurgery_case_sheet.city_scan AS opd_oncosurgery_case_sheet_city_scan,
     opd_oncosurgery_case_sheet.others AS opd_oncosurgery_case_sheet_others,
     opd_oncosurgery_case_sheet.biopsy_no AS opd_oncosurgery_case_sheet_biopsy_no,
     opd_oncosurgery_case_sheet.clinical_diagnosis AS opd_oncosurgery_case_sheet_clinical_diagnosis,
     opd_oncosurgery_case_sheet.tumour AS opd_oncosurgery_case_sheet_tumour,
     opd_oncosurgery_case_sheet.lx AS opd_oncosurgery_case_sheet_lx,
     opd_oncosurgery_case_sheet.mets AS opd_oncosurgery_case_sheet_mets,
     opd_oncosurgery_case_sheet.operation_findings_others AS opd_oncosurgery_case_sheet_operation_findings_others,
     opd_oncosurgery_case_sheet.operation_date_Procedure AS opd_oncosurgery_case_sheet_operation_date_Procedure,
     opd_oncosurgery_case_sheet.hpe AS opd_oncosurgery_case_sheet_hpe,
     visit_A.visit_no AS visit_A_visit_no,
     patient.service_no AS patient_service_no,
     (patient.p_first_name+' '+ISNULL(patient.p_middle_name,'')+' '+ISNULL(patient.p_last_name,''))AS patient_name,
     patient.age AS patient_age,
     visit_A.visit_date AS visit_A_visit_date,
     patient.date_of_birth AS patient_date_of_birth,
     mas_department.department_name AS mas_department_department_name,
     mas_hospital.hospital_name AS mas_hospital_hospital_name,
     opd_patient_details.plan1 AS opd_patient_details_plan,
     patient_investigation_details.clinical_notes AS patient_investigation_details_clinical_notes,
     mas_charge_code.charge_code_name AS mas_charge_code_charge_code_name,
     mas_icd.icd_name AS mas_icd_icd_name
FROM
     visit visit_A RIGHT OUTER JOIN opd_oncosurgery_case_sheet opd_oncosurgery_case_sheet ON visit_A.visit_id = opd_oncosurgery_case_sheet.visit_id
     LEFT OUTER JOIN patient patient ON opd_oncosurgery_case_sheet.hin_id = patient.hin_id
     LEFT OUTER JOIN mas_department mas_department ON opd_oncosurgery_case_sheet.department_id = mas_department.department_id
     LEFT OUTER JOIN mas_hospital mas_hospital ON opd_oncosurgery_case_sheet.hospital_id = mas_hospital.hospital_id
     AND mas_hospital.hospital_id = patient.hospital_id
     LEFT OUTER JOIN patient_investigation_header patient_investigation_header ON patient.hin_id = patient_investigation_header.hin_id
     AND mas_department.department_id = patient_investigation_header.department_id
     AND visit_A.visit_id = patient_investigation_header.visit_id
     LEFT OUTER JOIN patient_investigation_details patient_investigation_details ON patient_investigation_header.investigation_header_id = patient_investigation_details.investigation_header_id
     LEFT OUTER JOIN mas_charge_code mas_charge_code ON patient_investigation_details.charge_code_id = mas_charge_code.charge_code_id
     AND mas_department.department_id = mas_charge_code.department_id
     AND mas_department.department_id = visit_A.department_id
     LEFT OUTER JOIN opd_patient_details opd_patient_details ON visit_A.visit_id = opd_patient_details.visit_id
     LEFT OUTER JOIN discharge_icd_code discharge_icd_code ON visit_A.visit_id = discharge_icd_code.visit_id
     LEFT OUTER JOIN mas_icd mas_icd ON discharge_icd_code.icd_id = mas_icd.icd_id

WHERE
     visit_A.visit_no = $P{visitNo}
     And patient.hin_no = $P{hinNo}]]></queryString>

