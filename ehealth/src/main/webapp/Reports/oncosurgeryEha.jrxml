	<parameter name="opdvid" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select    
     visit_a.visit_no as visit_a_visit_no,
     (patient.p_first_name+' '+ISNULL(patient.p_middle_name,'')+' '+ISNULL(patient.p_last_name,''))as patient_name,
     visit_a.visit_date as visit_a_visit_date,
     mas_department.department_name as mas_department_department_name,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
     mas_hospital.address as mas_hospital_address,
     opd_oncology.stage_t as opd_oncology_stage_t,
     opd_oncology.stage_n as opd_oncology_stage_n,
     opd_oncology.stage_m as opd_oncology_stage_m,
     (case opd_oncology.stage_nor when 1 then 'i' when 2 then 'ii' when 3 then 'iii' when 4 then 'iv' else opd_oncology.stage_nor end)as opd_oncology_stage_nor,
     opd_oncology.stage_a as opd_oncology_stage_a,
     opd_oncology.stage_b as opd_oncology_stage_b,
     opd_oncology.medical_onco as opd_oncology_medical_onco,
     opd_oncology.surgery_onco as opd_oncology_surgery_onco,
     opd_oncology.rt as opd_oncology_rt,
     opd_oncology.remarks as opd_oncology_remarks,
     patient.age as patient_age,
     patient.date_of_birth as patient_date_of_birth,
     patient_investigation_details.quantity as patient_investigation_details_quantity,
     patient_investigation_details.clinical_notes as patient_investigation_details_clinical_notes,
     patient_prescription_details.dosage as patient_prescription_details_dosage,
     patient_prescription_details.no_of_days as patient_prescription_details_no_of_days,
     patient_prescription_details.total as patient_prescription_details_total,
     patient_prescription_details.type as patient_prescription_details_type,
     mas_store_item.nomenclature as mas_store_item_nomenclature,
     mas_charge_code.charge_code_name as mas_charge_code_charge_code_name,
     opd_patient_details.plan1 as opd_patient_details_plan,
     mas_icd.icd_name as mas_icd_icd_name,
     mas_frequency.frequency_name as mas_frequency_frequency_name,
     patient.hin_no as patient_hin_no
from
     visit visit_a right outer join opd_oncology opd_oncology on visit_a.visit_id = opd_oncology.visit_id
     left outer join mas_hospital mas_hospital on opd_oncology.hospital_id = mas_hospital.hospital_id
     left outer join patient patient on visit_a.hin_id = patient.hin_id
     left outer join patient_prescription_header patient_prescription_header on visit_a.visit_id = patient_prescription_header.visit_id
     left outer join mas_department mas_department on visit_a.department_id = mas_department.department_id
     left outer join patient_investigation_header patient_investigation_header on visit_a.visit_id = patient_investigation_header.visit_id
     left outer join opd_patient_details opd_patient_details on visit_a.visit_id = opd_patient_details.visit_id
     left outer join discharge_icd_code discharge_icd_code on visit_a.visit_id = discharge_icd_code.visit_id
     left outer join mas_icd mas_icd on discharge_icd_code.icd_id = mas_icd.icd_id
     and patient.hin_id = patient_investigation_header.hin_id
     left outer join patient_investigation_details patient_investigation_details on patient_investigation_header.investigation_header_id = patient_investigation_details.investigation_header_id
     left outer join mas_charge_code mas_charge_code on patient_investigation_details.charge_code_id = mas_charge_code.charge_code_id
     left outer join patient_prescription_details patient_prescription_details on patient_prescription_header.prescription_id = patient_prescription_details.prescription_id
     left outer join mas_store_item mas_store_item on patient_prescription_details.item_id = mas_store_item.item_id
     left outer join mas_frequency mas_frequency on patient_prescription_details.frequency_id = mas_frequency.frequency_id
where
     opd_oncology.visit_id = $P{opdvid}]]></queryString>

