	<parameter name="opdvid" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select
     visit_a.visit_no as visit_a_visit_no,
     (patient.p_first_name+''+ISNULL(patient.p_middle_name,'')+' '+ISNULL(patient.p_last_name,''))as patient_name,
     patient.age as patient_age,
     visit_a.visit_date as visit_a_visit_date,
     patient.date_of_birth as patient_date_of_birth,
     mas_hospital.hospital_name as mas_hospital_hospital_name,
     opd_case_sheet.clinical_note as opd_case_sheet_clinical_note,
     opd_case_sheet.dob as opd_case_sheet_dob,
     opd_case_sheet.rd_percentile as opd_case_sheet_rd_percentile,
     opd_case_sheet.th_percentile as opd_case_sheet_th_percentile,
     opd_case_sheet.age as opd_case_sheet_age,
     opd_case_sheet.height_in_cm_child as opd_case_sheet_height_in_cm_child,
     opd_case_sheet.height_in_cm_mother as opd_case_sheet_height_in_cm_mother,
     opd_case_sheet.height_in_cm_father as opd_case_sheet_height_in_cm_father,
     opd_case_sheet.remarks as opd_case_sheet_remarks,
     (mas_employee.first_name+' '+ISNULL(mas_employee.middle_name,'')+' '+ISNULL(mas_employee.last_name,''))as mas_employee_name,
     opd_case_sheet.bmi as opd_case_sheet_bmi,
     opd_case_sheet.wt as opd_case_sheet_wt,
     opd_case_sheet.ht as opd_case_sheet_ht,
     patient.hin_no as patient_hin_no
from
     visit visit_a right outer join opd_case_sheet opd_case_sheet on visit_a.visit_id = opd_case_sheet.visit_id
     left outer join patient patient on opd_case_sheet.hin_id = patient.hin_id
     left outer join mas_hospital mas_hospital on opd_case_sheet.hospital_id = mas_hospital.hospital_id
     left outer join mas_employee mas_employee on opd_case_sheet.refered_by = mas_employee.employee_id
where

     opd_case_sheet.visit_id  = $P{opdvid}]]></queryString>

