	<queryString><![CDATA[select distinct mas_physiotherapy_detail.modality_remarks as modality_remarks,mas_physiotherapy_header.hoexamination as hoexamination,
mas_physiotherapy_header.plan_goals as plan_goals, mas_physiotherapy_header.prognosis_reports as prognosis_reports,
mas_physiotherapy_header.visit_id as visit_id,visit.visit_date as visit_date,mas_hospital.hospital_name as hospital_name, mas_hospital.address as address,
patient.hin_id as hin_id,
(mas_employee.first_name+' '+ISNULL(mas_employee.middle_name,'')+' '+ISNULL(mas_employee.last_name,'')) as employee
from 
mas_physiotherapy_header mas_physiotherapy_header  
left outer join mas_physiotherapy_detail mas_physiotherapy_detail on mas_physiotherapy_detail.mp_id=mas_physiotherapy_header.mp_id
left outer join patient patient on mas_physiotherapy_header.hin_id = patient.hin_id
left outer join visit visit on mas_physiotherapy_header.visit_id =visit.visit_id
left outer join mas_hospital mas_hospital on patient.hospital_id =mas_hospital.hospital_id
left outer join mas_employee mas_employee on mas_physiotherapy_header.physiotherapist_id =mas_employee.employee_id

where mas_physiotherapy_header.visit_id =$P{visitId} and mas_physiotherapy_header.hin_id=$P{hinId}]]></queryString>

