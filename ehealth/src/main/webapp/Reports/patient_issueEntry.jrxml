	<queryString><![CDATA[select
     store_op_patient_issue_m.`issue_no` as store_op_patient_issue_m_issue_no,
     store_op_patient_issue_m.`issue_date` as store_op_patient_issue_m_issue_date,
     store_op_patient_issue_m.`prescription_no` as store_op_patient_issue_m_prescription_no,
     concat( mas_employee.`first_name`," ",mas_employee.`last_name`) as doctor_name,
     patient.`service_no` as patient_service_no,
     mas_store_item.`pvms_no` as mas_store_item_pvms_no,
     mas_store_item.`nomenclature` as mas_store_item_nomenclature,
     store_op_patient_issue_t.`qty_issued` as store_op_patient_issue_t_qty_issued,
     concat(patient.`p_first_name`," ",patient.`p_last_name`) as patient_name,
     store_op_patient_issue_t.`batch_no` as store_op_patient_issue_t_batch_no,
     store_op_patient_issue_t.`expiry_date` as store_op_patient_issue_t_expiry_date
from
     `store_op_patient_issue_m` store_op_patient_issue_m left join `store_op_patient_issue_t` store_op_patient_issue_t on store_op_patient_issue_m.`op_issue_id` = store_op_patient_issue_t.`op_issue_id`
    left join `patient` patient on store_op_patient_issue_m.`hin_id` = patient.`hin_id`
     left join `mas_employee` mas_employee on store_op_patient_issue_m.`emp_id` = mas_employee.`employee_id`
     left join `mas_store_item` mas_store_item on store_op_patient_issue_t.`item_id_issue` = mas_store_item.`item_id`
where store_op_patient_issue_m.hin_id = $P{hinId}  

and   store_op_patient_issue_m.department_id = $P{deptId}  
]]></queryString>

