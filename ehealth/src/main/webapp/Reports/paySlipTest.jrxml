	<queryString><![CDATA[select * from
(select pay_amount as hra,hep.employee_id as e1 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=1 )as a

left join

(select pay_amount as sa,hep.employee_id as e2 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=2 )as b on a.e1=b.e2

left join

(select pay_amount as ppay,hep.employee_id as e3 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=3 )as c on a.e1 = c.e3


left join

(select pay_amount as conv,hep.employee_id as e4 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=4)as d on a.e1 = d.e4

left join

(select pay_amount as medi,hep.employee_id as e5 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=5 )as g on a.e1 = g.e5

left join
(select pay_amount as lt,hep.employee_id as e6 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=6 )as h on a.e1 = h.e6

left join
(select pay_amount as mv,hep.employee_id as e7 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=7 )as i on  a.e1 = i.e7



left join
(select pay_amount as pf,hep.employee_id as e9 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=8 )as k on a.e1 = k.e9

left join
(select pay_amount as it,hep.employee_id as e10 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=22 )as l on a.e1 = l.e10

left join
(select pay_amount as grautity,hep.employee_id as e11 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=23 )as m on a.e1 = m.e11

left join
(select pay_amount as pf,hep.employee_id as e12 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=23 )as n on  a.e1 = n.e12

left join
(select pay_amount as super,hep.employee_id as e13 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=24 )as o on a.e1 = o.e13

left join
(select pay_amount as lunch,hep.employee_id as e14 from hr_employee_pay_elements hep
left join hr_mas_pay_element hmpe on hep.pay_element_id=hmpe.pay_element_id
where hep.pay_element_id=25 )as p on a.e1 = p.e14

left join mas_employee mas_employee4 on d.e4=mas_employee4.employee_id
left join `mas_rank` mas_rank4 on mas_employee4.`rank_id` = mas_rank4.`rank_id`
left join `mas_department` mas_department4 on mas_employee4.`department_id` = mas_department4.`department_id`
left join `hr_mas_location` hr_mas_location4 on mas_employee4.`location_id` = hr_mas_location4.`location_id`
left join (select basic_pay,employee_id as e5 from `hr_employee_pay_structure` hr_employee_pay_structure4
where hr_employee_pay_structure4.employee_id=$P{empId} ) as e on e.e5 = mas_employee4.employee_id
left join (select net_salary,total_earning,total_deduction,total_reimb,arrear_salary_amt,arrear_payelement_amt,bonus_amount,advance_installment,loan_installment,sal_days,employee_id as e6 from `hr_payroll_process_header` hr_payroll_process_header4
where hr_payroll_process_header4.employee_id=$P{empId} ) as f on f.e6 = mas_employee4.employee_id

where a.e1=$P{empId}   ;
]]></queryString>

