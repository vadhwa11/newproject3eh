	<queryString><![CDATA[select
     concat(mas_employee.`first_name`,mas_employee.`middle_name`,mas_employee.`last_name`)as ename,
     mas_employee.`employee_code` as mas_employee_employee_code,
     hr_employee_pay_structure.`basic_pay` as hr_employee_pay_structure_basic_pay,
     mas_rank.`rank_name` as mas_rank_rank_name,
     mas_hospital.`hospital_name` as mas_hospital_hospital_name,
     mas_grade.`grade_id` as mas_grade_grade_id,
     hr_employee_pay_structure.`employee_id` as hr_employee_pay_structure_employee_id,
     mas_department.`department_code` as mas_department_department_code,
     mas_grade.`grade_name` as mas_grade_grade_name,
     mas_department.`department_name` as mas_department_department_name
from
    `mas_employee` mas_employee  left join `hr_employee_pay_structure` hr_employee_pay_structure on hr_employee_pay_structure.`employee_id` = mas_employee .`employee_id`
   left join  `mas_rank` mas_rank on mas_rank.`rank_id` = mas_employee.`rank_id`
     left join `mas_grade` mas_grade on mas_employee.`grade_id` = mas_grade.`grade_id`

     left join `mas_department` mas_department on mas_employee.`department_id` = mas_department.`department_id`
     left join `mas_hospital` mas_hospital on mas_employee.`hospital_id` = mas_hospital.`hospital_id`
where
      mas_employee.`employee_code` between $P{fromempcode}   and  $P{toempcode}  

     and mas_grade.`grade_id` = $P{gradeId}  

]]></queryString>

