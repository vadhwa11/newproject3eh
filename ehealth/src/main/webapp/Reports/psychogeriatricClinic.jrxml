	<queryString><![CDATA[select
  patient.hin_no,
patient.date_of_birth as dob,
     patient.age as patient_age,
     hosp.hospital_name as mas_hospital_hospital_name,
     patient.hin_no as patient_hin_no,
  v.visit_date,
CASE WHEN msg.past_history_of_mental_illness='y'  THEN 'Yes' else 'No' end as past_history_of_mental_illness_y,
CASE WHEN msg.past_history_of_physical_illness='y'  THEN 'Yes' else 'No' end as past_history_of_physical_illness_y,
CASE WHEN msg.hypertension='y'  THEN 'Yes' else 'No' end as hypertension_y,
CASE WHEN msg.diabetes_mellitus='y' THEN 'Yes' else 'No' end as diabetes_mellitus_y,
CASE WHEN msg.cerebrovascular_accident='y' THEN 'Yes' else 'No' end as cerebrovascular_accident_y,
CASE WHEN msg.bronchial_asthma='y' THEN 'Yes' else 'No' end as bronchial_asthma_y,
CASE WHEN msg.tuberculosis='y' THEN 'Yes' else 'No' end as tuberculosis_y,
CASE WHEN msg.seizure_disorder='y' THEN 'Yes' else 'No' end as seizure_disorder_y,
CASE WHEN msg.other_physical_illness='y' THEN 'Yes' else 'No' end as other_physical_illness_y,

CASE WHEN msg.family_history_of_mental_illness='y' THEN 'Yes' else 'No' end as family_history_of_mental_illness_y,
CASE WHEN msg.substance_use='y' THEN 'Yes' else 'No' end as substance_use_y,
CASE WHEN msg.premorbid_personality='y' THEN 'Yes' else 'No' end as premorbid_personality_y,
msg.*,
      (patient.p_first_name||coalesce(patient.p_middle_name,'')||coalesce(patient.p_last_name,''))as patientname,
     mas.administrative_sex_name as mas_administrative_sex_administrative_sex_name,
   coalesce(mv.village_name, '') || coalesce(md.district_name, '') || coalesce(ms.state_name, '') as address
from
     opd_psychogeriatric_clinic_speciality  msg
     left outer join patient patient on patient.hin_id = msg.hin_id
	left outer join opd_patient_details opd on opd.id = msg.opd_patient_details
	left outer join visit v on v.visit_id=opd.visit_id
     left outer join mas_hospital hosp on patient.hospital_id = hosp.hospital_id
     left outer join mas_administrative_sex mas on patient.sex_id = mas.administrative_sex_id
left join patient_address pa on pa.hin_id = patient.hin_id and pa.address_type_id =
CASE
    WHEN (EXISTS(select address from patient_address where address_type_id = 2)) THEN 2
    WHEN (EXISTS(select address from patient_address where address_type_id = 1)) THEN 1
    ELSE 4
END
left join mas_village mv on mv.village_id = pa.village
left join mas_district md on md.district_id = pa.district
left join mas_state ms on ms.state_id = pa.state_id
where

 	v.visit_id = $P{opdvid} and opd.hospital_id=$P{hospitalId}]]></queryString>

