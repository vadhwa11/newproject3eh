	<parameter name="hinId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="fromDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="toDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="hospitalId" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select distinct bbh.bill_no,bbh.bill_date,pt.p_first_name,mpc.patient_category_name,ms.scheme_name
,(bbd.rate*quantity) as rate_amt ,bbd.net_amt,bbd.discount_amt,coalesce(brd.amount,0) as amt,
case 
when md.discount_percentage is null or md.discount_value is null then md.fixed_value
when md.fixed_value is null or md.discount_value is null then md.discount_percentage
when md.discount_percentage is null or md.fixed_value is null then md.discount_value
else '0.00' end,mh.hospital_name
from bl_op_bill_details bbd  
left outer join bl_op_bill_header bbh on bbh.op_bill_header_id=bbd.op_bill_header_id
left outer join bl_receipt_header brh on brh.op_bill_header_id=bbh.op_bill_header_id
left outer join bl_receipt_details brd on brd.receipt_header_id=brh.receipt_header_id

left outer join mas_charge_code mcc on mcc.charge_code_id=bbd.charge_code_id
left outer join patient pt on pt.hin_id=bbh.hin_id
left outer join mas_patient_category mpc on mpc.patient_category_id=pt.patient_category
left outer join mas_scheme ms on ms.scheme_id=bbh.scheme_id
left outer join mas_discount md on md.scheme_id=bbh.scheme_id
left outer join mas_hospital mh on mh.hospital_id=bbh.hospital_id
where
 bbh.hospital_id=$P{hospitalId} and 
 bbh.scheme_id is not null  
 and bbh.hin_id=$P{hinId} and bbh.bill_date 
between $P{fromDate} and $P{toDate}
]]></queryString>

