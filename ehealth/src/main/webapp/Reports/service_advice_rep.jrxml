	<queryString><![CDATA[SELECT hin_id,payment_advice_no,inpatient_id,
('cash amt'||'        '||' '||cast(coalesce(cash_advice_amt,0) AS varchar(20))) as cash_amount,
('on accout'||'        '||cast(coalesce(on_account_amt,0) AS varchar(20))) as on_account,
m.charge_code_name,d.advice_amt,d.advice_qty,h.hospital_name,h.address
FROM bl_payment_advice_header b
left outer join bl_payment_advice_details d on b.payment_advice_header_id = d.payment_advice_header_id
left outer join mas_charge_code m on d.charge_id=m.charge_code_id
left outer join mas_hospital as h on b.hospital_id=h.hospital_id
where payment_advice_no=$P{advice_no} and b.hospital_id=$P{hospitalId};]]></queryString>

