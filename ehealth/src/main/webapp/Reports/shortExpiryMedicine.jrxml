	<queryString><![CDATA[select msi.pvms_no,msi.nomenclature,
msic.item_unit_name,
sibs.batch_no,sibs.expiry_date,sibs.closing_stock,sibs.manufacture_date,
mm.manufacturer_name 
from 
     store_item_batch_stock sibs 
     left outer join mas_store_item msi ON msi.item_id =sibs.item_id
     left outer join mas_store_item_conversion msic ON msic.item_conversion_id=msi.item_conversion_id
     left outer join mas_manufacturer mm ON mm.manufacturer_id=sibs.manufacturer_id
where
     sibs.expiry_date >= $P{FROM_DATE}
     and sibs.expiry_date <= $P{TO_DATE}
     and sibs.hospital_id = $P{hospitalId}
     and sibs.department_id = $P{deptId}]]></queryString>

