	<queryString><![CDATA[select
     hr_training_schedule.`training_start_date` as t_start_date,
     hr_training_schedule.`training_end_date` as t_end_date,
     hr_training_evaluation.`average_rating` as eva_average_rating,
     hr_training_effective.`average_points` as eff_average_points,
     concat(mas_employee.`first_name`,mas_employee.`middle_name`,mas_employee.`last_name`) as empname,
      mas_employee.employee_code as ecode,
     hr_mas_training.`training_name` as t_name,
     hr_mas_training.`fees_charged` as t_fees_charged,
     hr_mas_instructor.`instructor_name` as i_name,
    mas_hospital.`hospital_name` as hname,
  hr_mas_instructor.`instructor_name` as i_name,
  mas_department.`department_name` as d_name
from
      `hr_training_effective` hr_training_effective left join  `mas_employee` mas_employee on mas_employee.`employee_id` = hr_training_effective.`employee_id`
     left join `hr_training_evaluation` hr_training_evaluation on mas_employee.`employee_id` = hr_training_evaluation.`employee_id`
     left join `hr_mas_instructor` hr_mas_instructor on hr_training_evaluation.`instructor_id` = hr_mas_instructor.`instructor_id`
     left join `hr_training_schedule` hr_training_schedule on hr_training_schedule.instructor_id = hr_mas_instructor.instructor_id
     left join `hr_mas_training` hr_mas_training on hr_training_schedule.training_id = hr_mas_training.training_id
      left join `mas_department` mas_department on hr_mas_training.department_id = mas_department.department_id
      left join `mas_hospital` mas_hospital on mas_hospital.hospital_id = hr_training_schedule.company_id
where
hr_training_schedule.`training_end_date` between  $P{fromDate}and $P{toDate}and hr_training_schedule.`training_start_date` between  $P{fromDate}and $P{toDate}

$P!{qryString}
]]></queryString>

