	<parameter name="SUBREPORT_DIR" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="inpatient_id" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="UHID_No" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select 
m.police_station,
p.p_first_name,
p.age,
a.administrative_sex_name,
(CASE WHEN(m.patient_address!='') then pa.address else 'No Data' end) as address,
opd.admission_date,
inp.ad_no,
d.date_of_discharge,
(CASE WHEN(d.instructions_to_patient!='') then d.instructions_to_patient else 'No Data' end) as instructions_to_patient,
(CASE WHEN(e.emp_name!='') then e.emp_name else 'No Data' end) as emp_name,
(CASE WHEN(ds.discharge_status_name!='') then ds.discharge_status_name else 'No Data' end) as discharge_status_name,
(CASE WHEN(m.injury_details!='') then m.injury_details else 'No Data' end) as injury_details,
(CASE WHEN(d.remarks!='') then d.remarks else 'No Data' end) as remarks,
m.patient_condition,m.remarks,m.advise_on_discharge,m.treatment
 
from medico_legal_details m
LEFT OUTER JOIN patient p ON p.hin_id = m.hin_id
left outer join inpatient inp on inp.inpatient_id=m.inpatient_id
left outer join discharge d on d.inpatient_id=inp.inpatient_id
left outer join mas_employee e on d.doctor_id=e.employee_id
left outer join mas_discharge_status ds on ds.discharge_status_id=d.discharge_status_id
left outer join opd_patient_details opd on opd.id=inp.opd_patient_details_id
left outer join mas_administrative_sex  a on a.administrative_sex_id=p.sex_id
left outer join patient_address pa on pa.hin_id=m.hin_id
where   p.hin_no = $P{UHID_No}   and inp.inpatient_id=$P{inpatient_id}]]></queryString>

