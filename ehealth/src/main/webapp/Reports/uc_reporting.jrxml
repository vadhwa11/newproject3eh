from fa_scheme_wise_fund_allocate sf
left join fa_voucher_header h on h.scheme_id = sf.scheme_id
left join mas_scheme ms on ms.scheme_id = sf.scheme_id
left join mas_store_financial f on f.financial_id = sf.f_year_id
left join mas_hospital mh on mh.hospital_id = sf.hospital_id
where h.f_year_id=$P{financialYearId} and h.hospital_id = $P{hospitalId} 
group by ms.scheme_id,
sf.letter_no,sf.sanction_date,f.financial_year,mh.hospital_name]]></queryString>

