	<queryString><![CDATA[select ptt.hin_no,ptt.full_name,chgcode.charge_code_name as service_name,ophdr.bill_no,ophdr.bill_date,sum(opdtl.net_amt)
,mh.hospital_name
 from dg_orderdt dt
inner join bl_op_bill_header ophdr on (dt.bill_id=ophdr.op_bill_header_id and dt.bill_id is not null)

inner join mas_charge_code chgcode on dt.charge_code_id=chgcode.charge_code_id
inner join bl_op_bill_details opdtl on opdtl.op_bill_header_id=ophdr.op_bill_header_id
inner join patient ptt on ophdr.hin_id=ptt.hin_id 
inner join mas_hospital mh on ophdr.hospital_id=mh.hospital_id where ophdr.bill_date between $P{fromDate} and $P{toDate} and ophdr.hospital_id=$P{hospitalId} and dt.order_status='P'
group by ptt.hin_no,ptt.full_name,chgcode.charge_code_name,ophdr.bill_no,ophdr.bill_date,mh.hospital_name]]></queryString>

