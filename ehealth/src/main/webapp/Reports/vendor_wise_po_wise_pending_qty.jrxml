	<queryString><![CDATA[select sph.po_id,item.nomenclature,spd.unit_rate,coalesce(quantity_ordered,0) as quantity_orderd,coalesce(quantity_received,0) as quantity_received,
case when (coalesce(quantity_ordered,0)-coalesce(quantity_received,0) )>0 then coalesce(quantity_ordered,0)-coalesce(quantity_received,0) else 0 end as pending_qty,
spd.unit_rate * case when (coalesce(quantity_ordered,0)-coalesce(quantity_received,0) )>0 then coalesce(quantity_ordered,0)-coalesce(quantity_received,0) else 0 end as cost,
sum(spd.unit_rate * case when (coalesce(quantity_ordered,0)-coalesce(quantity_received,0) )>0 then coalesce(quantity_ordered,0)-coalesce(quantity_received,0) else 0 end) as total_cost,
sph.po_number,sph.po_date,sph.net_amount,supplier.supplier_name
from store_po_header sph,store_po_detail spd,mas_store_item item ,mas_store_supplier supplier where sph.po_id=spd.po_id and item.item_id=spd.item_id
and supplier.supplier_id=sph.supplier_id
and (case when (coalesce(quantity_ordered,0)-coalesce(quantity_received,0) )>0 then coalesce(quantity_ordered,0)-coalesce(quantity_received,0) else 0 end ) >0
and sph.po_date between $P{fromDate}  and  $P{toDate} and sph.hospital_id=$P{hospitalId} $P!{query_vendor_wise_po_wise_pending_qty}
group by item.nomenclature,spd.unit_rate,quantity_ordered,quantity_received,sph.po_number,sph.po_date,sph.net_amount,supplier.supplier_name,sph.po_id]]></queryString>

